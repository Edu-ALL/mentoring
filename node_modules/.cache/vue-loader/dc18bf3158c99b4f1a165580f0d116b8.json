ll>{{ element.uni_major }}</small>\r\n                <div class=\"trash\">\r\n                  <i class=\"fa-solid fa-trash text-white\"></i>\r\n                </div>\r\n              </div>\r\n            </template>\r\n          </draggable>\r\n        </div>\r\n\r\n        <!-- Accepted  -->\r\n        <div class=\"col\">\r\n          <div class=\"card border-1 my-2 py-2 text-center bg-primary\">\r\n            <h6 class=\"my-0\">Accepted</h6>\r\n          </div>\r\n\r\n          <draggable\r\n            v-model=\"list.uni_accepted\"\r\n            group=\"uniList\"\r\n            class=\"dragArea list-group\"\r\n            tag=\"waitlisted\"\r\n            :component-data=\"{ status: 'accepted' }\"\r\n            @start=\"drag = true\"\r\n            @end=\"drag = false\"\r\n            :move=\"newLog\"\r\n            item-key=\"name\"\r\n          >\r\n            <template #item=\"{ element }\">\r\n              <div\r\n                class=\"\r\n                  card-uni\r\n                  border-1\r\n                  shadow-sm\r\n                  px-3\r\n                  py-2\r\n                  mb-2\r\n                  text-center\r\n                  position-relative\r\n                  overflow-hidden\r\n                \"\r\n              >\r\n                <h6 class=\"my-0\">{{ element.uni_name }}</h6>\r\n                <small>{{ element.uni_major }}</small>\r\n                <div class=\"trash\">\r\n                  <i class=\"fa-solid fa-trash text-white\"></i>\r\n                </div>\r\n              </div>\r\n            </template>\r\n          </draggable>\r\n        </div>\r\n\r\n        <!-- Rejected  -->\r\n        <div class=\"col\">\r\n          <div class=\"card border-1 my-2 py-2 text-center bg-primary\">\r\n            <h6 class=\"my-0\">Rejected</h6>\r\n          </div>\r\n\r\n          <draggable\r\n            v-model=\"list.uni_rejected\"\r\n            group=\"uniList\"\r\n            class=\"dragArea list-group