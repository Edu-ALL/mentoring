        .get(this.$url + \"overview/transaction\", {\r\n          headers: {\r\n            Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n          },\r\n        })\r\n        .then((response) => {\r\n          this.count = response.data.data;\r\n          // console.log(this.count);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.getCount();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"]}]}