\n        });\r\n    },\r\n\r\n    getPage(link) {\r\n      this.$axios\r\n        .get(link, {\r\n          headers: {\r\n            Authorization: \"Bearer \" + this.$adminToken,\r\n          },\r\n        })\r\n        .then((response) => {\r\n          this.mails = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"]}]}