/components/library/pagination.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <div id=\"pagination\">\r\n    <nav class=\"\" v-if=\"datas.from != null\">\r\n      <hr class=\"my-2\" />\r\n      <div class=\"d-flex align-items-start justify-content-between\">\r\n        <div class=\"text-start\">Result: {{ datas.total }} data.</div>\r\n        <div class=\"text-end\">\r\n          <ul class=\"pagination\">\r\n            <li class=\"page-item\">\r\n              <a\r\n                class=\"page-link pointer\"\r\n                @click=\"getResult(datas.prev_page_url)\"\r\n              >\r\n                <i class=\"fa-solid fa-chevron-left\"></i>\r\n              </a>\r\n            </li>\r\n            <div v-for=\"(i, index) in datas.last_page\" :key=\"index\">\r\n              <li\r\n                class=\"page-item\"\r\n                v-if=\"datas.current_page - 2 < i && datas.current_page + 2 > i\"\r\n              >\r\n                <a\r\n                  class=\"page-link\"\r\n                  :class=\"\r\n                    datas.current_page == i ? 'bg-primary text-white' : ''\r\n                  \"\r\n                  href=\"#\"\r\n                  @click=\"\r\n                    getResult(\r\n                      datas.current_page < i\r\n                        ? datas.next_page_url\r\n                        : datas.prev_page_url\r\n                    )\r\n                  \"\r\n                >\r\n                  {{ i }}\r\n                </a>\r\n              </li>\r\n            </div>\r\n            <li class=\"page-item\">\r\n              <a\r\n                class=\"page-link pointer\"\r\n                @click=\"getResult(datas.next_page_url)\"\r\n              >\r\n                <i class=\"fa-solid fa-chevron-right\"></i>\r\n              </a>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n    </nav>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"pagination\",\r\n  props: {\r\n    datas: Object,\r\n  },\r\n  methods: {\r\n    getResult(link) {\r\n      this.$emit(\"result\", link);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"]}]}