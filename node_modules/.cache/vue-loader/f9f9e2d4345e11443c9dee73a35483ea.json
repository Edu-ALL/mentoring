AAE,CAAC,CAAC,EAAE;QACV,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MAChB;MACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACrB,CAAC;EACH,CAAC;EACD,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;IACR,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EAChB,CAAC;AACH,CAAC","file":"C:/Users/dl6/Documents/Project/mentoring/src/components/admin/user/detail/student-activity/meeting.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <!-- {{ student_meeting }} -->\r\n  <div id=\"meetings\">\r\n    <div class=\"border p-3 rounded mt-3\">\r\n      <div class=\"table-responsive\">\r\n        <table class=\"table align-middle\">\r\n          <thead>\r\n            <tr class=\"text-center\">\r\n              <th>No</th>\r\n              <th>Call with</th>\r\n              <th>Subject</th>\r\n              <th>Date & Time</th>\r\n              <th>Status</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr class=\"text-center\" v-if=\"student_meeting?.data?.length == 0\">\r\n              <td class=\"text-muted\" colspan=\"5\">No data!</td>\r\n            </tr>\r\n            <tr\r\n              class=\"text-center\"\r\n              v-for=\"(i, index) in student_meeting.data\"\r\n              :key=\"index\"\r\n            >\r\n              <td>{{ parseInt(index) + 1 }}</td>\r\n\r\n              <td>\r\n                {{\r\n                  i.users.first_name +\r\n                  \" \" +\r\n                  i.users.last_name +\r\n                  \" - \" +\r\n                  i.call_with\r\n                }}\r\n              </td>\r\n              <td style=\"text-transform: capitalize\">{{ i.module }}</td>\r\n              <td>\r\n                <small>\r\n                  {{ $customDate.date(i.call_date) }} <br />\r\n                  {{ $customDate.time(i.call_date) }}\r\n                </small>\r\n              </td>\r\n              <td style=\"text-transform: capitalize\">\r\n                <i\r\n                  class=\"fa-solid fa-ban text-danger\"\r\n                  v-if=\"i.call_status == 'rejected'\"\r\n                ></i>\r\n                <i\r\n                  class=\"fa-solid fa-clock text-info\"\r\n                  v-if=\"i.call_status == 'waiting'\"\r\n                ></i>\r\n                <i\r\n                  class=\"fa-solid fa-times-circle text-danger\"\r\n                  v-if=\"i.call_status == 'canceled'\"\r\n                ></i>\r\n                <i\r\n                  class=\"fa-solid fa-check-circle text-success\"\r\n                  v-if=\"i.call_status == 'finished'\"\r\n                ></i>\r\n                {{ i.call_status }}\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n\r\n      <v-pagination :datas=\"student_meeting\" @result=\"getPage\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"studentMeeting\",\r\n  data() {\r\n    return {\r\n      student_meeting: [],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    async getData() {\r\n      this.$alert.loading();\r\n      const id = this.$route.params.key;\r\n      try {\r\n        const response = await this.$axios.get(\r\n          \"mentor/list/activities/1-on-1-call?student=\" + id\r\n        );\r\n        this.student_meeting = response.data.data;\r\n        this.$alert.close();\r\n        // console.log(response);\r\n      } catch (e) {\r\n        this.$alert.close();\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async getPage(link) {\r\n      this.student_meeting = [];\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.get(link);\r\n\r\n        this.student_meeting = response.data.data;\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e);\r\n      }\r\n      this.$alert.close();\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n"]}]}