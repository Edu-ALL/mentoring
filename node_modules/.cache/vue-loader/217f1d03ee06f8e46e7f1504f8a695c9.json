               <button\r\n                      class=\"btn-mentoring btn-outline-success btn-sm py-1 px-3\"\r\n                      @click=\"attendance(i.id)\"\r\n                    >\r\n                      Yes\r\n                    </button>\r\n                  </td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"card-white shadow-sm mt-2\">\r\n          <h6 class=\"act-title text-secondary\">\r\n            <div class=\"float-end\">Upcoming</div>\r\n            <div class=\"\">Group Meetings</div>\r\n          </h6>\r\n          <div class=\"card-activity mentoring-scroll table-responsive\">\r\n            <table class=\"table\">\r\n              <tbody>\r\n                <tr\r\n                  class=\"text-center\"\r\n                  v-if=\"recent?.group?.upcoming?.length == 0\"\r\n                >\r\n                  <td class=\"py-2\">No Group Meeting Yet</td>\r\n                </tr>\r\n                <tr\r\n                  v-for=\"i in recent.group.upcoming\"\r\n                  :key=\"i\"\r\n                  class=\"py-0 align-middle\"\r\n                >\r\n                  <td>{{ i.group_project.project_name }}</td>\r\n                  <td>{{ i.meeting_subject }}</td>\r\n                  <td class=\"text-center\">\r\n                    {{ $customDate.date(i.meeting_date) }} <br />\r\n                    {{ $customDate.time(i.meeting_date) }}\r\n                  </td>\r\n                  <td class=\"text-end\">\r\n                    <button\r\n                      class=\"btn-mentoring btn-primary btn-sm py-1 px-3\"\r\n                      @click=\"joinZoom(i.meeting_link)\"\r\n                    >\r\n                      Join\r\n                    </button>\r\n                  </td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <!-- Last Meeting  -->\r\n  <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n  <div\r\n    class=\"vue-side mentoring-scroll shadow\"\r\n    :class=\"modal == 'add' ? 'active' : ''\"\r\n  >\r\n    <div class=\"d-flex w-100 justify-content-between\">\r\n      <h6 class=\"mb-0\">Last Meeting Minutes</h6>\r\n      <div class=\"text-end\">\r\n        <button\r\n          class=\"btn btn-sm btn-mentoring btn-outline-info py-1\"\r\n          @click=\"view_status = 'add'\"\r\n          v-if=\"view_status == 'view'\"\r\n        >\r\n          Edit\r\n        </button>\r\n      </div>\r\n    </div>\r\n    <hr class=\"my-2\" />\r\n    <!-- ADD  -->\r\n    <form @submit.prevent=\"handleSubmit\">\r\n      <div class=\"row row-cols-md-2 row-cols-1\">\r\n        <div class=\"col mb-1\">\r\n          <input-group>\r\n            <span class=\"bg-white\">Academic Performance</span>\r\n            <textarea\r\n              class=\"form-mentoring w-100\"\r\n              v-model=\"meeting_minutes.academic_performance\"\r\n              rows=\"5\"\r\n              required\r\n            ></textarea>\r\n          </input-group>\r\n        </div>\r\n        <div class=\"col mb-1\">\r\n          <input-group>\r\n            <span class=\"bg-white\">Exploration</span>\r\n            <textarea\r\n              class=\"form-mentoring w-100\"\r\n              v-model=\"meeting_minutes.exploration\"\r\n              rows=\"5\"\r\n              required\r\n            ></textarea>\r\n          </input-group>\r\n        </div>\r\n        <div class=\"col mb-1\">\r\n          <input-group>\r\n            <span class=\"bg-white\">Writing Skills</span>\r\n            <textarea\r\n              class=\"form-mentoring w-100\"\r\n              v-model=\"meeting_minutes.writing_skills\"\r\n              rows=\"5\"\r\n              required\r\n            ></textarea>\r\n          </input-group>\r\n        </div>\r\n        <div class=\"col mb-1\">\r\n          <input-group>\r\n            <span class=\"bg-white\">Personal Brand</span>\r\n            <textarea\r\n              class=\"form-mentoring w-100\"\r\n              v-model=\"meeting_minutes.personal_brand\"\r\n              rows=\"5\"\r\n              required\r\n            ></textarea>\r\n          </input-group>\r\n        </div>\r\n      </div>\r\n      <div class=\"mb-1\">\r\n        <input-group>\r\n          <span class=\"bg-white\">Mentor Todos</span>\r\n          <textarea\r\n            class=\"form-mentoring w-100\"\r\n            v-model=\"meeting_minutes.mt_todos_note\"\r\n            rows=\"5\"\r\n            required\r\n          ></textarea>\r\n        </input-group>\r\n      </div>\r\n      <div class=\"mb-1\">\r\n        <input-group>\r\n          <span class=\"bg-white\">Mentee Todos</span>\r\n          <textarea\r\n            class=\"form-mentoring w-100\"\r\n            v-model=\"meeting_minutes.st_todos_note\"\r\n            rows=\"5\"\r\n            required\r\n          ></textarea>\r\n        </input-group>\r\n      </div>\r\n      <hr />\r\n      <div class=\"text-end\">\r\n        <button class=\"btn-mentoring btn-sm bg-primary py-1 px-3\" type=\"submit\">\r\n          <i class=\"fa-solid fa-save me-2\"></i>\r\n          Save Changes\r\n        </button>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"mentorDashboard\",\r\n  data() {\r\n    return {\r\n      total: [],\r\n      recent: {\r\n        meeting: [],\r\n        group: [],\r\n      },\r\n      modal: \"\",\r\n      meeting_minutes: {\r\n        st_act_id: \"\",\r\n        academic_performance: \"\",\r\n        exploration: \"\",\r\n        writing_skills: \"\",\r\n        personal_brand: \"\",\r\n        mt_todos_note: \"\",\r\n        st_todos_note: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    redirect(menu, submenu) {\r\n      this.$router.push({ path: \"/mentor/\" + menu + \"/\" + submenu });\r\n    },\r\n\r\n    async getTotal() {\r\n      try {\r\n        const response = await this.$axios.get(\"../v2/overview/mentor/total\");\r\n        this.total = response.data.data;\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async getRecentMeeting() {\r\n      try {\r\n        const response = await this.$axios.get(\r\n          \"../v2/list/activities/1-on-1-call/upcoming/recent\"\r\n        );\r\n        this.recent.meeting = response.data.data;\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async getRecentGroupMeeting() {\r\n      try {\r\n        const response = await this.$axios.get(\r\n          \"list/group/meeting/upcoming/recent\"\r\n        );\r\n        this.recent.group = response.data.data;\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    joinZoom(link) {\r\n      window.open(link, \"_blank\");\r\n    },\r\n\r\n    detail(i) {\r\n      this.modal = \"add\";\r\n      this.meeting_minutes = {\r\n        st_act_id: i,\r\n        academic_performance: \"\",\r\n        exploration: \"\",\r\n        writing_skills: \"\",\r\n        personal_brand: \"\",\r\n        mt_todos_note: \"\",\r\n        st_todos_note: \"\",\r\n      };\r\n    },\r\n\r\n    async handleSubmit() {\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\r\n          \"create/meeting-minutes\",\r\n          this.meeting_minutes\r\n        );\r\n\r\n        this.modal = \"\";\r\n\r\n        this.getRecentMeeting();\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        // console.log(response);\r\n      } catch (e) {\r\n        // console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again.\");\r\n      }\r\n    },\r\n\r\n    async attendance(id) {\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.put(\r\n          \"update/group/meeting/attendance/\" + id\r\n        );\r\n\r\n        this.getRecentGroupMeeting();\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        // console.log(response);\r\n      } catch (e) {\r\n        // console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again.\");\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.getTotal();\r\n    this.getRecentMeeting();\r\n    this.getRecentGroupMeeting();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.shadow-count {\r\n  position: absolute;\r\n  top: -30px;\r\n  right: -20px;\r\n  font-weight: 900;\r\n  font-size: 7em;\r\n  line-height: 1em;\r\n  padding: 0;\r\n  margin: 0;\r\n  color: #fff;\r\n  opacity: 0.2;\r\n}\r\n\r\n.card-activity {\r\n  height: 200px;\r\n  overflow: auto;\r\n}\r\n\r\n.vue-side {\r\n  position: fixed;\r\n  top: 0;\r\n  right: -50%;\r\n  height: 100vh;\r\n  padding: 20px;\r\n  width: 40%;\r\n  background: white;\r\n  overflow: auto;\r\n  z-index: 99999;\r\n  transition: all 0.3s;\r\n}\r\n\r\n.vue-side.active {\r\n  right: 0 !important;\r\n}\r\n\r\n@media only screen and (max-width: 600px) {\r\n  .vue-side {\r\n    position: fixed;\r\n    top: 0;\r\n    right: -90%;\r\n    width: 90%;\r\n  }\r\n}\r\n</style>"]}]}