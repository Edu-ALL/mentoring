\"text\"\r\n                name=\"text\"\r\n                class=\"form-control v-form text-center\"\r\n                placeholder=\"Verification Code\"\r\n              />\r\n              <p class=\"text-danger small\" v-if=\"error_reset.email\">\r\n                {{ error_reset.email[0] }}\r\n              </p>\r\n            </div>\r\n            <div class=\"text-center\">\r\n              <button\r\n                class=\"btn btn-allin bg-secondary my-3 px-5\"\r\n                type=\"submit\"\r\n              >\r\n                <strong> Verify Now </strong>\r\n              </button>\r\n              <hr />\r\n              <p v-if=\"!resend\">\r\n                Please wait <b class=\"text-secondary\">{{ time }}</b> to resend\r\n                the verification code.\r\n              </p>\r\n\r\n              <p v-if=\"resend\">\r\n                I haven't received the verification code, <br />\r\n                Please\r\n                <u class=\"text-secondary\" @click=\"resendProcess\">send again!</u>\r\n              </p>\r\n            </div>\r\n          </div>\r\n        </transition>\r\n        <transition name=\"fade\">\r\n          <div v-if=\"verified\">\r\n            <p class=\"text-center\">\r\n              Your password has been successfully reset, <br />\r\n              please\r\n              <u\r\n                class=\"text-secondary pointer\"\r\n                @click=\"this.$emit('show', 'login')\"\r\n                >sign in now!</u\r\n              >\r\n            </p>\r\n          </div>\r\n        </transition>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"verify\",\r\n  data() {\r\n    return {\r\n      verified: false,\r\n      resend: false,\r\n      time: 60,\r\n      reset: {\r\n        email: \"\",\r\n        password: \"\",\r\n        password_confirm: \"\",\r\n      },\r\n      error_reset: [],\r\n    };\r\n  },\r\n  methods: {\r\n    verifyProcess() {\r\n      this.changed = true;\r\n    },\r\n    timeCountDown() {\r\n      if (this.time > 0) {\r\n        setTimeout(() => {\r\n          this.time -= 1;\r\n          this.timeCountDown();\r\n        }, 1000);\r\n        this.resend = false;\r\n      } else if (this.time == 0) {\r\n        this.resend = true;\r\n      }\r\n    },\r\n    resendProcess() {\r\n      this.time = 60;\r\n      this.resend = false;\r\n      this.timeCountDown();\r\n    },\r\n  },\r\n  created() {\r\n    this.timeCountDown();\r\n  },\r\n};\r\n</script>\r\n"]}]}