    <div class=\"card-body py-1 text-center\">\r\n              <i class=\"fa-solid fa-plus me-2\"></i>\r\n              New Todos\r\n            </div>\r\n          </div>\r\n\r\n          <div\r\n            class=\"text-center p-1 text-muted border\"\r\n            v-if=\"todos_waiting?.waiting?.length == 0\"\r\n          >\r\n            No data!\r\n          </div>\r\n\r\n          <div\r\n            class=\"card border-1 shadow-sm my-2 card-todos\"\r\n            v-for=\"(i, index) in todos_waiting.waiting\"\r\n            :key=\"index\"\r\n          >\r\n            <div class=\"card-body p-0\">\r\n              <div\r\n                class=\"\r\n                  d-flex\r\n                  justify-content-between\r\n                  position-relative\r\n                  overflow-hidden\r\n                \"\r\n              >\r\n                <!-- {{ i.id }} -->\r\n                <div\r\n                  class=\"p-3 d-flex justify-content-between\"\r\n                  style=\"width: 90%\"\r\n                >\r\n                  <div style=\"width: 10%\">\r\n                    <i\r\n                      class=\"fa-solid fa-check-circle me-2 checklist pointer\"\r\n                      @click=\"switchTodos(i.id, 3)\"\r\n                    ></i>\r\n                  </div>\r\n                  <!-- </div> -->\r\n                  <div\r\n                    class=\"pointer\"\r\n                    style=\"width: 90%\"\r\n                    @click=\"todos_id = i.id\"\r\n                  >\r\n                    <div class=\"text-start p-0 m-0\">\r\n                      <transition name=\"fade\">\r\n                        <div class=\"short-desc\">\r\n                          {{ i.task_name }}\r\n                        </div>\r\n                      </transition>\r\n                      <transition name=\"fade\">\r\n                        <div class=\"long-desc\" v-if=\"todos_id == i.id\">\r\n                          {{ i.description }}\r\n                        </div>\r\n                      </transition>\r\n                      <small class=\"d-block mt-3 text-muted\">\r\n                        <i class=\"fa-solid fa-calendar me-2\"></i>\r\n                        {{ $customDate.date(i.due_date) }}\r\n                      </small>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"todos_action m-0 p-0\" style=\"width: 10%\">\r\n                  <div\r\n                    class=\"\r\n                      bg-danger\r\n                      text-center\r\n                      h-100\r\n                      d-flex\r\n                      align-items-center\r\n                      justify-content-center\r\n                    \"\r\n                    style=\"border-radius: 0 5px 5px 0\"\r\n                    @click=\"removeTodos(i.id)\"\r\n                  >\r\n                    <i class=\"fa-solid fa-trash text-white\"></i>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"col\">\r\n          <div class=\"card border-1 my-2 py-2 text-center bg-primary\">\r\n            <h6 class=\"my-0\">Confirmation Needs</h6>\r\n          </div>\r\n\r\n          <div\r\n            class=\"text-center p-1 text-muted border\"\r\n            v-if=\"todos_conf_need?.confirmation_need?.length == 0\"\r\n          >\r\n            No data!\r\n          </div>\r\n          <div\r\n            class=\"card border-1 shadow-sm my-2 card-todos\"\r\n            v-for=\"(i, index) in todos_conf_need.confirmation_need\"\r\n            :key=\"index\"\r\n          >\r\n            <div class=\"card-body p-0\">\r\n              <div\r\n                class=\"\r\n                  d-flex\r\n                  justify-content-between\r\n                  position-relative\r\n                  overflow-hidden\r\n                \"\r\n              >\r\n                <div\r\n                  class=\"p-3 d-flex justify-content-between\"\r\n                  style=\"width: 90%\"\r\n                >\r\n                  <div\r\n                    class=\"pointer\"\r\n                    style=\"width: 90%\"\r\n                    @click=\"todos_id = i.id\"\r\n                  >\r\n                    <div class=\"text-start p-0 m-0\">\r\n                      <transition name=\"fade\">\r\n                        <div class=\"short-desc\">\r\n                          {{ i.task_name }}\r\n                        </div>\r\n                      </transition>\r\n                      <transition name=\"fade\">\r\n                        <div class=\"long-desc\" v-if=\"todos_id == i.id\">\r\n                          {{ i.description }}\r\n                        </div>\r\n                      </transition>\r\n                      <small class=\"d-block mt-3 text-muted\">\r\n                        <i class=\"fa-solid fa-calendar me-2\"></i\r\n                        >{{ $customDate.date(i.due_date) }}\r\n                      </small>\r\n                    </div>\r\n                    <div class=\"d-flex mt-2\">\r\n                      <div class=\"checklist\" @click=\"switchTodos(i.id, 3)\">\r\n                        <i class=\"fa-solid fa-check-circle me-2\"> </i\r\n                        ><label>Confirm</label>\r\n                      </div>\r\n                      <div class=\"reject\" @click=\"switchTodos(i.id, 2)\">\r\n                        <i class=\"fa-solid fa-times-circle mx-2\"></i\r\n                        ><span>Reject</span>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"todos_action m-0 p-0\" style=\"width: 10%\">\r\n                  <div\r\n                    class=\"\r\n                      bg-danger\r\n                      text-center\r\n                      h-100\r\n                      d-flex\r\n                      align-items-center\r\n                      justify-content-center\r\n                    \"\r\n                    style=\"border-radius: 0 5px 5px 0\"\r\n                    @click=\"removeTodos(i.id)\"\r\n                  >\r\n                    <i class=\"fa-solid fa-trash text-white\"></i>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"col\">\r\n          <div class=\"card border-1 my-2 py-2 text-center bg-primary\">\r\n            <h6 class=\"my-0\">Completed</h6>\r\n          </div>\r\n\r\n          <div\r\n            class=\"text-center p-1 text-muted border\"\r\n            v-if=\"todos_confirmed?.completed?.length == 0\"\r\n          >\r\n            No data!\r\n          </div>\r\n          <div\r\n            class=\"card border-1 shadow-sm my-2 card-todos text-muted\"\r\n            style=\"background: #f2efef; opacity: 0.7\"\r\n            v-for=\"(i, index) in todos_confirmed.completed\"\r\n            :key=\"index\"\r\n          >\r\n            <div class=\"card-body p-0\">\r\n              <div\r\n                class=\"\r\n                  d-flex\r\n                  justify-content-between\r\n                  position-relative\r\n                  overflow-hidden\r\n                \"\r\n              >\r\n                <div\r\n                  class=\"p-3 d-flex justify-content-between\"\r\n                  style=\"width: 90%\"\r\n                >\r\n                  <div style=\"width: 10%\">\r\n                    <i\r\n                      class=\"fa-solid fa-check-circle me-2 text-success\"\r\n                      @click=\"switchTodos(i.id, 2)\"\r\n                    ></i>\r\n                  </div>\r\n                  <div\r\n                    class=\"pointer\"\r\n                    style=\"width: 90%\"\r\n                    @click=\"todos_id = i.id\"\r\n                  >\r\n                    <div class=\"text-start p-0 m-0\">\r\n                      <transition name=\"fade\">\r\n                        <div class=\"short-desc\">\r\n                          {{ i.task_name }}\r\n                        </div>\r\n                      </transition>\r\n                      <transition name=\"fade\">\r\n                        <div class=\"long-desc\" v-if=\"todos_id == i.id\">\r\n                          {{ i.description }}\r\n                        </div>\r\n                      </transition>\r\n                      <small class=\"d-block mt-3 text-muted\">\r\n                        <i class=\"fa-solid fa-calendar me-2\"></i>\r\n                        {{ $customDate.date(i.due_date) }}\r\n                      </small>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"todos_action m-0 p-0\" style=\"width: 10%\">\r\n                  <div\r\n                    class=\"\r\n                      bg-danger\r\n                      text-center\r\n                      h-100\r\n                      d-flex\r\n                      align-items-center\r\n                      justify-content-center\r\n                    \"\r\n                    style=\"border-radius: 0 5px 5px 0\"\r\n                    @click=\"removeTodos(i.id)\"\r\n                  >\r\n                    <i class=\"fa-solid fa-trash text-white\"></i>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n    <transition name=\"pop\">\r\n      <div class=\"vue-modal vue-modal-md\" v-if=\"modal == 'add'\">\r\n        <!-- {{ todos }} -->\r\n        <h6 class=\"my-0\">New Todos</h6>\r\n        <hr class=\"mb-1\" />\r\n        <form method=\"post\" @submit.prevent=\"handleSubmit\">\r\n          <div class=\"my-3\">\r\n            <input-group>\r\n              <input\r\n                type=\"text\"\r\n                class=\"form-control form-mentoring w-100\"\r\n                id=\"subject\"\r\n                v-model=\"todos.task_name\"\r\n                placeholder=\"Subject Name\"\r\n                required\r\n              />\r\n              <label for=\"subject\">Subject Name</label>\r\n            </input-group>\r\n          </div>\r\n          <div class=\"my-3\">\r\n            <input-group>\r\n              <input\r\n                type=\"date\"\r\n                class=\"form-control form-mentoring w-100\"\r\n                v-model=\"todos.due_date\"\r\n                placeholder=\"Date\"\r\n                id=\"deadline\"\r\n                required\r\n              />\r\n              <label for=\"deadline\">Deadline</label>\r\n            </input-group>\r\n          </div>\r\n          <div class=\"my-3\">\r\n            <input-group>\r\n              <textarea\r\n                cols=\"30\"\r\n                rows=\"5\"\r\n                class=\"form-control form-mentoring w-100\"\r\n                v-model=\"todos.description\"\r\n                placeholder=\"project\"\r\n                id=\"desc\"\r\n                required\r\n              ></textarea>\r\n              <label class=\"bg-white\" for=\"desc\">Project Description</label>\r\n            </input-group>\r\n          </div>\r\n          <hr class=\"my-1 mb-3\" />\r\n          <div class=\"d-flex justify-content-between\">\r\n            <button\r\n              class=\"btn-mentoring btn-outline-danger py-1 px-3\"\r\n              type=\"button\"\r\n              @click=\"modal = ''\"\r\n            >\r\n              Cancel\r\n            </button>\r\n            <button class=\"btn-mentoring btn-success py-1 px-3\" type=\"submit\">\r\n              Save\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n\r\n  <div class=\"vue-modal-overlay\" v-if=\"modal != ''\"></div>\r\n  <!-- Completed Group  -->\r\n  <transition name=\"pop\">\r\n    <div\r\n      class=\"vue-modal vue-modal-sm bg-primary text-center\"\r\n      v-if=\"modal == 'confirm'\"\r\n    >\r\n      <i class=\"fa-solid fa-circle-exclamation mx-1 fa-2xl\"></i>\r\n      <h5 class=\"mt-3 mb-3\">Are you sure to delete?</h5>\r\n      <!-- <h5 class=\"mt-3 mb-3\" v-if=\"confirm_status == 'in progress'\">\r\n        Are you sure this group back to in progress?\r\n      </h5> -->\r\n      <button\r\n        class=\"btn-mentoring btn-sm py-1 btn-danger mx-1\"\r\n        @click=\"modal = ''\"\r\n      >\r\n        Cancel\r\n      </button>\r\n      <button\r\n        class=\"btn-mentoring btn-sm py-1 btn-outline-success mx-1\"\r\n        @click=\"confirmDelete()\"\r\n      >\r\n        Yes\r\n      </button>\r\n    </div>\r\n  </transition>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"studentTodos\",\r\n  props: {\r\n    menus: Object,\r\n  },\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      todos_id: \"\",\r\n      todos: {\r\n        student_id: \"\",\r\n        task_name: \"\",\r\n        description: \"\",\r\n        due_date: \"\",\r\n      },\r\n      input: { todos: \"text\" },\r\n      todos_waiting: [],\r\n      todos_conf_need: [],\r\n      todos_confirmed: [],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    async getData() {\r\n      this.$alert.loading();\r\n      const id = this.$route.params.key;\r\n      try {\r\n        const response = await this.$axios.get(\"select/todos/\" + id);\r\n        this.todos_waiting = response.data.data;\r\n        this.todos_conf_need = response.data.data;\r\n        this.todos_confirmed = response.data.data;\r\n        // console.log(response);\r\n        this.$alert.close();\r\n      } catch (e) {\r\n        this.$alert.close();\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async handleSubmit() {\r\n      // const id = this.menus.submenu;\r\n      this.modal = \"\";\r\n      this.todos.student_id = this.menus.submenu;\r\n\r\n      // console.log(this.todos);\r\n\r\n      this.$alert.loading();\r\n\r\n      try {\r\n        const response = await this.$axios.post(\"create/todos\", this.todos);\r\n\r\n        this.todos.task_name = \"\";\r\n        this.todos.description = \"\";\r\n        this.todos.due_date = \"\";\r\n\r\n        // console.log(response.data);\r\n        this.getData();\r\n        this.$alert.toast(\"success\", response.data.message);\r\n      } catch (e) {\r\n        console.log(e.response.data);\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n\r\n    async switchTodos(j, s) {\r\n      // alert(id);\r\n      try {\r\n        const response = await this.$axios.post(\"switch/todos\", {\r\n          todos_id: j,\r\n          new_status: s,\r\n        });\r\n        console.log(response);\r\n        if (response.data.success) {\r\n          this.getData();\r\n          this.$alert.toast(\"success\", response.data.message);\r\n        } else {\r\n          this.$alert.toast(\"error\", response.data.error);\r\n        }\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n\r\n    async removeTodos(id) {\r\n      this.modal = \"confirm\";\r\n      this.todos_id = id;\r\n      // alert(id);\r\n    },\r\n\r\n    async confirmDelete() {\r\n      this.$alert.loading();\r\n      this.modal = \"\";\r\n      try {\r\n        const response = await this.$axios.delete(\r\n          \"delete/todos/\" + this.todos_id\r\n          // { remove_tag: j }\r\n        );\r\n        // console.log(response);\r\n        if (response.data.success) {\r\n          this.getData();\r\n          this.$alert.toast(\"success\", response.data.message);\r\n        } else {\r\n          this.$alert.toast(\"error\", response.data.error);\r\n        }\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.short-desc {\r\n  font-size: 1em !important;\r\n  font-weight: 600;\r\n}\r\n\r\n.long-desc {\r\n  font-size: 0.9em !important;\r\n}\r\n\r\n.card-todos small {\r\n  font-size: 0.7em;\r\n}\r\n\r\n.todos_action {\r\n  position: absolute;\r\n  z-index: 1;\r\n  right: -40px;\r\n  height: 100%;\r\n  transition: all 0.3s;\r\n  cursor: pointer;\r\n}\r\n\r\n.checklist,\r\n.reject {\r\n  color: #223872;\r\n}\r\n\r\n.checklist:hover,\r\n.checklist:hover i {\r\n  color: green;\r\n}\r\n\r\n.reject:hover,\r\n.reject:hover i {\r\n  color: red;\r\n}\r\n\r\n.card-todos:hover .todos_action {\r\n  right: 0;\r\n  z-index: 1;\r\n  height: 100%;\r\n}\r\n</style>"]}]}