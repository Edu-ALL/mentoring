ast.fire({\n        icon: status,\n        title: title,\n      });\n    },\n  },\n  created() {\n    const user = JSON.parse(sessionStorage.getItem(\"user\"));\n    if (user) {\n      this.token = user.data.token;\n      this.role = user.data.role_id;\n    } else {\n      this.toast(\"warning\", \"Please log in first !\");\n      this.$router.push({ path: \"/\" });\n    }\n  },\n};\n</script>"]}]}