ss=\"card border-0 shadow-sm\">\n                <div class=\"card-body\">\n                  <div class=\"call-picture\">\n                    <img src=\"\" alt=\"\" />\n                  </div>\n                </div>\n              </div>\n            </div>\n            <div class=\"col-md-8\">\n              <div class=\"card border-0 shadow-sm\">\n                <div class=\"card-body\">\n                  <div class=\"call-name\">Devi Kasih</div>\n                  <div class=\"call-desc\">Lorem ipsum</div>\n                  <hr class=\"my-2\" />\n                  <p class=\"my-0 mb-2\">Select 1-on-1 Call Date & Time</p>\n                  <div class=\"float-end\">\n                    <button type=\"date\">Test</button>\n                    <v-datepicker placeholder=\"Select\"></v-datepicker>\n                  </div>\n                  <div class=\"day-list\">\n                    <button class=\"btn-day\">\n                      Monday <br />\n                      07 Feb\n                    </button>\n                    <button class=\"btn-day d-md-inline d-none\">\n                      Wednesday <br />\n                      08 Feb\n                    </button>\n                    <button class=\"btn-day d-md-inline d-none\">\n                      Thursday <br />\n                      09 Feb\n                    </button>\n                  </div>\n                  <div class=\"time-list my-2\">\n                    <div class=\"card\">\n                      <div class=\"card-body pt-3\">\n                        <div\n                          class=\"\n                            row row-cols-md-1 row-cols-1\n                            justify-content-center\n                          \"\n                        >\n                          <div class=\"col\">\n                            <p class=\"mb-1\">Morning</p>\n                            <button\n                              class=\"btn-time mb-2\"\n                              