tion name=\"fade\">\n          <div v-if=\"!verified\">\n            <div class=\"mb-3\">\n              <input\n                v-model=\"reset.email\"\n                type=\"text\"\n                name=\"text\"\n                class=\"form-control v-form text-center\"\n                placeholder=\"Verification Code\"\n              />\n              <p class=\"text-danger small\" v-if=\"error_reset.email\">\n                {{ error_reset.email[0] }}\n              </p>\n            </div>\n            <div class=\"text-center\">\n              <button\n                class=\"btn btn-allin bg-secondary my-3 px-5\"\n                type=\"submit\"\n              >\n                <strong> Verify Now </strong>\n              </button>\n              <hr />\n              <p v-if=\"!resend\">\n                Please wait <b class=\"text-secondary\">{{ time }}</b> to resend\n                the verification code.\n              </p>\n\n              <p v-if=\"resend\">\n                I haven't received the verification code, <br />\n                Please\n                <u class=\"text-secondary\" @click=\"resendProcess\">send again!</u>\n              </p>\n            </div>\n          </div>\n        </transition>\n        <transition name=\"fade\">\n          <div v-if=\"verified\">\n            <p class=\"text-center\">\n              Your password has been successfully reset, <br />\n              please\n              <u\n                class=\"text-secondary pointer\"\n                @click=\"this.$emit('show', 'login')\"\n                >sign in now!</u\n              >\n            </p>\n          </div>\n        </transition>\n      </form>\n    </div>\n  </div>\n</template>\n<script>\nexport default {\n  name: \"verify\",\n  data() {\n    return {\n      verified: false,\n      resend: false,\n      time: 60,\n      reset: {\n        email: \"\",\n        password: \"\",\n        password_confirm: \"\",\n      },\n      error_reset: [],\n    };\n  },\n  methods: {\n    verifyProcess() {\n      this.changed = true;\n    },\n    timeCountDown() {\n      if (this.time > 0) {\n        setTimeout(() => {\n          this.time -= 1;\n          this.timeCountDown();\n        }, 1000);\n        this.resend = false;\n      } else if (this.time == 0) {\n        this.resend = true;\n      }\n    },\n    resendProcess() {\n      this.time = 60;\n      this.resend = false;\n      this.timeCountDown();\n    },\n  },\n  created() {\n    this.timeCountDown();\n  },\n};\n</script>\n"]}]}