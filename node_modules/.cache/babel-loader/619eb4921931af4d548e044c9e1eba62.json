,GAAR,CAAY,YAAE,QAAd;;AAZY;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcf,KAfM;AAiBP,IAAA,WAjBO,uBAiBK,CAjBL,EAiBQ;AACb,UAAI,IAAG,GAAI,IAAI,CAAC,KAAL,CAAW,CAAX,CAAX;AACA,UAAI,KAAI,GAAI,IAAI,CAAC,KAAL,CAAW,IAAG,IAAK,KAAK,EAAV,CAAd,CAAZ;AAEA,UAAI,mBAAkB,GAAI,IAAG,IAAK,KAAK,EAAV,CAA7B;AACA,UAAI,OAAM,GAAI,IAAI,CAAC,KAAL,CAAW,mBAAkB,GAAI,EAAjC,CAAd;AAEA,UAAI,mBAAkB,GAAI,mBAAkB,GAAI,EAAhD;AACA,UAAI,OAAM,GAAI,IAAI,CAAC,IAAL,CAAU,mBAAV,CAAd;AAEA,UAAI,IAAG,GACL,CAAC,KAAI,GAAI,CAAR,GAAY,KAAZ,GAAoB,MAAM,KAA3B,IACA,GADA,IAEC,OAAM,GAAI,CAAV,GAAc,OAAd,GAAwB,MAAM,OAF/B,IAGA,GAHA,IAIC,OAAM,GAAI,CAAV,GAAc,OAAd,GAAwB,MAAM,OAJ/B,CADF;AAMA,aAAO,IAAP;AACD,KAlCM;AAoCP,IAAA,UApCO,sBAoCI,IApCJ,EAoCU,QApCV,EAoCoB;AACzB,UAAI,OAAM,GAAK,IAAG,GAAI,QAAR,GAAoB,GAAlC;AACA,aAAO,OAAP;AACD;AAvCM,GARI;AAkDb,EAAA,OAlDa,qBAkDH;AACR,SAAK,OAAL;AACD;AApDY,CAAf","sourcesContent":["<template>\r\n  <div id=\"webinars\">\r\n    <div class=\"border p-3 rounded mt-3\">\r\n      <div class=\"table-responsive\">\r\n        <table class=\"table align-middle\">\r\n          <thead>\r\n            <tr class=\"text-center\">\r\n              <th>No</th>\r\n              <th>Topic</th>\r\n              <th>Category</th>\r\n              <th>Date & Time</th>\r\n              <th>Duration</th>\r\n              <th>Long Time Watching</th>\r\n              <th>Status</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr class=\"text-center\" v-if=\"student_webinar?.data?.length == 0\">\r\n              <td class=\"text-muted\" colspan=\"6\">No data!</td>\r\n            </tr>\r\n            <tr\r\n              class=\"text-center\"\r\n              v-for=\"(i, index) in student_webinar.data\"\r\n              :key=\"index\"\r\n            >\r\n              <td>{{ index + 1 }}</td>\r\n              <td nowrap>{{ i.programme_details.dtl_name }}</td>\r\n              <td nowrap style=\"text-transform: capitalize\">\r\n                {{ $customText.removeDash(i.programme_details.dtl_category) }}\r\n              </td>\r\n              <td>\r\n                <small>\r\n                  {{ $customDate.date(i.created_at) }} <br />\r\n                  {{ $customDate.time(i.created_at) }}\r\n                </small>\r\n              </td>\r\n              <td>\r\n                {{ convertTime(i.watch_detail.video_duration) }}\r\n              </td>\r\n              <td>\r\n                {{ convertTime(i.watch_detail.current_time) }}\r\n              </td>\r\n              <td nowrap>\r\n                <button\r\n                  class=\"btn-mentoring btn-sm py-1 px-3\"\r\n                  :class=\"\r\n                    percentage(\r\n                      i.watch_detail.current_time,\r\n                      i.watch_detail.video_duration\r\n                    ) > 95\r\n                      ? 'bg-primary'\r\n                      : 'bg-secondary'\r\n                  \"\r\n                  :disabled=\"\r\n                    percentage(\r\n                      i.watch_detail.current_time,\r\n                      i.watch_detail.video_duration\r\n                    ) > 95\r\n                  \"\r\n                >\r\n                  {{\r\n                    percentage(\r\n                      i.watch_detail.current_time,\r\n                      i.watch_detail.video_duration\r\n                    ) > 95\r\n                      ? \"Completed\"\r\n                      : \"In Progress\"\r\n                  }}\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n\r\n      <v-pagination :datas=\"student_webinar\" @result=\"getPage\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"webinarHistory\",\r\n  data() {\r\n    return {\r\n      student_webinar: [],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    async getData() {\r\n      this.$alert.loading();\r\n      const id = this.$route.params.key;\r\n      try {\r\n        const response = await this.$axios.get(\r\n          \"mentor/list/activities/webinar?student=\" + id\r\n        );\r\n        this.student_webinar = response.data.data;\r\n        this.$alert.close();\r\n        // console.log(response);\r\n      } catch (e) {\r\n        this.$alert.close();\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    convertTime(i) {\r\n      var secs = Math.round(i);\r\n      var hours = Math.floor(secs / (60 * 60));\r\n\r\n      var divisor_for_minutes = secs % (60 * 60);\r\n      var minutes = Math.floor(divisor_for_minutes / 60);\r\n\r\n      var divisor_for_seconds = divisor_for_minutes % 60;\r\n      var seconds = Math.ceil(divisor_for_seconds);\r\n\r\n      var time =\r\n        (hours > 9 ? hours : \"0\" + hours) +\r\n        \":\" +\r\n        (minutes > 9 ? minutes : \"0\" + minutes) +\r\n        \":\" +\r\n        (seconds > 9 ? seconds : \"0\" + seconds);\r\n      return time;\r\n    },\r\n\r\n    percentage(time, duration) {\r\n      var percent = (time / duration) * 100;\r\n      return percent;\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}