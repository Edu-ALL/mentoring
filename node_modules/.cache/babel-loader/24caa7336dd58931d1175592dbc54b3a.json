ccessfully reset, <br />\n              please\n              <u\n                class=\"text-secondary pointer\"\n                @click=\"this.$emit('show', 'login')\"\n                >sign in now!</u\n              >\n            </p>\n          </div>\n        </transition>\n      </form>\n    </div>\n  </div>\n</template>\n<script>\nexport default {\n  name: \"reset\",\n  data() {\n    return {\n      changed: false,\n      reset: {\n        email: \"\",\n        password: \"\",\n        password_confirm: \"\",\n      },\n      error_reset: [],\n    };\n  },\n  methods: {\n    resetProcess() {\n      this.changed = true;\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}