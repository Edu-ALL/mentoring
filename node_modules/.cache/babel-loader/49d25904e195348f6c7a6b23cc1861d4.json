AnC,SAAnB,CAAA;AAAA;AAYH,KAfL,E,CAKE,mBAAA,CAAuD,IAAvD,EAAA,WAAA,EAAuD,gBAAA,CAA5B,KAAA,CAAA,OAAA,CAAQ,IAAR,GAAe,KAAa,CAAvD,EAA+C;AAAA;AAA/C,K,EACA,mBAAA,CAGK,IAHL,EAGK,IAHL,EAGK,CAFH,WAEG,E,iBAFqC,MACxC,gBAAA,CAAG,CAAC,CAAC,UAAF,GAAY,GAAZ,GAAqB,CAAC,CAAC,SAA1B,C,EAAmC;AAAA;KAChC,CAHL,C,EAIA,mBAAA,CAA2D,IAA3D,EAA2D,IAA3D,EAA2D,CAAvD,WAAuD,E,iBAAnB,MAAC,gBAAA,CAAG,CAAC,CAAC,KAAL,C,EAAU;AAAA;KAAQ,CAA3D,C,EACA,mBAAA,CAGK,IAHL,EAGK,IAHL,EAGK,CAFH,WAEG,E,iBAF4C,MAC/C,gBAAA,CAAG,CAAC,CAAC,YAAF,IAAc,IAAd,GAAc,KAAd,GAAiC,CAAC,CAAC,YAAtC,C,EAAkD;AAAA;KAC/C,CAHL,C,CAXF,E;;AAAA,M,UAAA,C;GAeK,CAfL,E;;AAAA,GAgBM,EAjBR,CAkBM,CA3BR,CAmCI,EAN2B,KAAA,CAAA,OAAA,CAAQ,IAAR,IAAY,I,kBAA3C,mBAAA,CAGM,KAHN,EAAA,WAAA,EAGM,WAHN,C,qCAMI,EADJ,YAAA,CAAmD,uBAAnD,EAAmD;AAApC,IAAA,KAAK,EAAE,KAAA,CAAA,OAA6B;AAAnB,IAAA,QAAM,EAAE,QAAA,CAAA;AAAW,GAAnD,E,IAAA,E;;AAAA,I,qBAAA,CACI,CAlEN,CAmEI,CApEN,C","sourcesContent":["<template>\r\n  <div id=\"mentor\">\r\n    <div class=\"card-white\">\r\n      <div class=\"row mb-2 justify-content-end\">\r\n        <div class=\"col-md-3\">\r\n          <input-group>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-mentoring form-control w-100\"\r\n              v-model=\"search.name\"\r\n              @change=\"searchData\"\r\n              placeholder=\"Search\"\r\n              id=\"search\"\r\n            />\r\n            <label for=\"search\">Search</label>\r\n          </input-group>\r\n          <span\r\n            class=\"badge bg-primary px-3 d-inline-block float-end mt-1\"\r\n            v-if=\"search.bar && search.name != ''\"\r\n          >\r\n            {{ search.name }}\r\n            <i class=\"fa-solid fa-close ms-3 pointer\" @click=\"closeSearch\"></i>\r\n          </span>\r\n        </div>\r\n        <!-- <div class=\"col-md-6 text-md-end text-center\">\r\n        <button class=\"btn-mentoring btn-type-1 me-2\">\r\n          Sync Students Data\r\n        </button>\r\n        <button class=\"btn-mentoring btn-type-2\">Add a New Student</button>\r\n      </div> -->\r\n      </div>\r\n\r\n      <!-- {{ mentors }} -->\r\n      <table class=\"table table-bordered table-hover pointer\">\r\n        <thead>\r\n          <tr>\r\n            <th width=\"1%\" class=\"text-center\">No</th>\r\n            <th>Full Name</th>\r\n            <th>Email</th>\r\n            <th>Phone Number</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr\r\n            v-for=\"(i, index) in mentors.data\"\r\n            :key=\"index\"\r\n            @click=\"this.$router.push({ path: '/admin/user/mentor/' + i.id })\"\r\n          >\r\n            <td class=\"text-center\">{{ mentors.from + index }}</td>\r\n            <td>\r\n              <i class=\"fa-regular fa-user fa-fw\"></i>\r\n              {{ i.first_name + \" \" + i.last_name }}\r\n            </td>\r\n            <td><i class=\"fa-solid fa-at fa-fw\"></i> {{ i.email }}</td>\r\n            <td>\r\n              <i class=\"fa-solid fa-mobile-screen fa-fw\"></i>\r\n              {{ i.phone_number == null ? \"N/A\" : i.phone_number }}\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n\r\n      <div class=\"text-center\" v-if=\"mentors.from == null\">\r\n        <hr />\r\n        <h6>Sorry, data is not found</h6>\r\n      </div>\r\n\r\n      <v-pagination :datas=\"mentors\" @result=\"getPage\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"mentor\",\r\n  data() {\r\n    return {\r\n      search: {\r\n        bar: false,\r\n        name: \"\",\r\n      },\r\n      mentors: [],\r\n    };\r\n  },\r\n  methods: {\r\n    getData() {\r\n      this.$alert.loading();\r\n      this.$axios\r\n        .get(this.$url + \"list/user/mentor\")\r\n        .then((response) => {\r\n          this.$alert.close();\r\n          this.mentors = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$alert.close();\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    getPage(link) {\r\n      this.$axios\r\n        .get(link)\r\n        .then((response) => {\r\n          this.mentors = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    searchData() {\r\n      this.$alert.loading();\r\n      if (this.search.name == \"\") {\r\n        this.getData();\r\n        this.search.bar = false;\r\n        this.$alert.close();\r\n      } else {\r\n        this.$axios\r\n          .get(this.$url + \"find/user/mentor?keyword=\" + this.search.name)\r\n          .then((response) => {\r\n            this.$alert.close();\r\n            this.mentors = response.data.data;\r\n            this.search.bar = true;\r\n            // console.log(response);\r\n          })\r\n          .catch((error) => {\r\n            this.$alert.close();\r\n            console.log(error);\r\n          });\r\n      }\r\n    },\r\n\r\n    closeSearch() {\r\n      this.search.bar = false;\r\n      this.search.name = \"\";\r\n      this.getData();\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.badge {\r\n  border-radius: 15px;\r\n}\r\n</style>"],"sourceRoot":""}]}