nput-group class=\"w-100 ms-2\">\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-mentoring form-control w-100\"\r\n              v-model=\"search.name\"\r\n              @change=\"searchData\"\r\n              placeholder=\"Search\"\r\n              :readonly=\"keyword == ''\"\r\n              id=\"search\"\r\n            />\r\n            <label for=\"search\">Search</label>\r\n          </input-group>\r\n        </div>\r\n        <div class=\"col-12\">\r\n          <span\r\n            class=\"badge bg-primary px-3 d-inline-block float-end mt-1\"\r\n            v-if=\"search.bar && search.name != ''\"\r\n          >\r\n            {{ search.name }}\r\n            <i class=\"fa-solid fa-close ms-3 pointer\" @click=\"closeSearch\"></i>\r\n          </span>\r\n        </div>\r\n        <!-- <div class=\"col-md-6 text-md-end text-center\">\r\n        <button class=\"btn-mentoring btn-type-1 me-2\">\r\n          Sync Students Data\r\n        </button>\r\n        <button class=\"btn-mentoring btn-type-2\">Add a New Student</button>\r\n      </div> -->\r\n      </div>\r\n\r\n      <div class=\"table-responsive\">\r\n        <table class=\"table table-bordered align-middle table-hover\">\r\n          <thead>\r\n            <tr class=\"text-center\">\r\n              <th>No</th>\r\n              <th>Mentors Name</th>\r\n              <th>Mentees Name</th>\r\n              <th>Waiting</th>\r\n              <th>Confirmation Needs</th>\r\n              <th>Completed</th>\r\n              <th>Total</th>\r\n              <!-- <th>More</th> -->\r\n            </tr>\r\n          </thead>\r\n\r\n          <tbody>\r\n            <tr\r\n              class=\"text-center pointer\"\r\n              v-for=\"(i, index) in todos.data\"\r\n              :key=\"index\"\r\n            >\r\n              <td>{{ todos.from + parseInt(index) }}</td>\r\n              <td class=\"text-start\">\r\n                {{ i.mentor_name }}\r\n              </td>\r\n              <td class=\"text-start\">\r\n                {{ i.first_name + \" \" + i.last_name }}\r\n              </td>\r\n              <!-- <td style=\"text-transform: capitalize\">\r\n                {{ i.module + \" - \" + i.call_with }}\r\n              </td> -->\r\n              <td>\r\n                {{ i.waiting }}\r\n              </td>\r\n              <td>\r\n                {{ i.need_confirmation }}\r\n              </td>\r\n              <td>\r\n                {{ i.finished }}\r\n              </td>\r\n              <td>\r\n                {{ i.finished + i.need_confirmation }}\r\n              </td>\r\n              <!-- <td style=\"text-transform: capitalize\">\r\n                <i\r\n                  class=\"fa-solid fa-ban text-danger\"\r\n                  v-if=\"i.call_status == 'rejected'\"\r\n                ></i>\r\n                <i\r\n                  class=\"fa-solid fa-clock text-info\"\r\n                  v-if=\"i.call_status == 'waiting'\"\r\n                ></i>\r\n                <i\r\n                  class=\"fa-solid fa-times-circle text-danger\"\r\n                  v-if=\"i.call_status == 'canceled'\"\r\n                ></i>\r\n                <i\r\n                  class=\"fa-solid fa-check-circle text-success\"\r\n                  v-if=\"i.call_status == 'finished'\"\r\n                ></i>\r\n                {{ i.call_status }}\r\n              </td> -->\r\n              <!-- <td>\r\n                <button\r\n                  class=\"btn-mentoring py-1 bg-secondary\"\r\n                  @click=\"goLink(i.location_link)\"\r\n                >\r\n                  Detail <i class=\"fa-solid fa-paper-plane ms-2\"></i>\r\n                </button>\r\n              </td> -->\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <div class=\"text-center\" v-if=\"todos.from == null\">\r\n        <hr />\r\n        <h6>Sorry, data is not found</h6>\r\n      </div>\r\n\r\n      <v-pagination :datas=\"todos\" @result=\"getPage\" />\r\n    </div>\r\n\r\n    <!-- <div class=\"vue-modal-overlay\"></div> -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"todos\",\r\n  data() {\r\n    return {\r\n      todos: [],\r\n      keyword: \"\",\r\n      search: {\r\n        bar: false,\r\n        name: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    getData() {\r\n      this.$alert.loading();\r\n      this.$axios\r\n        .get(this.$url + \"list/todos\")\r\n        .then((response) => {\r\n          this.$alert.close();\r\n          this.todos = response.data.data;\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$alert.close();\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    getPage(link) {\r\n      this.$axios\r\n        .get(link)\r\n        .then((response) => {\r\n          this.todos = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    searchData() {\r\n      this.$alert.loading();\r\n      if (this.search.name == \"\") {\r\n        this.getData();\r\n        this.search.bar = false;\r\n        this.$alert.close();\r\n      } else {\r\n        let type = \"\";\r\n        if (this.keyword == \"mentor\") {\r\n          type = \"&type=mentor\";\r\n        } else {\r\n          type = \"\";\r\n        }\r\n\r\n        this.$axios\r\n          .get(this.$url + \"list/todos?keyword=\" + this.search.name + type, {\r\n            headers: {\r\n              Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n            },\r\n          })\r\n          .then((response) => {\r\n            this.$alert.close();\r\n            this.todos = response.data.data;\r\n            this.search.bar = true;\r\n            console.log(response);\r\n          })\r\n          .catch((error) => {\r\n            this.$alert.close();\r\n            console.log(error);\r\n          });\r\n      }\r\n    },\r\n\r\n    closeSearch() {\r\n      this.search.bar = false;\r\n      this.search.name = \"\";\r\n      this.keyword = \"\";\r\n      this.getData();\r\n    },\r\n\r\n    goLink(link) {\r\n      window.open(link, \"_blank\");\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":""}]}