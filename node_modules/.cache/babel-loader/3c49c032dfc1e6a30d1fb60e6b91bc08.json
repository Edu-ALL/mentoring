AAP;AAaD,GAnBY;AAoBb,EAAA,OAAO,EAAE;AACD,IAAA,MADC,kBACM,CADN,EACS;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACd,gBAAA,KAAI,CAAC,KAAL,GAAa,KAAb;AADc;AAAA,uBAES,KAAI,CAAC,MAAL,CAAY,GAAZ,CAAgB,4BAA4B,CAA5C,CAFT;;AAAA;AAER,gBAAA,QAFQ;;AAGd,oBAAI,QAAQ,CAAC,IAAT,CAAc,OAAlB,EAA2B;AACzB,kBAAA,KAAI,CAAC,eAAL,GAAuB,QAAQ,CAAC,IAAT,CAAc,IAArC;AACA,kBAAA,KAAI,CAAC,WAAL,GAAmB,MAAnB;AACF,iBAHA,MAGO;AACL,kBAAA,KAAI,CAAC,WAAL,GAAmB,KAAnB;AACA,kBAAA,KAAI,CAAC,eAAL,GAAuB;AACrB,oBAAA,SAAS,EAAE,CADU;AAErB,oBAAA,oBAAoB,EAAE,EAFD;AAGrB,oBAAA,WAAW,EAAE,EAHQ;AAIrB,oBAAA,cAAc,EAAE,EAJK;AAKrB,oBAAA,cAAc,EAAE,EALK;AAMrB,oBAAA,aAAa,EAAE,EANM;AAOrB,oBAAA,aAAa,EAAE;AAPM,mBAAvB;AASF;;AAjBc;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkBf,KAnBM;AAqBD,IAAA,YArBC,0BAqBc;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEM,MAAI,CAAC,MAAL,CAAY,IAAZ,CACrB,wBADqB,EAErB,MAAI,CAAC,eAFgB,CAFN;;AAAA;AAEX,gBAAA,QAFW;AAOjB,gBAAA,MAAI,CAAC,KAAL,GAAa,EAAb;;AAEA,gBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,EAAoB,SAApB;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C,EAViB,CAWjB;;;AAXiB;AAAA;;AAAA;AAAA;AAAA;;AAajB;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,mBAA3B;;AAdiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBpB;AArCM,GApBI;AA2Db,EAAA,OA3Da,qBA2DH,CAAE;AA3DC,CAAf","sourcesContent":["<template>\r\n  <div id=\"history\">\r\n    <div class=\"row p-4\" v-if=\"meeting?.data?.length == 0\">\r\n      <div class=\"col text-center\">\r\n        <p class=\"my-0\">No meeting yet.</p>\r\n      </div>\r\n    </div>\r\n    <div class=\"table-responsive\" v-if=\"meeting?.data?.length != 0\">\r\n      <table class=\"table\">\r\n        <thead>\r\n          <tr class=\"text-center\">\r\n            <th width=\"3%\">No</th>\r\n            <th>Call With</th>\r\n            <th>Subject</th>\r\n            <th>Date</th>\r\n            <th>Time</th>\r\n            <th>Status</th>\r\n            <th>Action</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr\r\n            v-for=\"(i, index) in meeting.data\"\r\n            :key=\"index\"\r\n            class=\"text-center align-middle\"\r\n          >\r\n            <td>{{ meeting.from + index }}</td>\r\n            <td class=\"text-start\" nowrap style=\"text-transform: capitalize\">\r\n              {{ i.students.first_name + \" \" + i.students.last_name }}\r\n            </td>\r\n            <td nowrap style=\"text-transform: capitalize\">\r\n              {{ i.module }}\r\n            </td>\r\n            <td nowrap>\r\n              {{ $customDate.date(i.call_date) }}\r\n            </td>\r\n            <td nowrap>\r\n              {{ $customDate.time(i.call_date) }}\r\n            </td>\r\n            <td style=\"text-transform: capitalize\">\r\n              <small class=\"text-warning\" v-if=\"i.call_status == 'canceled'\">\r\n                <i class=\"fa-solid fa-times\"></i>\r\n                {{ i.call_status }}\r\n              </small>\r\n              <small class=\"text-info\" v-if=\"i.call_status == 'finished'\">\r\n                <i class=\"fa-solid fa-check\"></i>\r\n                {{ i.call_status }}\r\n              </small>\r\n              <small class=\"text-danger\" v-if=\"i.call_status == 'rejected'\">\r\n                <i class=\"fa-solid fa-times\"></i>\r\n                {{ i.call_status }}\r\n              </small>\r\n            </td>\r\n            <td>\r\n              <button\r\n                v-if=\"i.call_status == 'finished' && i.meeting_minute == 1\"\r\n                class=\"btn-mentoring py-1 px-3 btn-sm btn-primary text-white\"\r\n                @click=\"detail(i.id)\"\r\n              >\r\n                View Result\r\n              </button>\r\n              <button\r\n                v-if=\"i.call_status == 'finished' && i.meeting_minute == 0\"\r\n                class=\"btn-mentoring py-1 px-3 btn-sm bg-success text-white\"\r\n                @click=\"detail(i.id)\"\r\n              >\r\n                Add Result\r\n              </button>\r\n              <div v-if=\"i.call_status != 'finished'\">-</div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n\r\n  <!-- Last Meeting  -->\r\n  <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n  <div\r\n    class=\"vue-side mentoring-scroll shadow\"\r\n    :class=\"modal == 'add' ? 'active' : ''\"\r\n  >\r\n    <div class=\"d-flex w-100 justify-content-between\">\r\n      <h6 class=\"mb-0\">Last Meeting Minutes</h6>\r\n      <div class=\"text-end\">\r\n        <button\r\n          class=\"btn btn-sm btn-mentoring btn-outline-info py-1\"\r\n          @click=\"view_status = 'add'\"\r\n          v-if=\"view_status == 'view'\"\r\n        >\r\n          Edit\r\n        </button>\r\n      </div>\r\n    </div>\r\n    <hr class=\"my-2\" />\r\n    <!-- ADD  -->\r\n    <form @submit.prevent=\"handleSubmit\" v-if=\"view_status == 'add'\">\r\n      <div class=\"row row-cols-md-2 row-cols-1\">\r\n        <div class=\"col mb-1\">\r\n          <input-group>\r\n            <span class=\"bg-white\">Academic Performance</span>\r\n            <textarea\r\n              class=\"form-mentoring w-100\"\r\n              v-model=\"meeting_minutes.academic_performance\"\r\n              rows=\"5\"\r\n              required\r\n            ></textarea>\r\n          </input-group>\r\n        </div>\r\n        <div class=\"col mb-1\">\r\n          <input-group>\r\n            <span class=\"bg-white\">Exploration</span>\r\n            <textarea\r\n              class=\"form-mentoring w-100\"\r\n              v-model=\"meeting_minutes.exploration\"\r\n              rows=\"5\"\r\n              required\r\n            ></textarea>\r\n          </input-group>\r\n        </div>\r\n        <div class=\"col mb-1\">\r\n          <input-group>\r\n            <span class=\"bg-white\">Writing Skills</span>\r\n            <textarea\r\n              class=\"form-mentoring w-100\"\r\n              v-model=\"meeting_minutes.writing_skills\"\r\n              rows=\"5\"\r\n              required\r\n            ></textarea>\r\n          </input-group>\r\n        </div>\r\n        <div class=\"col mb-1\">\r\n          <input-group>\r\n            <span class=\"bg-white\">Personal Brand</span>\r\n            <textarea\r\n              class=\"form-mentoring w-100\"\r\n              v-model=\"meeting_minutes.personal_brand\"\r\n              rows=\"5\"\r\n              required\r\n            ></textarea>\r\n          </input-group>\r\n        </div>\r\n      </div>\r\n      <div class=\"mb-1\">\r\n        <input-group>\r\n          <span class=\"bg-white\">Mentor Todos</span>\r\n          <textarea\r\n            class=\"form-mentoring w-100\"\r\n            v-model=\"meeting_minutes.mt_todos_note\"\r\n            rows=\"5\"\r\n            required\r\n          ></textarea>\r\n        </input-group>\r\n      </div>\r\n      <div class=\"mb-1\">\r\n        <input-group>\r\n          <span class=\"bg-white\">Mentee Todos</span>\r\n          <textarea\r\n            class=\"form-mentoring w-100\"\r\n            v-model=\"meeting_minutes.st_todos_note\"\r\n            rows=\"5\"\r\n            required\r\n          ></textarea>\r\n        </input-group>\r\n      </div>\r\n      <hr />\r\n      <div class=\"text-end\">\r\n        <button class=\"btn-mentoring btn-sm bg-primary py-1 px-3\" type=\"submit\">\r\n          <i class=\"fa-solid fa-save me-2\"></i>\r\n          Save Changes\r\n        </button>\r\n      </div>\r\n    </form>\r\n\r\n    <!-- VIEW  -->\r\n    <div class=\"row row-cols-1\" v-if=\"view_status == 'view'\">\r\n      <div class=\"col\">\r\n        <div class=\"mb-2\">\r\n          <label class=\"mb-1 text-primary\">Academic Performance :</label>\r\n          <div class=\"py-1 px-3\" style=\"border-left: 2px solid #dedede\">\r\n            <p class=\"py-0\" v-html=\"meeting_minutes.academic_performance\"></p>\r\n          </div>\r\n        </div>\r\n        <div class=\"mb-2\">\r\n          <label class=\"mb-1 text-primary\">Exploration :</label>\r\n          <div class=\"py-1 px-3\" style=\"border-left: 2px solid #dedede\">\r\n            <p class=\"py-0\" v-html=\"meeting_minutes.exploration\"></p>\r\n          </div>\r\n        </div>\r\n        <div class=\"mb-2\">\r\n          <label class=\"mb-1 text-primary\">Writing Skills :</label>\r\n          <div class=\"py-1 px-3\" style=\"border-left: 2px solid #dedede\">\r\n            <p class=\"py-0\" v-html=\"meeting_minutes.writing_skills\"></p>\r\n          </div>\r\n        </div>\r\n        <div class=\"mb-2\">\r\n          <label class=\"mb-1 text-primary\">Personal Brand :</label>\r\n          <div class=\"py-1 px-3\" style=\"border-left: 2px solid #dedede\">\r\n            <p class=\"py-0\" v-html=\"meeting_minutes.personal_brand\"></p>\r\n          </div>\r\n        </div>\r\n        <div class=\"mb-2\">\r\n          <label class=\"mb-1 text-primary\">My Todos :</label>\r\n          <div class=\"py-1 px-3\" style=\"border-left: 2px solid #dedede\">\r\n            <p class=\"py-0\" v-html=\"meeting_minutes.mt_todos_note\"></p>\r\n          </div>\r\n        </div>\r\n        <div class=\"mb-2\">\r\n          <label class=\"mb-1 text-primary\">Mentee Todos :</label>\r\n          <div class=\"py-1 px-3\" style=\"border-left: 2px solid #dedede\">\r\n            <p class=\"py-0\" v-html=\"meeting_minutes.st_todos_note\"></p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"history\",\r\n  props: {\r\n    meeting: Object,\r\n  },\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      view_status: \"\",\r\n      meeting_minutes: {\r\n        st_act_id: \"\",\r\n        academic_performance: \"\",\r\n        exploration: \"\",\r\n        writing_skills: \"\",\r\n        personal_brand: \"\",\r\n        mt_todos_note: \"\",\r\n        st_todos_note: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    async detail(i) {\r\n      this.modal = \"add\";\r\n      const response = await this.$axios.get(\"select/meeting-minutes/\" + i);\r\n      if (response.data.success) {\r\n        this.meeting_minutes = response.data.data;\r\n        this.view_status = \"view\";\r\n      } else {\r\n        this.view_status = \"add\";\r\n        this.meeting_minutes = {\r\n          st_act_id: i,\r\n          academic_performance: \"\",\r\n          exploration: \"\",\r\n          writing_skills: \"\",\r\n          personal_brand: \"\",\r\n          mt_todos_note: \"\",\r\n          st_todos_note: \"\",\r\n        };\r\n      }\r\n    },\r\n\r\n    async handleSubmit() {\r\n      try {\r\n        const response = await this.$axios.post(\r\n          \"create/meeting-minutes\",\r\n          this.meeting_minutes\r\n        );\r\n\r\n        this.modal = \"\";\r\n\r\n        this.$emit(\"check\", \"history\");\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        // console.log(response);\r\n      } catch (e) {\r\n        // console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again.\");\r\n      }\r\n    },\r\n  },\r\n  created() {},\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.vue-side {\r\n  position: fixed;\r\n  top: 0;\r\n  right: -50%;\r\n  height: 100vh;\r\n  padding: 20px;\r\n  width: 40%;\r\n  background: white;\r\n  overflow: auto;\r\n  z-index: 99999;\r\n  transition: all 0.3s;\r\n}\r\n\r\n.vue-side.active {\r\n  right: 0 !important;\r\n}\r\n\r\n@media only screen and (max-width: 600px) {\r\n  .vue-side {\r\n    position: fixed;\r\n    top: 0;\r\n    right: -90%;\r\n    width: 90%;\r\n  }\r\n}\r\n</style>"],"sourceRoot":""}]}