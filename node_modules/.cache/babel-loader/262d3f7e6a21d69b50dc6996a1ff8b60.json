AAQ,CAAC,IAAT,CAAc,IAA5B,CAJY,CAKZ;;AALY;AAAA;;AAAA;AAAA;AAAA;AAOZ,gBAAA,OAAO,CAAC,GAAR,CAAY,YAAE,QAAd;;AAPY;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASf,KAVM;AAYD,IAAA,OAZC,mBAYO,IAZP,EAYa;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEO,MAAI,CAAC,MAAL,CAAY,GAAZ,CAAgB,IAAhB,CAFP;;AAAA;AAEV,gBAAA,QAFU;AAIhB,gBAAA,MAAI,CAAC,MAAL,GAAc,QAAQ,CAAC,IAAT,CAAc,IAA5B,CAJgB,CAKhB;;AALgB;AAAA;;AAAA;AAAA;AAAA;AAOhB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AAPgB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASnB,KArBM;AAuBD,IAAA,aAvBC,yBAuBa,EAvBb,EAuBiB,MAvBjB,EAuByB;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAC9B,gBAAA,MAAI,CAAC,KAAL,GAAa,EAAb;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,OAAZ;;AAF8B;AAAA;AAAA,uBAIL,MAAI,CAAC,MAAL,CAAY,IAAZ,CACrB,wCADqB,EAErB;AACE,kBAAA,QAAQ,EAAE,EADZ;AAEE,kBAAA,MAAM,EAAE;AAFV,iBAFqB,CAJK;;AAAA;AAItB,gBAAA,QAJsB;;AAY5B,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C;;AACA,gBAAA,MAAI,CAAC,OAAL,GAb4B,CAc5B;;;AAd4B;AAAA;;AAAA;AAAA;AAAA;;AAgB5B,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AAjB4B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmB/B,KA1CM;AA4CP,IAAA,OA5CO,mBA4CC,EA5CD,EA4CK;AACV,WAAK,KAAL,GAAa,SAAb;AACA,WAAK,QAAL,GAAgB,EAAhB;AACD;AA/CM,GATI;AA0Db,EAAA,OA1Da,qBA0DH;AACR,SAAK,OAAL;AACD;AA5DY,CAAf","sourcesContent":["<template>\r\n  <div id=\"request\">\r\n    <div class=\"container mt-3\">\r\n      <div class=\"row p-4\" v-if=\"groups.data && groups.data.length == 0\">\r\n        <div class=\"col text-center\">\r\n          <p>No project group yet.</p>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"row row-cols-md-2 row-cols-1 g-2\">\r\n        <div class=\"col\" v-for=\"i in groups.data\" :key=\"i\">\r\n          <div class=\"card card-group border shadow-sm\">\r\n            <div class=\"card-body\">\r\n              <h5 class=\"mb-1\">{{ i.project_name }}</h5>\r\n              <span class=\"badge bg-secondary rounded-pill text-dark\">\r\n                {{ i.project_type }}\r\n              </span>\r\n              <div class=\"project-desc mt-2\">\r\n                {{ $customText.text(i.project_desc, 20) }}\r\n              </div>\r\n              <hr class=\"my-2\" />\r\n              <div class=\"row align-items-center\">\r\n                <div class=\"col-6\">\r\n                  <button\r\n                    class=\"btn-mentoring btn-sm py-1 btn-outline-danger\"\r\n                    @click=\"decline(i.id)\"\r\n                  >\r\n                    Decline\r\n                  </button>\r\n                </div>\r\n                <div class=\"col-6 text-end\">\r\n                  <button\r\n                    class=\"btn-mentoring btn-sm py-1 bg-primary\"\r\n                    @click=\"handleConfirm(i.id, 'accept')\"\r\n                  >\r\n                    Accept\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <nav class=\"mt-3\" v-if=\"groups.from != null\">\r\n        <ul class=\"pagination justify-content-center\">\r\n          <li class=\"page-item\" v-if=\"groups.current_page != 1\">\r\n            <a class=\"page-link\" @click=\"getPage(groups.links[0].url)\">\r\n              <i class=\"fa-solid fa-chevron-left\"></i>\r\n            </a>\r\n          </li>\r\n          <div v-for=\"(i, index) in groups.last_page\" :key=\"index\">\r\n            <li\r\n              class=\"page-item\"\r\n              v-if=\"groups.current_page - 2 < i && groups.current_page + 2 > i\"\r\n            >\r\n              <a\r\n                class=\"page-link\"\r\n                :class=\"groups.current_page == i ? 'bg-primary text-white' : ''\"\r\n                href=\"#\"\r\n                @click=\"getPage(groups.path + '?page=' + i)\"\r\n                >{{ i }}</a\r\n              >\r\n            </li>\r\n          </div>\r\n          <li class=\"page-item\" v-if=\"groups.current_page != groups.last_page\">\r\n            <a class=\"page-link\" @click=\"getPage(groups.next_page_url)\">\r\n              <i class=\"fa-solid fa-chevron-right\"></i>\r\n            </a>\r\n          </li>\r\n        </ul>\r\n      </nav>\r\n    </div>\r\n\r\n    <!-- MODAL -->\r\n    <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n    <transition name=\"pop\">\r\n      <div class=\"vue-modal vue-modal-sm bg-primary\" v-if=\"modal == 'decline'\">\r\n        <div class=\"text-center\">\r\n          <i class=\"fa-solid fa-circle-exclamation fa-2xl\"></i>\r\n          <h5 class=\"mt-2\">\r\n            Are you sure you want to refuse to join this group?\r\n          </h5>\r\n          <div class=\"mt-3\">\r\n            <button\r\n              class=\"btn-mentoring btn-warning btn-sm py-1 mx-1\"\r\n              @click=\"modal = ''\"\r\n            >\r\n              Cancel</button\r\n            ><button\r\n              class=\"btn-mentoring btn-outline-success btn-sm py-1 mx-1\"\r\n              @click=\"handleConfirm(group_id, 'decline')\"\r\n            >\r\n              Yes\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"request\",\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      group_id: \"\",\r\n      groups: [],\r\n    };\r\n  },\r\n  methods: {\r\n    async getData() {\r\n      try {\r\n        const response = await this.$axios.get(\"student/group/project/new\");\r\n\r\n        this.groups = response.data.data;\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async getPage(link) {\r\n      try {\r\n        const response = await this.$axios.get(link);\r\n\r\n        this.groups = response.data.data;\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async handleConfirm(id, status) {\r\n      this.modal = \"\";\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\r\n          \"student/group/project/confirmation/req\",\r\n          {\r\n            group_id: id,\r\n            action: status,\r\n          }\r\n        );\r\n\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        this.getData();\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        this.$alert.close();\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    decline(id) {\r\n      this.modal = \"decline\";\r\n      this.group_id = id;\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.card-group {\r\n  position: relative;\r\n  overflow: hidden;\r\n  cursor: pointer;\r\n  border-radius: 15px;\r\n  transition: all 0.3s ease-in-out;\r\n}\r\n\r\n.card-status {\r\n  position: absolute;\r\n  right: -10px;\r\n  top: -10px;\r\n  padding: 10px 10px 4px 4px;\r\n  border-radius: 50%;\r\n}\r\n</style>"],"sourceRoot":""}]}