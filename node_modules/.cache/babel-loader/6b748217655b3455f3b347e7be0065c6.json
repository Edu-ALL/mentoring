CAAkB,GAAlB,GAAwB,EAAxB;;AAEA,kBAAA,KAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,QAAQ,CAAC,IAAT,CAAc,KAAzC;AACF;;AA7CiB;AAAA;;AAAA;AAAA;AAAA;AA+CjB,gBAAA,OAAO,CAAC,GAAR,CAAY,YAAE,QAAd;;AACA,gBAAA,KAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,kBAA3B;;AAhDiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkDpB,KAnDM;AAqDP,IAAA,WArDO,uBAqDK,CArDL,EAqDQ;AACb,WAAK,KAAL,GAAa,gBAAb;AACA,WAAK,UAAL,GAAkB,CAAlB;AACD,KAxDM;AA0DD,IAAA,YA1DC,0BA0Dc;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACnB,gBAAA,MAAI,CAAC,KAAL,GAAa,EAAb;;AAEA,gBAAA,MAAI,CAAC,MAAL,CAAY,OAAZ;;AAHmB;AAAA;AAAA,uBAKM,MAAI,CAAC,MAAL,CAAY,GAAZ,CACrB,mCAAmC,MAAI,CAAC,UADnB,CALN;;AAAA;AAKX,gBAAA,QALW;;AASjB;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C;;AACA,gBAAA,UAAU,CAAC,YAAM;AACf,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,EAAoB,KAApB;AACD,iBAFS,EAEP,IAFO,CAAV;AAXiB;AAAA;;AAAA;AAAA;AAAA;AAejB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,kBAA3B;;AAhBiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkBpB,KA5EM;AA8EP,IAAA,IA9EO,gBA8EF,IA9EE,EA8EI;AACT,MAAA,MAAM,CAAC,IAAP,CAAY,IAAZ,EAAkB,QAAlB;AACD;AAhFM,GA7BI;AA+Gb,EAAA,OA/Ga,qBA+GH;AACR,SAAK,QAAL,GAAgB,KAAK,IAArB;AACD,GAjHY;AAmHb,EAAA,OAnHa,qBAmHH;AACR,SAAK,QAAL,GAAgB,KAAK,IAArB;AACD;AArHY,CAAf","sourcesContent":["<template>\r\n  <div id=\"groupMeeting\">\r\n    <div class=\"row\">\r\n      <div class=\"col-6\">\r\n        <h6>Group Meeting</h6>\r\n      </div>\r\n      <div class=\"col-6\" v-if=\"group.student_id == student.id\">\r\n        <div class=\"text-end\">\r\n          <i\r\n            class=\"fa-solid fa-add pointer\"\r\n            @click=\"modal = 'new-meeting'\"\r\n            v-if=\"menu.key == 'progress'\"\r\n          ></i>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-12 text-center p-4\" v-if=\"meetings.length == 0\">\r\n        No group meeting yet\r\n      </div>\r\n      <div class=\"col-12 mt-2\">\r\n        <ul class=\"list-group\">\r\n          <li\r\n            class=\"list-group-item\"\r\n            v-for=\"(i, index) in meetings\"\r\n            :key=\"index\"\r\n          >\r\n            <div class=\"row align-items-center\">\r\n              <div class=\"col-md-3 col-12 p-0\">\r\n                <div :class=\"i.status == 0 ? 'meeting-subject' : ''\">\r\n                  {{ i.meeting_subject }}\r\n                  <i\r\n                    class=\"bi bi-x-circle ms-2 text-end text-danger\"\r\n                    v-if=\"group.student_id == student.id && i.status == 0\"\r\n                  ></i>\r\n                  <div\r\n                    class=\"cancel justify-content-center\"\r\n                    @click=\"cancelModal(i.id)\"\r\n                    v-if=\"i.status == 0\"\r\n                  >\r\n                    <div class=\"text-center\">Cancel</div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"col-md-4 col-11 text-md-center\">\r\n                {{ $customDate.date(i.start_meeting_date) }} <br />\r\n                <small>\r\n                  {{\r\n                    $customDate.time(i.start_meeting_date) +\r\n                    \" - \" +\r\n                    $customDate.time(i.end_meeting_date)\r\n                  }}\r\n                </small>\r\n              </div>\r\n              <div class=\"col-md-3 text-md-center\">\r\n                <div class=\"text-primary\" v-if=\"i.status == 0\">Upcoming</div>\r\n                <div class=\"text-success\" v-if=\"i.status == 1\">Completed</div>\r\n                <div class=\"text-danger\" v-if=\"i.status == 2\">Canceled</div>\r\n              </div>\r\n              <div class=\"col-md-2 text-md-end p-0\">\r\n                <button\r\n                  :class=\"i.status != 0 ? 'bg-light text-muted' : 'bg-primary'\"\r\n                  :disabled=\"i.status != 0\"\r\n                  class=\"btn-mentoring py-1 px-4\"\r\n                  @click=\"join(i.meeting_link)\"\r\n                >\r\n                  Join\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"vue-modal-overlay\" v-if=\"modal != ''\"></div>\r\n    <!-- New Meeting  -->\r\n    <transition name=\"pop\">\r\n      <div class=\"vue-modal vue-modal-sm\" v-if=\"modal == 'new-meeting'\">\r\n        <form method=\"post\" @submit.prevent=\"handleSubmit\">\r\n          <h5>New Group Meeting</h5>\r\n          <hr class=\"my-0 mb-3\" />\r\n          <div class=\"mb-3\">\r\n            <input-group>\r\n              <input\r\n                type=\"text\"\r\n                class=\"form-mentoring form-control w-100\"\r\n                required\r\n                placeholder=\"fill in here\"\r\n                v-model=\"meeting.meeting_subject\"\r\n                id=\"subject\"\r\n              />\r\n              <label for=\"subject\">Meeting Subject</label>\r\n            </input-group>\r\n          </div>\r\n\r\n          <div class=\"mb-3\">\r\n            <div class=\"row g-2\">\r\n              <div class=\"col-12 mb-2\">\r\n                <input-group>\r\n                  <input\r\n                    :type=\"input.date\"\r\n                    class=\"form-mentoring form-control w-100\"\r\n                    v-model=\"meeting_date.date\"\r\n                    :min=\"this.$customDate.todayDate()\"\r\n                    required\r\n                    placeholder=\"fill in here\"\r\n                    @focus=\"input.date = 'date'\"\r\n                    @blur=\"input.date = 'text'\"\r\n                    id=\"date\"\r\n                  />\r\n                  <label for=\"date\">Meeting Date</label>\r\n                </input-group>\r\n              </div>\r\n              <div class=\"col-6\">\r\n                <input-group>\r\n                  <input\r\n                    :type=\"input.start\"\r\n                    class=\"form-mentoring form-control w-100\"\r\n                    v-model=\"meeting_date.start\"\r\n                    required\r\n                    placeholder=\"fill in here\"\r\n                    @focus=\"input.start = 'time'\"\r\n                    @blur=\"input.start = 'text'\"\r\n                    id=\"time\"\r\n                    :disabled=\"meeting_date.date == ''\"\r\n                  />\r\n                  <label for=\"time\">Start Time</label>\r\n                </input-group>\r\n              </div>\r\n              <div class=\"col-6\">\r\n                <input-group>\r\n                  <input\r\n                    :type=\"input.end\"\r\n                    class=\"form-mentoring form-control w-100\"\r\n                    v-model=\"meeting_date.end\"\r\n                    required\r\n                    placeholder=\"fill in here\"\r\n                    @focus=\"input.end = 'time'\"\r\n                    @blur=\"input.end = 'text'\"\r\n                    id=\"end\"\r\n                    :disabled=\"meeting_date.start == ''\"\r\n                  />\r\n                  <label for=\"end\">End Time</label>\r\n                </input-group>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"mb-3\">\r\n            <input-group>\r\n              <input\r\n                type=\"url\"\r\n                class=\"form-mentoring form-control w-100\"\r\n                v-model=\"meeting.meeting_link\"\r\n                required\r\n                placeholder=\"fill in here\"\r\n                id=\"link\"\r\n              />\r\n              <label for=\"link\">Location Link</label>\r\n            </input-group>\r\n          </div>\r\n          <hr />\r\n          <div class=\"row\">\r\n            <div class=\"col-6\">\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn-mentoring btn-sm py-1 btn-outline-danger\"\r\n                @click=\"modal = ''\"\r\n              >\r\n                <i class=\"bi bi-x-circle me-1\"></i>\r\n                Cancel\r\n              </button>\r\n            </div>\r\n            <div class=\"col-6 text-end\">\r\n              <button\r\n                type=\"submit\"\r\n                class=\"btn-mentoring btn-sm py-1 btn-success\"\r\n              >\r\n                <i class=\"bi bi-save me-1\"></i>\r\n                Save\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </transition>\r\n\r\n    <!-- Cancel Meeting  -->\r\n    <transition name=\"pop\">\r\n      <div\r\n        class=\"vue-modal vue-modal-sm bg-primary text-center\"\r\n        v-if=\"modal == 'cancel-meeting'\"\r\n      >\r\n        <i class=\"fa-solid fa-circle-exclamation mx-1 fa-2xl\"></i>\r\n        <h5 class=\"mt-3 mb-3\">Are you sure to cancel this meeting?</h5>\r\n        <button\r\n          class=\"btn-mentoring btn-sm py-1 btn-danger mx-1\"\r\n          @click=\"modal = ''\"\r\n        >\r\n          No\r\n        </button>\r\n        <button\r\n          class=\"btn-mentoring btn-sm py-1 btn-outline-success mx-1\"\r\n          @click=\"handleCancel()\"\r\n        >\r\n          Yes, Cancel\r\n        </button>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport moment from \"moment\";\r\nexport default {\r\n  name: \"meeting\",\r\n  props: {\r\n    menu: Object,\r\n    data: Object,\r\n    group: Object,\r\n    student: Object,\r\n  },\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      meeting_id: \"\",\r\n      meetings: [],\r\n      meeting: {\r\n        group_id: \"\",\r\n        meeting_subject: \"\",\r\n        start_date: \"\",\r\n        end_date: \"\",\r\n        meeting_link: \"\",\r\n        status: \"0\",\r\n      },\r\n      meeting_date: {\r\n        date: \"\",\r\n        start: \"\",\r\n        end: \"\",\r\n      },\r\n      input: { date: \"text\", start: \"text\", end: \"text\" },\r\n    };\r\n  },\r\n  methods: {\r\n    async handleSubmit() {\r\n      this.modal = \"\";\r\n      this.meeting.group_id = this.group.id;\r\n      // Start Date\r\n      this.meeting.start_date =\r\n        this.meeting_date.date + \" \" + this.meeting_date.start;\r\n      this.meeting.start_date = moment(this.meeting.start_date).format(\r\n        \"YYYY-MM-DD HH:mm\"\r\n      );\r\n      // End Date\r\n      this.meeting.end_date =\r\n        this.meeting_date.date + \" \" + this.meeting_date.end;\r\n      this.meeting.end_date = moment(this.meeting.end_date).format(\r\n        \"YYYY-MM-DD HH:mm\"\r\n      );\r\n\r\n      // console.log(this.meeting);\r\n\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\r\n          \"student/group/project/meeting\",\r\n          this.meeting\r\n        );\r\n\r\n        if (response.data.success) {\r\n          this.meeting.meeting_subject = \"\";\r\n          this.meeting.meeting_link = \"\";\r\n          this.meeting_date.date = \"\";\r\n          this.meeting_date.start = \"\";\r\n          this.meeting_date.end = \"\";\r\n\r\n          // console.log(response.data);\r\n          this.$alert.toast(\"success\", response.data.message);\r\n          setTimeout(() => {\r\n            this.$emit(\"check\", \"new\");\r\n          }, 3000);\r\n        } else {\r\n          this.modal = \"new-meeting\";\r\n\r\n          this.meeting_date.date = \"\";\r\n          this.meeting_date.start = \"\";\r\n          this.meeting_date.end = \"\";\r\n\r\n          this.$alert.toast(\"error\", response.data.error);\r\n        }\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n\r\n    cancelModal(i) {\r\n      this.modal = \"cancel-meeting\";\r\n      this.meeting_id = i;\r\n    },\r\n\r\n    async handleCancel() {\r\n      this.modal = \"\";\r\n\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.put(\r\n          \"student/group/project/meeting/\" + this.meeting_id\r\n        );\r\n\r\n        // console.log(response.data);\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        setTimeout(() => {\r\n          this.$emit(\"check\", \"new\");\r\n        }, 3000);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n\r\n    join(link) {\r\n      window.open(link, \"_blank\");\r\n    },\r\n  },\r\n  updated() {\r\n    this.meetings = this.data;\r\n  },\r\n\r\n  created() {\r\n    this.meetings = this.data;\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.list-group {\r\n  font-size: 0.8em;\r\n}\r\n\r\n.meeting-subject {\r\n  position: relative;\r\n  width: 100%;\r\n  height: 100%;\r\n  padding: 10px;\r\n  overflow: hidden;\r\n}\r\n\r\n.cancel {\r\n  position: absolute;\r\n  display: flex;\r\n  align-items: center;\r\n  cursor: pointer;\r\n  top: 0;\r\n  left: -100%;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: rgba(254, 123, 123, 0.907);\r\n  color: white;\r\n  font-weight: bold;\r\n  transition: all 0.5s;\r\n}\r\n\r\n.meeting-subject:hover .cancel {\r\n  left: 0 !important;\r\n}\r\n</style>"],"sourceRoot":""}]}