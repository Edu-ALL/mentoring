e d-block\"\r\n              @click=\"menu = false\"\r\n            >\r\n            </vue-feather>\r\n          </div>\r\n        </transition>\r\n\r\n        <transition name=\"fade\">\r\n          <div class=\"navbar-title d-md-none d-block\" v-if=\"menu\">MENU</div>\r\n        </transition>\r\n\r\n        <div\r\n          class=\"navbar-menu d-md-inline-block\"\r\n          :class=\"!menu ? 'd-none' : ''\"\r\n        >\r\n          <ul>\r\n            <li @click=\"goToMenu('')\" :class=\"params == '' ? 'active' : ''\">\r\n              <i class=\"bi bi-speedometer2 me-1\"></i>\r\n              Dashboard\r\n            </li>\r\n            <li\r\n              @click=\"goToMenu('my-activity')\"\r\n              :class=\"\r\n                params == 'my-activity' ||\r\n                params == 'groups' ||\r\n                params == 'webinar' ||\r\n                params == 'event'\r\n                  ? 'active'\r\n                  : ''\r\n              \"\r\n            >\r\n              <i class=\"bi bi-list-task me-1\"></i>\r\n              My Activities\r\n            </li>\r\n            <li\r\n              @click=\"goToMenu('uni')\"\r\n              :class=\"params == 'uni' ? 'active' : ''\"\r\n            >\r\n              <i class=\"bi bi-building me-1\"></i>\r\n              Universities\r\n            </li>\r\n          </ul>\r\n        </div>\r\n        <div class=\"navbar-button d-md-block\" :class=\"!menu ? 'd-none' : ''\">\r\n          <button\r\n            class=\"btn btn-allin btn-sm bg-secondary px-4\"\r\n            @click=\"handleLogout\"\r\n          >\r\n            <i class=\"bi bi-box-arrow-right me-1\"></i>\r\n            Sign Out\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"header\",\r\n  data() {\r\n    return {\r\n      menu: false,\r\n      params: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    showMenu() {\r\n      this.menu = true;\r\n    },\r\n\r\n    goToMenu(page) {\r\n      this.menu = false;\r\n      this.$router.push({ path: \"/user/\" + page });\r\n    },\r\n\r\n    handleLogout() {\r\n      localStorage.clear();\r\n      this.$router.push({ path: \"/\" });\r\n      this.$alert.toast(\"success\", \"You Successfully Logout\");\r\n    },\r\n  },\r\n  watch: {\r\n    $route(to) {\r\n      this.params = to.params.menu;\r\n    },\r\n  },\r\n  created() {\r\n    if (this.$route.params.menu) {\r\n      this.params = this.$route.params.menu;\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.navbar-menu {\r\n  margin-left: 150px;\r\n}\r\n\r\n@media only screen and (max-width: 800px) {\r\n  .navbar-menu {\r\n    margin-left: 0;\r\n  }\r\n}\r\n</style>"],"sourceRoot":""}]}