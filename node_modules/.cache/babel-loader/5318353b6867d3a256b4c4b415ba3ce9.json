n                >\r\n                  <div class=\"card group-card\">\r\n                    <div class=\"card-body\">\r\n                      <div class=\"row p-0 align-items-center\">\r\n                        <div class=\"col-md-9\">\r\n                          <div class=\"d-flex align-items-center\">\r\n                            <div class=\"group-image\">\r\n                              <img\r\n                                v-lazy=\"\r\n                                  i.picture\r\n                                    ? $base_url + '' + i.picture\r\n                                    : 'https://picsum.photos/id/' +\r\n                                      in_progress.from +\r\n                                      index * 2 +\r\n                                      '/300/300'\r\n                                \"\r\n                                class=\"w-100\"\r\n                              />\r\n                            </div>\r\n                            <div class=\"title ms-3\" style=\"width: 80%\">\r\n                              <h5>{{ i.project_name }}</h5>\r\n                              <p\r\n                                class=\"my-0\"\r\n                                v-html=\"$customText.text(i.project_desc, 20)\"\r\n                              ></p>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"mt-2\">\r\n                            <div class=\"badge badge-group bg-info px-3 py-1\">\r\n                              {{ i.project_type }}\r\n                            </div>\r\n                            <div\r\n                              class=\"badge badge-group bg-info px-3 py-1 ms-1\"\r\n                            >\r\n                              {{ i.group_participant_count }} Members\r\n                            </div>\r\n                            <div\r\n                              class=\"badge badge-group bg-info px-3 py-1 ms-1\"\r\n                              v-if=\"\r\n                                i.progress_status != '' ||\r\n                                i.progress_status != null\r\n                              \"\r\n                              style=\"text-transform: capitalize\"\r\n                            >\r\n                              {{ i.progress_status }}\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                        <div class=\"col-md-3 text-end\">\r\n                          <button\r\n                            class=\"btn-mentoring btn-secondary py-1 btn-sm\"\r\n                            @click=\"detail(i)\"\r\n                          >\r\n                            Detail\r\n                            <i class=\"fa-solid fa-arrow-right ms-2\"></i>\r\n                          </button>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </transition>\r\n\r\n          <transition name=\"fade\">\r\n            <div v-if=\"section == 'history'\">\r\n              <div\r\n                class=\"col-12 text-center p-4 shadow-sm\"\r\n                v-if=\"history_project?.data?.length == 0\"\r\n              >\r\n                No histories yet\r\n              </div>\r\n              <div class=\"row row-cols-1 g-2 max-height mentoring-scroll\">\r\n                <div\r\n                  class=\"col\"\r\n                  v-for=\"(i, index) in history_project.data\"\r\n                  :key=\"i\"\r\n                >\r\n                  <div class=\"card group-card\">\r\n                    <div class=\"card-body\">\r\n                      <div class=\"row p-0 align-items-center\">\r\n                        <div class=\"col-md-9\">\r\n                          <div class=\"d-flex align-items-center\">\r\n                