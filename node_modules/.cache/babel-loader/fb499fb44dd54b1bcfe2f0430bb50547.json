CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C,EAnBsB,CAoBtB;;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AArBsB;AAAA;;AAAA;AAAA;AAAA;;AAuBtB,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,mBAA3B;;AAzBsB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA2BzB,KAzDM;AA2DD,IAAA,gBA3DC,8BA2DkB;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACvB,gBAAA,MAAI,CAAC,MAAL,CAAY,OAAZ;;AADuB;AAAA;AAAA,uBAGE,MAAI,CAAC,MAAL,CAAY,GAAZ,CACrB,uCAAuC,MAAI,CAAC,OADvB,EAErB;AACE,kBAAA,IAAI,EAAE,MAAI,CAAC,YAAL,CAAkB,iBAD1B;AAEE,kBAAA,WAAW,EAAE,MAAI,CAAC,YAAL,CAAkB;AAFjC,iBAFqB,CAHF;;AAAA;AAGf,gBAAA,QAHe;AAWrB,gBAAA,MAAI,CAAC,UAAL,GAAkB,KAAlB;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C;;AACA,gBAAA,UAAU,CAAC,YAAM;AACf,kBAAA,MAAI,CAAC,OAAL;AACD,iBAFS,EAEP,IAFO,CAAV,CAbqB,CAgBrB;;AAhBqB;AAAA;;AAAA;AAAA;AAAA;;AAkBrB,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAF,CAAW,IAAvB;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,mBAA3B;;AApBqB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsBxB,KAjFM;AAmFP,IAAA,cAnFO,0BAmFQ,CAnFR,EAmFW;AAChB,UAAI,CAAA,IAAK,KAAT,EAAgB;AACd,aAAK,OAAL;AACF;AACD;AAvFM,GAzBI;AAmHb,EAAA,OAnHa,qBAmHH;AACR,SAAK,OAAL,GAAe,KAAK,MAAL,CAAY,MAAZ,CAAmB,IAAlC;;AACA,QAAI,KAAK,OAAT,EAAkB;AAChB,WAAK,OAAL;AACF,KAFA,MAEO;AACL,WAAK,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,4BAA3B;AACA,WAAK,OAAL,CAAa,IAAb,CAAkB;AAAE,QAAA,IAAI,EAAE;AAAR,OAAlB;AACF;AACD;AA3HY,CAAf","sourcesContent":["<template>\r\n  <div id=\"view\">\r\n    <div class=\"row mb-3 p-0\">\r\n      <div class=\"col-12\">\r\n        <div class=\"pointer\" style=\"margin-top: -20px\" @click=\"redirect\">\r\n          <i class=\"fa-solid fa-arrow-left me-2\"></i>\r\n          <h5 class=\"d-inline\">Groups</h5>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"container mt-3 p-0\">\r\n      <div class=\"row\">\r\n        <div class=\"col-md-7 mb-3\">\r\n          <div\r\n            class=\"card border-0 shadow mb-3 position-relative overflow-hidden\"\r\n            v-if=\"groups.group_info == null\"\r\n          >\r\n            <img\r\n              v-lazy=\"\r\n                'https://picsum.photos/id/' + group_info.id * 2 + '/900/300'\r\n              \"\r\n              class=\"thumbnail shadow\"\r\n            />\r\n            <div class=\"thumbnail-logo\">\r\n              <div class=\"logo shadow\">\r\n                <img\r\n                  v-lazy=\"\r\n                    group_info.picture != null && group_info.picture != ''\r\n                      ? $base_url + '' + group_info.picture\r\n                      : 'https://picsum.photos/id/' + group_info.id + '/200/300'\r\n                  \"\r\n                  class=\"w-100\"\r\n                />\r\n              </div>\r\n            </div>\r\n            <div class=\"card-body\">\r\n              <div\r\n                class=\"float-end\"\r\n                v-if=\"\r\n                  menu.key == 'progress' &&\r\n                  group_info.student_id == student_info.id\r\n                \"\r\n              >\r\n                <i\r\n                  class=\"fa-solid fa-edit pointer\"\r\n                  v-if=\"!editGroup\"\r\n                  @click=\"editGroup = !editGroup\"\r\n                ></i>\r\n                <i\r\n                  class=\"fa-solid fa-x pointer\"\r\n                  v-if=\"editGroup\"\r\n                  @click=\"editGroup = !editGroup\"\r\n                ></i>\r\n              </div>\r\n              <!-- Detail  -->\r\n              <div class=\"\" v-if=\"!editGroup\">\r\n                <h5>\r\n                  {{ group_info.project_name }}\r\n                </h5>\r\n                <span class=\"badge bg-secondary rounded-pill text-dark\">\r\n                  {{ group_info.project_type }}\r\n                </span>\r\n                <div\r\n                  class=\"project-desc mt-2\"\r\n                  v-html=\"group_info.project_desc\"\r\n                ></div>\r\n                <div class=\"row mt-3 align-items-center\">\r\n                  <div class=\"col-6\">\r\n                    <small class=\"text-muted\"\r\n                      ><i class=\"fa-solid fa-calendar me-2\"></i>\r\n                      {{ $customDate.date(group_info.created_at) }}\r\n                    </small>\r\n                  </div>\r\n                  <!-- <div class=\"col-6 text-end\">\r\n                    <i class=\"fa-solid fa-thumbs-up mx-1 text-success\"></i>\r\n                    <i\r\n                      class=\"fa-solid fa-circle-exclamation mx-1 text-warning\"\r\n                    ></i>\r\n                    <i\r\n                      class=\"fa-solid fa-triangle-exclamation mx-1 text-danger\"\r\n                    ></i>\r\n                  </div> -->\r\n                </div>\r\n              </div>\r\n\r\n              <!-- Edit Group  -->\r\n              <form method=\"post\" @submit.prevent=\"handleUpdateGroup()\">\r\n                <div class=\"\" v-if=\"editGroup\">\r\n                  <h6>Edit Group</h6>\r\n                  <hr />\r\n                  <div class=\"mb-3\">\r\n                    <input-group>\r\n                      <input\r\n                        v-model=\"group_info.project_name\"\r\n                        type=\"text\"\r\n                        required\r\n                        class=\"form-mentoring form-control w-100\"\r\n                        placeholder=\"fill in here ...\"\r\n                        id=\"project Name\"\r\n                      />\r\n                      <label for=\"projectName\">Group Name</label>\r\n                    </input-group>\r\n                  </div>\r\n                  <div class=\"mb-3\">\r\n                    <group-type\r\n                      v-model=\"group_info.project_type\"\r\n                      :options=\"options\"\r\n                      placeholder=\"Select project type\"\r\n                      @select=\"interestCheck\"\r\n                      class=\"mt-3\"\r\n                    >\r\n                    </group-type>\r\n                  </div>\r\n                  <div class=\"mb-4\">\r\n                    <input-group>\r\n                      <input\r\n                        type=\"file\"\r\n                        class=\"form-mentoring form-control form-control-sm\"\r\n                        placeholder=\"add a logo\"\r\n                        @change=\"addLogo\"\r\n                      />\r\n                      <label for=\"logo\"> Add a Logo </label>\r\n                    </input-group>\r\n                  </div>\r\n                  <div class=\"mb-3\">\r\n                    <v-editor\r\n                      cols=\"30\"\r\n                      rows=\"10\"\r\n                      api-key=\"h7t62ozvqkx2ifkeh051fsy3k9irz7axx1g2zitzpbaqfo8m\"\r\n                      v-model=\"group_info.project_desc\"\r\n                      placeholder=\"Project Description\"\r\n                      id=\"desc\"\r\n                    ></v-editor>\r\n                  </div>\r\n                  <hr class=\"my-0 mb-2\" />\r\n                  <div class=\"text-end\">\r\n                    <button\r\n                      type=\"submit\"\r\n                      class=\"btn-mentoring btn-sm bg-primary py-1\"\r\n                    >\r\n                      <i class=\"bi bi-save me-2\"></i>\r\n                      Save Changes\r\n                    </button>\r\n                  </div>\r\n                </div>\r\n              </form>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Meeting  -->\r\n          <div class=\"card border-0 shadow\">\r\n            <div class=\"card-body\">\r\n              <v-meeting\r\n                :menu=\"menu\"\r\n                :group=\"group_info\"\r\n                :student=\"student_info\"\r\n                :data=\"group_meeting\"\r\n                @check=\"checkComponent\"\r\n              ></v-meeting>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"col-md-5 mb-3\">\r\n          <form method=\"post\" @submit.prevent=\"handleUpdateRole()\">\r\n            <div class=\"card border-0 shadow mb-3\">\r\n              <div class=\"card-body\">\r\n                <div class=\"float-end\" v-if=\"menu.key == 'progress'\">\r\n                  <i\r\n                    class=\"fa-solid fa-edit pointer\"\r\n                    v-if=\"!editMember\"\r\n                    @click=\"editMember = !editMember\"\r\n                  ></i>\r\n                  <i\r\n                    class=\"fa-solid fa-x pointer\"\r\n                    v-if=\"editMember\"\r\n                    @click=\"editMember = !editMember\"\r\n                  ></i>\r\n                </div>\r\n                <div class=\"\">\r\n                  <h5 class=\"mb-3\">\r\n                    {{ student_info.first_name + \" \" + student_info.last_name }}\r\n                  </h5>\r\n                  <div class=\"member-role text-center\" v-if=\"!editMember\">\r\n                    {{\r\n                      student_info.contribution_role == null\r\n                        ? \"Not Available\"\r\n                        : student_info.contribution_role\r\n                    }}\r\n                  </div>\r\n                  <div class=\"mb-3\" v-if=\"editMember\">\r\n                    <input-group class=\"text-start\">\r\n                      <input\r\n                        v-model=\"student_info.contribution_role\"\r\n                        type=\"text\"\r\n                        class=\"form-mentoring form-control\"\r\n                        placeholder=\"Fill in your role here ..\"\r\n                        id=\"role\"\r\n                        required\r\n                      />\r\n                      <label for=\"role\">Your role</label>\r\n                    </input-group>\r\n                  </div>\r\n                  <div\r\n                    class=\"border rounded-3 member-contribution\"\r\n                    v-if=\"!editMember\"\r\n                  >\r\n                    {{\r\n                      student_info.contribution_description == null\r\n                        ? \"Describe your contribution here ..\"\r\n                        : student_info.contribution_description\r\n                    }}\r\n                  </div>\r\n                  <div class=\"mb-3\" v-if=\"editMember\">\r\n                    <input-group>\r\n                      <textarea\r\n                        placeholder=\"Describe your contribution here ..\"\r\n                        v-model=\"student_info.contribution_description\"\r\n                        rows=\"5\"\r\n                        class=\"form-mentoring form-control w-100\"\r\n                        id=\"contribute\"\r\n                        required\r\n                      ></textarea>\r\n                      <label for=\"contribute\">Your contribution</label>\r\n                    </input-group>\r\n                  </div>\r\n                  <div class=\"text-center\" v-if=\"editMember\">\r\n                    <button\r\n                      type=\"submit\"\r\n                      class=\"btn-mentoring btn-sm py-1 bg-primary\"\r\n                    >\r\n                      <i class=\"bi bi-save me-2\"></i>\r\n                      Save Changes\r\n                    </button>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </form>\r\n          <div class=\"card border-0 shadow\">\r\n            <div class=\"card-body\">\r\n              <v-member\r\n                :menu=\"menu\"\r\n                :member=\"group_member\"\r\n                :group=\"group_info\"\r\n                :student=\"student_info\"\r\n                @check=\"checkComponent\"\r\n              ></v-member>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Multiselect from \"vue-multiselect\";\r\n\r\nimport Meeting from \"@/components/student/page/activity/group-detail/view-detail/meeting\";\r\nimport Member from \"@/components/student/page/activity/group-detail/view-detail/member\";\r\n\r\nexport default {\r\n  name: \"view\",\r\n  props: {\r\n    menu: Object,\r\n  },\r\n  components: {\r\n    \"v-meeting\": Meeting,\r\n    \"v-member\": Member,\r\n    \"group-type\": Multiselect,\r\n  },\r\n  data() {\r\n    return {\r\n      editGroup: false,\r\n      editMember: false,\r\n      groupType: \"\",\r\n      options: [\"Group Mentoring\", \"Profile Building Mentoring\"],\r\n      groupId: \"\",\r\n      groups: [],\r\n      group_info: [],\r\n      group_logo: [],\r\n      group_meeting: [],\r\n      group_member: [],\r\n      student_info: [],\r\n    };\r\n  },\r\n  methods: {\r\n    redirect() {\r\n      this.$router.push({ path: \"/user/my-activity/group\" });\r\n    },\r\n\r\n    addLogo(e) {\r\n      this.group_logo = e.target.files[0];\r\n    },\r\n\r\n    async getData() {\r\n      this.$Progress.start();\r\n      try {\r\n        const response = await this.$axios.get(\r\n          \"student/detail/group/project/\" + this.groupId\r\n        );\r\n\r\n        this.group_info = response.data.data.group_info;\r\n        this.group_meeting = response.data.data.group_meeting;\r\n        this.group_member = response.data.data.group_member;\r\n        this.student_info = response.data.data.student_info;\r\n        // console.log(response.data);\r\n        this.$Progress.finish();\r\n      } catch (e) {\r\n        this.$Progress.fail();\r\n        this.$alert.toast(\"error\", \"Group project is not found\");\r\n        this.$router.push({ path: \"/user/my-activity/group\" });\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async handleUpdateGroup() {\r\n      let form = new FormData();\r\n      form.append(\"project_name\", this.group_info.project_name);\r\n      form.append(\"project_type\", this.group_info.project_type);\r\n      form.append(\"project_desc\", this.group_info.project_desc);\r\n      form.append(\"project_status\", this.group_info.project_status);\r\n      form.append(\"status\", this.group_info.status);\r\n      form.append(\"owner_type\", this.group_info.owner_type);\r\n      form.append(\"picture\", this.group_logo);\r\n\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\r\n          \"student/update/group/project/\" + this.groupId,\r\n          form\r\n        );\r\n\r\n        this.group_info = response.data.data;\r\n        this.editGroup = false;\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        // console.log(response.data);\r\n        this.$alert.close();\r\n      } catch (e) {\r\n        this.$alert.close();\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again.\");\r\n      }\r\n    },\r\n\r\n    async handleUpdateRole() {\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.put(\r\n          \"student/group/project/participant/\" + this.groupId,\r\n          {\r\n            role: this.student_info.contribution_role,\r\n            description: this.student_info.contribution_description,\r\n          }\r\n        );\r\n\r\n        this.editMember = false;\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        setTimeout(() => {\r\n          this.getData();\r\n        }, 3000);\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        this.$alert.close();\r\n        console.log(e.response.data);\r\n        this.$alert.toast(\"error\", \"Please try again.\");\r\n      }\r\n    },\r\n\r\n    checkComponent(i) {\r\n      if (i == \"new\") {\r\n        this.getData();\r\n      }\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.groupId = this.$route.params.key2;\r\n    if (this.groupId) {\r\n      this.getData();\r\n    } else {\r\n      this.$alert.toast(\"error\", \"Group project is not found\");\r\n      this.$router.push({ path: \"/user/my-activity/group\" });\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.member-role {\r\n  font-size: 0.9em;\r\n  background: #dedede;\r\n  padding: 5px 10px;\r\n  margin-top: 10px !important;\r\n  display: inline;\r\n  border-radius: 20px;\r\n}\r\n\r\n.member-contribution {\r\n  margin-top: 10px;\r\n  font-size: 0.9em;\r\n  border-radius: 10px !important;\r\n  padding: 10px 5px;\r\n}\r\n\r\n.card-overlay {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: #ffffff72;\r\n  z-index: 99;\r\n  cursor: not-allowed;\r\n}\r\n\r\nh6 {\r\n  color: #f0ab54;\r\n}\r\n</style>"],"sourceRoot":""}]}