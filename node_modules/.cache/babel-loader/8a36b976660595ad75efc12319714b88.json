2 py-2 text-center bg-primary\">\r\n            <h6 class=\"my-0\">Waitlisted</h6>\r\n          </div>\r\n\r\n          <draggable\r\n            v-model=\"list.item_1\"\r\n            group=\"uniList\"\r\n            class=\"dragArea list-group\"\r\n            tag=\"waitlisted\"\r\n            :component-data=\"{ status: 'waitlisted' }\"\r\n            @start=\"drag = true\"\r\n            @end=\"drag = false\"\r\n            :move=\"newLog\"\r\n            item-key=\"name\"\r\n          >\r\n            <template #item=\"{ element }\">\r\n              <div\r\n                class=\"card-uni border-1 shadow-sm px-3 py-2 mb-2 text-center\"\r\n              >\r\n                <h6 class=\"my-0\">{{ element.name }}</h6>\r\n                <small>{{ element.major }}</small>\r\n              </div>\r\n            </template>\r\n          </draggable>\r\n        </div>\r\n\r\n        <!-- Applied  -->\r\n        <div class=\"col\">\r\n          <div class=\"card border-1 my-2 py-2 text-center bg-primary\">\r\n            <h6 class=\"my-0\">Applied</h6>\r\n          </div>\r\n\r\n          <draggable\r\n            v-model=\"list.item_2\"\r\n            group=\"uniList\"\r\n            class=\"dragArea list-group\"\r\n            tag=\"waitlisted\"\r\n            :component-data=\"{ status: 'applied' }\"\r\n            @start=\"drag = true\"\r\n            @end=\"drag = false\"\r\n            :move=\"newLog\"\r\n            item-key=\"name\"\r\n          >\r\n            <template #item=\"{ element }\">\r\n              <div\r\n                class=\"card-uni border-1 shadow-sm px-3 py-2 mb-2 text-center\"\r\n              >\r\n                <h6 class=\"my-0\">{{ element.name }}</h6>\r\n                <small>{{ element.major }}</small>\r\n              </div>\r\n            </template>\r\n          </draggable>\r\n        </div>\r\n\r\n        <!-- Accepted  -->\r\n        <div class=\"col\">\r\n          <div class=\"card border-1 my-2 py-2 text-center bg-primary\">\r\n            <h6 class=\"my-0\">Accepted</h6>\r\n          </div>\r\n\r\n          <draggable\r\n            v-model=\"list.item_3\"\r\n            group=\"uniList\"\r\n            class=\"dragArea list-group\"\r\n            tag=\"waitlisted\"\r\n            :component-data=\"{ status: 'accepted' }\"\r\n            @start=\"drag = true\"\r\n            @end=\"drag = false\"\r\n            :move=\"newLog\"\r\n            item-key=\"name\"\r\n          >\r\n            <template #item=\"{ element }\">\r\n              <div\r\n                class=\"card-uni border-1 shadow-sm px-3 py-2 mb-2 text-center\"\r\n              >\r\n                <h6 class=\"my-0\">{{ element.name }}</h6>\r\n                <small>{{ element.major }}</small>\r\n              </div>\r\n            </template>\r\n          </draggable>\r\n        </div>\r\n\r\n        <!-- Rejected  -->\r\n        <div class=\"col\">\r\n          <div class=\"card border-1 my-2 py-2 text-center bg-primary\">\r\n            <h6 class=\"my-0\">Rejected</h6>\r\n          </div>\r\n\r\n          <draggable\r\n            v-model=\"list.item_4\"\r\n            group=\"uniList\"\r\n            class=\"dragArea list-group\"\r\n            tag=\"waitlisted\"\r\n            :component-data=\"{ status: 'rejected' }\"\r\n            @start=\"drag = true\"\r\n            @end=\"drag = false\"\r\n            :move=\"newLog\"\r\n            item-key=\"name\"\r\n          >\r\n            <template #item=\"{ element }\">\r\n              <div\r\n                class=\"card-uni border-1 shadow-sm px-3 py-2 mb-2 text-center\"\r\n              >\r\n                <h6 class=\"my-0\">{{ element.name }}</h6>\r\n                <small>{{ element.major }}</small>\r\n              </div>\r\n            </template>\r\n          </draggable>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n    <transition name=\"pop\">\r\n      <div class=\"vue-modal vue-modal-md\" v-if=\"modal == 'add'\">\r\n        <h6 class=\"my-0\">New University</h6>\r\n        <hr class=\"mb-1\" />\r\n        <form action=\"\">\r\n          <div class=\"mt-2\">\r\n            <v-uni\r\n              v-model=\"uni_select\"\r\n              :options=\"uni_list\"\r\n              placeholder=\"Select One\"\r\n              deselect-label=\"Can't remove this value\"\r\n              track-by=\"uni_name\"\r\n              :custom-label=\"customUnilabel\"\r\n              label=\"uni_name\"\r\n              :searchable=\"true\"\r\n              :allow-empty=\"false\"\r\n              required\r\n              @select=\"uniListCheck\"\r\n            >\r\n            </v-uni>\r\n          </div>\r\n          <div class=\"mb-3\">\r\n            <input-group>\r\n              <input type=\"text\" class=\"form-mentoring w-100\" required />\r\n              <label>Major</label>\r\n            </input-group>\r\n          </div>\r\n          <hr class=\"my-1 mb-3\" />\r\n          <div class=\"d-flex justify-content-between\">\r\n            <button\r\n              class=\"btn-mentoring btn-outline-danger py-1 px-3\"\r\n              type=\"button\"\r\n              @click=\"modal = ''\"\r\n            >\r\n              Cancel\r\n            </button>\r\n            <button class=\"btn-mentoring btn-success py-1 px-3\" type=\"submit\">\r\n              Save\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport draggable from \"vuedraggable\";\r\nimport Multiselect from \"vue-multiselect\";\r\n\r\nexport default {\r\n  name: \"uniShortlisted\",\r\n  components: {\r\n    draggable,\r\n    \"v-uni\": Multiselect,\r\n  },\r\n  data() {\r\n    return {\r\n      drag: false,\r\n      modal: \"\",\r\n      uni_select: [],\r\n      uni_list: [\r\n        {\r\n          id: \"general\",\r\n          uni_name: \"General\",\r\n          uni_m