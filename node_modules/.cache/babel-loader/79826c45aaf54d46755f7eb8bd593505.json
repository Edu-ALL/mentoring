dal = 'add'\"\r\n          >\r\n            <div class=\"card-body py-1 text-center\">\r\n              <i class=\"fa-solid fa-plus me-2\"></i>\r\n              New Todos\r\n            </div>\r\n          </div>\r\n\r\n          <div\r\n            class=\"card border-1 shadow-sm my-2 card-todos\"\r\n            v-for=\"(i, index) in todos_waiting.waiting\"\r\n            :key=\"index\"\r\n          >\r\n            <div class=\"card-body p-0\">\r\n              <div\r\n                class=\"\r\n                  d-flex\r\n                  justify-content-between\r\n                  position-relative\r\n                  overflow-hidden\r\n                \"\r\n              >\r\n                <div\r\n                  class=\"p-3 d-flex justify-content-between\"\r\n                  style=\"width: 90%\"\r\n                >\r\n                  <div style=\"width: 10%\">\r\n                    <i class=\"fa-regular fa-check-circle me-2 text-muted\"></i>\r\n                  </div>\r\n                  <div class=\"pointer\" style=\"width: 90%\" @click=\"todos_id = i\">\r\n                    <div class=\"text-start p-0 m-0\">\r\n                      <transition name=\"fade\">\r\n                        <div class=\"short-desc\" v-if=\"todos_id != i\">\r\n                          {{ i.task_name }}\r\n                        </div>\r\n                      </transition>\r\n                      <transition name=\"fade\">\r\n                        <div class=\"long-desc\" v-if=\"todos_id == i\">\r\n                          {{ i.description }}\r\n                        </div>\r\n                      </transition>\r\n                      <small class=\"d-block mt-3 text-muted\">\r\n                        <i class=\"fa-solid fa-calendar me-2\"></i>\r\n                        {{ i.due_date }}\r\n                      </small>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"todos_action m-0 p-0\" style=\"width: 10%\">\r\n                  <div\r\n                    class=\"\r\n                      bg-danger\r\n                      text-center\r\n                      h-100\r\n                      d-flex\r\n                      align-items-center\r\n                      justify-content-center\r\n                    \"\r\n                    style=\"border-radius: 0 5px 5px 0\"\r\n                  >\r\n                    <i class=\"fa-solid fa-trash text-white\"></i>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"col\">\r\n          <div class=\"card border-1 my-2 py-2 text-center bg-primary\">\r\n            <h6 class=\"my-0\">Confirmation Needs</h6>\r\n          </div>\r\n          <div\r\n            class=\"card border-1 shadow-sm my-2 card-todos\"\r\n            v-for=\"(i, index) in todos_conf_need.confirmation_need\"\r\n            :key=\"index\"\r\n          >\r\n            <div class=\"card-body p-0\">\r\n              <div\r\n                class=\"\r\n                  d-flex\r\n                  justify-content-between\r\n                  position-relative\r\n                  overflow-hidden\r\n                \"\r\n              >\r\n                <div\r\n                  class=\"p-3 d-flex justify-content-between\"\r\n                  style=\"width: 90%\"\r\n                >\r\n                  <div style=\"width: 10%\">\r\n                    <i class=\"fa-regular fa-check-circle me-2 text-muted\"></i>\r\n                  </div>\r\n                  <div class=\"pointer\" style=\"width: 90%\" @click=\"todos_id = i\">\r\n                    <div class=\"text-start p-0 m-0\">\r\n                      <transition name=\"fade\">\r\n                        <div class=\"short-desc\" v-if=\"todos_id != i\">\r\n                          {{ i.task_name }}\r\n                        </div>\r\n                      </transition>\r\n                      <transition name=\"fade\">\r\n                        <div class=\"long-desc\" v-if=\"todos_id == i\">\r\n                          {{ i.description }}\r\n                        </div>\r\n                      </transition>\r\n                      <small class=\"d-block mt-3 text-muted\">\r\n                        <i class=\"fa-solid fa-calendar me-2\"></i\r\n                        >{{ i.due_date }}\r\n                      </small>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"todos_action m-0 p-0\" style=\"width: 10%\">\r\n                  <div\r\n                    class=\"\r\n                      bg-danger\r\n                      text-center\r\n                      h-100\r\n                      d-flex\r\n                      align-items-center\r\n                      justify-content-center\r\n                    \"\r\n                    style=\"border-radius: 0 5px 5px 0\"\r\n                  >\r\n                    <i class=\"fa-solid fa-trash text-white\"></i>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"col\">\r\n          <div class=\"card border-1 my-2 py-2 text-center bg-primary\">\r\n            <h6 class=\"my-0\">Completed</h6>\r\n          </div>\r\n\r\n          <div\r\n            class=\"card border-1 shadow-sm my-2 card-todos text-muted\"\r\n            style=\"background: #f2efef; opacity: 0.7\"\r\n            v-for=\"(i, index) in todos_confirmed.confirmed\"\r\n            :key=\"index\"\r\n          >\r\n            <div class=\"card-body p-0\">\r\n              <div\r\n                class=\"\r\n                  d-flex\r\n                  justify-content-between\r\n                  position-relative\r\n                  overflow-hidden\r\n                \"\r\n              >\r\n                <div\r\n                  class=\"p-3 d-flex justify-content-between\"\r\n                  style=\"width: 90%\"\r\n                >\r\n                  <div style=\"width: 10%\">\r\n                    <i class=\"fa-regular fa-check-circle me-2 text-muted\"></i>\r\n                  </div>\r\n                  <div class=\"pointer\" style=\"width: 90%\" @click=\"todos_id = i\">\r\n                    <div class=\"text-start p-0 m-0\">\r\n                      <transition name=\"fade\">\r\n                        <div class=\"short-desc\" v-if=\"todos_id != i\">\r\n                          {{ i.task_name }}\r\n                        </div>\r\n                      </transition>\r\n                      <transition name=\"fade\">\r\n                        <div class=\"long-desc\" v-if=\"todos_id == i\">\r\n                          {{ i.description }}\r\n                        </div>\r\n                      </transition>\r\n                      <small class=\"d-block mt-3 text-muted\">\r\n                        <i class=\"fa-solid fa-calendar me-2\"></i>\r\n                        {{ i.due_date }}\r\n                      </small>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"todos_action m-0 p-0\" style=\"width: 10%\">\r\n                  <div\r\n                    class=\"\r\n                      bg-danger\r\n                      text-center\r\n                      h-100\r\n                      d-flex\r\n                      align-items-center\r\n                      justify-content-center\r\n                    \"\r\n                    style=\"border-radius: 0 5px 5px 0\"\r\n                  >\r\n                    <i class=\"fa-solid fa-trash text-white\"></i>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n    <transition name=\"pop\">\r\n      <div class=\"vue-modal vue-modal-md\" v-if=\"modal == 'add'\">\r\n        <h6 class=\"my-0\">New Todos</h6>\r\n        <hr class=\"mb-1\" />\r\n        <form action=\"\">\r\n          <div class=\"mt-0\">\r\n            <input-group>\r\n              <input type=\"text\" class=\"form-mentoring w-100\" required />\r\n              <label>Subject</label>\r\n            </input-group>\r\n          </div>\r\n          <div class=\"mt-2\">\r\n            <input-group>\r\n              <input type=\"date\" class=\"form-mentoring w-100\" />\r\n              <label>Deadline</label>\r\n            </input-group>\r\n          </div>\r\n          <div class=\"mt-2\">\r\n            <input-group>\r\n              <span class=\"bg-white\">Project Description</span>\r\n              <textarea cols=\"30\" rows=\"5\" class=\"w-100\" required></textarea>\r\n            </input-group>\r\n          </div>\r\n          <hr class=\"my-1 mb-3\" />\r\n          <div class=\"d-flex justify-content-between\">\r\n            <button\r\n              class=\"btn-mentoring btn-outline-danger py-1 px-3\"\r\n              type=\"button\"\r\n              @click=\"modal = ''\"\r\n            >\r\n              Cancel\r\n            </button>\r\n            <button class=\"btn-mentoring btn-success py-1 px-3\" type=\"submit\">\r\n              Save\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"studentTodos\",\r\n  props: {\r\n    menus: Object,\r\n  },\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      todos_id: \"\",\r\n      todos_waiting: [],\r\n      todos_conf_need: [],\r\n      todos_confirmed: [],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    async getData() {\r\n      const id = this.menus.submenu;\r\n      try {\r\n        const response = await this.$axios.get(\"select/todos/\" + id);\r\n        this.todos_waiting = response.data.data;\r\n        this.todos_conf_need = response.data.data;\r\n        this.todos_confirmed = response.data.data;\r\n        console.log(response);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.short-desc,\r\n.long-desc {\r\n  font-size: 0.9em !important;\r\n}\r\n\r\n.card-todos small {\r\n  font-size: 0.7em;\r\n}\r\n\r\n.todos_action {\r\n  position: absolute;\r\n  z-index: 1;\r\n  right: -40px;\r\n  height: 100%;\r\n  transition: all 0.3s;\r\n  cursor: pointer;\r\n}\r\n\r\n.card-todos:hover .todos_action {\r\n  right: 0;\r\n  z-index: 1;\r\n  height: 100%;\r\n}\r\n</style>"],"sourceRoot":""}]}