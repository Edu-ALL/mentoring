      <div class=\"data\">\r\n                <multiselect\r\n                  v-model=\"interest.name\"\r\n                  :options=\"options\"\r\n                  placeholder=\"Select One\"\r\n                  @select=\"interestCheck\"\r\n                >\r\n                </multiselect>\r\n\r\n                <!-- new -->\r\n                <input\r\n                  type=\"text\"\r\n                  name=\"\"\r\n                  class=\"form-mentoring w-100 form-control-sm mt-2\"\r\n                  placeholder=\"New Interest\"\r\n                  v-if=\"newField\"\r\n                />\r\n\r\n                <div class=\"text-end mt-3\">\r\n                  <button class=\"btn-mentoring btn-sm btn-outline-success py-1\">\r\n                    Save Changes\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </fieldset>\r\n          <table class=\"table table-bordered table-hover mt-2\">\r\n            <tbody>\r\n              <tr v-for=\"i in 3\" :key=\"i\" class=\"align-middle\">\r\n                <td width=\"5%\" align=\"center\">{{ i }}</td>\r\n                <td>\r\n                  <div class=\"d-flex align-items-center\">\r\n                    <div class=\"text\" style=\"width: 95%\">\r\n                      Lorem ipsum dolor sit amet consectetur adipisicing elit.\r\n                    </div>\r\n                    <div class=\"icon text-end\" style=\"width: 5%\">\r\n                      <i\r\n                        class=\"fa-solid fa-trash text-danger pointer\"\r\n                        @click=\"deleteInterest(i)\"\r\n                      ></i>\r\n                    </div>\r\n                  </div>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Multiselect from \"vue-multiselect\";\r\n\r\nexport default {\r\n  name: \"interest\",\r\n  components: { Multiselect },\r\n  data() {\r\n    return {\r\n      add: false,\r\n      newField: false,\r\n      options: [\"Value 1\", \"Value 2\", \"Value 3\", \"Others\"],\r\n      interest: {\r\n        name: null,\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    addCheck() {\r\n      this.add = !this.add;\r\n    },\r\n\r\n    interestCheck(value) {\r\n      if (value == \"Others\") {\r\n        this.newField = true;\r\n      } else {\r\n        this.newField = false;\r\n      }\r\n    },\r\n\r\n    deleteInterest(i) {\r\n      let endPoint = \"delete/interest/\" + i;\r\n      this.$emit(\"check-delete\", endPoint);\r\n    },\r\n  },\r\n  updated() {},\r\n};\r\n</script>\r\n\r\n<style>\r\n.placeholder {\r\n  background-color: white !important;\r\n  color: #000;\r\n}\r\n</style>"],"sourceRoot":""}]}