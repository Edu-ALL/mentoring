;AAAoB,GAA5E,C,iCAwBI,EAvBJ,YAAA,CAsBa,WAtBb,EAsBa;AAtBD,IAAA,IAAI,EAAC;AAsBJ,GAtBb,EAAsB;sBACpB;AAAA,aAoBM,CApB+C,KAAA,CAAA,KAAA,IAAK,S,kBAA1D,mBAAA,CAoBM,KApBN,EAAA,WAAA,EAoBM,CAnBJ,mBAAA,CAkBM,KAlBN,EAAA,WAAA,EAkBM,CAjBJ,WAiBI,EAhBJ,WAgBI,EAbJ,mBAAA,CAYM,KAZN,EAAA,WAAA,EAYM,CAXJ,mBAAA,CAKC,QALD,EAKC;AAJC,QAAA,KAAK,EAAC,4CAIP;AAHE,QAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,iBAAE,KAAA,CAAA,KAAA,GAAK,EAAP;AAAA,SAAA;AAGP,OALD,EAGC,SAHD,CAWI,EANH,mBAAA,CAKQ,QALR,EAKQ;AAJP,QAAA,KAAK,EAAC,oDAIC;AAHN,QAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,iBAAE,QAAA,CAAA,aAAA,CAAc,KAAA,CAAA,QAAd,EAAsB,SAAtB,CAAF;AAAA,SAAA;AAGC,OALR,EAGA,OAHA,CAMG,CAZN,CAaI,CAlBN,CAmBI,CApBN,C,iCAoBM,CApBN;AAAA,K,CADoB;;AAAA,GAAtB,CAuBI,CAlGN,C","sourcesContent":["<template>\r\n  <div id=\"request\">\r\n    <div class=\"container mt-3\">\r\n      <div class=\"row p-4\" v-if=\"groups.data && groups.data.length == 0\">\r\n        <div class=\"col text-center\">\r\n          <p>No project group yet.</p>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"row row-cols-md-2 row-cols-1 g-2\">\r\n        <div class=\"col\" v-for=\"i in groups.data\" :key=\"i\">\r\n          <div class=\"card card-group border shadow-sm\">\r\n            <div class=\"card-body\">\r\n              <h5 class=\"mb-1\">{{ i.project_name }}</h5>\r\n              <span class=\"badge bg-secondary rounded-pill text-dark\">\r\n                {{ i.project_type }}\r\n              </span>\r\n              <div class=\"project-desc mt-2\">\r\n                {{ $customText.text(i.project_desc, 20) }}\r\n              </div>\r\n              <hr class=\"my-2\" />\r\n              <div class=\"row align-items-center\">\r\n                <div class=\"col-6\">\r\n                  <button\r\n                    class=\"btn-mentoring btn-sm py-1 btn-outline-danger\"\r\n                    @click=\"decline(i.id)\"\r\n                  >\r\n                    Decline\r\n                  </button>\r\n                </div>\r\n                <div class=\"col-6 text-end\">\r\n                  <button\r\n                    class=\"btn-mentoring btn-sm py-1 bg-primary\"\r\n                    @click=\"handleConfirm(i.id, 'accept')\"\r\n                  >\r\n                    Accept\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <nav class=\"mt-3\" v-if=\"groups.from != null\">\r\n        <ul class=\"pagination justify-content-center\">\r\n          <li class=\"page-item\" v-if=\"groups.current_page != 1\">\r\n            <a class=\"page-link\" @click=\"getPage(groups.links[0].url)\">\r\n              <i class=\"fa-solid fa-chevron-left\"></i>\r\n            </a>\r\n          </li>\r\n          <div v-for=\"(i, index) in groups.last_page\" :key=\"index\">\r\n            <li\r\n              class=\"page-item\"\r\n              v-if=\"groups.current_page - 2 < i && groups.current_page + 2 > i\"\r\n            >\r\n              <a\r\n                class=\"page-link\"\r\n                :class=\"groups.current_page == i ? 'bg-primary text-white' : ''\"\r\n                href=\"#\"\r\n                @click=\"getPage(groups.path + '?page=' + i)\"\r\n                >{{ i }}</a\r\n              >\r\n            </li>\r\n          </div>\r\n          <li class=\"page-item\" v-if=\"groups.current_page != groups.last_page\">\r\n            <a class=\"page-link\" @click=\"getPage(groups.next_page_url)\">\r\n              <i class=\"fa-solid fa-chevron-right\"></i>\r\n            </a>\r\n          </li>\r\n        </ul>\r\n      </nav>\r\n    </div>\r\n\r\n    <!-- MODAL -->\r\n    <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n    <transition name=\"pop\">\r\n      <div class=\"vue-modal vue-modal-sm bg-primary\" v-if=\"modal == 'decline'\">\r\n        <div class=\"text-center\">\r\n          <i class=\"fa-solid fa-circle-exclamation fa-2xl\"></i>\r\n          <h5 class=\"mt-2\">\r\n            Are you sure you want to refuse to join this group?\r\n          </h5>\r\n          <div class=\"mt-3\">\r\n            <button\r\n              class=\"btn-mentoring btn-warning btn-sm py-1 mx-1\"\r\n              @click=\"modal = ''\"\r\n            >\r\n              Cancel</button\r\n            ><button\r\n              class=\"btn-mentoring btn-outline-success btn-sm py-1 mx-1\"\r\n              @click=\"handleConfirm(group_id, 'decline')\"\r\n            >\r\n              Yes\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"request\",\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      group_id: \"\",\r\n      groups: [],\r\n    };\r\n  },\r\n  methods: {\r\n    async getData() {\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.get(\"student/group/project/new\");\r\n\r\n        this.groups = response.data.data;\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n      this.$alert.close();\r\n    },\r\n\r\n    async getPage(link) {\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.get(link);\r\n\r\n        this.groups = response.data.data;\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n      this.$alert.close();\r\n    },\r\n\r\n    async handleConfirm(id, status) {\r\n      this.modal = \"\";\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\r\n          \"student/group/project/confirmation/req\",\r\n          {\r\n            group_id: id,\r\n            action: status,\r\n          }\r\n        );\r\n\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        this.getData();\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        this.$alert.close();\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    decline(id) {\r\n      this.modal = \"decline\";\r\n      this.group_id = id;\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.card-group {\r\n  position: relative;\r\n  overflow: hidden;\r\n  cursor: pointer;\r\n  border-radius: 15px;\r\n  transition: all 0.3s ease-in-out;\r\n}\r\n\r\n.card-status {\r\n  position: absolute;\r\n  right: -10px;\r\n  top: -10px;\r\n  padding: 10px 10px 4px 4px;\r\n  border-radius: 50%;\r\n}\r\n</style>"],"sourceRoot":""}]}