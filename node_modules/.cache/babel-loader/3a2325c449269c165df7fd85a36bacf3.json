,MAAA,GAAG,EAAE,KADA;AAEL,MAAA,SAAS,EAAE,EAFN;AAGL,MAAA,QAAQ,EAAE,EAHL;AAIL,MAAA,UAAU,EAAE,KAJP;AAKL,MAAA,QAAQ,EAAE;AALL,KAAP;AAOD,GAvBY;AAwBb,EAAA,OAAO,EAAE;AACP,IAAA,QADO,sBACI;AACT;AACA,WAAK,eAAL,CAAqB,KAAK,UAA1B;AACD,KAJM;AAMP,IAAA,SANO,qBAMG,CANH,EAMM;AACX,UAAI,CAAA,IAAK,UAAT,EAAqB;AACnB,aAAK,WAAL;AACF,OAFA,MAEO,IAAI,CAAA,IAAK,MAAT,EAAiB;AACtB,aAAK,eAAL,CAAqB,KAAK,UAA1B;AACF;AACD,KAZM;AAcD,IAAA,eAdC,6BAc0B;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAX,gBAAA,CAAW,0EAAP,KAAO;;AAC/B,gBAAA,KAAI,CAAC,SAAL,CAAe,KAAf;;AACM,gBAAA,EAFyB,GAEpB,KAAI,CAAC,KAAL,CAAW,OAFS;AAAA;AAAA;AAAA,uBAIN,KAAI,CAAC,MAAL,CAAY,GAAZ,CACrB,+BAA+B,EAA/B,GAAoC,GAApC,GAA0C,CADrB,CAJM;;AAAA;AAIvB,gBAAA,QAJuB;AAQ7B,gBAAA,KAAI,CAAC,SAAL,GAAiB,QAAQ,CAAC,IAAT,CAAc,IAA/B;;AACA,gBAAA,KAAI,CAAC,SAAL,CAAe,MAAf,GAT6B,CAU7B;;;AAV6B;AAAA;;AAAA;AAAA;AAAA;;AAY7B,gBAAA,KAAI,CAAC,SAAL,CAAe,IAAf;;AACA,gBAAA,OAAO,CAAC,GAAR,CAAY,YAAE,QAAd;;AAb6B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAehC,KA7BM;AA+BD,IAAA,WA/BC,yBA+Ba;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACZ,gBAAA,EADY,GACP,MAAI,CAAC,KAAL,CAAW,OADJ;AAAA;AAAA;AAAA,uBAGO,MAAI,CAAC,MAAL,CAAY,GAAZ,CACrB,+BAA+B,EADV,CAHP;;AAAA;AAGV,gBAAA,QAHU;AAOhB,gBAAA,MAAI,CAAC,QAAL,GAAgB,QAAQ,CAAC,IAAT,CAAc,IAA9B,CAPgB,CAQhB;;AARgB;AAAA;;AAAA;AAAA;AAAA;AAUhB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AAVgB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYnB,KA3CM;AA6CD,IAAA,UA7CC,wBA6CY;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACX,gBAAA,EADW,GACN,MAAI,CAAC,KAAL,CAAW,OADL;AAAA;AAAA;AAAA,uBAGQ,MAAI,CAAC,MAAL,CAAY,GAAZ,EACrB;AACA,wCAAwB,EAAxB,GAA6B,MAFR,CAHR;;AAAA;AAGT,gBAAA,QAHS;AAQf,gBAAA,MAAI,CAAC,QAAL,GAAgB,QAAQ,CAAC,IAAT,CAAc,IAA9B,CARe,CASf;;AATe;AAAA;;AAAA;AAAA;AAAA;AAWf,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AAXe;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAalB;AA1DM,GAxBI;AAoFb,EAAA,OApFa,qBAoFH;AACR,SAAK,eAAL;AACA,SAAK,WAAL;AACA,SAAK,UAAL;AACD;AAxFY,CAAf","sourcesContent":["<template>\r\n  <div id=\"uni_req\">\r\n    <div class=\"border p-3 rounded mt-3\">\r\n      <div\r\n        class=\"mb-3 overflow-auto d-flex w-100 mentoring-scroll\"\r\n        style=\"white-space: nowrap\"\r\n      >\r\n        <!-- <button\r\n          class=\"btn-mentoring btn-sm me-2 py-0 mt-2 mx-1\"\r\n          :class=\"tab == 'all' ? 'bg-secondary' : 'btn-type-2'\"\r\n          @click=\"checkTab('all')\"\r\n        >\r\n          ALL\r\n        </button> -->\r\n        <div class=\"text-end\" style=\"white-space: nowrap\">\r\n          <select class=\"form-select\" v-model=\"uni_select\" @change=\"checkTab()\">\r\n            <option value=\"all\">General</option>\r\n            <option :value=\"i.imported_id\" v-for=\"i in uni_list\" :key=\"i\">\r\n              {{ i.uni_name }}\r\n            </option>\r\n          </select>\r\n        </div>\r\n        <!-- <div v-for=\"i in uni_list\" :key=\"i\">\r\n          <button\r\n            class=\"btn-mentoring btn-sm mt-1 me-2\"\r\n            :class=\"tab == i.imported_id ? 'bg-secondary' : 'btn-type-2'\"\r\n            @click=\"checkTab(i.imported_id)\"\r\n          >\r\n            {{ i.uni_name }}\r\n          </button>\r\n        </div> -->\r\n      </div>\r\n      <v-essay :data=\"documents.essay\" @check=\"checkData\" />\r\n      <v-sat :data=\"academic.sat\" @check=\"checkData\" />\r\n      <v-lor :data=\"documents.lor\" @check=\"checkData\" />\r\n      <v-transcript :data=\"documents.transcript\" @check=\"checkData\" />\r\n      <v-link :data=\"academic.publication_links\" @check=\"checkData\" />\r\n      <v-score\r\n        :toefl=\"academic.toefl\"\r\n        :ielts=\"academic.ielts\"\r\n        @check=\"checkData\"\r\n      />\r\n      <v-ap :data=\"academic.ap_score\" @check=\"checkData\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Essay from \"@/components/mentor/page/student/activity/requirement/essay\";\r\nimport SAT from \"@/components/mentor/page/student/activity/requirement/sat\";\r\nimport LOR from \"@/components/mentor/page/student/activity/requirement/lor\";\r\nimport Transcript from \"@/components/mentor/page/student/activity/requirement/transcript\";\r\nimport Link from \"@/components/mentor/page/student/activity/requirement/link\";\r\nimport Score from \"@/components/mentor/page/student/activity/requirement/score\";\r\nimport AP from \"@/components/mentor/page/student/activity/requirement/ap\";\r\n\r\nexport default {\r\n  name: \"UniReq\",\r\n  props: {\r\n    menus: Object,\r\n  },\r\n  components: {\r\n    \"v-essay\": Essay,\r\n    \"v-sat\": SAT,\r\n    \"v-lor\": LOR,\r\n    \"v-transcript\": Transcript,\r\n    \"v-link\": Link,\r\n    \"v-score\": Score,\r\n    \"v-ap\": AP,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      tab: \"all\",\r\n      documents: [],\r\n      academic: [],\r\n      uni_select: \"all\",\r\n      uni_list: [],\r\n    };\r\n  },\r\n  methods: {\r\n    checkTab() {\r\n      // alert(this.uni_select);\r\n      this.getDataDocument(this.uni_select);\r\n    },\r\n\r\n    checkData(i) {\r\n      if (i == \"academic\") {\r\n        this.getDataText();\r\n      } else if (i == \"file\") {\r\n        this.getDataDocument(this.uni_select);\r\n      }\r\n    },\r\n\r\n    async getDataDocument(i = \"all\") {\r\n      this.$Progress.start();\r\n      const id = this.menus.submenu;\r\n      try {\r\n        const response = await this.$axios.get(\r\n          \"list/requirement/document/\" + id + \"/\" + i\r\n        );\r\n\r\n        this.documents = response.data.data;\r\n        this.$Progress.finish();\r\n        // console.log(response);\r\n      } catch (e) {\r\n        this.$Progress.fail();\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async getDataText() {\r\n      const id = this.menus.submenu;\r\n      try {\r\n        const response = await this.$axios.get(\r\n          \"list/requirement/academic/\" + id\r\n        );\r\n\r\n        this.academic = response.data.data;\r\n        // console.log(response.data.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async getUniList() {\r\n      const id = this.menus.submenu;\r\n      try {\r\n        const response = await this.$axios.get(\r\n          // https://services.all-inedu.com/api/v1/select/shortlisted/{{student_id}}/all\r\n          \"select/shortlisted/\" + id + \"/all\"\r\n        );\r\n\r\n        this.uni_list = response.data.data;\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.getDataDocument();\r\n    this.getDataText();\r\n    this.getUniList();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.req-card {\r\n  position: relative;\r\n  box-shadow: 0 0.125rem 0.25rem rgba(0, 0, 0, 0.075) !important;\r\n  /* height: 200px; */\r\n  overflow-x: hidden;\r\n  overflow-y: auto;\r\n}\r\n\r\n.req-card:hover .req-header {\r\n  background: #223872;\r\n  color: #fff;\r\n}\r\n\r\n.req-header {\r\n  background: #f3f3f3;\r\n  color: #f0ab54;\r\n  position: sticky;\r\n  font-size: 1.1em;\r\n  font-weight: 600;\r\n  top: 0;\r\n  z-index: 5;\r\n  transition: all 0.3s ease-in-out;\r\n}\r\n\r\n.req-body .card .docs-detail {\r\n  transition: all 0.3s ease-in-out;\r\n  cursor: pointer;\r\n}\r\n\r\n.req-body .card:hover .docs-detail {\r\n  filter: blur(2px);\r\n}\r\n\r\n/* Scroll  */\r\n/* width */\r\n.req-card::-webkit-scrollbar {\r\n  width: 5px;\r\n}\r\n\r\n/* Track */\r\n.req-card::-webkit-scrollbar-track {\r\n  background: #223872;\r\n}\r\n\r\n/* Handle */\r\n.req-card::-webkit-scrollbar-thumb {\r\n  background: #ffc783;\r\n  border-radius: 10px;\r\n}\r\n\r\n/* Handle on hover */\r\n.req-card::-webkit-scrollbar-thumb:hover {\r\n  background: #ffba66;\r\n}\r\n\r\n.docs {\r\n  border: 1px solid rgb(243, 243, 243);\r\n  font-weight: 500;\r\n  border-radius: 10px;\r\n  overflow: hidden;\r\n}\r\n\r\n.docs-content {\r\n  position: absolute;\r\n  top: 100%;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  border-top: 5px solid #223872;\r\n  background: #dedede;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  transition: all 0.4s;\r\n}\r\n\r\n.docs:hover .docs-content {\r\n  top: 30%;\r\n  height: 70%;\r\n}\r\n\r\n.docs-content button i {\r\n  transition: all 0.8s;\r\n}\r\n\r\n.docs-content button:hover,\r\n.docs-content a:hover {\r\n  background: #223872 !important;\r\n}\r\n\r\n.docs-content button:hover i,\r\n.docs-content a:hover i {\r\n  color: #fff !important;\r\n}\r\n</style>"],"sourceRoot":""}]}