N,KAAP;AASD,GAhBY;AAiBb,EAAA,OAAO,EAAE;AACP,IAAA,WADO,uBACK,CADL,EACQ;AACb,WAAK,UAAL,GAAkB,CAAlB;AACA,WAAK,MAAL,GAAc,IAAd;AACD,KAJM;AAMP,IAAA,OANO,qBAMG;AAAA;;AACR,WAAK,MAAL,CACG,GADH,CACO,KAAK,IAAL,GAAY,uBADnB,EAEG,IAFH,CAEQ,UAAC,QAAD,EAAc;AAClB,QAAA,KAAI,CAAC,KAAL,GAAa,QAAQ,CAAC,IAAT,CAAc,IAA3B,CADkB,CAElB;AACD,OALH,EAMG,KANH,CAMS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OARH;AASD,KAhBM;AAkBP,IAAA,OAlBO,mBAkBC,IAlBD,EAkBO;AAAA;;AACZ,WAAK,MAAL,CACG,GADH,CACO,IADP,EAEG,IAFH,CAEQ,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,KAAL,GAAa,QAAQ,CAAC,IAAT,CAAc,IAA3B,CADkB,CAElB;AACD,OALH,EAMG,KANH,CAMS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OARH;AASD,KA5BM;AA8BP,IAAA,UA9BO,wBA8BM;AAAA;;AACX,WAAK,MAAL,CAAY,OAAZ;AACA,WAAK,MAAL,CACG,GADH,CACO,KAAK,IAAL,GAAY,gCAAZ,GAA+C,KAAK,MAAL,CAAY,IADlE,EAEG,IAFH,CAEQ,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,QAAA,MAAI,CAAC,KAAL,GAAa,QAAQ,CAAC,IAAT,CAAc,IAA3B;AACA,QAAA,MAAI,CAAC,MAAL,CAAY,GAAZ,GAAkB,IAAlB;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,QAAZ;AACD,OAPH,EAQG,KARH,CAQS,UAAC,KAAD,EAAW;AAChB,QAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAXH;AAYD,KA5CM;AA8CP,IAAA,WA9CO,yBA8CO;AACZ,WAAK,MAAL,CAAY,GAAZ,GAAkB,KAAlB;AACA,WAAK,MAAL,CAAY,IAAZ,GAAmB,EAAnB;AACA,WAAK,OAAL;AACD;AAlDM,GAjBI;AAqEb,EAAA,OArEa,qBAqEH;AACR,SAAK,OAAL;AACD;AAvEY,CAAf","sourcesContent":["<template>\r\n  <div id=\"successMail\">\r\n    <div class=\"card-white\">\r\n      <div class=\"row mb-2 align-items-center justify-content-between\">\r\n        <div class=\"col-md-6\">\r\n          <button\r\n            type=\"button\"\r\n            class=\"btn-sm btn-mentoring py-1 position-relative me-3\"\r\n            :class=\"menus.submenu == '' ? 'btn-type-1' : ' btn-type-2'\"\r\n            @click=\"this.$router.push({ path: '/admin/mail/' })\"\r\n          >\r\n            Error\r\n            <span\r\n              class=\"\r\n                position-absolute\r\n                top-0\r\n                start-100\r\n                translate-middle\r\n                badge\r\n                rounded-pill\r\n                bg-primary\r\n              \"\r\n              v-if=\"mails_count.error > 0\"\r\n            >\r\n              {{ mails_count.error > 99 ? \"99+\" : mails_count.error }}\r\n            </span>\r\n          </button>\r\n          <button\r\n            type=\"button\"\r\n            class=\"btn-sm btn-mentoring py-1 position-relative me-3\"\r\n            :class=\"menus.submenu == 'success' ? 'btn-type-1' : ' btn-type-2'\"\r\n            @click=\"this.$router.push({ path: '/admin/mail/success' })\"\r\n          >\r\n            Success\r\n            <span\r\n              class=\"\r\n                position-absolute\r\n                top-0\r\n                start-100\r\n                translate-middle\r\n                badge\r\n                rounded-pill\r\n                bg-primary\r\n              \"\r\n              v-if=\"mails_count.success > 0\"\r\n            >\r\n              {{ mails_count.success > 99 ? \"99+\" : mails_count.success }}\r\n            </span>\r\n          </button>\r\n        </div>\r\n\r\n        <div class=\"col-md-3\">\r\n          <input-group>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-mentoring form-control w-100\"\r\n              v-model=\"search.name\"\r\n              @change=\"searchData\"\r\n              placeholder=\"Search\"\r\n              id=\"search\"\r\n            />\r\n            <label for=\"search\">Search</label>\r\n          </input-group>\r\n          <span\r\n            class=\"badge bg-primary px-3 d-inline-block float-end mt-1\"\r\n            v-if=\"search.bar && search.name != ''\"\r\n          >\r\n            {{ search.name }}\r\n            <i class=\"fa-solid fa-close ms-3 pointer\" @click=\"closeSearch\"></i>\r\n          </span>\r\n        </div>\r\n        <!-- <div class=\"col-md-6 text-md-end text-center\">\r\n        <button class=\"btn-mentoring btn-type-1 me-2\">\r\n          Sync Students Data\r\n        </button>\r\n        <button class=\"btn-mentoring btn-type-2\">Add a New Student</button>\r\n      </div> -->\r\n      </div>\r\n      <!-- {{ mails }} -->\r\n      <div class=\"table-responsive\">\r\n        <table class=\"table table-bordered align-middle table-hover\">\r\n          <thead>\r\n            <tr class=\"text-center\">\r\n              <th>No</th>\r\n              <th>Receiver</th>\r\n              <th>Sender</th>\r\n              <th>Subject</th>\r\n              <th>Date</th>\r\n              <th>Status</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr\r\n              class=\"text-center\"\r\n              v-for=\"(i, index) in mails.data\"\r\n              :key=\"index\"\r\n            >\r\n              <td>{{ mails.from + index }}</td>\r\n              <td>\r\n                <i class=\"fa-regular fa-user fa-fw\"></i> {{ i.recipient }}\r\n              </td>\r\n              <td><i class=\"fa-regular fa-user fa-fw\"></i> {{ i.sender }}</td>\r\n              <td>{{ i.subject }}</td>\r\n              <td>\r\n                <i class=\"fa-regular fa-calendar fa-fw\"></i>\r\n                {{ $customDate.date(i.date_sent) }}\r\n              </td>\r\n              <td class=\"pointer\">\r\n                <div\r\n                  class=\"text-primary\"\r\n                  v-if=\"i.error_status\"\r\n                  @click=\"checkDetail(i)\"\r\n                >\r\n                  <i class=\"fa-solid fa-check fa-fw\"></i>\r\n                  Solved\r\n                </div>\r\n                <div class=\"text-success\" v-if=\"!i.error_status\">\r\n                  <i class=\"fa-regular fa-circle-check fa-fw\"></i>\r\n                  Success\r\n                </div>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <div class=\"text-center\" v-if=\"mails.from == null\">\r\n        <hr />\r\n        <h6>Sorry, data is not found</h6>\r\n      </div>\r\n\r\n      <v-pagination :datas=\"mails\" @result=\"getPage\" />\r\n    </div>\r\n\r\n    <transition name=\"fade\">\r\n      <div class=\"\" v-if=\"detail\">\r\n        <div class=\"vue-modal vue-modal-md\">\r\n          <h6>Error Description</h6>\r\n          <hr class=\"my-0 mb-2\" />\r\n          <p>\r\n            {{ detailData.error_message }}\r\n          </p>\r\n        </div>\r\n        <div class=\"vue-modal-overlay\" @click=\"detail = false\"></div>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"successMail\",\r\n  props: {\r\n    menus: Object,\r\n    mails_count: Object,\r\n  },\r\n  data() {\r\n    return {\r\n      mails: [],\r\n      search: {\r\n        bar: false,\r\n        name: \"\",\r\n      },\r\n      detail: false,\r\n      detalData: [],\r\n    };\r\n  },\r\n  methods: {\r\n    checkDetail(i) {\r\n      this.detailData = i;\r\n      this.detail = true;\r\n    },\r\n\r\n    getData() {\r\n      this.$axios\r\n        .get(this.$url + \"list/mail/log/success\")\r\n        .then((response) => {\r\n          this.mails = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    getPage(link) {\r\n      this.$axios\r\n        .get(link)\r\n        .then((response) => {\r\n          this.mails = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    searchData() {\r\n      this.$alert.loading();\r\n      this.$axios\r\n        .get(this.$url + \"list/mail/log/success?keyword=\" + this.search.name)\r\n        .then((response) => {\r\n          this.$alert.close();\r\n          this.mails = response.data.data;\r\n          this.search.bar = true;\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$alert.close();\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    closeSearch() {\r\n      this.search.bar = false;\r\n      this.search.name = \"\";\r\n      this.getData();\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":""}]}