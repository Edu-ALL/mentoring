B,CAAC,CAAC,eAAF,IAAiB,EAAjB,IAA2B,CAAC,CAAC,eAAF,IAAiB,I,kBAT9E,mBAAA,CAeM,KAfN,EAeM;YAAA;AAdJ,MAAA,KAAK,EAAA,eAAA,CAAA,CAAC,kCAAD,EAC6B,CAAC,CAAC,eAAF,IAAiB,OAAjB,G,YAAA,GAAoG,CAAC,CAAC,eAAF,IAAiB,UAAjB,G,SAAA,G,WADjI,CAAA,CAcD;AAHJ,MAAA,KAAkC,EAAlC;AAAA,0BAAA;AAAA;AAGI,KAfN,E,iBAcK,CAAC,CAAC,e,CAdP,EAcsB;AAAA;AAdtB,K,qCAgBI,CAvBN,CAwBI,CA/CN,CAyDI,EATJ,mBAAA,CAQM,KARN,EAAA,WAAA,EAQM,CAPJ,mBAAA,CAMS,QANT,EAMS;AALP,MAAA,KAAK,EAAC,yCAKC;AAJN,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,QAAA,CAAA,MAAA,CAAO,CAAC,CAAC,EAAT,CAAF;AAAA;AAIC,KANT,E,WAAA,E;;AAAA,M,WAAA,CAOI,CARN,CASI,CA1DN,CA2DI,CA5DN,CA6DI,CA9DN,C,CADF,C;GAgEM,CAhEN,E;;AAAA,GAiEI,EAlEN,CAoEI,EADJ,YAAA,CAAkD,uBAAlD,EAAkD;AAAnC,IAAA,KAAK,EAAE,KAAA,CAAA,MAA4B;AAAnB,IAAA,QAAM,EAAE,QAAA,CAAA;AAAW,GAAlD,E,IAAA,E;;AAAA,I,qBAAA,CACI,CA9EN,C,qCA+EI,CAlFN,C","sourcesContent":["<template>\r\n  <div id=\"progress\" class=\"p-0\">\r\n    <!-- LIST -->\r\n    <!-- {{ check }} -->\r\n    <div\r\n      class=\"container mt-3 p-2\"\r\n      v-if=\"groups.data && groups.data.length >= 0\"\r\n    >\r\n      <div class=\"row p-4\" v-if=\"groups.data && groups.data.length == 0\">\r\n        <div class=\"col text-center\">\r\n          <p>No project group yet.</p>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"row row-cols-1 g-2\">\r\n        <div class=\"col\" v-for=\"(i, index) in groups.data\" :key=\"i\">\r\n          <div class=\"card group-card\">\r\n            <div class=\"card-body\">\r\n              <div class=\"row p-0 align-items-center\">\r\n                <div class=\"col-md-9\">\r\n                  <div class=\"d-flex align-items-center\">\r\n                    <div class=\"group-image\">\r\n                      <img\r\n                        v-lazy=\"\r\n                          i.picture\r\n                            ? $base_url + '' + i.picture\r\n                            : 'https://picsum.photos/id/' +\r\n                              groups.from +\r\n                              index * 2 +\r\n                              '/300/300'\r\n                        \"\r\n                        class=\"w-100\"\r\n                      />\r\n                    </div>\r\n                    <div class=\"title ms-3\">\r\n                      <h5>{{ i.project_name }}</h5>\r\n                      <p\r\n                        class=\"my-0\"\r\n                        v-html=\"$customText.text(i.project_desc, 20)\"\r\n                      ></p>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"mt-2\">\r\n                    <!-- <div class=\"badge badge-group bg-info px-3 py-1\">\r\n                      {{ i.project_type }}\r\n                    </div> -->\r\n                    <div class=\"badge badge-group bg-primary px-3 py-1 ms-1\">\r\n                      {{ i.group_participant_count }} Members\r\n                    </div>\r\n                    <div\r\n                      class=\"badge badge-group px-3 py-1 ms-1\"\r\n                      :class=\"\r\n                        i.progress_status == 'ahead'\r\n                          ? 'bg-success'\r\n                          : i.progress_status == 'on-track'\r\n                          ? 'bg-info'\r\n                          : 'bg-danger'\r\n                      \"\r\n                      v-if=\"\r\n                        i.progress_status != '' || i.progress_status != null\r\n                      \"\r\n                      style=\"text-transform: capitalize\"\r\n                    >\r\n                      {{ i.progress_status }}\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-md-3 text-md-center mt-md-0 mt-3\">\r\n                  <button\r\n                    class=\"btn-mentoring btn-secondary py-1 btn-sm\"\r\n                    @click=\"detail(i.id)\"\r\n                  >\r\n                    View Detail\r\n                    <i class=\"fa-solid fa-arrow-right ms-2\"></i>\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <v-pagination :datas=\"groups\" @result=\"getPage\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"progress\",\r\n  props: {\r\n    check: String,\r\n  },\r\n  components: {},\r\n  data() {\r\n    return {\r\n      groups: [],\r\n    };\r\n  },\r\n  methods: {\r\n    detail(i) {\r\n      this.$router.push({ path: \"/user/my-activity/group/progress/\" + i });\r\n    },\r\n\r\n    async getData() {\r\n      this.$Progress.start();\r\n      try {\r\n        const response = await this.$axios.get(\r\n          \"student/group/project/in-progress\"\r\n        );\r\n\r\n        this.groups = response.data.data;\r\n        // console.log(response.data);\r\n        this.$Progress.finish();\r\n      } catch (e) {\r\n        this.$Progress.fail();\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async getPage(link) {\r\n      this.$Progress.start();\r\n      try {\r\n        const response = await this.$axios.get(link);\r\n\r\n        this.groups = response.data.data;\r\n        // console.log(response.data);\r\n        this.$Progress.finish();\r\n      } catch (e) {\r\n        this.$Progress.fail();\r\n        console.log(e.response);\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n  updated() {\r\n    if (this.check == \"new-group\") {\r\n      this.getData();\r\n      this.$emit(\"modal\", \"\");\r\n    }\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}