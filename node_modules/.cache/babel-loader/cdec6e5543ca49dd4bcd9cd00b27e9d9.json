ass=\"req-header p-2 py-1\">\r\n      <i class=\"fa-solid fa-circle-xmark me-1 text-danger\"></i>\r\n      <i class=\"fa-solid fa-circle-check me-1 text-success\"></i>\r\n      AP Score\r\n      <div class=\"float-end\">\r\n        <i class=\"fa-solid fa-add pointer\" @click=\"modal = 'add'\"></i>\r\n      </div>\r\n    </div>\r\n    <div class=\"req-body p-2\">\r\n      <div class=\"row row-cols-1\">\r\n        <div class=\"col\">\r\n          <table class=\"table\">\r\n            <tr>\r\n              <td>Reading</td>\r\n              <td class=\"text-end\">20</td>\r\n            </tr>\r\n            <tr>\r\n              <td>Listening</td>\r\n              <td class=\"text-end\">20</td>\r\n            </tr>\r\n            <tr>\r\n              <td>Speaking</td>\r\n              <td class=\"text-end\">20</td>\r\n            </tr>\r\n            <tr>\r\n              <td>Writing</td>\r\n              <td class=\"text-end\">20</td>\r\n            </tr>\r\n            <tfoot>\r\n              <tr>\r\n                <td>Total Score</td>\r\n                <td class=\"text-end\">80</td>\r\n              </tr>\r\n            </tfoot>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n  <!-- Modal Document  -->\r\n  <transition name=\"pop\">\r\n    <div class=\"vue-modal vue-modal-sm\" v-if=\"modal == 'add'\">\r\n      <h6>AP Score</h6>\r\n      <div class=\"row\">\r\n        <div class=\"col\">\r\n          <div class=\"mb-1\">\r\n            <label>Subject</label>\r\n            <multiselect\r\n              v-model=\"value\"\r\n              :options=\"options\"\r\n              placeholder=\"Select One\"\r\n            >\r\n            </multiselect>\r\n          </div>\r\n          <div class=\"mb-1\">\r\n            <label>Score</label>\r\n            <input\r\n              type=\"number\"\r\n              name=\"\"\r\n              id=\"\"\r\n              max=\"30\"\r\n              class=\"form-mentoring form-control-sm w-100\"\r\n            />\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"text-center mt-3\">\r\n        <button class=\"btn-mentoring btn-sm py-1 bg-primary\">\r\n          <i class=\"fa-solid fa-save me-2\"></i>\r\n          Save\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </transition>\r\n</template>\r\n\r\n<script>\r\nimport Multiselect from \"vue-multiselect\";\r\n\r\nexport default {\r\n  name: \"apScore\",\r\n  components: {\r\n    Multiselect,\r\n  },\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      value: \"\",\r\n      options: [\r\n        \"AP Art History\",\r\n        \"AP Biology\",\r\n        \"AP Calculus AB\",\r\n        \"AP Calculus BC\",\r\n        \"AP Chemistry\",\r\n        \"AP Chinese Language & Culture\",\r\n        \"AP Comparative Government & Politics\",\r\n        \"AP Computer Science A\",\r\n      ],\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nlabel::after {\r\n  content: \"\";\r\n}\r\n\r\n.score-card {\r\n  display: block;\r\n  width: 100%;\r\n  height: 30px;\r\n  text-align: center;\r\n  background: blue;\r\n}\r\n\r\n.score-cat {\r\n  display: flex;\r\n  align-items: middle;\r\n}\r\n\r\n.score-card input {\r\n  display: none;\r\n}\r\n\r\ninput:checked {\r\n  background: red;\r\n}\r\n</style>"],"sourceRoot":""}]}