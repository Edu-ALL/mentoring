,GAAR,CAAY,YAAE,QAAd;;AAZY;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcf,KAfM;AAiBD,IAAA,OAjBC,mBAiBO,IAjBP,EAiBa;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAClB,gBAAA,MAAI,CAAC,eAAL,GAAuB,EAAvB;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,OAAZ;;AAFkB;AAAA;AAAA,uBAIO,MAAI,CAAC,MAAL,CAAY,GAAZ,CAAgB,IAAhB,CAJP;;AAAA;AAIV,gBAAA,QAJU;AAMhB,gBAAA,MAAI,CAAC,eAAL,GAAuB,QAAQ,CAAC,IAAT,CAAc,IAArC,CANgB,CAOhB;;AAPgB;AAAA;;AAAA;AAAA;AAAA;AAShB,gBAAA,OAAO,CAAC,GAAR;;AATgB;AAWlB,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AAXkB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYnB;AA7BM,GARI;AAuCb,EAAA,OAvCa,qBAuCH;AACR,SAAK,OAAL;AACD;AAzCY,CAAf","sourcesContent":["<template>\r\n  <!-- {{ student_meeting }} -->\r\n  <div id=\"meetings\">\r\n    <div class=\"border p-3 rounded mt-3\">\r\n      <div class=\"table-responsive\">\r\n        <table class=\"table align-middle\">\r\n          <thead>\r\n            <tr class=\"text-center\">\r\n              <th>No</th>\r\n              <th>Call with</th>\r\n              <th>Subject</th>\r\n              <th>Date & Time</th>\r\n              <th>Status</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr class=\"text-center\" v-if=\"student_meeting?.data?.length == 0\">\r\n              <td class=\"py-2\" colspan=\"4\">No data!</td>\r\n            </tr>\r\n            <tr\r\n              class=\"text-center\"\r\n              v-for=\"(i, index) in student_meeting.data\"\r\n              :key=\"index\"\r\n            >\r\n              <td>{{ parseInt(index) + 1 }}</td>\r\n\r\n              <td>\r\n                {{\r\n                  i.users.first_name +\r\n                  \" \" +\r\n                  i.users.last_name +\r\n                  \" - \" +\r\n                  i.call_with\r\n                }}\r\n              </td>\r\n              <td style=\"text-transform: capitalize\">{{ i.module }}</td>\r\n              <td>\r\n                <small>\r\n                  {{ $customDate.date(i.call_date) }} <br />\r\n                  {{ $customDate.time(i.call_date) }}\r\n                </small>\r\n              </td>\r\n              <td style=\"text-transform: capitalize\">\r\n                <i\r\n                  class=\"fa-solid fa-ban text-danger\"\r\n                  v-if=\"i.call_status == 'rejected'\"\r\n                ></i>\r\n                <i\r\n                  class=\"fa-solid fa-clock text-info\"\r\n                  v-if=\"i.call_status == 'waiting'\"\r\n                ></i>\r\n                <i\r\n                  class=\"fa-solid fa-times-circle text-danger\"\r\n                  v-if=\"i.call_status == 'canceled'\"\r\n                ></i>\r\n                <i\r\n                  class=\"fa-solid fa-check-circle text-success\"\r\n                  v-if=\"i.call_status == 'finished'\"\r\n                ></i>\r\n                {{ i.call_status }}\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <nav class=\"mt-3\" v-if=\"student_meeting.from != null\">\r\n        <ul class=\"pagination justify-content-center\">\r\n          <li class=\"page-item\" v-if=\"student_meeting.current_page != 1\">\r\n            <a class=\"page-link\" @click=\"getPage(student_meeting.links[0].url)\">\r\n              <i class=\"fa-solid fa-chevron-left\"></i>\r\n            </a>\r\n          </li>\r\n          <div v-for=\"(i, index) in student_meeting.last_page\" :key=\"index\">\r\n            <li\r\n              class=\"page-item\"\r\n              v-if=\"\r\n                student_meeting.current_page - 2 < i &&\r\n                student_meeting.current_page + 2 > i\r\n              \"\r\n            >\r\n              <a\r\n                class=\"page-link\"\r\n                :class=\"\r\n                  student_meeting.current_page == i\r\n                    ? 'bg-primary text-white'\r\n                    : ''\r\n                \"\r\n                href=\"#\"\r\n                @click=\"\r\n                  getPage(\r\n                    student_meeting.path +\r\n                      '?student=' +\r\n                      menus.submenu +\r\n                      '&page=' +\r\n                      i\r\n                  )\r\n                \"\r\n                >{{ i }}</a\r\n              >\r\n            </li>\r\n          </div>\r\n          <li\r\n            class=\"page-item\"\r\n            v-if=\"student_meeting.current_page != student_meeting.last_page\"\r\n          >\r\n            <a\r\n              class=\"page-link\"\r\n              @click=\"getPage(student_meeting.next_page_url)\"\r\n            >\r\n              <i class=\"fa-solid fa-chevron-right\"></i>\r\n            </a>\r\n          </li>\r\n        </ul>\r\n      </nav>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"studentMeeting\",\r\n  data() {\r\n    return {\r\n      student_meeting: [],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    async getData() {\r\n      this.$alert.loading();\r\n      const id = this.$route.params.key;\r\n      try {\r\n        const response = await this.$axios.get(\r\n          \"mentor/list/activities/1-on-1-call?student=\" + id\r\n        );\r\n        this.student_meeting = response.data.data;\r\n        this.$alert.close();\r\n        // console.log(response);\r\n      } catch (e) {\r\n        this.$alert.close();\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async getPage(link) {\r\n      this.student_meeting = [];\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.get(link);\r\n\r\n        this.student_meeting = response.data.data;\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e);\r\n      }\r\n      this.$alert.close();\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}