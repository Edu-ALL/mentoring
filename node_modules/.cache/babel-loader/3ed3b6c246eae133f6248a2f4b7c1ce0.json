,EAAU;AACR,WAAK,KAAL,GAAa,IAAI,CAAC,IAAL,CAAU,KAAvB;AACA,WAAK,IAAL,GAAY,IAAI,CAAC,IAAL,CAAU,OAAtB;AACF,KAHA,MAGO;AACL,WAAK,KAAL,CAAW,SAAX,EAAsB,uBAAtB;AACA,WAAK,OAAL,CAAa,IAAb,CAAkB;AAAE,QAAA,IAAI,EAAE;AAAR,OAAlB;AACF;AACD;AAtCY,CAAf","sourcesContent":["<template>\n  <div id=\"user\"></div>\n</template>\n\n<script>\n// import axios from \"axios\";\nimport Swal from \"sweetalert2\";\n\nexport default {\n  name: \"User\",\n  data() {\n    return {\n      token: \"\",\n      role: \"\",\n    };\n  },\n  methods: {\n    toast(status, title) {\n      const Toast = Swal.mixin({\n        toast: true,\n        width: \"33%\",\n        position: \"top-end\",\n        showConfirmButton: false,\n        timer: 3000,\n        timerProgressBar: true,\n        didOpen: (toast) => {\n          toast.addEventListener(\"mouseenter\", Swal.stopTimer);\n          toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\n        },\n      });\n\n      Toast.fire({\n        icon: status,\n        title: title,\n      });\n    },\n  },\n  created() {\n    const user = JSON.parse(sessionStorage.getItem(\"user\"));\n    if (user) {\n      this.token = user.data.token;\n      this.role = user.data.role_id;\n    } else {\n      this.toast(\"warning\", \"Please log in first !\");\n      this.$router.push({ path: \"/\" });\n    }\n  },\n};\n</script>"],"sourceRoot":""}]}