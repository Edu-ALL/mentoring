a,EAAb;;AAEA,gBAAA,MAAI,CAAC,OAAL;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C,EAXiB,CAYjB;;;AAZiB;AAAA;;AAAA;AAAA;AAAA;;AAcjB,oBAAI,aAAE,QAAF,CAAW,MAAX,IAAqB,GAAzB,EAA8B;AAC5B,kBAAA,MAAI,CAAC,KAAL,GAAa,aAAE,QAAF,CAAW,IAAX,CAAgB,KAA7B;AACF,iBAFA,MAEO;AACL,kBAAA,MAAI,CAAC,KAAL,GAAa,EAAb;AACF;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,mBAA3B;;AAnBiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqBpB;AAxEM,GAnBI;AA6Fb,EAAA,OA7Fa,qBA6FH;AACR,SAAK,OAAL;AACD;AA/FY,CAAf","sourcesContent":["<template>\r\n  <div id=\"lastMeeting\">\r\n    <div class=\"row p-4\" v-if=\"meeting?.data?.length == 0\">\r\n      <div class=\"col text-center\">\r\n        <p class=\"my-0\">No meeting yet.</p>\r\n      </div>\r\n    </div>\r\n    <div class=\"table-responsive\" v-if=\"meeting?.data?.length != 0\">\r\n      <table class=\"table table-bordered table-hover\">\r\n        <thead>\r\n          <tr class=\"text-center\">\r\n            <th width=\"3%\">No</th>\r\n            <th>Call With</th>\r\n            <th>Subject</th>\r\n            <th>Date</th>\r\n            <th>Time</th>\r\n            <th>Status</th>\r\n            <th>Action</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr\r\n            v-for=\"(i, index) in meeting.data\"\r\n            :key=\"index\"\r\n            class=\"text-center align-middle\"\r\n          >\r\n            <td>{{ meeting.from + index }}</td>\r\n            <td class=\"text-start\" nowrap style=\"text-transform: capitalize\">\r\n              {{ i.students.first_name + \" \" + i.students.last_name }}\r\n            </td>\r\n            <td nowrap style=\"text-transform: capitalize\">\r\n              {{ i.module }}\r\n            </td>\r\n            <td nowrap>\r\n              {{ $customDate.date(i.start_call_date) }}\r\n            </td>\r\n            <td nowrap>\r\n              {{\r\n                $customDate.time(i.start_call_date) +\r\n                \" - \" +\r\n                $customDate.time(i.end_call_date)\r\n              }}\r\n            </td>\r\n            <td nowrap style=\"text-transform: capitalize\">\r\n              <small class=\"text-warning\" v-if=\"i.call_status == 'canceled'\">\r\n                <i class=\"fa-solid fa-times\"></i>\r\n                {{ i.call_status }}\r\n              </small>\r\n              <small class=\"text-info\" v-if=\"i.call_status == 'finished'\">\r\n                <i class=\"fa-solid fa-check\"></i>\r\n                {{ i.call_status }}\r\n              </small>\r\n              <small class=\"text-danger\" v-if=\"i.call_status == 'rejected'\">\r\n                <i class=\"fa-solid fa-times\"></i>\r\n                {{ i.call_status }}\r\n              </small>\r\n            </td>\r\n            <td nowrap>\r\n              <button\r\n                v-if=\"i.call_status == 'finished' && i.meeting_minute == 1\"\r\n                class=\"btn-mentoring py-1 px-3 btn-sm btn-primary text-white\"\r\n                @click=\"detail(i.id)\"\r\n              >\r\n                View Result\r\n              </button>\r\n              <button\r\n                v-if=\"i.call_status == 'finished' && i.meeting_minute == 0\"\r\n                class=\"btn-mentoring py-1 px-3 btn-sm bg-success text-white\"\r\n                @click=\"detail(i.id)\"\r\n              >\r\n                Add Result\r\n              </button>\r\n              <div v-if=\"i.call_status != 'finished'\">-</div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n    <v-pagination :datas=\"meeting\" @result=\"getPage\" />\r\n  </div>\r\n\r\n  <!-- Last Meeting  -->\r\n  <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n  <div\r\n    class=\"vue-side mentoring-scroll shadow\"\r\n    :class=\"modal == 'add' ? 'active' : ''\"\r\n  >\r\n    <div class=\"d-flex w-100 justify-content-between\">\r\n      <h6 class=\"mb-0\">Last Meeting Minutes</h6>\r\n      <div class=\"text-end\">\r\n        <button\r\n          class=\"btn btn-sm btn-mentoring btn-outline-info py-1\"\r\n          @click=\"view_status = 'add'\"\r\n          v-if=\"view_status == 'view'\"\r\n        >\r\n          Edit\r\n        </button>\r\n      </div>\r\n    </div>\r\n    <hr class=\"my-2\" />\r\n    <!-- ADD  -->\r\n    <form @submit.prevent=\"handleSubmit\" v-if=\"view_status == 'add'\">\r\n      <div class=\"row row-cols-md-2 row-cols-1\">\r\n        <div class=\"col mb-3\">\r\n          <input-group>\r\n            <v-editor\r\n              api-key=\"h7t62ozvqkx2ifkeh051fsy3k9irz7axx1g2zitzpbaqfo8m\"\r\n              class=\"form-control form-mentoring w-100\"\r\n              placeholder=\"Academic Performance\"\r\n              v-model=\"meeting_minutes.academic_performance\"\r\n              rows=\"5\"\r\n              id=\"acad\"\r\n            ></v-editor>\r\n          </input-group>\r\n        </div>\r\n        <div class=\"col mb-3\">\r\n          <input-group>\r\n            <v-editor\r\n              api-key=\"h7t62ozvqkx2ifkeh051fsy3k9irz7axx1g2zitzpbaqfo8m\"\r\n              class=\"form-control form-mentoring w-100\"\r\n              placeholder=\"Exploration\"\r\n              v-model=\"meeting_minutes.exploration\"\r\n              rows=\"5\"\r\n              id=\"exploration\"\r\n            ></v-editor>\r\n          </input-group>\r\n        </div>\r\n        <div class=\"col mb-3\">\r\n          <input-group>\r\n            <v-editor\r\n              api-key=\"h7t62ozvqkx2ifkeh051fsy3k9irz7axx1g2zitzpbaqfo8m\"\r\n              class=\"form-control form-mentoring w-100\"\r\n              placeholder=\"Writing Skills\"\r\n              v-model=\"meeting_minutes.writing_skills\"\r\n              rows=\"5\"\r\n              id=\"writing\"\r\n            ></v-editor>\r\n          </input-group>\r\n        </div>\r\n        <div class=\"col mb-3\">\r\n          <input-group>\r\n            <v-editor\r\n              api-key=\"h7t62ozvqkx2ifkeh051fsy3k9irz7axx1g2zitzpbaqfo8m\"\r\n              class=\"form-control form-mentoring w-100\"\r\n              placeholder=\"Personal Brand\"\r\n              v-model=\"meeting_minutes.personal_brand\"\r\n              rows=\"5\"\r\n              id=\"personal\"\r\n            ></v-editor>\r\n          </input-group>\r\n        </div>\r\n      </div>\r\n      <div class=\"mb-3\">\r\n        <small class=\"text-danger\" v-if=\"error?.mt_todos_note\">\r\n          {{ error.mt_todos_note[0] }}\r\n        </small>\r\n        <input-group>\r\n          <v-editor\r\n            api-key=\"h7t62ozvqkx2ifkeh051fsy3k9irz7axx1g2zitzpbaqfo8m\"\r\n            class=\"form-control form-mentoring w-100\"\r\n            placeholder=\"Mentor Todos\"\r\n            v-model=\"meeting_minutes.mt_todos_note\"\r\n            rows=\"5\"\r\n            id=\"mentorTodos\"\r\n          ></v-editor>\r\n        </input-group>\r\n      </div>\r\n      <div class=\"mb-3\">\r\n        <small class=\"text-danger\" v-if=\"error?.st_todos_note\">\r\n          {{ error.st_todos_note[0] }}\r\n        </small>\r\n        <input-group>\r\n          <v-editor\r\n            api-key=\"h7t62ozvqkx2ifkeh051fsy3k9irz7axx1g2zitzpbaqfo8m\"\r\n            class=\"form-control form-mentoring w-100\"\r\n            placeholder=\"Mentee Todos\"\r\n            v-model=\"meeting_minutes.st_todos_note\"\r\n            rows=\"5\"\r\n            id=\"menteeTodos\"\r\n          ></v-editor>\r\n        </input-group>\r\n      </div>\r\n      <hr />\r\n      <div class=\"text-end\">\r\n        <button class=\"btn-mentoring btn-sm bg-primary py-1 px-3\" type=\"submit\">\r\n          <i class=\"fa-solid fa-save me-2\"></i>\r\n          Save Changes\r\n        </button>\r\n      </div>\r\n    </form>\r\n\r\n    <!-- VIEW  -->\r\n    <div class=\"row row-cols-1\" v-if=\"view_status == 'view'\">\r\n      <div class=\"col\">\r\n        <div class=\"mb-2\">\r\n          <label class=\"mb-3 text-primary\">Academic Performance :</label>\r\n          <div class=\"py-1 px-3\" style=\"border-left: 2px solid #dedede\">\r\n            <p class=\"py-0\" v-html=\"meeting_minutes.academic_performance\"></p>\r\n          </div>\r\n        </div>\r\n        <div class=\"mb-2\">\r\n          <label class=\"mb-3 text-primary\">Exploration :</label>\r\n          <div class=\"py-1 px-3\" style=\"border-left: 2px solid #dedede\">\r\n            <p class=\"py-0\" v-html=\"meeting_minutes.exploration\"></p>\r\n          </div>\r\n        </div>\r\n        <div class=\"mb-2\">\r\n          <label class=\"mb-3 text-primary\">Writing Skills :</label>\r\n          <div class=\"py-1 px-3\" style=\"border-left: 2px solid #dedede\">\r\n            <p class=\"py-0\" v-html=\"meeting_minutes.writing_skills\"></p>\r\n          </div>\r\n        </div>\r\n        <div class=\"mb-2\">\r\n          <label class=\"mb-3 text-primary\">Personal Brand :</label>\r\n          <div class=\"py-1 px-3\" style=\"border-left: 2px solid #dedede\">\r\n            <p class=\"py-0\" v-html=\"meeting_minutes.personal_brand\"></p>\r\n          </div>\r\n        </div>\r\n        <div class=\"mb-2\">\r\n          <label class=\"mb-3 text-primary\">My Todos :</label>\r\n          <div class=\"py-1 px-3\" style=\"border-left: 2px solid #dedede\">\r\n            <p class=\"py-0\" v-html=\"meeting_minutes.mt_todos_note\"></p>\r\n          </div>\r\n        </div>\r\n        <div class=\"mb-2\">\r\n          <label class=\"mb-3 text-primary\">Mentee Todos :</label>\r\n          <div class=\"py-1 px-3\" style=\"border-left: 2px solid #dedede\">\r\n            <p class=\"py-0\" v-html=\"meeting_minutes.st_todos_note\"></p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"lastMeetingComponent\",\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      view_status: \"\",\r\n      meeting: [],\r\n      meeting_minutes: {\r\n        st_act_id: \"\",\r\n        academic_performance: \"\",\r\n        exploration: \"\",\r\n        writing_skills: \"\",\r\n        personal_brand: \"\",\r\n        mt_todos_note: \"\",\r\n        st_todos_note: \"\",\r\n      },\r\n      error: [],\r\n    };\r\n  },\r\n  methods: {\r\n    async getData() {\r\n      this.$Progress.start();\r\n      try {\r\n        const response = await this.$axios.get(\r\n          \"../v2/list/activities/1-on-1-call/history?meeting-minutes=yes\"\r\n        );\r\n        this.$Progress.finish();\r\n        this.meeting = response.data.data;\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        this.$Progress.fail();\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async getPage(link) {\r\n      if (link != null) {\r\n        this.$Progress.start();\r\n        try {\r\n          this.$Progress.finish();\r\n          const response = await this.$axios.get(link);\r\n          this.meeting = response.data.data;\r\n          // console.log(response.data);\r\n        } catch (e) {\r\n          this.$Progress.fail();\r\n          console.log(e.response);\r\n        }\r\n      }\r\n    },\r\n\r\n    async detail(i) {\r\n      this.modal = \"add\";\r\n      const response = await this.$axios.get(\"select/meeting-minutes/\" + i);\r\n      if (response.data.success) {\r\n        this.meeting_minutes = response.data.data;\r\n        this.view_status = \"view\";\r\n      } else {\r\n        this.view_status = \"add\";\r\n        this.meeting_minutes = {\r\n          st_act_id: i,\r\n          academic_performance: \"\",\r\n          exploration: \"\",\r\n          writing_skills: \"\",\r\n          personal_brand: \"\",\r\n          mt_todos_note: \"\",\r\n          st_todos_note: \"\",\r\n        };\r\n      }\r\n    },\r\n\r\n    async handleSubmit() {\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\r\n          \"create/meeting-minutes\",\r\n          this.meeting_minutes\r\n        );\r\n\r\n        this.modal = \"\";\r\n\r\n        this.getData();\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        // console.log(response);\r\n      } catch (e) {\r\n        if (e.response.status == 400) {\r\n          this.error = e.response.data.error;\r\n        } else {\r\n          this.modal = \"\";\r\n        }\r\n        this.$alert.toast(\"error\", \"Please try again.\");\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.vue-side {\r\n  position: fixed;\r\n  top: 0;\r\n  right: -50%;\r\n  height: 100vh;\r\n  padding: 20px;\r\n  width: 40%;\r\n  background: white;\r\n  overflow: auto;\r\n  z-index: 99999;\r\n  transition: all 0.3s;\r\n}\r\n\r\n.vue-side.active {\r\n  right: 0 !important;\r\n}\r\n\r\n@media only screen and (max-width: 600px) {\r\n  .vue-side {\r\n    position: fixed;\r\n    top: 0;\r\n    right: -90%;\r\n    width: 90%;\r\n  }\r\n}\r\n</style>"],"sourceRoot":""}]}