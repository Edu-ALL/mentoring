,EACF,WADE,CALF;AAAA,S,CAOY;;AAAA,OARd,CASI,CAVN,C,EAWA,W,EACA,mBAAA,CAiBM,KAjBN,EAAA,WAAA,EAiBM,CAhBJ,mBAAA,CAOM,KAPN,EAAA,WAAA,EAOM,CANJ,mBAAA,CAKS,QALT,EAKS;AAJP,QAAA,KAAK,EAAC,8CAIC;AAHN,QAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,iBAAE,KAAA,CAAA,KAAA,GAAK,EAAP;AAAA,SAAA;AAGC,OALT,EAGC,UAHD,CAMI,CAPN,CAgBI,EARJ,WAQI,CAjBN,C,CAfF,E,EAAA,CAkCI,CAtCN,C,iCAsCM,CAtCN;AAAA,K,CADoB;;AAAA,GAAtB,CAyCI,CAxFN,C","sourcesContent":["<template>\r\n  <div id=\"groupMember\">\r\n    <div class=\"row\">\r\n      <div class=\"col-6\">\r\n        <h6>Group Members</h6>\r\n      </div>\r\n      <div class=\"col-6\">\r\n        <div class=\"text-end\">\r\n          <i class=\"fa-solid fa-add pointer\" @click=\"modal = 'new-meeting'\"></i>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"col-12\">\r\n        <ul class=\"list-group list-group-flush members\">\r\n          <li class=\"list-group-item\" v-for=\"i in 5\" :key=\"i\">\r\n            <div class=\"row align-items-center pointer\">\r\n              <div class=\"col-6\">\r\n                <div class=\"member-name\" @click=\"contribution = i\">\r\n                  Full Name\r\n                  <i class=\"fa-solid fa-crown text-warning ms-2\"></i>\r\n                </div>\r\n              </div>\r\n              <div class=\"col-5 text-end\">\r\n                <div class=\"text-muted\">Role</div>\r\n              </div>\r\n              <div class=\"col-1\">\r\n                <v-tooltip arrow=\"true\" placement=\"left\" content=\"Active\">\r\n                  <i class=\"fa-solid fa-check-circle text-success text-end\"></i>\r\n                </v-tooltip>\r\n                <v-tooltip arrow=\"true\" placement=\"left\" content=\"Pending\">\r\n                  <i class=\"fa-solid fa-clock text-warning text-end\"></i>\r\n                </v-tooltip>\r\n              </div>\r\n            </div>\r\n            <transition>\r\n              <div class=\"row mt-1\">\r\n                <div class=\"col-12\" style=\"text-align: justify\">\r\n                  <div class=\"border rounded-2 p-2\">Contributions</div>\r\n                </div>\r\n              </div>\r\n            </transition>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"vue-modal-overlay\" v-if=\"modal != ''\"></div>\r\n    <!-- New Member  -->\r\n    <transition name=\"pop\">\r\n      <div\r\n        class=\"vue-modal vue-modal-sm bg-secondary\"\r\n        v-if=\"modal == 'new-meeting'\"\r\n      >\r\n        <form method=\"post\" @submit.prevent=\"handleSubmit()\">\r\n          <h5>Invite New Member</h5>\r\n          <hr class=\"my-0 mb-3\" />\r\n          <div class=\"mb-3\">\r\n            <input-group>\r\n              <input\r\n                type=\"email\"\r\n                class=\"form-mentoring w-100\"\r\n                v-model=\"participant\"\r\n                required\r\n              />\r\n              <label class=\"bg-secondary text-white\">Email</label>\r\n            </input-group>\r\n          </div>\r\n          <hr />\r\n          <div class=\"row\">\r\n            <div class=\"col-6\">\r\n              <button\r\n                class=\"btn-mentoring btn-sm py-1 btn-outline-danger\"\r\n                @click=\"modal = ''\"\r\n              >\r\n                Cancel\r\n              </button>\r\n            </div>\r\n            <div class=\"col-6 text-end\">\r\n              <button\r\n                type=\"submit\"\r\n                class=\"btn-mentoring btn-sm py-1 btn-success\"\r\n              >\r\n                Invite\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"groupMember\",\r\n  props: {\r\n    menu: Object,\r\n    member: Object,\r\n    group: Object,\r\n  },\r\n  data() {\r\n    return {\r\n      contribution: \"\",\r\n      modal: \"\",\r\n      members: [],\r\n      participant: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    async handleSubmit() {\r\n      this.modal = \"\";\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\r\n          \"student/group/project/participant\",\r\n          {\r\n            group_id: this.group.id,\r\n            participant: [this.participant],\r\n          }\r\n        );\r\n\r\n        console.log(response.data);\r\n\r\n        if (response.data.success) {\r\n          this.$alert.toast(\"success\", response.data.message);\r\n          this.$emit(\"check\", \"new\");\r\n        } else {\r\n          this.$alert.toast(\r\n            \"error\",\r\n            response.data.error.exists\r\n              ? response.data.error.exists\r\n              : response.data.error.joined\r\n          );\r\n        }\r\n\r\n        this.participant = \"\";\r\n\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n  },\r\n  updated() {\r\n    this.members = this.member;\r\n  },\r\n  created() {\r\n    this.members = this.member;\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ninput-group input:focus,\r\ninput-group textarea:focus {\r\n  border: 2px solid #fff;\r\n}\r\n\r\ninput::placeholder {\r\n  color: rgb(237, 237, 237) !important;\r\n  font-size: 0.8em;\r\n}\r\n\r\n.members {\r\n  font-size: 0.9em;\r\n}\r\n</style>"],"sourceRoot":""}]}