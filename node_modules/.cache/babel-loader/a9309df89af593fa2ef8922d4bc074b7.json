transition>\r\n\r\n    {{ event }}\r\n    <!-- Detail  -->\r\n    <transition name=\"fade\">\r\n      <div class=\"card-white\" v-if=\"menus.submenu == 'detail'\">\r\n        <div class=\"flex\">\r\n          <h5 class=\"d-inline\">Event</h5>\r\n          <div class=\"float-end\">\r\n            <button\r\n              class=\"btn-mentoring btn-type-2 btn-sm\"\r\n              @click=\"this.$router.push({ path: '/admin/events/edit' })\"\r\n            >\r\n              Edit Event\r\n            </button>\r\n          </div>\r\n        </div>\r\n        <hr />\r\n        <div class=\"row\">\r\n          <div class=\"col-md-7\">\r\n            <div class=\"mb-3\">\r\n              <label>Event Name</label> <br />\r\n              {{ event.dtl_name }}\r\n            </div>\r\n            <div class=\"mb-3\">\r\n              <label>Category</label> <br />\r\n              {{ event.dtl_category }}\r\n            </div>\r\n            <div class=\"mb-3\">\r\n              <label>Price</label> <br />\r\n              Rp {{ event.dtl_price }}\r\n            </div>\r\n            <div class=\"mb-3\">\r\n              <label>Descriptions</label> <br />\r\n              {{ event.dtl_desc }}\r\n            </div>\r\n          </div>\r\n          <div class=\"col-md-5\">\r\n            <div class=\"card shadow border-0\">\r\n              <div class=\"card-body\">\r\n                <h6>Date & Time</h6>\r\n                <ul class=\"list-group\" v-if=\"event.schedule\">\r\n                  <li\r\n                    class=\"list-group-item\"\r\n                    v-for=\"i in event.schedule\"\r\n                    :key=\"i\"\r\n                  >\r\n                    <div class=\"float-start\">{{ i.prog_sch_start_date }}</div>\r\n                    <div class=\"float-end\">\r\n                      {{ i.prog_sch_start_time }} -{{ i.prog_sch_end_time }}\r\n                    </div>\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"col-md-12 mt-3\" v-if=\"event.speaker\">\r\n            <h6>Speaker List</h6>\r\n            <div class=\"row row-cols-3 row-cols-1\">\r\n              <div class=\"col\" v-for=\"i in event.speaker\" :key=\"i\">\r\n                <div\r\n                  class=\"card border-0 shadow\"\r\n                  @click=\"modal = 'speakerDetail'\"\r\n                >\r\n                  <div class=\"card-body\">\r\n                    <div class=\"row align-items-center\">\r\n                      <div class=\"col-md-3\">\r\n                        <div class=\"text-center\">\r\n                          <img\r\n                            v-lazy=\"'https://picsum.photos/100'\"\r\n                            alt=\"\"\r\n                            class=\"w-100\"\r\n                          />\r\n                        </div>\r\n                      </div>\r\n                      <div class=\"col-md-9\">\r\n                        <b>{{ i.sp_name }}</b>\r\n                        <div class=\"text-muted\">{{ i.sp_title }}</div>\r\n                        <p class=\"mb-0 text-justify\">\r\n                          {{ i.sp_short_desc.substr(0, 20) + \"...\" }}\r\n                        </p>\r\n                        <small class=\"pointer text-info\"\r\n                          >More info\r\n\r\n                          <vue-feather\r\n                            type=\"arrow-right\"\r\n                            size=\"16\"\r\n                            class=\"float-end mt-2\"\r\n                          ></vue-feather>\r\n                        </small>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"\" v-if=\"modal == 'speakerDetail'\">\r\n            <div class=\"vue-modal vue-modal-lg\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-3\">\r\n                  <img\r\n                    v-lazy=\"'https://picsum.photos/300'\"\r\n                    alt=\"\"\r\n                    class=\"w-100\"\r\n                  />\r\n                </div>\r\n                <div class=\"col-md-9\">\r\n                  <b>Speaker Name</b>\r\n                  <div class=\"text-muted\">Position Name</div>\r\n                  <p class=\"mb-0\">\r\n                    Lorem ipsum dolor sit amet consectetur, adipisicing elit.\r\n                    Voluptatem labore rem quaerat odit, sit, dolores aut itaque\r\n                    dolor voluptas perspiciatis nulla ducimus. Omnis rem quaerat\r\n                    eveniet molestiae reprehenderit quo rerum!\r\n                  </p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"vue-modal-overlay\" @click=\"modal = ''\"></div>\r\n          </div>\r\n\r\n          <div class=\"col-md-12 mt-3\" v-if=\"event.partnership\">\r\n            <h6>Partner List</h6>\r\n            <div class=\"row row-cols-6 row-cols-2\">\r\n              <div class=\"col\" v-for=\"i in event.partnership\" :key=\"i\">\r\n                <div class=\"card border-0 shadow\">\r\n                  <div class=\"card-body text-center\">\r\n                    <img v-lazy=\"'https://picsum.photos/100'\" alt=\"\" />\r\n                    <b class=\"d-block mt-2\">{{ i.pt_name }}</b>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"row\">\r\n          <div class=\"col-md-12\">\r\n            <v-participants></v-participants>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n\r\n    <!-- Edit  -->\r\n    <transition name=\"fade\">\r\n      <div class=\"card-white\" v-if=\"menus.submenu == 'edit'\">\r\n        <v-edit></v-edit>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AddEvent from \"@/components/admin/event/add\";\r\nimport EditEvent from \"@/components/admin/event/edit\";\r\nimport Participants from \"@/components/admin/event/participants\";\r\n\r\nexport default {\r\n  name: \"eventDetail\",\r\n  props: {\r\n    menus: Object,\r\n  },\r\n  components: {\r\n    \"v-add\": AddEvent,\r\n    \"v-edit\": EditEvent,\r\n    \"v-participants\": Participants,\r\n  },\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      event_id: \"\",\r\n      event: [],\r\n    };\r\n  },\r\n  methods: {\r\n    getData(id) {\r\n      this.$alert.loading();\r\n      this.$axios\r\n        .get(this.$url + \"find/programme/detail/\" + id, {\r\n          headers: {\r\n            Authorization: \"Bearer \" + this.$adminToken,\r\n          },\r\n        })\r\n        .then((response) => {\r\n          this.$alert.close();\r\n          this.event = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$alert.close();\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    getSchedule(id) {\r\n      this.$axios\r\n        .get(\r\n          this.$url + \"select/programme-schedule/use/programme-detail/\" + id,\r\n          {\r\n            headers: {\r\n              Authorization: \"Bearer \" + this.$adminToken,\r\n            },\r\n          }\r\n        )\r\n        .then((response) => {\r\n          this.event.schedule = response.data.data;\r\n          // console.log(response.data);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    getSpeaker(id) {\r\n      this.$axios\r\n        .get(this.$url + \"select/speakers/use/programme-detail/\" + id, {\r\n          headers: {\r\n            Authorization: \"Bearer \" + this.$adminToken,\r\n          },\r\n        })\r\n        .then((response) => {\r\n          this.event.speaker = response.data.data;\r\n          // console.log(response.data);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    getParnertship(id) {\r\n      this.$axios\r\n        .get(this.$url + \"select/partners/use/programme-detail/\" + id, {\r\n          headers: {\r\n            Authorization: \"Bearer \" + this.$adminToken,\r\n          },\r\n        })\r\n        .then((response) => {\r\n          this.event.partnership = response.data.data;\r\n          // console.log(response.data);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n  },\r\n  created() {\r\n    this.event_id = this.$route.params.key;\r\n    this.getData(this.event_id);\r\n    this.getSchedule(this.event_id);\r\n    this.getSpeaker(this.event_id);\r\n    this.getParnertship(this.event_id);\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.form-control {\r\n  box-shadow: none !important;\r\n}\r\n</style>"],"sourceRoot":""}]}