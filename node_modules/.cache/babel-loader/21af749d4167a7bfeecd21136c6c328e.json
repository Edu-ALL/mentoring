AbD,EAaO;AAAA;;AACZ,WAAK,MAAL,CACG,GADH,CACO,IADP,EAEG,IAFH,CAEQ,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,QAAL,GAAgB,QAAQ,CAAC,IAAT,CAAc,IAA9B,CADkB,CAElB;AACD,OALH,EAMG,KANH,CAMS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OARH;AASD,KAvBM;AAyBP,IAAA,cAzBO,0BAyBQ,CAzBR,EAyBW;AAAA;;AAChB,WAAK,MAAL,CACG,IADH,CAEI,KAAK,IAAL,GAAY,yBAFhB,EAGI;AACE,QAAA,cAAc,EAAE;AADlB,OAHJ,EAMI;AACE,QAAA,OAAO,EAAE;AACP,UAAA,aAAa,EAAE,YAAY,YAAY,CAAC,OAAb,CAAqB,OAArB;AADpB;AADX,OANJ,EAYG,IAZH,CAYQ,YAAM;AACV,QAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,4BAA7B,EADU,CAEV;;AACD,OAfH,EAgBG,KAhBH,CAgBS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAlBH;AAmBD,KA7CM;AA+CP,IAAA,UA/CO,wBA+CM;AAAA;;AACX,WAAK,MAAL,CAAY,OAAZ;AACA,WAAK,MAAL,CACG,GADH,CAEI,KAAK,IAAL,GAAY,mCAAZ,GAAkD,KAAK,MAAL,CAAY,IAFlE,EAGI;AACE,QAAA,OAAO,EAAE;AACP,UAAA,aAAa,EAAE,YAAY,YAAY,CAAC,OAAb,CAAqB,OAArB;AADpB;AADX,OAHJ,EASG,IATH,CASQ,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,QAAA,MAAI,CAAC,QAAL,GAAgB,QAAQ,CAAC,IAAT,CAAc,IAA9B;AACA,QAAA,MAAI,CAAC,MAAL,CAAY,GAAZ,GAAkB,IAAlB,CAHkB,CAIlB;AACD,OAdH,EAeG,KAfH,CAeS,UAAC,KAAD,EAAW;AAChB,QAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAlBH;AAmBD,KApEM;AAsEP,IAAA,WAtEO,yBAsEO;AACZ,WAAK,MAAL,CAAY,GAAZ,GAAkB,KAAlB;AACA,WAAK,MAAL,CAAY,IAAZ,GAAmB,EAAnB;AACA,WAAK,OAAL;AACD;AA1EM,GAhBI;AA4Fb,EAAA,OA5Fa,qBA4FH;AACR,SAAK,OAAL;AACD;AA9FY,CAAf","sourcesContent":["<template>\r\n  <div id=\"expired\">\r\n    <div class=\"row mb-4\">\r\n      <div class=\"col-md-6 text-start\">\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-mentoring\"\r\n          v-model=\"search.name\"\r\n          @change=\"searchData\"\r\n          placeholder=\"Search\"\r\n        />\r\n        <br />\r\n        <span class=\"badge bg-primary px-3 d-inline-block\" v-if=\"search.bar\">\r\n          {{ search.name }}\r\n          <i class=\"fa-solid fa-close ms-3 pointer\" @click=\"closeSearch\"></i>\r\n        </span>\r\n      </div>\r\n    </div>\r\n    <div class=\"row\">\r\n      <!-- {{ expireds }} -->\r\n      <div class=\"col\">\r\n        <table class=\"table table-hover pointer\">\r\n          <thead>\r\n            <tr>\r\n              <th width=\"1%\">No</th>\r\n              <th>Full Name</th>\r\n              <th>Service</th>\r\n              <th>Date</th>\r\n              <th>Report</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"(i, index) in expireds.data\" :key=\"index\">\r\n              <td>{{ expireds.from + index }}</td>\r\n              <td>\r\n                {{\r\n                  i.student_activities.students.first_name +\r\n                  \" \" +\r\n                  i.student_activities.students.last_name\r\n                }}\r\n              </td>\r\n              <td>\r\n                <div v-if=\"!i.student_activities.programme_details\">\r\n                  {{ i.student_activities.prog_id == 1 ? \"1on1 Call\" : \"\" }}\r\n                </div>\r\n                <div v-if=\"i.student_activities.programme_details\">\r\n                  {{ i.student_activities.programme_details.dtl_name }}\r\n                </div>\r\n              </td>\r\n              <td>\r\n                <i class=\"fa-regular fa-calendar fa-fw\"></i>\r\n                {{ $customDate.date(i.created_at) }}\r\n              </td>\r\n              <td>\r\n                <a\r\n                  :href=\"$base_url + 'transaction/' + i.trx_id + '/invoice'\"\r\n                  target=\"_blank\"\r\n                  class=\"btn btn-sm btn-outline-info me-2\"\r\n                >\r\n                  <i class=\"fa-regular fa-file fa-fw\"></i>Invoice\r\n                </a>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n        <div class=\"text-center\" v-if=\"expireds.from == null\">\r\n          <hr />\r\n          <h6>Sorry, data is not found</h6>\r\n        </div>\r\n        <nav class=\"mt-2\" v-if=\"expireds.from != null\">\r\n          <ul class=\"pagination justify-content-center\">\r\n            <li class=\"page-item\" v-if=\"expireds.current_page != 1\">\r\n              <a class=\"page-link\" @click=\"getPage(expireds.links[0].url)\">\r\n                <i class=\"fa-solid fa-chevron-left\"></i>\r\n              </a>\r\n            </li>\r\n            <div v-for=\"(i, index) in expireds.last_page\" :key=\"index\">\r\n              <li\r\n                class=\"page-item\"\r\n                v-if=\"\r\n                  expireds.current_page - 2 < i && expireds.current_page + 2 > i\r\n                \"\r\n              >\r\n                <a\r\n                  class=\"page-link\"\r\n                  :class=\"\r\n                    expireds.current_page == i ? 'bg-primary text-white' : ''\r\n                  \"\r\n                  href=\"#\"\r\n                  @click=\"getPage(expireds.path + '?page=' + i)\"\r\n                  >{{ i }}</a\r\n                >\r\n              </li>\r\n            </div>\r\n            <li\r\n              class=\"page-item\"\r\n              v-if=\"expireds.current_page != expireds.last_page\"\r\n            >\r\n              <a class=\"page-link\" @click=\"getPage(expireds.next_page_url)\">\r\n                <i class=\"fa-solid fa-chevron-right\"></i>\r\n              </a>\r\n            </li>\r\n          </ul>\r\n        </nav>\r\n      </div>\r\n    </div>\r\n\r\n    <transition name=\"fade\">\r\n      <div class=\"p-3\" v-if=\"check\">\r\n        <div class=\"vue-modal vue-modal-md\">\r\n          <h6>Proof of Payment</h6>\r\n          <hr />\r\n          <img src=\"https://picsum.photos/500\" class=\"w-100\" alt=\"\" />\r\n        </div>\r\n        <div class=\"vue-modal-overlay\" @click=\"check = false\"></div>\r\n      </div>\r\n    </transition>\r\n\r\n    <transition name=\"fade\">\r\n      <div class=\"\" v-if=\"report == 'invoice'\">\r\n        <div class=\"vue-modal vue-modal-lg\">\r\n          <v-invoice />\r\n        </div>\r\n        <div class=\"vue-modal-overlay\" @click=\"report = ''\"></div>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Invoice from \"@/components/admin/transaction/report/invoice\";\r\nexport default {\r\n  name: \"expired\",\r\n  components: {\r\n    \"v-invoice\": Invoice,\r\n  },\r\n  data() {\r\n    return {\r\n      search: {\r\n        bar: false,\r\n        name: \"\",\r\n      },\r\n      expireds: [],\r\n      check: false,\r\n      report: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    getData() {\r\n      this.$axios\r\n        .get(this.$url + \"list/transaction/expired\")\r\n        .then((response) => {\r\n          this.expireds = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    getPage(link) {\r\n      this.$axios\r\n        .get(link)\r\n        .then((response) => {\r\n          this.expireds = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    confirmPayment(i) {\r\n      this.$axios\r\n        .post(\r\n          this.$url + \"switch/transaction/paid\",\r\n          {\r\n            transaction_id: i,\r\n          },\r\n          {\r\n            headers: {\r\n              Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n            },\r\n          }\r\n        )\r\n        .then(() => {\r\n          this.$alert.toast(\"success\", \"Payment has been confirmed\");\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    searchData() {\r\n      this.$alert.loading();\r\n      this.$axios\r\n        .get(\r\n          this.$url + \"list/transaction/expired?keyword=\" + this.search.name,\r\n          {\r\n            headers: {\r\n              Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n            },\r\n          }\r\n        )\r\n        .then((response) => {\r\n          this.$alert.close();\r\n          this.expireds = response.data.data;\r\n          this.search.bar = true;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$alert.close();\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    closeSearch() {\r\n      this.search.bar = false;\r\n      this.search.name = \"\";\r\n      this.getData();\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.form-control {\r\n  box-shadow: none;\r\n}\r\n</style>"],"sourceRoot":""}]}