ror_submit = e.response.data.error;\r\n        // console.log(e.response.data);\r\n        this.$alert.close();\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.placeholder {\r\n  background-color: white !important;\r\n  color: #000;\r\n}\r\n</style>"],"sourceRoot":""}]}