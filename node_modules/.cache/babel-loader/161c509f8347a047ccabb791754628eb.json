,CAFL;AAGJ,QAAA,KAAK,EAAE,CAAC,EAAD;AAHH,OAHD;AAQL,MAAA,UAAU,EAAE;AARP,KAAP;AAUD,GAhBY;AAiBb,EAAA,OAAO,EAAE;AACP,IAAA,IADO,gBACF,CADE,EACC;AACN,MAAA,MAAM,CAAC,IAAP,CAAY,CAAZ,EAAe,QAAf;AACD,KAHM;AAKD,IAAA,YALC,0BAKc;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACnB,gBAAA,KAAI,CAAC,KAAL,GAAa,EAAb,CADmB,CAGnB;;AACA,gBAAA,KAAI,CAAC,MAAL,CAAY,OAAZ;;AAJmB;AAAA;AAAA,uBAOM,KAAI,CAAC,MAAL,CAAY,IAAZ,CACrB,8BADqB,EAErB,KAAI,CAAC,IAFgB,CAPN;;AAAA;AAOX,gBAAA,QAPW;AAYjB,gBAAA,KAAI,CAAC,IAAL,CAAU,OAAV,CAAkB,CAAlB,IAAuB,EAAvB;AACA,gBAAA,KAAI,CAAC,IAAL,CAAU,KAAV,CAAgB,CAAhB,IAAqB,EAArB;;AACA,gBAAA,KAAI,CAAC,KAAL,CAAW,OAAX,EAAoB,UAApB;;AAEA,gBAAA,KAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C,EAhBiB,CAiBjB;;;AAjBiB;AAAA;;AAAA;AAAA;AAAA;AAmBjB,gBAAA,OAAO,CAAC,GAAR,CAAY,YAAE,QAAd;;AACA,gBAAA,KAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,mBAA3B;;AApBiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsBpB,KA3BM;AA6BP,IAAA,UA7BO,sBA6BI,EA7BJ,EA6BQ;AACb,WAAK,KAAL,GAAa,QAAb;AACA,WAAK,OAAL,GAAe,EAAf;AACD,KAhCM;AAkCD,IAAA,YAlCC,0BAkCc;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACnB,gBAAA,MAAI,CAAC,MAAL,CAAY,OAAZ;;AADmB;AAAA;AAAA,uBAGM,MAAI,CAAC,MAAL,CAAY,MAAZ,CACrB,kCAAkC,MAAI,CAAC,OADlB,CAHN;;AAAA;AAGX,gBAAA,QAHW;AAOjB,gBAAA,MAAI,CAAC,KAAL,GAAa,EAAb,CAPiB,CAQjB;;AACA,gBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,EAAoB,UAApB;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C;;AAViB;AAAA;;AAAA;AAAA;AAAA;AAYjB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,kBAA3B;;AAbiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAepB;AAjDM;AAjBI,CAAf","sourcesContent":["<template>\r\n  <div id=\"links\">\r\n    <div class=\"req-header p-2 py-1\">\r\n      <i\r\n        class=\"bi bi-bookmark-x-fill me-1 text-danger\"\r\n        v-if=\"data?.length == 0\"\r\n      ></i>\r\n      <i\r\n        class=\"bi bi-bookmark-check-fill me-1 text-success\"\r\n        v-if=\"data?.length != 0\"\r\n      ></i>\r\n      Publication Links\r\n      <div class=\"float-end\">\r\n        <i class=\"fa-solid fa-add pointer\" @click=\"modal = 'add'\"></i>\r\n      </div>\r\n    </div>\r\n    <div class=\"req-body p-2 py-4\">\r\n      <!-- IF EMPTY  -->\r\n      <div class=\"row\" v-if=\"data?.length == 0\">\r\n        <div class=\"col text-center text-muted pb-4\" @click=\"modal = 'add'\">\r\n          Please add your publication links here.\r\n        </div>\r\n      </div>\r\n      <div class=\"row row-cols-1\">\r\n        <div class=\"col\">\r\n          <ul class=\"list-group list-group-flush\">\r\n            <li\r\n              class=\"list-group-item p-0\"\r\n              v-for=\"(i, index) in data\"\r\n              :key=\"index\"\r\n            >\r\n              <div class=\"row align-items-center\">\r\n                <div class=\"col-12 link-content\">\r\n                  <div class=\"link-title text-muted d-flex align-items-center\">\r\n                    <strong class=\"me-3\">{{ index + 1 }}</strong>\r\n                    <strong class=\"w-100\">\r\n                      {{ i.subject }}\r\n                    </strong>\r\n                  </div>\r\n                  <div class=\"action\">\r\n                    <div class=\"goto\" @click=\"goto(i.value)\">\r\n                      <i class=\"fa-solid fa-paper-plane\"></i>\r\n                    </div>\r\n                    <div class=\"delete\" @click=\"deleteData(i.id)\">\r\n                      <i class=\"fa-solid fa-trash text-end\"></i>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n  <!-- Modal Document  -->\r\n  <!-- Add  -->\r\n  <transition name=\"pop\">\r\n    <div class=\"vue-modal vue-modal-sm\" v-if=\"modal == 'add'\">\r\n      <form @submit.prevent=\"handleSubmit\" method=\"post\">\r\n        <h6 class=\"mb-0\">Publication Links</h6>\r\n        <hr class=\"my-1 mb-3\" />\r\n        <div class=\"mb-3\">\r\n          <input-group>\r\n            <input\r\n              type=\"text\"\r\n              v-model=\"link.subject[0]\"\r\n              class=\"form-mentoring form-control w-100\"\r\n              required\r\n              placeholder=\"fill in here ...\"\r\n              id=\"title\"\r\n            />\r\n            <label for=\"title\">Title</label>\r\n          </input-group>\r\n        </div>\r\n        <div class=\"mb-3\">\r\n          <input-group>\r\n            <input\r\n              type=\"url\"\r\n              v-model=\"link.value[0]\"\r\n              class=\"form-mentoring form-control w-100\"\r\n              required\r\n              placeholder=\"fill in here ...\"\r\n              id=\"link\"\r\n            />\r\n            <label for=\"link\">Publication URL</label>\r\n          </input-group>\r\n        </div>\r\n        <div class=\"text-end\">\r\n          <button type=\"submit\" class=\"btn-mentoring btn-sm py-1 bg-primary\">\r\n            <i class=\"fa-solid fa-save me-2\"></i>\r\n            Save\r\n          </button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </transition>\r\n\r\n  <!-- Delete  -->\r\n  <transition name=\"pop\">\r\n    <div class=\"vue-modal vue-modal-sm bg-primary\" v-if=\"modal == 'delete'\">\r\n      <div class=\"text-center\">\r\n        <i class=\"fa-solid fa-circle-exclamation fa-2xl\"></i>\r\n        <h6 class=\"mt-2 text-light\">Are you sure you want to delete this?</h6>\r\n        <div class=\"mt-3\">\r\n          <button\r\n            class=\"btn-mentoring btn-warning btn-sm py-1 mx-1\"\r\n            @click=\"modal = ''\"\r\n          >\r\n            Cancel</button\r\n          ><button\r\n            class=\"btn-mentoring btn-outline-success btn-sm py-1 mx-1\"\r\n            @click=\"handleDelete\"\r\n          >\r\n            Yes\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </transition>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"links\",\r\n  props: {\r\n    data: Object,\r\n  },\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      link_id: \"\",\r\n      link: {\r\n        category: \"publication_links\",\r\n        subject: [\"\"],\r\n        value: [\"\"],\r\n      },\r\n      error_link: [],\r\n    };\r\n  },\r\n  methods: {\r\n    goto(i) {\r\n      window.open(i, \"_blank\");\r\n    },\r\n\r\n    async handleSubmit() {\r\n      this.modal = \"\";\r\n\r\n      // console.log(this.link);\r\n      this.$alert.loading();\r\n\r\n      try {\r\n        const response = await this.$axios.post(\r\n          \"student/academic/requirement\",\r\n          this.link\r\n        );\r\n\r\n        this.link.subject[0] = \"\";\r\n        this.link.value[0] = \"\";\r\n        this.$emit(\"check\", \"academic\");\r\n\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        // console.log(response);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again.\");\r\n      }\r\n    },\r\n\r\n    deleteData(id) {\r\n      this.modal = \"delete\";\r\n      this.link_id = id;\r\n    },\r\n\r\n    async handleDelete() {\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.delete(\r\n          \"student/academic/requirement/\" + this.link_id\r\n        );\r\n\r\n        this.modal = \"\";\r\n        // console.log(response.data);\r\n        this.$emit(\"check\", \"academic\");\r\n        this.$alert.toast(\"success\", response.data.message);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.link-content {\r\n  position: relative;\r\n  width: 100%;\r\n  padding: 10px;\r\n  padding-left: 30px;\r\n}\r\n\r\n.action {\r\n  position: absolute;\r\n  right: -15%;\r\n  top: 0;\r\n  width: 15%;\r\n  height: 100%;\r\n  display: flex;\r\n  color: #fff;\r\n  transition: all 0.4s;\r\n  cursor: pointer;\r\n}\r\n\r\n.goto {\r\n  width: 50%;\r\n  height: 100%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background: #223872;\r\n}\r\n\r\n.delete {\r\n  width: 50%;\r\n  height: 100%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background: red;\r\n}\r\n\r\n.link-content:hover .action {\r\n  right: 0;\r\n}\r\n</style>"],"sourceRoot":""}]}