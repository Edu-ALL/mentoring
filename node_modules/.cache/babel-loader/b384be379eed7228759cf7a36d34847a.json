BAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,kBAA3B;;AA9BiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgCpB,KA9DM;AAgEP,IAAA,cAhEO,0BAgEQ,CAhER,EAgEW;AAChB,WAAK,MAAL,GAAc,CAAd;AACA,WAAK,KAAL,GAAa,QAAb;AACD,KAnEM;AAqED,IAAA,aArEC,2BAqEe;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAGK,MAAI,CAAC,MAAL,CAAY,MAAZ,CACrB,sBAAsB,MAAI,CAAC,MADN,CAHL;;AAAA;AAGZ,gBAAA,QAHY;;AAMlB,gBAAA,MAAI,CAAC,OAAL;;AACA,gBAAA,MAAI,CAAC,KAAL,GAAa,EAAb;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C,EATkB,CAUlB;;;AAVkB;AAAA;;AAAA;AAAA;AAAA;AAYlB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,kBAA3B;;AAdkB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBrB;AArFM,GArBI;AA4Gb,EAAA,OA5Ga,qBA4GH;AACR,SAAK,OAAL;AACD;AA9GY,CAAf","sourcesContent":["<template>\r\n  <div id=\"academic\" style=\"scroll-margin-top: 120px\">\r\n    <div\r\n      class=\"\r\n        heading\r\n        px-3\r\n        py-1\r\n        d-flex\r\n        justify-content-between\r\n        align-items-center\r\n      \"\r\n    >\r\n      <div class=\"fw-light\">Academics</div>\r\n      <div class=\"text-end\">\r\n        <button\r\n          class=\"btn-mentoring btn-sm bg-primary py-1 px-2\"\r\n          v-if=\"!add\"\r\n          @click=\"add = true\"\r\n        >\r\n          <i class=\"bi bi-plus\"></i>\r\n        </button>\r\n        <button\r\n          class=\"btn-mentoring btn-sm btn-outline-danger py-1 px-2\"\r\n          v-if=\"add\"\r\n          @click=\"add = false\"\r\n        >\r\n          <i class=\"bi bi-x\"></i>\r\n        </button>\r\n      </div>\r\n    </div>\r\n    <div class=\"desc\">\r\n      <div class=\"row mb-2\">\r\n        <div class=\"col-12\">\r\n          <form @submit.prevent=\"handleSubmit\" method=\"post\">\r\n            <fieldset class=\"border p-2 mb-2\" v-if=\"add\">\r\n              <legend class=\"float-none w-auto\" style=\"font-size: 0.8em\">\r\n                New Academic\r\n              </legend>\r\n              <div class=\"field\">\r\n                <div class=\"data\">\r\n                  <div class=\"row g-2\">\r\n                    <div class=\"col-md-5\">\r\n                      <div class=\"mb-3\">\r\n                        <input-group>\r\n                          <input\r\n                            type=\"text\"\r\n                            v-model=\"academic.subject\"\r\n                            class=\"form-mentoring form-control w-100\"\r\n                            required\r\n                            placeholder=\"fill in here ...\"\r\n                            id=\"school\"\r\n                          />\r\n                          <label for=\"school\">School Subject</label>\r\n                        </input-group>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"col-md-5 col-8\">\r\n                      <div class=\"mb-3\">\r\n                        <input-group>\r\n                          <select\r\n                            v-model=\"academic.curriculum\"\r\n                            required\r\n                            class=\"form-mentoring form-control w-100\"\r\n                            @change=\"checkCurriculum\"\r\n                          >\r\n                            <option value=\"National\" selected>\r\n                              National (out of 100)\r\n                            </option>\r\n                            <option value=\"IB\">IB (out of 7)</option>\r\n                            <option value=\"Cambridge\">Cambridge</option>\r\n                          </select>\r\n                          <label for=\"score\">Curriculum</label>\r\n                        </input-group>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"col-md-2 col-4\">\r\n                      <div class=\"mb-3\">\r\n                        <input-group v-if=\"academic.curriculum != 'Cambridge'\">\r\n                          <input\r\n                            :type=\"input_type.score_type\"\r\n                            :max=\"input_type.max_score\"\r\n                            :maxlength=\"input_type.length\"\r\n                            v-model=\"academic.score\"\r\n                            required\r\n                            placeholder=\"fill in here ...\"\r\n                            id=\"score\"\r\n                            class=\"form-mentoring form-control w-100\"\r\n                          />\r\n                          <label for=\"score\">Score</label>\r\n                        </input-group>\r\n\r\n                        <input-group v-if=\"academic.curriculum == 'Cambridge'\">\r\n                          <select\r\n                            v-model=\"academic.score\"\r\n                            required\r\n                            class=\"form-mentoring form-control w-100\"\r\n                          >\r\n                            <option value=\"A*\" selected>A*</option>\r\n                            <option value=\"A\">A</option>\r\n                            <option value=\"B\">B</option>\r\n                            <option value=\"C\">C</option>\r\n                            <option value=\"D\">D</option>\r\n                            <option value=\"E\">E</option>\r\n                            <option value=\"F\">F</option>\r\n                          </select>\r\n                          <label for=\"score\">Score</label>\r\n                        </input-group>\r\n                      </div>\r\n                    </div>\r\n                    <!-- <div class=\"col-md-3 col-6\">\r\n                      <div class=\"mb-3\">\r\n                        <input-group>\r\n                          <input\r\n                            :type=\"input_type.score_type\"\r\n                            v-model=\"academic.max_score\"\r\n                            :max=\"input_type.max_score\"\r\n                            required\r\n                            placeholder=\"fill in here ...\"\r\n                            id=\"maxScore\"\r\n                            class=\"form-mentoring form-control w-100\"\r\n                          />\r\n                          <label for=\"maxScore\">Max Score</label>\r\n                        </input-group>\r\n                      </div>\r\n                    </div> -->\r\n                  </div>\r\n\r\n                  <div class=\"text-end mt-1\">\r\n                    <button\r\n                      type=\"submit\"\r\n                      class=\"btn-mentoring btn-sm btn-outline-success py-1\"\r\n                    >\r\n                      <i class=\"bi bi-save me-2\"></i>\r\n                      Save Changes\r\n                    </button>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </fieldset>\r\n          </form>\r\n\r\n          <table class=\"table table-bordered table-hover mt-2\">\r\n            <tbody>\r\n              <tr\r\n                v-for=\"(i, index) in academic_list\"\r\n                :key=\"index\"\r\n                class=\"align-middle\"\r\n              >\r\n                <td width=\"5%\" align=\"center\">{{ index + 1 }}</td>\r\n                <td>\r\n                  <div class=\"d-flex align-items-center\">\r\n                    <div class=\"text\" style=\"width: 80%\">\r\n                      {{ i.school_subject }}\r\n                    </div>\r\n                    <div class=\"score text-end\" style=\"width: 20%\">\r\n                      {{ i.score }}/{{ i.max_score }}\r\n                    </div>\r\n                  </div>\r\n                </td>\r\n                <td class=\"text-center\" width=\"4%\">\r\n                  <i\r\n                    class=\"bi bi-x-circle text-danger pointer\"\r\n                    @click=\"deleteAcademic(i.id)\"\r\n                  ></i>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n          <div\r\n            class=\"row\"\r\n            v-if=\"academic_list.length == 0\"\r\n            style=\"margin-top: -10px\"\r\n          >\r\n            <div class=\"col text-center pb-2 text-muted\">\r\n              Please fill in your academic record here.\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n  <!-- delete  -->\r\n  <transition name=\"pop\">\r\n    <div class=\"vue-modal vue-modal-sm bg-primary\" v-if=\"modal == 'delete'\">\r\n      <div class=\"text-center\">\r\n        <i class=\"fa-solid fa-circle-exclamation fa-2xl\"></i>\r\n        <h6 class=\"mt-2 text-light\">Are you sure you want to delete this?</h6>\r\n        <div class=\"mt-3\">\r\n          <button\r\n            class=\"btn-mentoring btn-warning btn-sm py-1 mx-1\"\r\n            @click=\"modal = ''\"\r\n          >\r\n            Cancel</button\r\n          ><button\r\n            class=\"btn-mentoring btn-outline-success btn-sm py-1 mx-1\"\r\n            @click=\"deleteProcess\"\r\n          >\r\n            Yes\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </transition>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"academic\",\r\n  data() {\r\n    return {\r\n      add: false,\r\n      modal: \"\",\r\n      aca_id: \"\",\r\n      academic_list: [],\r\n      academic: {\r\n        subject: \"\",\r\n        curriculum: \"National\",\r\n        score: \"\",\r\n        max_score: \"\",\r\n      },\r\n      input_type: {\r\n        score_type: \"number\",\r\n        max_score: \"100\",\r\n        length: \"3\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    checkCurriculum() {\r\n      let cur = this.academic.curriculum;\r\n      if (cur == \"National\") {\r\n        this.input_type.score_type = \"number\";\r\n        this.input_type.max_score = 100;\r\n        this.academic.max_score = 100;\r\n        this.input_type.length = 3;\r\n      } else if (cur == \"IB\") {\r\n        this.input_type.score_type = \"number\";\r\n        this.input_type.max_score = 7;\r\n        this.academic.max_score = 7;\r\n        this.input_type.length = 1;\r\n      } else if (cur == \"Cambridge\") {\r\n        this.input_type.score_type = \"text\";\r\n        this.input_type.max_score = \"F\";\r\n        this.academic.max_score = \"F\";\r\n        this.input_type.length = 2;\r\n      }\r\n    },\r\n\r\n    async getData() {\r\n      try {\r\n        const response = await this.$axios.get(\"student/academic\");\r\n        this.academic_list = response.data.data;\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async handleSubmit() {\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\"student/academic\", {\r\n          school_subject: this.academic.subject,\r\n          score: this.academic.score,\r\n          max_score: this.academic.max_score,\r\n        });\r\n\r\n        // console.log(response.data);\r\n        this.$alert.close();\r\n\r\n        // Reset\r\n\r\n        if (response.data.success) {\r\n          this.add = false;\r\n          this.academic.subject = \"\";\r\n          this.academic.score = \"\";\r\n          this.academic.max_score = \"\";\r\n          // console.log(response.data);\r\n          this.$alert.toast(\"success\", response.data.message);\r\n          this.getData();\r\n        } else {\r\n          this.academic.score = \"\";\r\n          this.academic.max_score = \"\";\r\n          this.$alert.toast(\"error\", response.data.error);\r\n        }\r\n      } catch (e) {\r\n        console.log(e.response.data);\r\n        this.$alert.close();\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n\r\n    deleteAcademic(i) {\r\n      this.aca_id = i;\r\n      this.modal = \"delete\";\r\n    },\r\n\r\n    async deleteProcess() {\r\n      // this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.delete(\r\n          \"student/academic/\" + this.aca_id\r\n        );\r\n        this.getData();\r\n        this.modal = \"\";\r\n        this.$alert.close();\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.close();\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.placeholder {\r\n  background-color: white !important;\r\n  color: #000;\r\n}\r\n</style>"],"sourceRoot":""}]}