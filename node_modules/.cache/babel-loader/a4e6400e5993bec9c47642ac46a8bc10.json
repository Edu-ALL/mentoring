,MAAZ,CACrB,sBAAsB,MAAI,CAAC,WADN,CAHL;;AAAA;AAGZ,gBAAA,QAHY;;AAMlB,gBAAA,MAAI,CAAC,OAAL;;AACA,gBAAA,MAAI,CAAC,KAAL,GAAa,EAAb;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C,EATkB,CAUlB;;;AAVkB;AAAA;;AAAA;AAAA;AAAA;AAYlB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,kBAA3B;;AAdkB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBrB;AAhFM,GAlBI;AAoGb,EAAA,OApGa,qBAoGH;AACR,SAAK,OAAL;AACD;AAtGY,CAAf","sourcesContent":["<template>\r\n  <div id=\"interest\" style=\"scroll-margin-top: 120px\">\r\n    <div class=\"heading\">\r\n      Interest List\r\n\r\n      <div class=\"float-end\">\r\n        <button\r\n          class=\"btn-mentoring btn-sm bg-primary py-1\"\r\n          v-if=\"!add\"\r\n          @click=\"add = true\"\r\n        >\r\n          <i class=\"fa-solid fa-add\"></i>\r\n        </button>\r\n        <button\r\n          class=\"btn-mentoring btn-sm btn-outline-danger py-1\"\r\n          v-if=\"add\"\r\n          @click=\"add = false\"\r\n        >\r\n          <i class=\"fa-solid fa-x\"></i>\r\n        </button>\r\n      </div>\r\n    </div>\r\n    <div class=\"desc\">\r\n      <div class=\"row mb-2\">\r\n        <div class=\"col-12\">\r\n          <form method=\"post\" @submit.prevent=\"handleSubmit\">\r\n            <fieldset class=\"border p-2\" :class=\"add ? 'd-block' : 'd-none'\">\r\n              <legend class=\"float-none w-auto\" style=\"font-size: 0.8em\">\r\n                New Interest\r\n              </legend>\r\n              <div class=\"field\">\r\n                <div class=\"data\">\r\n                  <multiselect\r\n                    v-model=\"interest.name\"\r\n                    :options=\"options\"\r\n                    placeholder=\"Select One\"\r\n                    @select=\"interestCheck\"\r\n                  >\r\n                  </multiselect>\r\n\r\n                  <!-- new -->\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"\"\r\n                    class=\"form-mentoring w-100 form-control-sm mt-2\"\r\n                    placeholder=\"New Interest\"\r\n                    v-model=\"interest.other\"\r\n                    v-if=\"newField\"\r\n                  />\r\n\r\n                  <div class=\"text-end mt-3\">\r\n                    <button\r\n                      type=\"submit\"\r\n                      class=\"btn-mentoring btn-sm btn-outline-success py-1\"\r\n                    >\r\n                      Save Changes\r\n                    </button>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </fieldset>\r\n          </form>\r\n\r\n          <table class=\"table table-bordered table-hover mt-2\">\r\n            <tbody>\r\n              <tr\r\n                v-for=\"(i, index) in interest_list\"\r\n                :key=\"index\"\r\n                class=\"align-middle\"\r\n              >\r\n                <td width=\"5%\" align=\"center\">{{ index + 1 }}</td>\r\n                <td>\r\n                  <div class=\"d-flex align-items-center\">\r\n                    <div class=\"text\" style=\"width: 95%\">\r\n                      {{ i.career_major_name }}\r\n                    </div>\r\n                    <div class=\"icon text-end\" style=\"width: 5%\">\r\n                      <i\r\n                        class=\"fa-solid fa-trash text-danger pointer\"\r\n                        @click=\"deleteInterest(i.id)\"\r\n                      ></i>\r\n                    </div>\r\n                  </div>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n\r\n          <div\r\n            class=\"row\"\r\n            v-if=\"interest_list.length == 0\"\r\n            style=\"margin-top: -10px\"\r\n          >\r\n            <div class=\"col text-center pb-2 text-muted\">\r\n              Please fill in your interest here.\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n  <!-- delete  -->\r\n  <transition name=\"pop\">\r\n    <div class=\"vue-modal vue-modal-sm bg-primary\" v-if=\"modal == 'delete'\">\r\n      <div class=\"text-center\">\r\n        <i class=\"fa-solid fa-circle-exclamation fa-2xl\"></i>\r\n        <h5 class=\"mt-2\">Are you sure to delete?</h5>\r\n        <div class=\"mt-3\">\r\n          <button\r\n            class=\"btn-mentoring btn-warning btn-sm py-1 mx-1\"\r\n            @click=\"modal = ''\"\r\n          >\r\n            Cancel</button\r\n          ><button\r\n            class=\"btn-mentoring btn-outline-success btn-sm py-1 mx-1\"\r\n            @click=\"deleteProcess\"\r\n          >\r\n            Yes\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </transition>\r\n</template>\r\n\r\n<script>\r\nimport Multiselect from \"vue-multiselect\";\r\n\r\nexport default {\r\n  name: \"interest\",\r\n  components: { Multiselect },\r\n  data() {\r\n    return {\r\n      add: false,\r\n      modal: \"\",\r\n      newField: false,\r\n      options: [\"HRD\", \"Customer Service\", \"Digital Marketing\", \"Others\"],\r\n      interest_id: \"\",\r\n      interest_list: [],\r\n      interest: {\r\n        exist: false,\r\n        name: null,\r\n        other: null,\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    addCheck() {\r\n      this.add = !this.add;\r\n    },\r\n\r\n    interestCheck(value) {\r\n      if (value == \"Others\") {\r\n        this.newField = true;\r\n      } else {\r\n        this.newField = false;\r\n      }\r\n    },\r\n\r\n    async getData() {\r\n      try {\r\n        const response = await this.$axios.get(\"student/interest\");\r\n        this.interest_list = response.data.data;\r\n        // console.log(response);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async handleSubmit() {\r\n      // check others name\r\n      if (this.interest.name == \"Others\") {\r\n        this.interest.exist = false;\r\n        this.interest.name = null;\r\n      } else {\r\n        this.interest.exist = true;\r\n      }\r\n\r\n      console.log(this.interest);\r\n\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\"student/interest\", {\r\n          exists: this.interest.exist,\r\n          career_major_name: this.interest.name,\r\n          career_major_other: this.interest.other,\r\n        });\r\n\r\n        // console.log(response.data);\r\n        this.$alert.close();\r\n\r\n        // Reset\r\n        this.add = false;\r\n        this.interest.name = \"\";\r\n        this.interest.other = \"\";\r\n\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        this.getData();\r\n      } catch (e) {\r\n        console.log(e.response.data);\r\n        this.$alert.close();\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n\r\n    deleteInterest(i) {\r\n      this.interest_id = i;\r\n      this.modal = \"delete\";\r\n    },\r\n\r\n    async deleteProcess() {\r\n      // this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.delete(\r\n          \"student/interest/\" + this.interest_id\r\n        );\r\n        this.getData();\r\n        this.modal = \"\";\r\n        this.$alert.close();\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.close();\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.placeholder {\r\n  background-color: white !important;\r\n  color: #000;\r\n}\r\n</style>"],"sourceRoot":""}]}