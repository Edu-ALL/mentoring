K,CA3CL,E,GAAA,CA4CG,EA7CL,CA8CI,CA/CN,CAgDI,CAhEN,CAqJI,EAnFiC,KAAA,CAAA,KAAA,IAAK,E,kBAA1C,mBAAA,CAAwD,KAAxD,EAAA,WAAA,C,iCAmFI,EAjFJ,YAAA,CAyDa,WAzDb,EAyDa;AAzDD,IAAA,IAAI,EAAC;AAyDJ,GAzDb,EAAsB;sBACpB;AAAA,aAuDM,CArDE,KAAA,CAAA,KAAA,IAAK,a,kBAFb,mBAAA,CAuDM,KAvDN,EAAA,WAAA,EAuDM,CAnDJ,mBAAA,CAkDO,MAlDP,EAkDO;AAlDD,QAAA,MAAM,EAAC,MAkDN;AAlDc,QAAA,QAAM,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA;AAAA,iBAAU,QAAA,CAAA,YAAA,IAAA,QAAA,CAAA,YAAA,OAAA,QAAA,YAAV;AAAA,SAAA,EAAsB,CAAA,SAAA,CAAtB,CAAA;AAkDpB,OAlDP,E,CACE,W,EACA,W,EACA,mBAAA,CAOM,KAPN,EAAA,WAAA,EAOM,CANJ,WAMI,E,gBALJ,mBAAA,CAIE,OAJF,EAIE;AAHA,QAAA,IAAI,EAAC,MAGL;AAFA,QAAA,KAAK,EAAC,sBAEN;;iBADS,KAAA,CAAA,OAAA,CAAQ,e,GAAe,M;;AAChC,OAJF,E,IAAA,E,GAAA,C,iBAGW,KAAA,CAAA,OAAA,CAAQ,e,GAEf,CAPN,C,EASA,mBAAA,CAQM,KARN,EAAA,WAAA,EAQM,CAPJ,WAOI,E,gBANJ,mBAAA,CAKE,OALF,EAKE;AAJA,QAAA,IAAI,EAAC,gBAIL;AAHC,QAAA,GAAG,EAAA,KAAA,CAAO,WAAP,CAAmB,QAAnB,EAGJ;AAFA,QAAA,KAAK,EAAC,sBAEN;;iBADS,KAAA,CAAA,OAAA,CAAQ,Y,GAAY,M;;AAC7B,OALF,E,IAAA,E,CAAA,E,WAAA,C,iBAIW,KAAA,CAAA,OAAA,CAAQ,Y,GAEf,CARN,C,EAUA,mBAAA,CAOM,KAPN,EAAA,WAAA,EAOM,CANJ,WAMI,E,gBALJ,mBAAA,CAIE,OAJF,EAIE;AAHA,QAAA,IAAI,EAAC,MAGL;AAFA,QAAA,KAAK,EAAC,sBAEN;;iBADS,KAAA,CAAA,OAAA,CAAQ,Y,GAAY,M;;AAC7B,OAJF,E,IAAA,E,GAAA,C,iBAGW,KAAA,CAAA,OAAA,CAAQ,Y,GAEf,CAPN,C,EAQA,W,EACA,mBAAA,CAkBM,KAlBN,EAAA,WAAA,EAkBM,CAjBJ,mBAAA,CAQM,KARN,EAAA,WAAA,EAQM,CAPJ,mBAAA,CAMS,QANT,EAMS;AALP,QAAA,IAAI,EAAC,QAKE;AAJP,QAAA,KAAK,EAAC,8CAIC;AAHN,QAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,iBAAE,KAAA,CAAA,KAAA,GAAK,EAAP;AAAA,SAAA;AAGC,OANT,EAIC,UAJD,CAOI,CARN,CAiBI,EARJ,WAQI,CAlBN,C,CA/BF,E,EAAA,CAmDI,CAvDN,C,iCAuDM,CAvDN;AAAA,K,CADoB;;AAAA,GAAtB,CAiFI,EArBJ,YAAA,CAoBa,WApBb,EAoBa;AApBD,IAAA,IAAI,EAAC;AAoBJ,GApBb,EAAsB;sBACpB;AAAA,aAkBM,CAhBE,KAAA,CAAA,KAAA,IAAK,gB,kBAFb,mBAAA,CAkBM,KAlBN,EAAA,WAAA,EAkBM,CAdJ,WAcI,EAbJ,WAaI,EAZJ,mBAAA,CAKS,QALT,EAKS;AAJP,QAAA,KAAK,EAAC,2CAIC;AAHN,QAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,iBAAE,KAAA,CAAA,KAAA,GAAK,EAAP;AAAA,SAAA;AAGC,OALT,EAGC,UAHD,CAYI,EANJ,mBAAA,CAKS,QALT,EAKS;AAJP,QAAA,KAAK,EAAC,oDAIC;AAHN,QAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,iBAAE,QAAA,CAAA,YAAA,EAAF;AAAA,SAAA;AAGC,OALT,EAGC,OAHD,CAMI,CAlBN,C,iCAkBM,CAlBN;AAAA,K,CADoB;;AAAA,GAAtB,CAqBI,CAtJN,C","sourcesContent":["<template>\r\n  <div id=\"groupMeeting\">\r\n    <div class=\"row\">\r\n      <div class=\"col-6\">\r\n        <h6>Group Meeting</h6>\r\n      </div>\r\n      <div class=\"col-6\" v-if=\"group.student_id == student.id\">\r\n        <div class=\"text-end\">\r\n          <i\r\n            class=\"fa-solid fa-add pointer\"\r\n            @click=\"modal = 'new-meeting'\"\r\n            v-if=\"menu.key == 'progress'\"\r\n          ></i>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-12 text-center p-4\" v-if=\"meetings.length == 0\">\r\n        No group meeting yet\r\n      </div>\r\n      <div class=\"col-12 mt-2\">\r\n        <ul class=\"list-group\">\r\n          <li\r\n            class=\"list-group-item\"\r\n            v-for=\"(i, index) in meetings\"\r\n            :key=\"index\"\r\n          >\r\n            <div class=\"row align-items-center\">\r\n              <div class=\"col-1\">{{ index + 1 }}</div>\r\n              <div class=\"col-md-3 col-12 p-0\">\r\n                <div\r\n                  :class=\"\r\n                    group.student_id == student.id ? 'meeting-subject' : ''\r\n                  \"\r\n                >\r\n                  {{ i.meeting_subject }}\r\n                  <div\r\n                    class=\"cancel justify-content-center\"\r\n                    v-if=\"group.student_id == student.id\"\r\n                    @click=\"cancelModal(i.id)\"\r\n                  >\r\n                    <div class=\"text-center\">Cancel</div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"col-md-3 col-11 text-md-center\">\r\n                {{ $customDate.date(i.meeting_date) }} <br />\r\n                {{ $customDate.time(i.meeting_date) }}\r\n              </div>\r\n              <div class=\"col-md-3 text-md-center\">\r\n                <div class=\"text-primary\" v-if=\"i.status == 0\">Upcoming</div>\r\n                <div class=\"text-success\" v-if=\"i.status == 1\">Completed</div>\r\n                <div class=\"text-danger\" v-if=\"i.status == 2\">Cancel</div>\r\n              </div>\r\n              <div class=\"col-md-2 text-md-end\">\r\n                <button\r\n                  :class=\"i.status != 0 ? 'bg-light text-muted' : 'bg-primary'\"\r\n                  :disabled=\"i.status != 0\"\r\n                  class=\"btn-mentoring py-1 px-4\"\r\n                  @click=\"join(i.meeting_link)\"\r\n                >\r\n                  Join\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"vue-modal-overlay\" v-if=\"modal != ''\"></div>\r\n    <!-- New Meeting  -->\r\n    <transition name=\"pop\">\r\n      <div\r\n        class=\"vue-modal vue-modal-sm bg-secondary\"\r\n        v-if=\"modal == 'new-meeting'\"\r\n      >\r\n        <form method=\"post\" @submit.prevent=\"handleSubmit\">\r\n          <h5>New Group Meeting</h5>\r\n          <hr class=\"my-0 mb-3\" />\r\n          <div class=\"mb-2\">\r\n            <label>Meeting Subject</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-mentoring w-100\"\r\n              v-model=\"meeting.meeting_subject\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"mb-2\">\r\n            <label>Meeting Date</label>\r\n            <input\r\n              type=\"datetime-local\"\r\n              :min=\"this.$customDate.tomorrow()\"\r\n              class=\"form-mentoring w-100\"\r\n              v-model=\"meeting.meeting_date\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"mb-3\">\r\n            <label>Location Link</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-mentoring w-100\"\r\n              v-model=\"meeting.meeting_link\"\r\n            />\r\n          </div>\r\n          <hr />\r\n          <div class=\"row\">\r\n            <div class=\"col-6\">\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn-mentoring btn-sm py-1 btn-outline-danger\"\r\n                @click=\"modal = ''\"\r\n              >\r\n                Cancel\r\n              </button>\r\n            </div>\r\n            <div class=\"col-6 text-end\">\r\n              <button\r\n                type=\"submit\"\r\n                class=\"btn-mentoring btn-sm py-1 btn-success\"\r\n              >\r\n                Save\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </transition>\r\n\r\n    <!-- Cancel Meeting  -->\r\n    <transition name=\"pop\">\r\n      <div\r\n        class=\"vue-modal vue-modal-sm bg-primary text-center\"\r\n        v-if=\"modal == 'cancel-meeting'\"\r\n      >\r\n        <i class=\"fa-solid fa-circle-exclamation mx-1 fa-2xl\"></i>\r\n        <h5 class=\"mt-3 mb-3\">Are you sure to cancel this meeting?</h5>\r\n        <button\r\n          class=\"btn-mentoring btn-sm py-1 btn-danger mx-1\"\r\n          @click=\"modal = ''\"\r\n        >\r\n          Cancel\r\n        </button>\r\n        <button\r\n          class=\"btn-mentoring btn-sm py-1 btn-outline-success mx-1\"\r\n          @click=\"handleCancel()\"\r\n        >\r\n          Yes\r\n        </button>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport moment from \"moment\";\r\nexport default {\r\n  name: \"meeting\",\r\n  props: {\r\n    menu: Object,\r\n    data: Object,\r\n    group: Object,\r\n    student: Object,\r\n  },\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      meeting_id: \"\",\r\n      meetings: [],\r\n      meeting: {\r\n        group_id: \"\",\r\n        meeting_subject: \"\",\r\n        meeting_date: \"\",\r\n        meeting_link: \"\",\r\n        status: \"0\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    async handleSubmit() {\r\n      this.modal = \"\";\r\n      this.meeting.group_id = this.group.id;\r\n      this.meeting.meeting_date = moment(this.meeting.meeting_date).format(\r\n        \"YYYY-MM-DD HH:mm\"\r\n      );\r\n\r\n      // console.log(this.meeting);\r\n\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\r\n          \"student/group/project/meeting\",\r\n          this.meeting\r\n        );\r\n\r\n        this.meeting.meeting_subject = \"\";\r\n        this.meeting.meeting_link = \"\";\r\n\r\n        console.log(response.data);\r\n        this.$emit(\"check\", \"new\");\r\n        this.$alert.toast(\"success\", response.data.message);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n\r\n    cancelModal(i) {\r\n      this.modal = \"cancel-meeting\";\r\n      this.meeting_id = i;\r\n    },\r\n\r\n    async handleCancel() {\r\n      this.modal = \"\";\r\n\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.put(\r\n          \"student/group/project/meeting/\" + this.meeting_id\r\n        );\r\n\r\n        console.log(response.data);\r\n        this.$emit(\"check\", \"new\");\r\n        this.$alert.toast(\"success\", response.data.message);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n  },\r\n  updated() {\r\n    this.meetings = this.data;\r\n  },\r\n\r\n  created() {\r\n    this.meetings = this.data;\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.list-group {\r\n  font-size: 0.8em;\r\n}\r\n\r\n.meeting-subject {\r\n  position: relative;\r\n  width: 100%;\r\n  height: 100%;\r\n  padding: 10px;\r\n  overflow: hidden;\r\n}\r\n\r\n.cancel {\r\n  position: absolute;\r\n  display: flex;\r\n  align-items: center;\r\n  cursor: pointer;\r\n  top: 0;\r\n  left: -100%;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: rgba(254, 123, 123, 0.907);\r\n  color: white;\r\n  font-weight: bold;\r\n  transition: all 0.5s;\r\n}\r\n\r\n.meeting-subject:hover .cancel {\r\n  left: 0 !important;\r\n}\r\n</style>"],"sourceRoot":""}]}