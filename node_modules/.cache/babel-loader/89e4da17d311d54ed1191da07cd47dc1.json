rer \" + localStorage.getItem(\"token\"),\r\n          },\r\n        })\r\n        .then((response) => {\r\n          this.mails.success = response.data.data.data.length;\r\n          console.log(this.mails.error);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n  },\r\n  created() {\r\n    this.getMailError();\r\n    this.getMailSuccess();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":""}]}