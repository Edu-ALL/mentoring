,QAFM;AAGZ,gBAAA,KAAI,CAAC,gBAAL,GAAwB,QAAQ,CAAC,IAAT,CAAc,IAAtC,CAHY,CAIZ;;AAJY;AAAA;;AAAA;AAAA;AAAA;AAMZ,gBAAA,OAAO,CAAC,GAAR,CAAY,YAAE,QAAd;;AANY;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQf,KATM;AAWD,IAAA,YAXC,0BAWc;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACnB,gBAAA,MAAI,CAAC,MAAL,CAAY,OAAZ;;AADmB;AAAA;AAAA,uBAGM,MAAI,CAAC,MAAL,CAAY,IAAZ,CAAiB,qBAAjB,EAAwC;AAC7D,kBAAA,SAAS,EAAE,MAAI,CAAC,WAAL,CAAiB,IADiC;AAE7D,kBAAA,mBAAmB,EAAE,MAAI,CAAC,WAAL,CAAiB,KAFuB;AAG7D,kBAAA,cAAc,EAAE,MAAI,CAAC,WAAL,CAAiB,cAH4B;AAI7D,kBAAA,KAAK,EAAE,MAAI,CAAC,WAAL,CAAiB,KAJqC;AAK7D,kBAAA,IAAI,EAAE,MAAI,CAAC,WAAL,CAAiB,IAAjB,CAAsB,QAAtB;AALuD,iBAAxC,CAHN;;AAAA;AAGX,gBAAA,QAHW;AAWjB;AACA;AAEA;AACA,gBAAA,MAAI,CAAC,GAAL,GAAW,KAAX;AACA,gBAAA,MAAI,CAAC,WAAL,CAAiB,IAAjB,GAAwB,EAAxB;AACA,gBAAA,MAAI,CAAC,WAAL,CAAiB,KAAjB,GAAyB,EAAzB;AACA,gBAAA,MAAI,CAAC,WAAL,CAAiB,cAAjB,GAAkC,EAAlC;AACA,gBAAA,MAAI,CAAC,WAAL,CAAiB,KAAjB,GAAyB,EAAzB;AACA,gBAAA,MAAI,CAAC,WAAL,CAAiB,IAAjB,GAAwB,EAAxB;;AAEA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C;;AACA,gBAAA,MAAI,CAAC,OAAL;;AAvBiB;AAAA;;AAAA;AAAA;AAAA;AAyBjB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAF,CAAW,IAAvB,EAzBiB,CA0BjB;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,kBAA3B;;AA3BiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA6BpB,KAxCM;AA0CP,IAAA,iBA1CO,6BA0CW,CA1CX,EA0Cc;AACnB,WAAK,OAAL,GAAe,CAAf;AACA,WAAK,KAAL,GAAa,QAAb;AACD,KA7CM;AA+CD,IAAA,aA/CC,2BA+Ce;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAGK,MAAI,CAAC,MAAL,CAAY,MAAZ,CACrB,yBAAyB,MAAI,CAAC,OADT,CAHL;;AAAA;AAGZ,gBAAA,QAHY;;AAMlB,gBAAA,MAAI,CAAC,OAAL;;AACA,gBAAA,MAAI,CAAC,KAAL,GAAa,EAAb;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C,EATkB,CAUlB;;;AAVkB;AAAA;;AAAA;AAAA;AAAA;AAYlB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,kBAA3B;;AAdkB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBrB;AA/DM,GAlBI;AAmFb,EAAA,OAnFa,qBAmFH;AACR,SAAK,OAAL;AACD;AArFY,CAAf","sourcesContent":["<template>\r\n  <div id=\"competition\" style=\"scroll-margin-top: 120px\">\r\n    <div\r\n      class=\"\r\n        heading\r\n        px-3\r\n        py-1\r\n        d-flex\r\n        justify-content-between\r\n        align-items-center\r\n      \"\r\n    >\r\n      <div class=\"fw-light\">Competitions</div>\r\n      <div class=\"text-end\">\r\n        <button\r\n          class=\"btn-mentoring btn-sm bg-primary py-1 px-2\"\r\n          v-if=\"!add\"\r\n          @click=\"add = true\"\r\n        >\r\n          <i class=\"bi bi-plus\"></i>\r\n        </button>\r\n        <button\r\n          class=\"btn-mentoring btn-sm btn-outline-danger py-1 px-2\"\r\n          v-if=\"add\"\r\n          @click=\"add = false\"\r\n        >\r\n          <i class=\"bi bi-x\"></i>\r\n        </button>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"desc\">\r\n      <div class=\"row mb-2\">\r\n        <div class=\"col-12\">\r\n          <form @submit.prevent=\"handleSubmit\" method=\"post\">\r\n            <fieldset class=\"border p-2 mb-2\" v-if=\"add\">\r\n              <legend class=\"float-none w-auto\" style=\"font-size: 0.8em\">\r\n                New Competition\r\n              </legend>\r\n              <div class=\"field\">\r\n                <div class=\"data\">\r\n                  <!-- new -->\r\n                  <div class=\"row g-3\">\r\n                    <div class=\"col-md-6 mb-1\">\r\n                      <input-group>\r\n                        <input\r\n                          type=\"text\"\r\n                          v-model=\"competition.name\"\r\n                          required\r\n                          placeholder=\"fiil in here ...\"\r\n                          class=\"form-mentoring form-control w-100\"\r\n                          id=\"compName\"\r\n                        />\r\n                        <label for=\"compName\">Competition Name</label>\r\n                      </input-group>\r\n                      <small></small>\r\n                    </div>\r\n                    <div class=\"col-md-6 mb-1\">\r\n                      <input-group>\r\n                        <input\r\n                          type=\"text\"\r\n                          v-model=\"competition.level\"\r\n                          required\r\n                          placeholder=\"fiil in here ...\"\r\n                          class=\"form-mentoring form-control w-100\"\r\n                          id=\"participationLevel\"\r\n                        />\r\n                        <label for=\"participationLevel\"\r\n                          >Participation Level</label\r\n                        >\r\n                      </input-group>\r\n                    </div>\r\n                    <div class=\"col-md-6 mb-1\">\r\n                      <input-group>\r\n                        <input\r\n                          type=\"text\"\r\n                          v-model=\"competition.accomplishment\"\r\n                          class=\"form-mentoring form-control w-100\"\r\n                          required\r\n                          placeholder=\"fiil in here ...\"\r\n                          id=\"accomplishment\"\r\n                        />\r\n                        <label for=\"accomplishment\">Accomplishment</label>\r\n                      </input-group>\r\n                    </div>\r\n                    <div class=\"col-md-3 mb-1\">\r\n                      <input-group>\r\n                        <select\r\n                          class=\"form-mentoring form-control\"\r\n                          id=\"month\"\r\n                          v-model=\"competition.month\"\r\n                          required\r\n                        >\r\n                          <option value=\"01\">January</option>\r\n                          <option value=\"02\">February</option>\r\n                          <option value=\"03\">March</option>\r\n                          <option value=\"04\">April</option>\r\n                          <option value=\"05\">May</option>\r\n                          <option value=\"06\">June</option>\r\n                          <option value=\"07\">July</option>\r\n                          <option value=\"08\">August</option>\r\n                          <option value=\"09\">September</option>\r\n                          <option value=\"10\">October</option>\r\n                          <option value=\"11\">November</option>\r\n                          <option value=\"12\">December</option>\r\n                        </select>\r\n                        <label for=\"month\">Month</label>\r\n                      </input-group>\r\n                    </div>\r\n                    <div class=\"col-md-3 mb-1\">\r\n                      <input-group>\r\n                        <select\r\n                          class=\"form-mentoring form-control\"\r\n                          id=\"year\"\r\n                          v-model=\"competition.year\"\r\n                          required\r\n                        >\r\n                          <option\r\n                            :value=\"parseInt(year) - i + 1\"\r\n                            v-for=\"i in 10\"\r\n                            :key=\"i\"\r\n                          >\r\n                            {{ parseInt(year) - i + 1 }}\r\n                          </option>\r\n                        </select>\r\n                        <label for=\"year\">Year</label>\r\n                      </input-group>\r\n                    </div>\r\n                  </div>\r\n\r\n                  <div class=\"text-end mt-3\">\r\n                    <button\r\n                      type=\"submit\"\r\n                      class=\"btn-mentoring btn-sm btn-outline-success py-1\"\r\n                    >\r\n                      <i class=\"bi bi-save me-2\"></i>\r\n                      Save Changes\r\n                    </button>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </fieldset>\r\n          </form>\r\n\r\n          <div class=\"row row-cols-md-2 row-cols-1 g-2 align-items-stretch\">\r\n            <div class=\"col\" v-for=\"i in competition_list\" :key=\"i\">\r\n              <div class=\"card shadow-sm border-0 h-100\">\r\n                <div class=\"card-body\">\r\n                  <div class=\"mb-2\">\r\n                    <div class=\"d-flex align-items-start\">\r\n                      <div class=\"title\" style=\"width: 90%\">\r\n                        <h5 class=\"text-muted my-0\">\r\n                          {{ i.comp_name }}\r\n                        </h5>\r\n                      </div>\r\n                      <div class=\"text-end\" style=\"width: 10%\">\r\n                        <i\r\n                          class=\"bi bi-x-circle text-danger pointer\"\r\n                          @click=\"deleteCompetition(i.id)\"\r\n                        ></i>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"badge bg-primary shadow-sm\">\r\n                      {{ i.participation_level }}\r\n                    </div>\r\n                    <div class=\"badge bg-primary shadow-sm ms-1\">\r\n                      {{ i.month + \"/\" + i.year }}\r\n                    </div>\r\n                    <div class=\"my-0 mt-2\">{{ i.accomplishments }}</div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"row\" v-if=\"competition_list.length == 0\">\r\n            <div class=\"col text-center py-3 text-muted\">\r\n              Please fill in your competition here.\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n  <!-- delete  -->\r\n  <transition name=\"pop\">\r\n    <div class=\"vue-modal vue-modal-sm bg-primary\" v-if=\"modal == 'delete'\">\r\n      <div class=\"text-center\">\r\n        <i class=\"fa-solid fa-circle-exclamation fa-2xl\"></i>\r\n        <h5 class=\"mt-2\">Are you sure to delete?</h5>\r\n        <div class=\"mt-3\">\r\n          <button\r\n            class=\"btn-mentoring btn-warning btn-sm py-1 mx-1\"\r\n            @click=\"modal = ''\"\r\n          >\r\n            Cancel</button\r\n          ><button\r\n            class=\"btn-mentoring btn-outline-success btn-sm py-1 mx-1\"\r\n            @click=\"deleteProcess\"\r\n          >\r\n            Yes\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </transition>\r\n</template>\r\n\r\n<script>\r\nimport moment from \"moment\";\r\nexport default {\r\n  name: \"competition\",\r\n  data() {\r\n    return {\r\n      add: false,\r\n      modal: \"\",\r\n      comp_id: \"\",\r\n      competition_list: [],\r\n      competition: {\r\n        name: \"\",\r\n        level: \"\",\r\n        accomplishment: \"\",\r\n        month: \"\",\r\n        year: \"\",\r\n      },\r\n      year: moment().format(\"YYYY\"),\r\n    };\r\n  },\r\n  methods: {\r\n    async getData() {\r\n      try {\r\n        const response = await this.$axios.get(\"student/competition\");\r\n        this.competition_list = response.data.data;\r\n        // console.log(response);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async handleSubmit() {\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\"student/competition\", {\r\n          comp_name: this.competition.name,\r\n          participation_level: this.competition.level,\r\n          accomplishment: this.competition.accomplishment,\r\n          month: this.competition.month,\r\n          year: this.competition.year.toString(),\r\n        });\r\n\r\n        // console.log(response.data);\r\n        // this.$alert.close();\r\n\r\n        // Reset\r\n        this.add = false;\r\n        this.competition.name = \"\";\r\n        this.competition.level = \"\";\r\n        this.competition.accomplishment = \"\";\r\n        this.competition.month = \"\";\r\n        this.competition.year = \"\";\r\n\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        this.getData();\r\n      } catch (e) {\r\n        console.log(e.response.data);\r\n        // this.$alert.close();\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n\r\n    deleteCompetition(i) {\r\n      this.comp_id = i;\r\n      this.modal = \"delete\";\r\n    },\r\n\r\n    async deleteProcess() {\r\n      // this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.delete(\r\n          \"student/competition/\" + this.comp_id\r\n        );\r\n        this.getData();\r\n        this.modal = \"\";\r\n        this.$alert.close();\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.close();\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.placeholder {\r\n  background-color: white !important;\r\n  color: #000;\r\n}\r\n</style>"],"sourceRoot":""}]}