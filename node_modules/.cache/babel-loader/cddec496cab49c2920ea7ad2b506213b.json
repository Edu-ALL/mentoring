;AAClB,QAAA,KAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,QAAA,KAAI,CAAC,OAAL,GAAe,QAAQ,CAAC,IAAT,CAAc,IAA7B,CAFkB,CAGlB;AACD,OAVH,EAWG,KAXH,CAWS,UAAC,KAAD,EAAW;AAChB,QAAA,KAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAdH;AAeD,KAlBM;AAoBP,IAAA,OApBO,mBAoBC,IApBD,EAoBO;AAAA;;AACZ,WAAK,MAAL,CACG,GADH,CACO,IADP,EACa;AACT,QAAA,OAAO,EAAE;AACP,UAAA,aAAa,EAAE,YAAY,YAAY,CAAC,OAAb,CAAqB,OAArB;AADpB;AADA,OADb,EAMG,IANH,CAMQ,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,OAAL,GAAe,QAAQ,CAAC,IAAT,CAAc,IAA7B,CADkB,CAElB;AACD,OATH,EAUG,KAVH,CAUS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAZH;AAaD,KAlCM;AAoCP,IAAA,UApCO,wBAoCM;AAAA;;AACX,WAAK,MAAL,CAAY,OAAZ;AACA,WAAK,MAAL,CACG,GADH,CACO,KAAK,IAAL,GAAY,2BAAZ,GAA0C,KAAK,MAAL,CAAY,IAD7D,EACmE;AAC/D,QAAA,OAAO,EAAE;AACP,UAAA,aAAa,EAAE,YAAY,YAAY,CAAC,OAAb,CAAqB,OAArB;AADpB;AADsD,OADnE,EAMG,IANH,CAMQ,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,QAAA,MAAI,CAAC,OAAL,GAAe,QAAQ,CAAC,IAAT,CAAc,IAA7B;AACA,QAAA,MAAI,CAAC,MAAL,CAAY,GAAZ,GAAkB,IAAlB,CAHkB,CAIlB;AACD,OAXH,EAYG,KAZH,CAYS,UAAC,KAAD,EAAW;AAChB,QAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAfH;AAgBD,KAtDM;AAwDP,IAAA,WAxDO,yBAwDO;AACZ,WAAK,MAAL,CAAY,GAAZ,GAAkB,KAAlB;AACA,WAAK,MAAL,CAAY,IAAZ,GAAmB,EAAnB;AACA,WAAK,OAAL;AACD;AA5DM,GAXI;AAyEb,EAAA,OAzEa,qBAyEH;AACR,SAAK,OAAL;AACD;AA3EY,CAAf","sourcesContent":["<template>\r\n  <div id=\"mentor\">\r\n    <div class=\"row mb-4\">\r\n      <div class=\"col-md-6 text-start\">\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-mentoring\"\r\n          v-model=\"search.name\"\r\n          @change=\"searchData\"\r\n          placeholder=\"Search\"\r\n        />\r\n        <br />\r\n        <span class=\"badge bg-primary px-3 d-inline-block\" v-if=\"search.bar\">\r\n          {{ search.name }}\r\n          <i class=\"fa-solid fa-close ms-3 pointer\" @click=\"closeSearch\"></i>\r\n        </span>\r\n      </div>\r\n      <!-- <div class=\"col-md-6 text-md-end text-center\">\r\n        <button class=\"btn-mentoring btn-type-1 me-2\">Sync Mentor Data</button>\r\n      </div> -->\r\n    </div>\r\n    <div class=\"card-white\">\r\n      <!-- {{ mentors }} -->\r\n      <table class=\"table table-borderless table-hover pointer\">\r\n        <thead>\r\n          <tr>\r\n            <th width=\"1%\">No</th>\r\n            <th>Full Name</th>\r\n            <th>Email</th>\r\n            <th>Phone Number</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr\r\n            v-for=\"(i, index) in mentors.data\"\r\n            :key=\"index\"\r\n            @click=\"this.$router.push({ path: '/admin/user/mentor/' + i.id })\"\r\n          >\r\n            <td>{{ mentors.from + index }}</td>\r\n            <td>\r\n              <i class=\"fa-regular fa-user fa-fw\"></i>\r\n              {{ i.first_name + \" \" + i.last_name }}\r\n            </td>\r\n            <td><i class=\"fa-solid fa-at fa-fw\"></i> {{ i.email }}</td>\r\n            <td>\r\n              <i class=\"fa-solid fa-mobile-screen fa-fw\"></i>\r\n              {{ i.phone_number == null ? \"N/A\" : i.phone_number }}\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n\r\n      <div class=\"text-center\" v-if=\"mentors.from == null\">\r\n        <hr />\r\n        <h6>Sorry, data is not found</h6>\r\n      </div>\r\n      <nav class=\"mt-2\" v-if=\"mentors.from != null\">\r\n        <ul class=\"pagination justify-content-center\">\r\n          <li class=\"page-item\" v-if=\"mentors.current_page != 1\">\r\n            <a class=\"page-link\" @click=\"getPage(mentors.links[0].url)\">\r\n              <i class=\"fa-solid fa-chevron-left\"></i>\r\n            </a>\r\n          </li>\r\n          <div v-for=\"i in mentors.last_page\" :key=\"i\">\r\n            <li\r\n              class=\"page-item\"\r\n              v-if=\"\r\n                mentors.current_page - 2 < i && mentors.current_page + 2 > i\r\n              \"\r\n            >\r\n              <a\r\n                class=\"page-link\"\r\n                :class=\"\r\n                  mentors.current_page == i ? 'bg-primary text-white' : ''\r\n                \"\r\n                href=\"#\"\r\n                @click=\"getPage(mentors.path + '?page=' + i)\"\r\n                >{{ i }}</a\r\n              >\r\n            </li>\r\n          </div>\r\n          <li\r\n            class=\"page-item\"\r\n            v-if=\"mentors.current_page != mentors.last_page\"\r\n          >\r\n            <a class=\"page-link\" @click=\"getPage(mentors.next_page_url)\">\r\n              <i class=\"fa-solid fa-chevron-right\"></i>\r\n            </a>\r\n          </li>\r\n        </ul>\r\n      </nav>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"mentor\",\r\n  data() {\r\n    return {\r\n      search: {\r\n        bar: false,\r\n        name: \"\",\r\n      },\r\n      mentors: [],\r\n    };\r\n  },\r\n  methods: {\r\n    getData() {\r\n      this.$alert.loading();\r\n      this.$axios\r\n        .get(this.$url + \"list/user/mentor\", {\r\n          headers: {\r\n            Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n          },\r\n        })\r\n        .then((response) => {\r\n          this.$alert.close();\r\n          this.mentors = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$alert.close();\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    getPage(link) {\r\n      this.$axios\r\n        .get(link, {\r\n          headers: {\r\n            Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n          },\r\n        })\r\n        .then((response) => {\r\n          this.mentors = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    searchData() {\r\n      this.$alert.loading();\r\n      this.$axios\r\n        .get(this.$url + \"find/user/mentor?keyword=\" + this.search.name, {\r\n          headers: {\r\n            Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n          },\r\n        })\r\n        .then((response) => {\r\n          this.$alert.close();\r\n          this.mentors = response.data.data;\r\n          this.search.bar = true;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$alert.close();\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    closeSearch() {\r\n      this.search.bar = false;\r\n      this.search.name = \"\";\r\n      this.getData();\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.badge {\r\n  border-radius: 15px;\r\n}\r\n</style>"],"sourceRoot":""}]}