ADJ,EAErB;AAAE,kBAAA,GAAG,EAAE,MAAI,CAAC;AAAZ,iBAFqB,CAHF;;AAAA;AAGf,gBAAA,QAHe;;AAOrB;AACA,oBAAI,QAAQ,CAAC,IAAT,CAAc,OAAlB,EAA2B;AACzB,kBAAA,MAAI,CAAC,UAAL;;AACA,kBAAA,MAAI,CAAC,SAAL,GAAiB,EAAjB;AACA,kBAAA,MAAI,CAAC,QAAL,CAAc,KAAd,IAAuB,KAAvB;;AACA,kBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C;AACF,iBALA,MAKO;AACL,kBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,QAAQ,CAAC,IAAT,CAAc,KAAzC;AACF;;AAfqB;AAAA;;AAAA;AAAA;AAAA;AAiBrB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,mBAA3B;;AAlBqB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoBxB,KA7FM;AA+FD,IAAA,OA/FC,qBA+FS;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAEZ,gBAAA,MAAI,CAAC,MAAL,CAAY,OAAZ;;AAFY;AAAA,uBAGW,MAAI,CAAC,MAAL,CAAY,GAAZ,CAAgB,UAAhB,CAHX;;AAAA;AAGN,gBAAA,QAHM;AAKZ,gBAAA,MAAI,CAAC,QAAL,GAAgB,QAAQ,CAAC,IAAT,CAAc,IAA9B,CALY,CAMZ;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AAPY;AAAA;;AAAA;AAAA;AAAA;;AASZ,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AAVY;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYf,KA3GM;AA6GD,IAAA,UA7GC,sBA6GU,EA7GV,EA6Gc,CA7Gd,EA6GiB;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAGG,MAAI,CAAC,MAAL,CAAY,GAAZ,CACrB,oBAAoB,EAApB,GAAyB,MADJ,EAErB;AAAE,kBAAA,UAAU,EAAE;AAAd,iBAFqB,CAHH;;AAAA;AAGd,gBAAA,QAHc;;AAOpB;AACA,oBAAI,QAAQ,CAAC,IAAT,CAAc,OAAlB,EAA2B;AACzB,kBAAA,MAAI,CAAC,UAAL;;AACA,kBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C;AACF,iBAHA,MAGO;AACL,kBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,QAAQ,CAAC,IAAT,CAAc,KAAzC;AACF;;AAboB;AAAA;;AAAA;AAAA;AAAA;AAepB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,kBAA3B;;AAhBoB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkBvB,KA/HM;AAiID,IAAA,WAjIC,uBAiIW,EAjIX,EAiIe;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACpB,gBAAA,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB;AAAE,kBAAA,IAAI,EAAE,qBAAqB,EAArB,GAA0B;AAAlC,iBAAlB;;AADoB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAErB,KAnIM;AAqID,IAAA,UArIC,wBAqIY;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAEf,gBAAA,MAAI,CAAC,MAAL,CAAY,OAAZ;;AAFe;AAAA,uBAGQ,MAAI,CAAC,MAAL,CAAY,GAAZ,CAAgB,cAAhB,CAHR;;AAAA;AAGT,gBAAA,QAHS;AAKf,gBAAA,MAAI,CAAC,aAAL,GAAqB,QAAQ,CAAC,IAAT,CAAc,IAAnC,CALe,CAMf;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AAPe;AAAA;;AAAA;AAAA;AAAA;;AASf,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AAVe;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYlB,KAjJM;AAmJP,IAAA,OAnJO,mBAmJC,IAnJD,EAmJO;AAAA;;AACZ,WAAK,MAAL,CACG,GADH,CACO,IADP,EAEG,IAFH,CAEQ,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,aAAL,GAAqB,QAAQ,CAAC,IAAT,CAAc,IAAnC,CADkB,CAElB;AACD,OALH,EAMG,KANH,CAMS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OARH;AASD;AA7JM,GAvBI;AAsLb,EAAA,OAtLa,qBAsLH;AACR,SAAK,UAAL;AACA,SAAK,OAAL;AACD;AAzLY,CAAf","sourcesContent":["<template>\r\n  <div id=\"student\">\r\n    <div class=\"\" v-if=\"menus.submenu == ''\">\r\n      <div class=\"row mb-4\">\r\n        <div class=\"col-md-12 d-flex align-items-center\">\r\n          <div class=\"mt-3 w-50\">\r\n            <input-group>\r\n              <input\r\n                v-model=\"search.name\"\r\n                type=\"text\"\r\n                placeholder=\"fill in\"\r\n                id=\"search\"\r\n                class=\"form-mentoring form-control\"\r\n              />\r\n              <label for=\"search\">Search</label>\r\n            </input-group>\r\n          </div>\r\n\r\n          <div class=\"mt-3 mx-3 w-25\">\r\n            <select class=\"form-mentoring w-100\" v-model=\"search.progress\">\r\n              <option value=\"\">Select progress status</option>\r\n              <option value=\"ahead\">Ahead</option>\r\n              <option value=\"ontrack\">On-Track</option>\r\n              <option value=\"behind\">Behind</option>\r\n            </select>\r\n          </div>\r\n\r\n          <div class=\"mt-3 w-25\">\r\n            <select class=\"form-mentoring w-100\" v-model=\"search.tag\">\r\n              <option value=\"\">Select progress status</option>\r\n              <option :value=\"i.name\" v-for=\"i in tag_list\" :key=\"i\">\r\n                {{ i.name }}\r\n              </option>\r\n            </select>\r\n          </div>\r\n\r\n          <div class=\"mt-3 text-end\" style=\"width: 12%\">\r\n            <button\r\n              class=\"btn-mentoring btn-secondary py-1\"\r\n              @click=\"searchData\"\r\n            >\r\n              <i class=\"fa-solid fa-search me-3\"></i>\r\n              Search\r\n            </button>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"col-12\">\r\n          <div class=\"card-white m-0 px-2 py-1 mt-2\">\r\n            <div class=\"table-responsive\">\r\n              <table class=\"table table-bordered table-hover align-middle\">\r\n                <thead>\r\n                  <tr class=\"text-center\">\r\n                    <td nowrap>No</td>\r\n                    <td nowrap>Full Name</td>\r\n                    <td nowrap>Email</td>\r\n                    <td nowrap>School Name</td>\r\n                    <td nowrap>Current Grade</td>\r\n                    <td nowrap>Phone Number</td>\r\n                    <td nowrap>Progress Status</td>\r\n                    <td nowrap>Tags</td>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <div\r\n                    class=\"col-6 text-center p-4\"\r\n                    v-if=\"students_data.length == 0\"\r\n                  >\r\n                    No students yet\r\n                  </div>\r\n                  <tr v-for=\"(i, index) in students_data.data\" :key=\"index\">\r\n                    <td class=\"text-center\">{{ index + 1 }}</td>\r\n                    <td nowrap class=\"pointer\" @click=\"checkDetail(i.id)\">\r\n                      {{ i.first_name + \" \" + i.last_name }}\r\n                    </td>\r\n                    <td nowrap class=\"pointer\" @click=\"checkDetail(i.id)\">\r\n                      {{ i.email }}\r\n                    </td>\r\n                    <td nowrap class=\"pointer\" @click=\"checkDetail(i.id)\">\r\n                      {{ i.school_name }}\r\n                    </td>\r\n                    <td nowrap class=\"pointer\" @click=\"checkDetail(i.id)\">\r\n                      <label v-if=\"i.grade == 0\"> N/A</label>\r\n                      <label v-if=\"i.grade != 0\"> {{ i.grade }}</label>\r\n                    </td>\r\n                    <td nowrap class=\"pointer\" @click=\"checkDetail(i.id)\">\r\n                      {{ i.phone_number }}\r\n                    </td>\r\n                    <td nowrap>\r\n                      <select\r\n                        v-model=\"i.progress_status\"\r\n                        class=\"form-mentoring form-control-sm\"\r\n                        @change=\"changeProgresStatus(i.id, i.progress_status)\"\r\n                      >\r\n                        <option value=\"\" disabled>\r\n                          Select progress status\r\n                        </option>\r\n                        <option value=\"ahead\">Ahead</option>\r\n                        <option value=\"ontrack\">On-Track</option>\r\n                        <option value=\"behind\">Behind</option>\r\n                      </select>\r\n                    </td>\r\n                    <td width=\"400px\" nowrap>\r\n                      <transition name=\"fade\">\r\n                        <!-- {{ tag_list }} -->\r\n                        <div\r\n                          class=\"d-flex align-items-center\"\r\n                          v-if=\"add_tags[index]\"\r\n                        >\r\n                          <select\r\n                            v-model=\"tags_name\"\r\n                            class=\"form-mentoring w-100 form-control-sm\"\r\n                            @change=\"addTags(i.id, index)\"\r\n                          >\r\n                            <option value=\"\">Select tag</option>\r\n                            <option\r\n                              :value=\"i.name\"\r\n                              v-for=\"i in tag_list\"\r\n                              :key=\"i\"\r\n                            >\r\n                              {{ i.name }}\r\n                            </option>\r\n                            <!-- <option value=\"val2\">Value 2</option>\r\n                            <option value=\"val3\">Value 3</option> -->\r\n                          </select>\r\n                          <i\r\n                            class=\"fa-solid fa-close ms-2 text-danger pointer\"\r\n                            @click=\"add_tags[index] = !add_tags[index]\"\r\n                          ></i>\r\n                        </div>\r\n                      </transition>\r\n                      <!-- {{i.tag}} -->\r\n                      <div class=\"d-flex flex-wrap align-items-center\">\r\n                        <div v-for=\"j in i.tag\" :key=\"j\">\r\n                          <div class=\"badge bg-primary me-1\">\r\n                            {{ j }}\r\n\r\n                            <div\r\n                              class=\"badge bg-white ms-2 pointer\"\r\n                              @click=\"removeTags(i.id, j)\"\r\n                            >\r\n                              <i class=\"fa-solid fa-close text-danger\"></i>\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                        <div\r\n                          class=\"pointer mt-1\"\r\n                          v-if=\"!add_tags[index]\"\r\n                          @click=\"add_tags[index] = !add_tags[index]\"\r\n                        >\r\n                          <i\r\n                            class=\"fa-solid fa-plus-square text-success\"\r\n                            style=\"font-size: 1.1em\"\r\n                          ></i>\r\n                        </div>\r\n                      </div>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n              <div class=\"text-center\" v-if=\"students_data.from == null\">\r\n                <hr />\r\n                <h6>Sorry, data is not found</h6>\r\n              </div>\r\n            </div>\r\n\r\n            <v-pagination :datas=\"students_data\" @result=\"getPage\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <v-detail\r\n      v-if=\"menus.menu == 'student' && menus.submenu != ''\"\r\n      :menus=\"menus\"\r\n      :detail=\"students_detail\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// import Multiselect from \"vue-multiselect\";\r\nimport Detail from \"@/components/mentor/page/student/view\";\r\nexport default {\r\n  name: \"student\",\r\n  props: {\r\n    menus: Object,\r\n  },\r\n  components: {\r\n    \"v-detail\": Detail,\r\n    // \"v-tag\": Multiselect,\r\n  },\r\n  data() {\r\n    return {\r\n      search: {\r\n        name: \"\",\r\n        progress: \"\",\r\n        tag: \"\",\r\n      },\r\n      add_tags: [],\r\n      tag_list: [],\r\n      students_data: [],\r\n      tags_name: \"\",\r\n      students_detail: [],\r\n    };\r\n  },\r\n  methods: {\r\n    async changeProgresStatus(id, s) {\r\n      try {\r\n        const response = await this.$axios.put(\r\n          \"update/student/\" + id + \"/progress-status\",\r\n          { progress: s }\r\n        );\r\n        this.getHistory();\r\n\r\n        this.$alert.toast(\"success\", response.data.message);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again.\");\r\n      }\r\n    },\r\n\r\n    async handleSubmit() {\r\n      // const id = this.menus.submenu;\r\n      this.modal = \"\";\r\n      this.uni.student_id = this.menus.submenu;\r\n      this.uni.univ_id = this.uni_select.univ_id;\r\n\r\n      console.log(this.uni);\r\n\r\n      this.$alert.loading();\r\n\r\n      try {\r\n        const response = await this.$axios.post(\"create/shortlisted\", this.uni);\r\n\r\n        this.uni.univ_id = \"\";\r\n        this.uni.major = \"\";\r\n\r\n        console.log(response.data);\r\n        this.getData();\r\n        this.$alert.toast(\"success\", response.data.message);\r\n      } catch (e) {\r\n        console.log(e.response.data);\r\n        // if (e.response.data.error) {\r\n        //   this.$alert.toast(\"error\", e.response.data.error.univ_id[0]);\r\n        // } else {\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n        // }\r\n      }\r\n    },\r\n\r\n    searchData() {\r\n      this.$alert.loading();\r\n      this.$axios\r\n        .get(\r\n          \"student/list?keyword=\" +\r\n            this.search.name +\r\n            \"&status=\" +\r\n            this.search.progress +\r\n            \"&tag=\" +\r\n            this.search.tag\r\n        )\r\n        .then((response) => {\r\n          this.$alert.close();\r\n          this.students_data = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$alert.close();\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    closeSearch() {\r\n      this.search.bar = false;\r\n      this.search.name = \"\";\r\n      this.getHistory();\r\n    },\r\n\r\n    async addTags(id, index) {\r\n      // alert(id + this.tags_name)\r\n      try {\r\n        const response = await this.$axios.put(\r\n          \"update/student/\" + id + \"/tag\",\r\n          { tag: this.tags_name }\r\n        );\r\n        // console.log(response)\r\n        if (response.data.success) {\r\n          this.getHistory();\r\n          this.tags_name = \"\";\r\n          this.add_tags[index] = false;\r\n          this.$alert.toast(\"success\", response.data.message);\r\n        } else {\r\n          this.$alert.toast(\"error\", response.data.error);\r\n        }\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again.\");\r\n      }\r\n    },\r\n\r\n    async tagList() {\r\n      try {\r\n        this.$alert.loading();\r\n        const response = await this.$axios.get(\"list/tag\");\r\n\r\n        this.tag_list = response.data.data;\r\n        // console.log(response.data);\r\n        this.$alert.close();\r\n      } catch (e) {\r\n        this.$alert.close();\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async removeTags(id, j) {\r\n      // alert(id + j)\r\n      try {\r\n        const response = await this.$axios.put(\r\n          \"update/student/\" + id + \"/tag\",\r\n          { remove_tag: j }\r\n        );\r\n        // console.log(response)\r\n        if (response.data.success) {\r\n          this.getHistory();\r\n          this.$alert.toast(\"success\", response.data.message);\r\n        } else {\r\n          this.$alert.toast(\"error\", response.data.error);\r\n        }\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n\r\n    async checkDetail(id) {\r\n      this.$router.push({ path: \"/mentor/student/\" + id + \"/meeting\" });\r\n    },\r\n\r\n    async getHistory() {\r\n      try {\r\n        this.$alert.loading();\r\n        const response = await this.$axios.get(\"student/list\");\r\n\r\n        this.students_data = response.data.data;\r\n        // console.log(response.data);\r\n        this.$alert.close();\r\n      } catch (e) {\r\n        this.$alert.close();\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    getPage(link) {\r\n      this.$axios\r\n        .get(link)\r\n        .then((response) => {\r\n          this.students_data = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n  },\r\n  created() {\r\n    this.getHistory();\r\n    this.tagList();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.search-desc {\r\n  font-size: 11px;\r\n  font-style: italic;\r\n}\r\n\r\n.search-student {\r\n  align-items: center;\r\n}\r\n\r\n.card-white {\r\n  background: #fff;\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n}\r\n</style>"],"sourceRoot":""}]}