AA,GAAtB,C","sourcesContent":["<template>\r\n  <div id=\"essay\">\r\n    <div class=\"req-header p-2 py-1\">\r\n      <i\r\n        class=\"fa-solid fa-circle-xmark me-1 text-danger\"\r\n        v-if=\"data?.length == 0\"\r\n      ></i>\r\n      <i\r\n        class=\"fa-solid fa-circle-check me-1 text-success\"\r\n        v-if=\"data?.length != 0\"\r\n      ></i>\r\n      ESSAYS\r\n      <div class=\"float-end\">\r\n        <i class=\"fa-solid fa-add pointer\" @click=\"modal = 'files'\"></i>\r\n      </div>\r\n    </div>\r\n    <div class=\"req-body p-2 py-4\">\r\n      <!-- IF EMPTY  -->\r\n      <div class=\"row\" v-if=\"data?.length == 0\">\r\n        <div class=\"col text-center text-muted pb-4\" @click=\"modal = 'add'\">\r\n          Please upload your essay here.\r\n        </div>\r\n      </div>\r\n      <div class=\"row row-cols-md-5 row-cols-1 g-2 align-items-stretch\">\r\n        <div class=\"col\" v-for=\"i in data\" :key=\"i\">\r\n          <div class=\"card shadow-sm docs h-100\">\r\n            <div class=\"card-body text-center d-flex align-items-center\">\r\n              <div class=\"w-100 justify-content-center docs-detail\">\r\n                <div class=\"d-block mb-2\">\r\n                  <i\r\n                    class=\"fa-solid fa-file-pdf fa-2x text-danger\"\r\n                    v-if=\"i.med_file_format == 'pdf'\"\r\n                  ></i>\r\n                  <i\r\n                    class=\"fa-solid fa-file-word fa-2x text-primary\"\r\n                    v-if=\"\r\n                      i.med_file_format == 'docx' || i.med_file_format == 'doc'\r\n                    \"\r\n                  ></i>\r\n                  <i\r\n                    class=\"fa-solid fa-file-excel fa-2x text-success\"\r\n                    v-if=\"i.med_file_format == 'xlsx'\"\r\n                  ></i>\r\n                  <i\r\n                    class=\"fa-solid fa-file-image fa-2x text-secondary\"\r\n                    v-if=\"\r\n                      i.med_file_format == 'png' ||\r\n                      i.med_file_format == 'jpeg' ||\r\n                      i.med_file_format == 'jpg'\r\n                    \"\r\n                  ></i>\r\n                </div>\r\n                {{ i.med_title }}\r\n              </div>\r\n              <div class=\"docs-content\">\r\n                <div class=\"\">\r\n                  <a\r\n                    :href=\"$base_url + i.med_file_path\"\r\n                    target=\"_blank\"\r\n                    class=\"btn btn-mentoring bg-white p-1 px-3 mx-1\"\r\n                    download\r\n                  >\r\n                    <i class=\"fa-solid fa-download text-primary\"></i>\r\n                  </a>\r\n                  <button\r\n                    class=\"btn btn-mentoring bg-white p-1 px-3 mx-1\"\r\n                    @click=\"editMedia(i)\"\r\n                  >\r\n                    <i class=\"fa-solid fa-edit text-info\"></i>\r\n                  </button>\r\n                  <button\r\n                    class=\"btn btn-mentoring bg-white p-1 px-3 mx-1\"\r\n                    @click=\"deleteMedia(i.id)\"\r\n                  >\r\n                    <i class=\"fa-solid fa-trash text-danger\"></i>\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n  <!-- Modal Document  -->\r\n\r\n  <!-- Upload  -->\r\n  <transition name=\"pop\">\r\n    <div class=\"vue-modal vue-modal-md\" v-if=\"modal == 'files'\">\r\n      <h6>Upload New Essay</h6>\r\n      <form @submit.prevent=\"handleUpload\" method=\"post\">\r\n        <div class=\"border p-2\" v-if=\"tabModal == 'new'\">\r\n          <div class=\"mb-2\">\r\n            <input-group>\r\n              <input\r\n                type=\"text\"\r\n                class=\"form-mentoring form-control-sm w-100\"\r\n                v-model=\"file.name\"\r\n                required\r\n              />\r\n              <label>File Name</label>\r\n            </input-group>\r\n          </div>\r\n          <div class=\"mb-2\">\r\n            <v-uni\r\n              v-model=\"uni_select\"\r\n              :options=\"uni_list\"\r\n              placeholder=\"Select One\"\r\n              deselect-label=\"Can't remove this value\"\r\n              track-by=\"uni_name\"\r\n              :custom-label=\"customUnilabel\"\r\n              label=\"uni_name\"\r\n              :searchable=\"true\"\r\n              :allow-empty=\"false\"\r\n              required\r\n              @select=\"uniListCheck\"\r\n            >\r\n            </v-uni>\r\n          </div>\r\n          <div class=\"border mb-2\">\r\n            <div\r\n              @dragenter.prevent=\"toggleActive\"\r\n              @dragleave.prevent=\"toggleActive\"\r\n              @dragover.prevent\r\n              @drop.prevent=\"drop\"\r\n              :class=\"{ 'active-dropzone': active }\"\r\n              class=\"dropzone\"\r\n            >\r\n              <span>Drag or Drop File</span>\r\n              <span>OR</span>\r\n              <label for=\"dropzoneFile\">Select File</label>\r\n              <input\r\n                type=\"file\"\r\n                id=\"dropzoneFile\"\r\n                class=\"dropzoneFile\"\r\n                @change=\"selectedFile\"\r\n              />\r\n            </div>\r\n            <div class=\"my-2\">\r\n              <span class=\"file-info\">\r\n                File: {{ dropzoneFile.name }}\r\n                <i\r\n                  class=\"fa-solid fa-times text-danger ms-2 pointer\"\r\n                  v-if=\"dropzoneFile\"\r\n                  @click=\"deleteFile\"\r\n                ></i>\r\n              </span>\r\n            </div>\r\n          </div>\r\n          <div class=\"text-end\">\r\n            <button type=\"submit\" class=\"btn-mentoring btn-sm py-1 bg-primary\">\r\n              Submit\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </transition>\r\n\r\n  <!-- Edit  -->\r\n  <transition name=\"pop\">\r\n    <div class=\"vue-modal vue-modal-md\" v-if=\"modal == 'edit'\">\r\n      <h6>Update Essay</h6>\r\n      <form @submit.prevent=\"handleUpdate\" method=\"post\">\r\n        <hr class=\"mt-1 mb-3\" />\r\n        <div class=\"mb-2\">\r\n          <input v-model=\"media_id\" hidden />\r\n          <input-group>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-mentoring form-control-sm w-100\"\r\n              v-model=\"file.name\"\r\n              required\r\n            />\r\n            <label>File Name</label>\r\n          </input-group>\r\n        </div>\r\n        <div class=\"mb-2\">\r\n          <v-uni\r\n            v-model=\"file.uni_id\"\r\n            :options=\"uni_list\"\r\n            placeholder=\"Select One\"\r\n            deselect-label=\"Can't remove this value\"\r\n            track-by=\"uni_name\"\r\n            :custom-label=\"customUnilabel\"\r\n            label=\"uni_name\"\r\n            :searchable=\"true\"\r\n            :allow-empty=\"false\"\r\n            required\r\n            @select=\"uniListCheck\"\r\n          >\r\n          </v-uni>\r\n        </div>\r\n\r\n        <div class=\"float-start\">\r\n          <button\r\n            type=\"button\"\r\n            class=\"btn-mentoring btn-sm py-1 bg-secondary mt-3\"\r\n            @click=\"modal = ''\"\r\n          >\r\n            Cancel\r\n          </button>\r\n        </div>\r\n        <div class=\"float-end mt-3\">\r\n          <button type=\"submit\" class=\"btn-mentoring btn-sm py-1 bg-primary\">\r\n            Submit\r\n          </button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </transition>\r\n\r\n  <!-- Delete  -->\r\n  <transition name=\"pop\">\r\n    <div class=\"vue-modal vue-modal-sm bg-primary\" v-if=\"modal == 'delete'\">\r\n      <div class=\"text-center\">\r\n        <i class=\"fa-solid fa-circle-exclamation fa-2xl\"></i>\r\n        <h5 class=\"mt-2\">Are you sure to delete?</h5>\r\n        <div class=\"mt-3\">\r\n          <button\r\n            class=\"btn-mentoring btn-warning btn-sm py-1 mx-1\"\r\n            @click=\"modal = ''\"\r\n          >\r\n            Cancel</button\r\n          ><button\r\n            class=\"btn-mentoring btn-outline-success btn-sm py-1 mx-1\"\r\n            @click=\"handleDelete\"\r\n          >\r\n            Yes\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </transition>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from \"vue\";\r\nimport Multiselect from \"vue-multiselect\";\r\n\r\nexport default {\r\n  name: \"essay\",\r\n  props: {\r\n    data: Object,\r\n  },\r\n  components: {\r\n    \"v-uni\": Multiselect,\r\n  },\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      tabModal: \"new\",\r\n      uni_select: [],\r\n      uni_list: [\r\n        {\r\n          id: \"general\",\r\n          uni_name: \"General\",\r\n          uni_major: \"\",\r\n        },\r\n      ],\r\n      media_id: \"\",\r\n      file: {\r\n        uni_id: \"\",\r\n        name: \"\",\r\n        file_category: \"essay\",\r\n        subject: \"\",\r\n        uploaded_file: [],\r\n      },\r\n    };\r\n  },\r\n\r\n  setup() {\r\n    const active = ref(false);\r\n    const toggleActive = () => {\r\n      active.value = !active.value;\r\n    };\r\n\r\n    let dropzoneFile = ref(\"\");\r\n    const drop = (e) => {\r\n      dropzoneFile.value = e.dataTransfer.files[0];\r\n    };\r\n\r\n    const selectedFile = () => {\r\n      dropzoneFile.value = document.querySelector(\".dropzoneFile\").files[0];\r\n    };\r\n\r\n    const deleteFile = () => {\r\n      dropzoneFile.value = \"\";\r\n    };\r\n\r\n    return {\r\n      active,\r\n      toggleActive,\r\n      dropzoneFile,\r\n      drop,\r\n      selectedFile,\r\n      deleteFile,\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    customUnilabel({ uni_name, uni_major }) {\r\n      if (uni_major == \"\") {\r\n        return `${uni_name}`;\r\n      } else {\r\n        return `${uni_name} â€” [ ${uni_major} ]`;\r\n      }\r\n    },\r\n\r\n    uniListCheck(i) {\r\n      if (i.id == \"general\") {\r\n        this.file.uni_id = \"\";\r\n      } else {\r\n        this.file.uni_id = i.imported_id;\r\n      }\r\n\r\n      // console.log(this.file.uni_id);\r\n    },\r\n\r\n    async getUniList() {\r\n      try {\r\n        const response = await this.$axios.get(\r\n          \"student/university/shortlisted/all\"\r\n        );\r\n        let response_uni = response.data.data;\r\n        if (response_uni && response_uni.length != 0) {\r\n          response_uni.forEach((element) => {\r\n            this.uni_list.push(element);\r\n          });\r\n        }\r\n        // console.log(this.uni_list);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async handleUpload() {\r\n      this.modal = \"\";\r\n\r\n      if (this.dropzoneFile) {\r\n        this.file.uploaded_file = this.dropzoneFile;\r\n      }\r\n\r\n      let form = new FormData();\r\n      form.append(\"uni_id\", this.file.uni_id);\r\n      form.append(\"name\", this.file.name);\r\n      form.append(\"uploaded_file\", this.file.uploaded_file);\r\n      form.append(\"file_category\", this.file.file_category);\r\n\r\n      // for (var pair of form.entries()) {\r\n      //   console.log(pair[0] + \", \" + pair[1]);\r\n      // }\r\n\r\n      this.$alert.loading();\r\n\r\n      try {\r\n        const response = await this.$axios.post(\r\n          \"student/document/requirement\",\r\n          form\r\n        );\r\n\r\n        this.$emit(\"check\", \"file\");\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response.data);\r\n        if (e.response.data.error.uploaded_file) {\r\n          this.$alert.toast(\"error\", e.response.data.error.uploaded_file[0]);\r\n        } else if (e.response.data.error.uni_id) {\r\n          this.$alert.toast(\"error\", e.response.data.error.uni_id[0]);\r\n        } else {\r\n          this.$alert.toast(\"error\", \"Please try again.\");\r\n        }\r\n      }\r\n    },\r\n\r\n    editMedia(i) {\r\n      this.modal = \"edit\";\r\n      this.media_id = i.id;\r\n      this.file.name = i.med_title;\r\n      if (i.uni_shortlisted && i.uni_shortlisted.length == 0) {\r\n        this.file.uni_id = {\r\n          id: \"general\",\r\n          uni_name: \"General\",\r\n          uni_major: \"\",\r\n        };\r\n      } else {\r\n        this.file.uni_id = i.uni_shortlisted[0];\r\n      }\r\n    },\r\n\r\n    async handleUpdate() {\r\n      let form_data = [];\r\n      if (this.file.uni_id.id == \"general\") {\r\n        form_data = {\r\n          general: 1,\r\n          media_id: this.media_id,\r\n          name: this.file.name,\r\n          uni_id: \"\",\r\n        };\r\n      } else {\r\n        form_data = {\r\n          general: 0,\r\n          media_id: this.media_id,\r\n          uni_id: this.file.uni_id.imported_id,\r\n          name: this.file.name,\r\n        };\r\n      }\r\n\r\n      this.modal = \"\";\r\n\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\r\n          \"student/media/pair\",\r\n          form_data\r\n        );\r\n\r\n        console.log(response.data);\r\n        this.$emit(\"check\", \"file\");\r\n        if (response.data.success) {\r\n          this.$alert.toast(\"success\", response.data.message);\r\n        } else {\r\n          this.$alert.toast(\"success\", response.data.error);\r\n        }\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$emit(\"check\", \"file\");\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n\r\n    deleteMedia(i) {\r\n      this.modal = \"delete\";\r\n      this.media_id = i;\r\n    },\r\n\r\n    async handleDelete() {\r\n      this.modal = \"\";\r\n\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.delete(\r\n          \"student/media/delete/\" + this.media_id\r\n        );\r\n\r\n        // console.log(response.data);\r\n        this.$emit(\"check\", \"file\");\r\n        this.$alert.toast(\"success\", response.data.message);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.getUniList();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":""}]}