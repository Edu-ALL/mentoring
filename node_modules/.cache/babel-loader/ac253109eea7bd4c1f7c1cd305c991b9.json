N;AAJA,QAAA,QAAQ,EAAR,EAIA;AAHA,QAAA,WAAW,EAAC,UAGZ;AAFA,QAAA,EAAE,EAAC,UAEH;AADA,QAAA,YAAY,EAAC;AACb,OARF,E,IAAA,E;;AAAA,O,iBACW,KAAA,CAAA,IAAA,CAAK,Q,GAOd,EACF,WADE,CARF;AAAA,K,CAYY;;;;AAAA,GAbd,CAcI,CAfN,C,EAgBA,W,EACA,W,CArCF,E;;AAAA,GA+CI,CAhDN,CAiDI,CAnDN,CAoDI,CArDN,CAsDI,CAvDN,CAwDI,CAzDN,CA0DI,CA3DN,CA4DI,CA7DN,C","sourcesContent":["<template>\r\n  <div id=\"login\" class=\"bg-primary\">\r\n    <div class=\"container\">\r\n      <div class=\"row justify-content-center\">\r\n        <div class=\"col-md-5\">\r\n          <div class=\"card bg-secondary\" style=\"border-radius: 20px\">\r\n            <div class=\"card-body\">\r\n              <h5 class=\"text-center\">Administrator</h5>\r\n              <div class=\"border border-2 p-3 rounded rounded-5\">\r\n                <form @submit.prevent=\"handleSubmit\" method=\"post\">\r\n                  <div class=\"mb-3\">\r\n                    <input-group>\r\n                      <input\r\n                        v-model=\"user.email\"\r\n                        type=\"email\"\r\n                        class=\"form-mentoring form-control w-100\"\r\n                        placeholder=\"email\"\r\n                        required\r\n                        id=\"email\"\r\n                        autocomplete=\"off\"\r\n                      />\r\n                      <label class=\"bg-secondary text-white\" for=\"email\"\r\n                        >Email</label\r\n                      >\r\n                    </input-group>\r\n                    <small class=\"text-white\" v-if=\"error?.email\">{{\r\n                      error.email[0]\r\n                    }}</small>\r\n                  </div>\r\n                  <div class=\"mb-3\">\r\n                    <input-group>\r\n                      <input\r\n                        v-model=\"user.password\"\r\n                        type=\"password\"\r\n                        class=\"form-mentoring form-control w-100\"\r\n                        required\r\n                        placeholder=\"password\"\r\n                        id=\"password\"\r\n                        autocomplete=\"off\"\r\n                      />\r\n                      <label class=\"bg-secondary text-white\" for=\"password\"\r\n                        >Password</label\r\n                      >\r\n                    </input-group>\r\n                  </div>\r\n                  <hr />\r\n                  <div class=\"text-end\">\r\n                    <button\r\n                      class=\"btn-mentoring btn-sm py-1 px-3 bg-primary\"\r\n                      type=\"submit\"\r\n                    >\r\n                      <i class=\"fa-solid fa-sign-in me-2\"></i>\r\n                      Sign In\r\n                    </button>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"login\",\r\n  data() {\r\n    return {\r\n      user: {\r\n        email: \"\",\r\n        password: \"\",\r\n      },\r\n      error: [],\r\n    };\r\n  },\r\n  methods: {\r\n    async handleSubmit() {\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\"auth/u/login\", this.user);\r\n        // console.log(response);\r\n\r\n        // Admin Data\r\n        localStorage.setItem(\"role\", \"admin\");\r\n        localStorage.setItem(\"token\", response.data.data.access_token);\r\n        localStorage.setItem(\"admin\", JSON.stringify(response.data.data.user));\r\n        this.$alert.close();\r\n        this.$alert.toast(\"success\", \"You Successfully Login\");\r\n        setTimeout(function () {\r\n          window.location.href = \"/admin/home\";\r\n        });\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.error_login = e.response.data.error;\r\n        if (e.response.status == 400) {\r\n          this.$alert.toast(\"error\", e.response.data.error);\r\n        } else {\r\n          this.error = e.response.data.error;\r\n          this.$alert.close();\r\n        }\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    document.title = \"Sign-In Admin\";\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#login {\r\n  display: flex;\r\n  align-items: center;\r\n  height: 100vh;\r\n}\r\n\r\n.form-control {\r\n  position: relative;\r\n  border: 2px solid #fff !important;\r\n  background: transparent !important;\r\n  color: #fff !important;\r\n  z-index: 1 !important;\r\n}\r\n</style>> \r\n"],"sourceRoot":""}]}