AA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAA,KAAA,CAAO,KAAP,CAAY,MAAZ,EAAY,QAAZ,CAAA;AAAA,KAAA;AAGJ,GALJ,EAGC,mBAHD,CAMI,CAtBN,C,EAuBA,mBAAA,CAkCM,KAlCN,EAAA,WAAA,EAkCM,CAjCJ,mBAAA,CAMS,QANT,EAMS;AALP,IAAA,KAAK,EAAA,eAAA,CAAA,CAAC,yBAAD,EACG,KAAA,CAAA,QAAA,IAAQ,SAAR,GAAQ,cAAR,GAAQ,YADX,CAAA,CAKE;AAHP,IAAA,IAAI,EAAC;AAGE,GANT,E,WAAA,E;;AAAA,GAiCI,EA1BJ,mBAAA,CAAA,8RAAA,CA0BI,EAjBJ,mBAAA,CAgBI,GAhBJ,EAgBI,IAhBJ,EAgBI,C,WAAA,EAXM,KAAA,CAAA,QAAA,IAAQ,S,kBAHhB,mBAAA,CAMC,MAND,EAMC;UAAA;AALE,IAAA,KAAK,EAAA,eAAA,CAAE,KAAA,CAAA,QAAA,IAAQ,SAAR,GAAQ,gBAAR,GAAQ,cAAV,CAKP;AAJE,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,KAAA,CAAA,QAAA,GAAQ,QAAV;AAAA,KAAA;AAIP,GAND,EAIC,SAJD,EAKQ;AAAA;AALR,G,qCAcE,EAJM,KAAA,CAAA,QAAA,IAAQ,Q,kBAHhB,mBAAA,CAMC,MAND,EAMC;UAAA;AALE,IAAA,KAAK,EAAA,eAAA,CAAE,KAAA,CAAA,QAAA,IAAQ,SAAR,GAAQ,gBAAR,GAAQ,cAAV,CAKP;AAJE,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,KAAA,CAAA,QAAA,GAAQ,SAAV;AAAA,KAAA;AAIP,GAND,EAIC,UAJD,EAKS;AAAA;AALT,G,qCAOE,CAhBJ,CAiBI,CAlCN,C,CAtDF,E;;AAAA,G,CAJF,E;;AAAA,GA+FI,CAhGN,C","sourcesContent":["<template>\n  <div id=\"login\">\n    <div\n      class=\"vue-modal vue-modal-sm\"\n      :class=\"login_as == 'student' ? 'bg-primary' : 'bg-secondary'\"\n    >\n      <form @submit.prevent=\"handleSubmit\">\n        <div class=\"text-center mb-4\">\n          <img\n            src=\"@/assets/img/logo-white.webp\"\n            alt=\"ALL-in Eduspace\"\n            class=\"vue-modal-logo\"\n          />\n        </div>\n        <h4\n          class=\"mb-4 text-center\"\n          :class=\"login_as == 'student' ? 'text-secondary' : 'text-primary'\"\n        >\n          <strong> Login to Your Account </strong>\n        </h4>\n        <div class=\"mb-3\">\n          <div class=\"input-group\">\n            <input\n              v-model=\"login.email\"\n              type=\"email\"\n              autocomplete=\"off\"\n              class=\"form-control v-form\"\n            />\n            <label\n              :class=\"login_as == 'student' ? 'bg-primary' : 'bg-secondary'\"\n              >Email</label\n            >\n          </div>\n          <p class=\"text-warning mt-1 small\" v-if=\"error_login.email\">\n            {{ error_login.email[0] }}\n          </p>\n        </div>\n        <div class=\"mb-3\">\n          <div class=\"input-group\">\n            <input\n              v-model=\"login.password\"\n              type=\"password\"\n              autocomplete=\"off\"\n              class=\"form-control v-form\"\n            />\n            <label\n              :class=\"login_as == 'student' ? 'bg-primary' : 'bg-secondary'\"\n              >Password</label\n            >\n          </div>\n          <p class=\"text-warning mt-1 small\" v-if=\"error_login.password\">\n            {{ error_login.password[0] }}\n          </p>\n          <p\n            class=\"mt-3 pointer text-center\"\n            @click=\"this.$emit('show', 'forgot')\"\n          >\n            Forgot Password\n          </p>\n        </div>\n        <div class=\"text-center\">\n          <button\n            class=\"btn btn-allin my-3 px-5\"\n            :class=\"login_as == 'student' ? 'bg-secondary' : 'bg-primary'\"\n            type=\"submit\"\n          >\n            <strong> Sign In </strong>\n          </button>\n          <!-- <p class=\"mb-0\">\n            Don't have an account? <br />\n            <u\n              class=\"text-secondary pointer\"\n              @click=\"this.$emit('show', 'register')\"\n              >Take our profile quiz</u\n            >\n            to create one!\n          </p> -->\n          <p>\n            Sign in as\n            <span\n              :class=\"login_as == 'student' ? 'text-secondary' : 'text-primary'\"\n              @click=\"login_as = 'mentor'\"\n              v-if=\"login_as == 'student'\"\n            >\n              Mentor</span\n            >\n            <span\n              :class=\"login_as == 'student' ? 'text-secondary' : 'text-primary'\"\n              @click=\"login_as = 'student'\"\n              v-if=\"login_as == 'mentor'\"\n            >\n              Student</span\n            >\n          </p>\n        </div>\n      </form>\n    </div>\n  </div>\n</template>\n<script>\nexport default {\n  name: \"login\",\n  data() {\n    return {\n      login: {\n        email: \"\",\n        password: \"\",\n      },\n      login_as: \"student\",\n      error_login: [],\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      this.$alert.loading();\n      try {\n        const response = await this.$axios.post(\"auth/s/login\", {\n          email: this.login.email,\n          password: this.login.password,\n        });\n        this.$emit(\"show\", \"\");\n\n        // Mentee Data\n        localStorage.setItem(\"role\", \"mentee\");\n        localStorage.setItem(\"token\", response.data.data.access_token);\n        localStorage.setItem(\n          \"mentee\",\n          JSON.stringify(response.data.data.student)\n        );\n        this.$alert.close();\n        this.$alert.toast(\"success\", \"You Successfully Login\");\n\n        window.location.href = \"/user\";\n        // this.$router.push({ path: \"/user\" });\n      } catch (e) {\n        this.error_login = e.response.data.error;\n        if (e.response.status == 400) {\n          this.$emit(\"show\", \"\");\n          this.$alert.toast(\"error\", e.response.data.error);\n        } else {\n          this.$alert.close();\n        }\n        // console.log(e.response);\n      }\n    },\n  },\n  created() {},\n};\n</script>\n<style scoped>\n.form-control {\n  position: relative;\n  border: 2px solid #fff !important;\n  background: transparent !important;\n  color: #fff !important;\n  z-index: 1 !important;\n}\n\ninput:focus,\ninput:active {\n  outline: none !important;\n  box-shadow: none !important;\n}\n\n.input-group {\n  position: relative;\n  z-index: 1;\n}\n\n.input-group label {\n  position: absolute;\n  top: 0;\n  left: 0;\n  color: #fff;\n  transform: translate(20px, 5px);\n  transition: all 0.4s;\n}\n\ninput:focus + label,\ninput:valid + label {\n  position: absolute;\n  color: #fff;\n  transform: translate(10px, -12px) scale(0.7);\n  background: #223872;\n  padding: 0 4px;\n  z-index: 2;\n}\n</style>"],"sourceRoot":""}]}