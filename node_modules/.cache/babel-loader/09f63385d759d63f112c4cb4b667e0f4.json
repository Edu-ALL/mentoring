del=\"reset.password\"\n                type=\"password\"\n                name=\"password\"\n                placeholder=\"New Password\"\n                class=\"form-control v-form\"\n              />\n              <p class=\"text-danger small\" v-if=\"error_reset.password\">\n                {{ error_reset.password[0] }}\n              </p>\n            </div>\n            <div class=\"mb-3\">\n              <input\n                v-model=\"reset.password_confirm\"\n                type=\"password\"\n                name=\"password\"\n                placeholder=\"Password Confirmation\"\n                class=\"form-control v-form\"\n              />\n              <p class=\"text-danger small\" v-if=\"error_reset.password\">\n                {{ error_reset.password_confirm[0] }}\n              </p>\n            </div>\n            <div class=\"text-center\">\n              <button\n                class=\"btn btn-allin bg-secondary my-3 px-5\"\n                type=\"submit\"\n              >\n                <strong> Reset </strong>\n              </button>\n            </div>\n          </div>\n        </transition>\n        <transition name=\"fade\">\n          <div v-if=\"changed\">\n            <p class=\"text-center\">\n              Your password has been successfully reset, <br />\n              please\n              <u\n                class=\"text-secondary pointer\"\n                @click=\"this.$emit('show', 'login')\"\n                >sign in now!</u\n              >\n            </p>\n          </div>\n        </transition>\n      </form>\n    </div>\n  </div>\n</template>\n<script>\nexport default {\n  name: \"reset\",\n  data() {\n    return {\n      changed: false,\n      reset: {\n        email: \"\",\n        password: \"\",\n        password_confirm: \"\",\n      },\n      error_reset: [],\n    };\n  },\n  methods: {\n    resetProcess() {\n      this.changed = true;\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}