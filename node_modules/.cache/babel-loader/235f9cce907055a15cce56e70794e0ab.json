      <a class=\"nav-link text-white\" href=\"#\">Home</a>\n                    </li>\n                    <li class=\"nav-item\">\n                      <a class=\"nav-link text-white\" href=\"#\">Guideline</a>\n                    </li>\n                  </ul>\n                </div>\n              </div>\n            </div>\n          </nav>\n\n          <img src=\"../assets/mentoring.png\" class=\"home-img mx-auto d-block\" />\n          <h2 class=\"mt-2\">\n            Lorem ipsum dolor sit amet consectetur adipisicing elit.\n          </h2>\n          <h6 class=\"mt-3\">\n            Lorem ipsum dolor sit amet consectetur adipisicing elit. Impedit\n            perferendis repudiandae nisi mollitia nesciunt eligendi doloremque.\n          </h6>\n        </div>\n        <div class=\"col-md-4 bg-white d-flex align-items-center px-md-5\">\n          <div class=\"card shadow box\">\n            <div class=\"card-body\" v-if=\"login_form\">\n              <form @submit.prevent=\"loginProcess\">\n                <h3>Log In</h3>\n                <hr />\n                <p>Lorem ipsum dolor sit amet consectetur adipisicing elit.</p>\n                <label>Username</label>\n                <input\n                  v-model=\"login.email\"\n                  type=\"text\"\n                  name=\"email\"\n                  class=\"form-control mb-3\"\n                />\n                <label>Password</label>\n                <input\n                  v-model=\"login.password\"\n                  type=\"text\"\n                  name=\"password\"\n                  class=\"form-control\"\n                />\n                <div class=\"d-grid gap-2 mt-3 mb-3\">\n                  <button class=\"btn btn-primary\" type=\"submit\">Log In</button>\n                </div>\n                <hr />\n                <div class=\"text-center\">\n                  <span class=\"me-3 pointer\" @click=\"forgot_password\"\n                    >Forgot Password</span\n                  >\n                  |\n                  <span class=\"ms-3 pointer\" @click=\"form\">Sign Up</span>\n                </div>\n              </form>\n            </div>\n\n            <div class=\"card-body\" v-if=\"!login_form\">\n              <form @submit.prevent=\"registerProcess\">\n                <h3>Sign Up as {{ role }}</h3>\n                <hr />\n                <ul class=\"nav nav-tabs mb-3\" style=\"font-size: 12px\">\n                  <li class=\"nav-item\">\n                    <a\n                      class=\"nav-link pointer\"\n                      :class=\"register.role_id == 1 ? 'active' : ''\"\n                      @click=\"signUpAs('1')\"\n                      >Student</a\n                    >\n                  </li>\n                  <li class=\"nav-item\">\n                    <a\n                      class=\"nav-link pointer\"\n                      :class=\"register.role_id == 2 ? 'active' : ''\"\n                      @click=\"signUpAs('2')\"\n                      >Mentor</a\n                    >\n                  </li>\n                  <li class=\"nav-item\">\n                    <a\n                      class=\"nav-link pointer\"\n                      :class=\"register.role_id == 3 ? 'active' : ''\"\n                      @click=\"signUpAs('3')\"\n                      >Editor</a\n                    >\n                  </li>\n                  <li class=\"nav-item\">\n                    <a\n                      class=\"nav-link pointer\"\n                      :class=\"register.role_id == 4 ? 'active' : ''\"\n                      @click=\"signUpAs('4')\"\n                      >Alumni</a\n                    >\n                  </li>\n                </ul>\n                <div class=\"row\">\n                  <div class=\"col-md-6 mb-2\">\n                    <label>First Name</label>\n                    <input\n                      v-model=\"register.first_name\"\n                      type=\"text\"\n                      name=\"first_name\"\n                      class=\"form-control\"\n                    />\n                    <p class=\"text-danger small\" v-if=\"erorr_form.first_name\">\n                      {{ erorr_form.first_name[0] }}\n                    </p>\n                  </div>\n                  <div class=\"col-md-6 mb-2\">\n                    <label>Last Name</label>\n                    <input\n                      v-model=\"register.last_name\"\n                      type=\"text\"\n                      name=\"last_name\"\n                      class=\"form-control\"\n                    />\n                    <p class=\"text-danger small\" v-if=\"erorr_form.last_name\">\n                      {{ erorr_form.last_name[0] }}\n                    </p>\n                  </div>\n                </div>\n                <div class=\"row\">\n                  <div class=\"col-md-12 mb-2\">\n                    <label>Email</label>\n                    <input\n                      v-model=\"register.email\"\n                      type=\"email\"\n                      name=\"email\"\n                      class=\"form-control\"\n                    />\n                    <p class=\"text-danger small\" v-if=\"erorr_form.email\">\n                      {{ erorr_form.email[0] }}\n                    </p>\n                  </div>\n                </div>\n                <div class=\"row\">\n                  <div class=\"col-md-6 mb-2\">\n                    <label>Birth Day</label>\n                    <input\n                      v-model=\"register.birthday\"\n                      type=\"date\"\n                      name=\"birthday\"\n                      class=\"form-control\"\n                    />\n                    <p class=\"text-danger small\" v-if=\"erorr_form.birthday\">\n                      {{ erorr_form.birthday[0] }}\n                    </p>\n                  </div>\n                  <div class=\"col-md-6 mb-2\">\n                    <label>Phone Number</label>\n                    <input\n                      v-model=\"register.phone_number\"\n                      type=\"text\"\n                      name=\"phone_number\"\n                      class=\"form-control\"\n                    />\n                    <p class=\"text-danger small\" v-if=\"erorr_form.phone_number\">\n                      {{ erorr_form.phone_number[0] }}\n                    </p>\n                  </div>\n                </div>\n                <div class=\"row\">\n                  <div class=\"col-md-6 mb-2\">\n                    <label>Password</label>\n                    <input\n                      v-model=\"register.password\"\n                      type=\"password\"\n                      name=\"password\"\n                      class=\"form-control\"\n                    />\n                    <p class=\"text-danger small\" v-if=\"erorr_form.password\">\n                      {{ erorr_form.password[0] }}\n                    </p>\n                  </div>\n                  <div class=\"col-md-6 mb-2\">\n                    <label>Password Confirm</label>\n                    <input\n                      v-model=\"register.password_confirmation\"\n                      type=\"password\"\n                      name=\"password_confirmation\"\n                      class=\"form-control\"\n                    />\n                  </div>\n                </div>\n                <div class=\"d-grid gap-2 mt-3\">\n                  <button class=\"btn btn-primary\" type=\"submit\">Sign Up</button>\n                </div>\n                <hr />\n                <div class=\"text-center\">\n                  <span class=\"ms-3 pointer\" @click=\"form\">Log in</span>\n                </div>\n              </form>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n// import Swal from \"sweetalert2\";\n\nexport default {\n  name: \"Home\",\n  components: {},\n  data() {\n    return {\n      login_form: true,\n      role: \"\",\n      login: {\n        email: \"\",\n        password: \"\",\n      },\n      erorr_form: {},\n      register: {\n        first_name: \"\",\n        last_name: \"\",\n        email: \"\",\n        birthday: \"\",\n        phone_number: \"\",\n        role_id: \"\",\n        password: \"\",\n        password_confirmation: \"\",\n      },\n    };\n  },\n  methods: {\n    form() {\n      if (this.login_form == true) {\n        this.login_form = false;\n      } else {\n        this.login_form = true;\n      }\n    },\n    signUpAs(role_id = 1) {\n      if (role_id == 1) {\n        this.role = \"Student\";\n      } else if (role_id == 2) {\n        this.role = \"Mentor\";\n      } else if (role_id == 3) {\n        this.role = \"Editor\";\n      } else if (role_id == 4) {\n        this.role = \"Alumni\";\n      }\n      this.register.role_id = role_id;\n    },\n    registerProcess() {\n      axios\n        .post(\"https://services.all-inedu.com/api/v1/register\", this.register)\n        .then((response) => {\n          if (response.data.success == false) {\n            this.erorr_form = response.data.message;\n          }\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    },\n    loginProcess() {\n      axios\n        .post(\"https://services.all-inedu.com/api/v1/login\", this.login)\n        .then((response) => {\n          console.log(response.data);\n        })\n        .catch((error) => {\n          console.error(error.response.data);\n        });\n    },\n  },\n  created() {\n    // this.checkUser();\n    this.signUpAs();\n  },\n};\n</script>\n<style scoped>\n#login {\n  height: 100vh;\n  background: url(\"../assets/bg.jpg\");\n  background-size: cover;\n  padding: auto 5%;\n}\n\n.home-img {\n  width: 50%;\n}\n</style>\n"],"sourceRoot":""}]}