,KAAI,CAAC,IAAL,GAAY,IAAZ;AACA,gBAAA,YAAY,CAAC,OAAb,CAAqB,aAArB,EAAoC,KAAI,CAAC,MAAL,CAAY,KAAhD,EAPiB,CAQjB;;AARiB;AAAA;;AAAA;AAAA;AAAA;AAUjB,gBAAA,KAAI,CAAC,YAAL,GAAoB,YAAE,QAAF,CAAW,IAAX,CAAgB,KAApC;;AACA,oBAAI,YAAE,QAAF,CAAW,MAAX,IAAqB,GAAzB,EAA8B;AAC5B,kBAAA,KAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,YAAE,QAAF,CAAW,IAAX,CAAgB,KAA3C;AACF,iBAbiB,CAcjB;;;AAdiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBpB;AArBM;AAXI,CAAf","sourcesContent":["<template>\r\n  <div id=\"forgot\">\r\n    <div class=\"vue-modal vue-modal-sm bg-primary\">\r\n      <form @submit.prevent=\"handleSubmit\">\r\n        <div class=\"text-center mb-4\">\r\n          <img\r\n            src=\"@/assets/img/logo-white.webp\"\r\n            alt=\"ALL-in Eduspace\"\r\n            class=\"vue-modal-logo\"\r\n          />\r\n        </div>\r\n        <h3 class=\"text-secondary mb-4 text-center\">\r\n          <strong> Forgot Password </strong>\r\n        </h3>\r\n        <div class=\"mb-3\">\r\n          <transition name=\"fade\">\r\n            <div v-if=\"!sent\">\r\n              <div class=\"text-center mb-2\">\r\n                Please enter your email and we'll send an email to reset your\r\n                password\r\n              </div>\r\n              <input-group>\r\n                <input\r\n                  v-model=\"forgot.email\"\r\n                  type=\"email\"\r\n                  name=\"email\"\r\n                  class=\"form-control form-mentoring w-100\"\r\n                  placeholder=\"Email\"\r\n                  required\r\n                  id=\"email\"\r\n                />\r\n                <label class=\"bg-primary rounded\" for=\"email\">Email</label>\r\n              </input-group>\r\n              <p class=\"text-white small\" v-if=\"error_forgot.email\">\r\n                {{ error_forgot.email[0] }}\r\n              </p>\r\n              <div class=\"text-center\">\r\n                <button\r\n                  class=\"btn btn-allin bg-secondary my-3 px-5\"\r\n                  type=\"submit\"\r\n                >\r\n                  <strong> Reset Password </strong>\r\n                </button>\r\n                <p class=\"mb-0\">\r\n                  I'v remembered my password,\r\n                  <u\r\n                    class=\"text-secondary pointer\"\r\n                    @click=\"this.$emit('show', 'login')\"\r\n                    >Sign in now!</u\r\n                  >\r\n                </p>\r\n              </div>\r\n            </div>\r\n          </transition>\r\n\r\n          <transition name=\"fade\">\r\n            <div v-if=\"sent\">\r\n              <p class=\"text-center text-white\">\r\n                <vue-feather type=\"check-circle\" stroke=\"#F0AB54\"></vue-feather>\r\n                <b class=\"d-block\"> Reset password link has been seent </b>\r\n              </p>\r\n              <hr />\r\n              <p class=\"text-center\">\r\n                I haven't received the email yet, <br />\r\n                please\r\n                <a @click=\"sent = false\" class=\"text-secondary pointer\"\r\n                  >send it again!</a\r\n                >\r\n              </p>\r\n            </div>\r\n          </transition>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"forgot\",\r\n  data() {\r\n    return {\r\n      sent: false,\r\n      forgot: {\r\n        email: \"\",\r\n      },\r\n      error_forgot: [],\r\n    };\r\n  },\r\n  methods: {\r\n    forgotProcess() {\r\n      this.sent = true;\r\n    },\r\n\r\n    async handleSubmit() {\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\"reset-password\", this.forgot);\r\n\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        this.sent = true;\r\n        localStorage.setItem(\"email_reset\", this.forgot.email);\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        this.error_forgot = e.response.data.error;\r\n        if (e.response.status == 400) {\r\n          this.$alert.toast(\"error\", e.response.data.error);\r\n        }\r\n        // console.log(e.response.data);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.form-control {\r\n  position: relative;\r\n  border: 2px solid #fff !important;\r\n  background: transparent !important;\r\n  color: #fff !important;\r\n  z-index: 1 !important;\r\n}\r\n</style>> \r\n"],"sourceRoot":""}]}