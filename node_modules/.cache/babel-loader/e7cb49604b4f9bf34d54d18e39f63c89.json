Y,OAAZ;;AADmB;AAAA;AAAA,uBAGM,MAAI,CAAC,MAAL,CAAY,GAAZ,CACrB,iCAAiC,MAAI,CAAC,OADjB,EAErB;AACE,kBAAA,KAAK,EAAE,MAAI,CAAC;AADd,iBAFqB,CAHN;;AAAA;AAGX,gBAAA,QAHW;;AAUjB;AACA,oBAAI,QAAQ,CAAC,IAAT,CAAc,OAAlB,EAA2B;AACzB,kBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C;;AACA,sBAAI,MAAI,CAAC,cAAL,IAAuB,WAA3B,EAAwC;AACtC,oBAAA,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB;AAChB,sBAAA,IAAI,EAAE,sCAAsC,MAAI,CAAC;AADjC,qBAAlB;AAGF,mBAJA,MAIO;AACL,oBAAA,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB;AAChB,sBAAA,IAAI,EAAE,wCAAwC,MAAI,CAAC;AADnC,qBAAlB;AAGF;AACF,iBAXA,MAWO;AACL,kBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,QAAQ,CAAC,IAAT,CAAc,KAAzC;AACF;;AAEA,gBAAA,MAAI,CAAC,KAAL,GAAa,EAAb;AA1BiB;AAAA;;AAAA;AAAA;AAAA;AA4BjB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,kBAA3B;;AACA,gBAAA,MAAI,CAAC,KAAL,GAAa,EAAb;;AA9BiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgCpB,KA7FM;AA+FD,IAAA,cA/FC,4BA+FgB;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACrB,gBAAA,MAAI,CAAC,MAAL,CAAY,OAAZ;;AADqB;AAAA;AAAA,uBAGI,MAAI,CAAC,MAAL,CAAY,GAAZ,CACrB,0CAA0C,MAAI,CAAC,OAD1B,EAErB;AACE,kBAAA,KAAK,EAAE,MAAI,CAAC,UAAL,CAAgB;AADzB,iBAFqB,CAHJ;;AAAA;AAGb,gBAAA,QAHa;;AAUnB,oBAAI,QAAQ,CAAC,IAAT,CAAc,OAAlB,EAA2B;AACzB,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,EAAoB,MAAI,CAAC,MAAL,CAAY,MAAZ,CAAmB,GAAvC;;AACA,kBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C;AACF,iBAHA,MAGO;AACL,kBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,QAAQ,CAAC,IAAT,CAAc,KAAzC;AACF;;AAEA,gBAAA,MAAI,CAAC,KAAL,GAAa,EAAb;AAjBmB;AAAA;;AAAA;AAAA;AAAA;AAmBnB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,kBAA3B;;AApBmB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsBtB;AArHM,GA3BI;AAmJb,EAAA,OAnJa,qBAmJH;AACR,SAAK,OAAL,GAAe,KAAK,MAAL,CAAY,MAAZ,CAAmB,OAAlC;AACA,SAAK,OAAL;AACD;AAtJY,CAAf","sourcesContent":["<template>\r\n  <div id=\"view\">\r\n    <div class=\"row align-items-center\">\r\n      <div class=\"col-6\">\r\n        <div class=\"pointer\" @click=\"redirect\">\r\n          <h6 class=\"py-0 my-0\">\r\n            <i class=\"fa-solid fa-arrow-left me-2\"></i> Groups\r\n          </h6>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-6 text-end\">\r\n        <select\r\n          v-model=\"group_info.progress_status\"\r\n          class=\"form-mentoring py-1 me-1\"\r\n          :disabled=\"group_info.status != 'in progress'\"\r\n          @change=\"changeProgress\"\r\n        >\r\n          <option value=\"null\" v-if=\"group_info.progress_status == null\">\r\n            Update progress here\r\n          </option>\r\n          <option value=\"ahead\">Ahead</option>\r\n          <option value=\"on-track\">On-track</option>\r\n          <option value=\"behind\">Behind</option>\r\n        </select>\r\n        <button\r\n          class=\"btn-mentoring py-1 px-3 btn-sm btn-outline-success\"\r\n          @click=\"confirmStatus('completed')\"\r\n          v-if=\"group_info.status == 'in progress'\"\r\n        >\r\n          <i class=\"fa-solid fa-check me-1\"></i>\r\n          Complete\r\n        </button>\r\n        <button\r\n          class=\"btn-mentoring py-1 px-3 btn-sm btn-danger\"\r\n          @click=\"confirmStatus('in progress')\"\r\n          v-if=\"group_info.status != 'in progress'\"\r\n        >\r\n          <i class=\"fa-solid fa-times me-1\"></i>\r\n          Cancel\r\n        </button>\r\n      </div>\r\n    </div>\r\n    <hr class=\"my-1 mb-3\" />\r\n    <div class=\"row\">\r\n      <div class=\"col-md-7 mb-3\">\r\n        <div class=\"card border-0 shadow mb-3\">\r\n          <div class=\"card-body\">\r\n            <div class=\"float-end\" v-if=\"mentor.id == group_info.user_id\">\r\n              <i\r\n                class=\"fa-solid fa-edit pointer\"\r\n                v-if=\"!editGroup\"\r\n                @click=\"editGroup = !editGroup\"\r\n              ></i>\r\n              <i\r\n                class=\"fa-solid fa-x pointer\"\r\n                v-if=\"editGroup\"\r\n                @click=\"editGroup = !editGroup\"\r\n              ></i>\r\n            </div>\r\n            <!-- Detail  -->\r\n            <div class=\"\" v-if=\"!editGroup\">\r\n              <h5>{{ group_info.project_name }}</h5>\r\n              <span class=\"badge bg-secondary rounded-pill text-dark\">\r\n                {{ group_info.project_type }}\r\n              </span>\r\n              <div class=\"project-desc mt-2\">\r\n                {{ group_info.project_desc }}\r\n              </div>\r\n              <div class=\"row mt-3 align-items-center\">\r\n                <div class=\"col-6\">\r\n                  <small class=\"text-muted\"\r\n                    ><i class=\"fa-solid fa-calendar me-2\"></i>\r\n                    {{ $customDate.date(group_info_updated_at) }}\r\n                  </small>\r\n                </div>\r\n                <!-- <div class=\"col-6 text-end\">\r\n                    <i class=\"fa-solid fa-thumbs-up mx-1 text-success\"></i>\r\n                    <i\r\n                      class=\"fa-solid fa-circle-exclamation mx-1 text-warning\"\r\n                    ></i>\r\n                    <i\r\n                      class=\"fa-solid fa-triangle-exclamation mx-1 text-danger\"\r\n                    ></i>\r\n                  </div> -->\r\n              </div>\r\n            </div>\r\n\r\n            <!-- Edit Group  -->\r\n            <form method=\"post\" @submit.prevent=\"handleUpdateGroup()\">\r\n              <div class=\"\" v-if=\"editGroup\">\r\n                <h6>Edit Group</h6>\r\n                <hr />\r\n                <div class=\"mb-3\">\r\n                  <input-group>\r\n                    <input\r\n                      v-model=\"group_info.project_name\"\r\n                      type=\"text\"\r\n                      required\r\n                      class=\"form-mentoring form-control w-100\"\r\n                      placeholder=\"fill in here..\"\r\n                      id=\"groupName\"\r\n                    />\r\n                    <label for=\"groupName\">Group Name</label>\r\n                  </input-group>\r\n                </div>\r\n                <div class=\"mb-3\">\r\n                  <group-type\r\n                    v-model=\"group_info.project_type\"\r\n                    :options=\"options\"\r\n                    placeholder=\"Select project type\"\r\n                    @select=\"interestCheck\"\r\n                    class=\"mt-3\"\r\n                  >\r\n                  </group-type>\r\n                </div>\r\n                <div class=\"mb-2\">\r\n                  <input-group>\r\n                    <textarea\r\n                      v-model=\"group_info.project_desc\"\r\n                      class=\"form-mentoring form-control w-100\"\r\n                      rows=\"5\"\r\n                      placeholder=\"fill in here...\"\r\n                      id=\"desc\"\r\n                    ></textarea>\r\n                    <label class=\"bg-white\" for=\"desc\">Group Description</label>\r\n                  </input-group>\r\n                </div>\r\n                <hr class=\"my-0 mb-2\" />\r\n                <div class=\"text-end\">\r\n                  <button\r\n                    type=\"submit\"\r\n                    class=\"btn-mentoring btn-sm bg-primary py-1\"\r\n                  >\r\n                    Save Changes\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </form>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Meeting  -->\r\n        <div class=\"card border-0 shadow\">\r\n          <div class=\"card-body\">\r\n            <v-meeting\r\n              :group=\"group_info\"\r\n              :student=\"student_info\"\r\n              :data=\"group_meeting\"\r\n              @check=\"checkComponent\"\r\n            ></v-meeting>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!-- <div class=\"col-md-5 mb-3\">\r\n        <div class=\"card border-0 shadow\">\r\n          <div class=\"card-body\">\r\n            <v-member\r\n              :menus=\"menus\"\r\n              :member=\"group_member\"\r\n              :group=\"group_info\"\r\n              @check=\"checkComponent\"\r\n            ></v-member>\r\n          </div>\r\n        </div>\r\n      </div> -->\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"vue-modal-overlay\" v-if=\"modal != ''\"></div>\r\n  <!-- Completed Group  -->\r\n  <transition name=\"pop\">\r\n    <div\r\n      class=\"vue-modal vue-modal-sm bg-primary text-center\"\r\n      v-if=\"modal == 'confirm'\"\r\n    >\r\n      <i class=\"fa-solid fa-circle-exclamation mx-1 fa-2xl\"></i>\r\n      <h5 class=\"mt-3 mb-3\" v-if=\"confirm_status == 'completed'\">\r\n        Are you sure to completing this group?\r\n      </h5>\r\n      <h5 class=\"mt-3 mb-3\" v-if=\"confirm_status == 'in progress'\">\r\n        Are you sure this group back to in progress?\r\n      </h5>\r\n      <button\r\n        class=\"btn-mentoring btn-sm py-1 btn-danger mx-1\"\r\n        @click=\"modal = ''\"\r\n      >\r\n        Cancel\r\n      </button>\r\n      <button\r\n        class=\"btn-mentoring btn-sm py-1 btn-outline-success mx-1\"\r\n        @click=\"changeStatus()\"\r\n      >\r\n        Yes\r\n      </button>\r\n    </div>\r\n  </transition>\r\n</template>\r\n\r\n<script>\r\nimport Multiselect from \"vue-multiselect\";\r\n\r\nimport Meeting from \"@/components/admin/group/view-detail/meeting\";\r\n// import Member from \"@/components/admin/group/view-detail/member\";\r\n\r\nexport default {\r\n  name: \"view\",\r\n  props: {\r\n    menus: Object,\r\n  },\r\n  components: {\r\n    \"v-meeting\": Meeting,\r\n    // \"v-member\": Member,\r\n    \"group-type\": Multiselect,\r\n  },\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      editGroup: false,\r\n      editMember: false,\r\n      groupType: \"\",\r\n      options: [\"Group Mentoring\", \"Profile Building Mentoring\"],\r\n      groupId: \"\",\r\n      mentor: [],\r\n      groups: [],\r\n      group_info: [],\r\n      group_meeting: [],\r\n      group_member: [],\r\n      student_info: [],\r\n      confirm_status: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    redirect() {\r\n      this.$router.push({ path: \"/admin/group/\" });\r\n    },\r\n\r\n    checkComponent(i) {\r\n      if (i == \"new\") {\r\n        this.getData();\r\n      }\r\n    },\r\n\r\n    async getData() {\r\n      try {\r\n        const response = await this.$axios.get(\r\n          \"mentor/detail/group/project/\" + this.groupId\r\n        );\r\n\r\n        this.group_info = response.data.data.group_info;\r\n        this.group_meeting = response.data.data.group_meeting;\r\n        this.group_member = response.data.data.group_member;\r\n        this.student_info = response.data.data.student_info;\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        this.$alert.toast(\"error\", \"Group project is not found\");\r\n        this.$router.push({ path: \"/admin/group\" });\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async handleUpdateGroup() {\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.put(\r\n          \"update/group/project/\" + this.groupId,\r\n          {\r\n            project_name: this.group_info.project_name,\r\n            project_type: this.group_info.project_type,\r\n            project_desc: this.group_info.project_desc,\r\n            status: \"in progress\",\r\n          }\r\n        );\r\n\r\n        this.group_info = response.data.data;\r\n        this.editGroup = false;\r\n        if (response.data.success) {\r\n          this.$alert.toast(\"success\", response.data.message);\r\n        } else {\r\n          this.$alert.toast(\"error\", response.data.error);\r\n        }\r\n        console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again.\");\r\n      }\r\n    },\r\n\r\n    confirmStatus(status) {\r\n      this.modal = \"confirm\";\r\n      this.confirm_status = status;\r\n    },\r\n\r\n    async changeStatus() {\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.put(\r\n          \"update/status/group/project/\" + this.groupId,\r\n          {\r\n            value: this.confirm_status,\r\n          }\r\n        );\r\n\r\n        // console.log(response.data);\r\n        if (response.data.success) {\r\n          this.$alert.toast(\"success\", response.data.message);\r\n          if (this.confirm_status == \"completed\") {\r\n            this.$router.push({\r\n              path: \"/mentor/activity/group/completed/\" + this.groupId,\r\n            });\r\n          } else {\r\n            this.$router.push({\r\n              path: \"/mentor/activity/group/in-progress/\" + this.groupId,\r\n            });\r\n          }\r\n        } else {\r\n          this.$alert.toast(\"error\", response.data.error);\r\n        }\r\n\r\n        this.modal = \"\";\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n        this.modal = \"\";\r\n      }\r\n    },\r\n\r\n    async changeProgress() {\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.put(\r\n          \"update/progress-status/group/project/\" + this.groupId,\r\n          {\r\n            value: this.group_info.progress_status,\r\n          }\r\n        );\r\n\r\n        if (response.data.success) {\r\n          this.$emit(\"check\", this.$route.params.key);\r\n          this.$alert.toast(\"success\", response.data.message);\r\n        } else {\r\n          this.$alert.toast(\"error\", response.data.error);\r\n        }\r\n\r\n        this.modal = \"\";\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.groupId = this.$route.params.submenu;\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.member-role {\r\n  font-size: 0.9em;\r\n  background: #dedede;\r\n  padding: 5px 10px;\r\n  margin-top: 10px !important;\r\n  display: inline;\r\n  border-radius: 20px;\r\n}\r\n\r\n.member-contribution {\r\n  margin-top: 10px;\r\n  font-size: 0.9em;\r\n  border-radius: 10px !important;\r\n  padding: 10px 5px;\r\n}\r\n\r\n.card-overlay {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: #ffffff72;\r\n  z-index: 99;\r\n  cursor: not-allowed;\r\n}\r\n\r\nh6 {\r\n  color: #f0ab54;\r\n}\r\n</style>"],"sourceRoot":""}]}