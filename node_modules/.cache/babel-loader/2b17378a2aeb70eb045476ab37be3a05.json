;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEM,KAAI,CAAC,MAAL,CAAY,GAAZ,CAAgB,SAAhB,CAFN;;AAAA;AAEX,gBAAA,QAFW;AAIb,gBAAA,OAJa,GAIH,QAAQ,CAAC,IAAT,CAAc,IAJX;AAMjB,gBAAA,OAAO,CAAC,OAAR,CAAgB,UAAC,OAAD,EAAa;AAC3B,kBAAA,KAAI,CAAC,OAAL,CAAa,IAAb,CAAkB,OAAO,CAAC,OAA1B;AACD,iBAFD,EANiB,CASjB;;AATiB;AAAA;;AAAA;AAAA;AAAA;AAWjB,gBAAA,OAAO,CAAC,GAAR,CAAY,YAAE,QAAd;;AAXiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAapB,KAdM;AAgBD,IAAA,YAhBC,0BAgBc;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACnB;AACA,gBAAA,MAAI,CAAC,KAAL,GAAa,EAAb;;AAEA,gBAAA,MAAI,CAAC,MAAL,CAAY,OAAZ;;AAJmB;AAAA;AAAA,uBAMM,MAAI,CAAC,MAAL,CAAY,IAAZ,CACrB,8BADqB,EAErB,MAAI,CAAC,KAFgB,CANN;;AAAA;AAMX,gBAAA,QANW;AAWjB,gBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,CAAnB,IAAwB,EAAxB;AACA,gBAAA,MAAI,CAAC,KAAL,CAAW,KAAX,CAAiB,CAAjB,IAAsB,EAAtB;;AACA,gBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,EAAoB,UAApB;;AAEA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C,EAfiB,CAgBjB;;;AAhBiB;AAAA;;AAAA;AAAA;AAAA;AAkBjB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AAnBiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqBpB,KArCM;AAuCP,IAAA,UAvCO,sBAuCI,EAvCJ,EAuCQ;AACb,WAAK,KAAL,GAAa,QAAb;AACA,WAAK,QAAL,GAAgB,EAAhB;AACD,KA1CM;AA4CD,IAAA,YA5CC,0BA4Cc;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACnB,gBAAA,MAAI,CAAC,KAAL,GAAa,EAAb;;AAEA,gBAAA,MAAI,CAAC,MAAL,CAAY,OAAZ;;AAHmB;AAAA;AAAA,uBAKM,MAAI,CAAC,MAAL,CAAY,MAAZ,CACrB,kCAAkC,MAAI,CAAC,QADlB,CALN;;AAAA;AAKX,gBAAA,QALW;;AASjB;AACA,gBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,EAAoB,UAApB;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C;;AAXiB;AAAA;;AAAA;AAAA;AAAA;AAajB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,kBAA3B;;AAdiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBpB;AA5DM,GArBI;AAoFb,EAAA,OApFa,qBAoFH;AACR,SAAK,YAAL;AACD;AAtFY,CAAf","sourcesContent":["<template>\r\n  <div id=\"ap\">\r\n    <div class=\"req-header p-2 py-1\">\r\n      <i\r\n        class=\"bi bi-bookmark-x-fill me-1 text-danger\"\r\n        v-if=\"data?.length == 0\"\r\n      ></i>\r\n      <i\r\n        class=\"bi bi-bookmark-check-fill me-1 text-success\"\r\n        v-if=\"data?.length != 0\"\r\n      ></i>\r\n      AP Score\r\n      <div class=\"float-end\">\r\n        <i class=\"fa-solid fa-add pointer\" @click=\"modal = 'add'\"></i>\r\n      </div>\r\n    </div>\r\n    <div class=\"req-body p-2 py-4\">\r\n      <!-- IF EMPTY  -->\r\n      <div class=\"row\" v-if=\"data?.length == 0\">\r\n        <div class=\"col text-center text-muted pb-4\" @click=\"modal = 'add'\">\r\n          Please add your ap score.\r\n        </div>\r\n      </div>\r\n      <div class=\"row row-cols-1\" v-if=\"data?.length != 0\">\r\n        <div class=\"col\">\r\n          <table class=\"table table-hover\">\r\n            <tbody>\r\n              <tr v-for=\"(i, index) in data\" :key=\"index\">\r\n                <td width=\"2%\">{{ index + 1 }}.</td>\r\n                <td>\r\n                  {{ i.subject }}\r\n                  <i\r\n                    class=\"fa-solid fa-close text-danger ms-3 pointer\"\r\n                    @click=\"deleteData(i.id)\"\r\n                  ></i>\r\n                </td>\r\n                <td width=\"5%\" class=\"text-end\">{{ i.value }}</td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n  <!-- Modal Document  -->\r\n  <transition name=\"pop\">\r\n    <div class=\"vue-modal vue-modal-md\" v-if=\"modal == 'add'\">\r\n      <form @submit.prevent=\"handleSubmit\" method=\"post\">\r\n        <h6>AP Score</h6>\r\n        <hr class=\"mt-1 mb-3\" />\r\n        <div class=\"row\">\r\n          <div class=\"col\">\r\n            <div class=\"mb-3\">\r\n              <multiselect\r\n                v-model=\"score.subject[0]\"\r\n                :options=\"options\"\r\n                placeholder=\"Select AP subject\"\r\n              >\r\n              </multiselect>\r\n            </div>\r\n            <div class=\"mb-3\">\r\n              <input-group>\r\n                <input\r\n                  type=\"number\"\r\n                  v-model=\"score.value[0]\"\r\n                  max=\"30\"\r\n                  class=\"form-mentoring form-control w-100\"\r\n                  required\r\n                  placeholder=\"fill in here ...\"\r\n                  id=\"score\"\r\n                />\r\n                <label for=\"score\">Score</label>\r\n              </input-group>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"text-center mt-3\">\r\n          <button type=\"submit\" class=\"btn-mentoring btn-sm py-1 bg-primary\">\r\n            <i class=\"fa-solid fa-save me-2\"></i>\r\n            Save\r\n          </button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </transition>\r\n\r\n  <!-- Delete  -->\r\n  <transition name=\"pop\">\r\n    <div class=\"vue-modal vue-modal-sm bg-primary\" v-if=\"modal == 'delete'\">\r\n      <div class=\"text-center\">\r\n        <i class=\"fa-solid fa-circle-exclamation fa-2xl\"></i>\r\n        <h5 class=\"mt-2\">Are you sure to delete?</h5>\r\n        <div class=\"mt-3\">\r\n          <button\r\n            class=\"btn-mentoring btn-warning btn-sm py-1 mx-1\"\r\n            @click=\"modal = ''\"\r\n          >\r\n            Cancel</button\r\n          ><button\r\n            class=\"btn-mentoring btn-outline-success btn-sm py-1 mx-1\"\r\n            @click=\"handleDelete\"\r\n          >\r\n            Yes\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </transition>\r\n</template>\r\n\r\n<script>\r\nimport Multiselect from \"vue-multiselect\";\r\n\r\nexport default {\r\n  name: \"apScore\",\r\n  props: {\r\n    data: Object,\r\n  },\r\n  components: {\r\n    Multiselect,\r\n  },\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      value: \"\",\r\n      score_id: \"\",\r\n      score: {\r\n        category: \"ap_score\",\r\n        subject: [\"\"],\r\n        value: [\"\"],\r\n      },\r\n      options: [],\r\n    };\r\n  },\r\n  methods: {\r\n    async getAPSubject() {\r\n      try {\r\n        const response = await this.$axios.get(\"ap/list\");\r\n\r\n        let subject = response.data.data;\r\n\r\n        subject.forEach((element) => {\r\n          this.options.push(element.subject);\r\n        });\r\n        // console.log(this.options);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async handleSubmit() {\r\n      // console.log(this.score);\r\n      this.modal = \"\";\r\n\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\r\n          \"student/academic/requirement\",\r\n          this.score\r\n        );\r\n\r\n        this.score.subject[0] = \"\";\r\n        this.score.value[0] = \"\";\r\n        this.$emit(\"check\", \"academic\");\r\n\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        // console.log(response);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.close();\r\n      }\r\n    },\r\n\r\n    deleteData(id) {\r\n      this.modal = \"delete\";\r\n      this.score_id = id;\r\n    },\r\n\r\n    async handleDelete() {\r\n      this.modal = \"\";\r\n\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.delete(\r\n          \"student/academic/requirement/\" + this.score_id\r\n        );\r\n\r\n        // console.log(response.data);\r\n        this.$emit(\"check\", \"academic\");\r\n        this.$alert.toast(\"success\", response.data.message);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.getAPSubject();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nlabel::after {\r\n  content: \"\";\r\n}\r\n</style>"],"sourceRoot":""}]}