AElB;AACD,OALH,EAMG,KANH,CAMS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OARH;AASD,KA5BM;AA8BP,IAAA,UA9BO,wBA8BM;AAAA;;AACX,WAAK,MAAL,CAAY,OAAZ;;AACA,UAAI,KAAK,MAAL,CAAY,IAAZ,IAAoB,EAAxB,EAA4B;AAC1B,aAAK,OAAL;AACA,aAAK,MAAL,CAAY,GAAZ,GAAkB,KAAlB;AACA,aAAK,MAAL,CAAY,KAAZ;AACF,OAJA,MAIO;AACL,aAAK,MAAL,CACG,GADH,CACO,KAAK,IAAL,GAAY,6BAAZ,GAA4C,KAAK,MAAL,CAAY,IAD/D,EACqE;AACjE,UAAA,OAAO,EAAE;AACP,YAAA,aAAa,EAAE,YAAY,YAAY,CAAC,OAAb,CAAqB,OAArB;AADpB;AADwD,SADrE,EAMG,IANH,CAMQ,UAAC,QAAD,EAAc;AAClB,UAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,UAAA,MAAI,CAAC,MAAL,GAAc,QAAQ,CAAC,IAAT,CAAc,IAA5B;AACA,UAAA,MAAI,CAAC,MAAL,CAAY,GAAZ,GAAkB,IAAlB,CAHkB,CAIlB;AACD,SAXH,EAYG,KAZH,CAYS,UAAC,KAAD,EAAW;AAChB,UAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,UAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,SAfH;AAgBF;AACD,KAtDM;AAwDP,IAAA,WAxDO,yBAwDO;AACZ,WAAK,MAAL,CAAY,GAAZ,GAAkB,KAAlB;AACA,WAAK,MAAL,CAAY,IAAZ,GAAmB,EAAnB;AACA,WAAK,OAAL;AACD,KA5DM;AA8DP,IAAA,MA9DO,kBA8DA,IA9DA,EA8DM;AACX,MAAA,MAAM,CAAC,IAAP,CAAY,IAAZ,EAAkB,QAAlB;AACD;AAhEM,GAZI;AA8Eb,EAAA,OA9Ea,qBA8EH;AACR,SAAK,OAAL;AACD;AAhFY,CAAf","sourcesContent":["<template>\r\n  <div id=\"group\">\r\n    <div class=\"card-white\">\r\n      <div class=\"row mb-2 justify-content-end\">\r\n        <div class=\"col-md-3\">\r\n          <input-group>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-mentoring form-control w-100\"\r\n              v-model=\"search.name\"\r\n              @change=\"searchData\"\r\n              placeholder=\"Search\"\r\n              id=\"search\"\r\n            />\r\n            <label for=\"search\">Search</label>\r\n          </input-group>\r\n          <span\r\n            class=\"badge bg-primary px-3 d-inline-block float-end mt-1\"\r\n            v-if=\"search.bar && search.name != ''\"\r\n          >\r\n            {{ search.name }}\r\n            <i class=\"fa-solid fa-close ms-3 pointer\" @click=\"closeSearch\"></i>\r\n          </span>\r\n        </div>\r\n        <!-- <div class=\"col-md-6 text-md-end text-center\">\r\n        <button class=\"btn-mentoring btn-type-1 me-2\">\r\n          Sync Students Data\r\n        </button>\r\n        <button class=\"btn-mentoring btn-type-2\">Add a New Student</button>\r\n      </div> -->\r\n      </div>\r\n\r\n      <div class=\"table-responsive\">\r\n        <table class=\"table table-bordered align-middle table-hover\">\r\n          <thead>\r\n            <tr class=\"text-center\">\r\n              <th>No</th>\r\n              <th>Projects Name</th>\r\n              <th>Projects Type</th>\r\n              <th>Progress Status</th>\r\n              <th>Status</th>\r\n              <th>Member</th>\r\n              <th>Mentor</th>\r\n              <!-- <th>More</th> -->\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr\r\n              class=\"text-start pointer\"\r\n              v-for=\"(i, index) in groups.data\"\r\n              :key=\"index\"\r\n            >\r\n              <td>{{ groups.from + parseInt(index) }}</td>\r\n              <td>\r\n                {{ i.project_name }}\r\n              </td>\r\n              <td>\r\n                {{ i.project_type }}\r\n              </td>\r\n              <td class=\"text-center\">\r\n                <div\r\n                  class=\"badge\"\r\n                  :class=\"\r\n                    i.progress_status == 'ahead'\r\n                      ? 'bg-success'\r\n                      : i.progress_status == 'on-track'\r\n                      ? 'bg-info text-dark'\r\n                      : 'bg-danger'\r\n                  \"\r\n                  style=\"text-transform: capitalize\"\r\n                >\r\n                  {{ i.progress_status }}\r\n                </div>\r\n                <i\r\n                  class=\"fa-solid fa-close\"\r\n                  v-if=\"i.progress_status == null\"\r\n                ></i>\r\n\r\n                <!-- {{ i.progress_status }} -->\r\n              </td>\r\n              <td nowrap>\r\n                <div class=\"col-6 text-end\">\r\n                  <i\r\n                    class=\"fa-solid text-success fa-check-circle\"\r\n                    v-if=\"i.status == 'completed'\"\r\n                  ></i>\r\n                  <i\r\n                    class=\"fa-solid text-warning fa-exclamation-triangle\"\r\n                    v-if=\"i.status == 'in progress'\"\r\n                  ></i>\r\n                  {{ i.status }}\r\n                </div>\r\n              </td>\r\n              <td nowrap>\r\n                {{ i.total_member }}\r\n              </td>\r\n              <td nowrap>\r\n                {{ i.total_mentor }}\r\n              </td>\r\n              <!-- <td>\r\n                <button\r\n                  class=\"btn-mentoring py-1 bg-secondary\"\r\n                  @click=\"goLink(i.location_link)\"\r\n                >\r\n                  Detail <i class=\"fa-solid fa-paper-plane ms-2\"></i>\r\n                </button>\r\n              </td> -->\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <div class=\"text-center\" v-if=\"groups.from == null\">\r\n        <hr />\r\n        <!-- {{ groups.data }} -->\r\n        <h6>Sorry, data is not found</h6>\r\n      </div>\r\n      <nav class=\"mt-2\" v-if=\"groups.from != null\">\r\n        <ul class=\"pagination justify-content-center\">\r\n          <li class=\"page-item\" v-if=\"groups.current_page != 1\">\r\n            <a class=\"page-link\" @click=\"getPage(groups.prev_page_url)\">\r\n              <i class=\"fa-solid fa-chevron-left\"></i>\r\n            </a>\r\n          </li>\r\n          <div v-for=\"i in groups.last_page\" :key=\"i\">\r\n            <li\r\n              class=\"page-item\"\r\n              v-if=\"groups.current_page - 2 < i && groups.current_page + 2 > i\"\r\n            >\r\n              <a\r\n                class=\"page-link\"\r\n                :class=\"groups.current_page == i ? 'bg-primary text-white' : ''\"\r\n                href=\"#\"\r\n                @click=\"getPage(groups.path + '?page=' + i)\"\r\n                >{{ i }}</a\r\n              >\r\n            </li>\r\n          </div>\r\n          <li class=\"page-item\" v-if=\"groups.current_page != groups.last_page\">\r\n            <a class=\"page-link\" @click=\"getPage(groups.next_page_url)\">\r\n              <i class=\"fa-solid fa-chevron-right\"></i>\r\n            </a>\r\n          </li>\r\n        </ul>\r\n      </nav>\r\n    </div>\r\n\r\n    <!-- <div class=\"vue-modal-overlay\"></div> -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"group\",\r\n  data() {\r\n    return {\r\n      groups: [],\r\n      // participant: [],\r\n      search: {\r\n        bar: false,\r\n        name: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    getData() {\r\n      this.$alert.loading();\r\n      this.$axios\r\n        .get(this.$url + \"list/group-project\")\r\n        .then((response) => {\r\n          this.$alert.close();\r\n          this.groups = response.data.data;\r\n          // this.participant = response.data.group_participant;\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$alert.close();\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    getPage(link) {\r\n      alert(link);\r\n      this.$axios\r\n        .get(link)\r\n        .then((response) => {\r\n          this.groups = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    searchData() {\r\n      this.$alert.loading();\r\n      if (this.search.name == \"\") {\r\n        this.getData();\r\n        this.search.bar = false;\r\n        this.$alert.close();\r\n      } else {\r\n        this.$axios\r\n          .get(this.$url + \"list/group-project?keyword=\" + this.search.name, {\r\n            headers: {\r\n              Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n            },\r\n          })\r\n          .then((response) => {\r\n            this.$alert.close();\r\n            this.groups = response.data.data;\r\n            this.search.bar = true;\r\n            // console.log(response);\r\n          })\r\n          .catch((error) => {\r\n            this.$alert.close();\r\n            console.log(error);\r\n          });\r\n      }\r\n    },\r\n\r\n    closeSearch() {\r\n      this.search.bar = false;\r\n      this.search.name = \"\";\r\n      this.getData();\r\n    },\r\n\r\n    goLink(link) {\r\n      window.open(link, \"_blank\");\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":""}]}