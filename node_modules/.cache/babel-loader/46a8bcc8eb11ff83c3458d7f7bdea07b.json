AA;AAAA;AAAA;AAWf,KAZM;AAcD,IAAA,OAdC,mBAcO,IAdP,EAca;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEO,MAAI,CAAC,MAAL,CAAY,GAAZ,CAAgB,IAAhB,CAFP;;AAAA;AAEV,gBAAA,QAFU;AAIhB,gBAAA,MAAI,CAAC,IAAL,GAAY,QAAQ,CAAC,IAAT,CAAc,IAA1B,CAJgB,CAKhB;;AALgB;AAAA;;AAAA;AAAA;AAAA;AAOhB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AAPgB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASnB,KAvBM;AAyBP,IAAA,aAzBO,yBAyBO,EAzBP,EAyBW;AAChB,WAAK,UAAL,GAAkB,EAAlB;AACA,WAAK,KAAL,GAAa,QAAb;AACD,KA5BM;AA8BD,IAAA,YA9BC,0BA8Bc;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACnB,gBAAA,MAAI,CAAC,KAAL,GAAa,EAAb;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,OAAZ;;AAFmB;AAAA;AAAA,uBAIM,MAAI,CAAC,MAAL,CAAY,GAAZ,CACrB,+BAA+B,MAAI,CAAC,UADf,EAErB;AACE,kBAAA,MAAM,EAAE;AADV,iBAFqB,CAJN;;AAAA;AAIX,gBAAA,QAJW;;AAWjB,gBAAA,MAAI,CAAC,OAAL;;AAEA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C,EAbiB,CAcjB;;;AAdiB;AAAA;;AAAA;AAAA;AAAA;AAgBjB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,mBAA3B;;AAjBiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBpB;AAjDM,GAZI;AAgEb,EAAA,OAhEa,qBAgEH;AACR,SAAK,OAAL;AACD,GAlEY;AAoEb,EAAA,OApEa,qBAoEH;AACR,QAAI,KAAK,WAAT,EAAsB;AACpB,WAAK,OAAL;AACF;AACD;AAxEY,CAAf","sourcesContent":["<template>\r\n  <div id=\"pending\">\r\n    <div class=\"container\">\r\n      <!-- Empty  -->\r\n      <div class=\"row\" v-if=\"data.data?.length == 0\">\r\n        <div class=\"col py-4 text-center\">\r\n          <span class=\"text-muted\">No meeting yet.</span>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Data  -->\r\n      <div class=\"table-responsive\" v-if=\"data.data?.length != 0\">\r\n        <table class=\"table\">\r\n          <thead>\r\n            <tr class=\"text-center\">\r\n              <th width=\"3%\">No</th>\r\n              <th>Call With</th>\r\n              <th>Subject</th>\r\n              <th>Date</th>\r\n              <th>Time</th>\r\n              <th>Status</th>\r\n              <th>Action</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr\r\n              v-for=\"(i, index) in data.data\"\r\n              :key=\"index\"\r\n              class=\"text-center align-middle\"\r\n            >\r\n              <td>{{ index + 1 }}</td>\r\n              <td class=\"text-start\" nowrap style=\"text-transform: capitalize\">\r\n                {{\r\n                  i.users.first_name +\r\n                  \" \" +\r\n                  i.users.last_name +\r\n                  \" - \" +\r\n                  i.call_with\r\n                }}\r\n              </td>\r\n              <td nowrap style=\"text-transform: capitalize\">{{ i.module }}</td>\r\n              <td nowrap>{{ $customDate.date(i.call_date) }}</td>\r\n              <td nowrap>{{ $customDate.time(i.call_date) }}</td>\r\n              <td style=\"text-transform: capitalize\">\r\n                <small class=\"text-info\">\r\n                  <i class=\"fa-solid fa-clock\"></i>\r\n                  {{ i.mt_confirm_status }}\r\n                </small>\r\n              </td>\r\n              <td>\r\n                <button\r\n                  class=\"btn-mentoring btn-sm btn-type-1 mx-1 mt-1 py-1\"\r\n                  @click=\"cancelMeeting(i.id)\"\r\n                >\r\n                  Cancel\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n\r\n      <!-- Pagination  -->\r\n      <nav class=\"mt-3\" v-if=\"data.from != null\">\r\n        <ul class=\"pagination justify-content-center\">\r\n          <li class=\"page-item\" v-if=\"data.current_page != 1\">\r\n            <a class=\"page-link\" @click=\"getPage(data.links[0].url)\">\r\n              <i class=\"fa-solid fa-chevron-left\"></i>\r\n            </a>\r\n          </li>\r\n          <div v-for=\"(i, index) in data.last_page\" :key=\"index\">\r\n            <li\r\n              class=\"page-item\"\r\n              v-if=\"data.current_page - 2 < i && data.current_page + 2 > i\"\r\n            >\r\n              <a\r\n                class=\"page-link\"\r\n                :class=\"data.current_page == i ? 'bg-primary text-white' : ''\"\r\n                href=\"#\"\r\n                @click=\"getPage(data.path + '?page=' + i)\"\r\n                >{{ i }}</a\r\n              >\r\n            </li>\r\n          </div>\r\n          <li class=\"page-item\" v-if=\"data.current_page != data.last_page\">\r\n            <a class=\"page-link\" @click=\"getPage(data.next_page_url)\">\r\n              <i class=\"fa-solid fa-chevron-right\"></i>\r\n            </a>\r\n          </li>\r\n        </ul>\r\n      </nav>\r\n    </div>\r\n\r\n    <!-- MODAL -->\r\n\r\n    <!-- Cancel Meeting  -->\r\n    <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n    <transition name=\"pop\">\r\n      <div class=\"vue-modal vue-modal-sm bg-primary\" v-if=\"modal == 'cancel'\">\r\n        <div class=\"text-center\">\r\n          <i class=\"fa-solid fa-circle-exclamation fa-2xl\"></i>\r\n          <h5 class=\"mt-2\">Are you sure to cancel this meeting?</h5>\r\n          <div class=\"mt-3\">\r\n            <button\r\n              class=\"btn-mentoring btn-warning btn-sm py-1 mx-1\"\r\n              @click=\"modal = ''\"\r\n            >\r\n              Cancel</button\r\n            ><button\r\n              class=\"btn-mentoring btn-outline-success btn-sm py-1 mx-1\"\r\n              @click=\"handleCancel\"\r\n            >\r\n              Yes\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"pending\",\r\n  props: {\r\n    new_meeting: String,\r\n  },\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      meeting_id: \"\",\r\n      data: [],\r\n    };\r\n  },\r\n  methods: {\r\n    async getData() {\r\n      try {\r\n        const response = await this.$axios.get(\r\n          \"student/list/activities/1-on-1-call/pending\"\r\n        );\r\n\r\n        this.data = response.data.data;\r\n        // console.log(response.data.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    async getPage(link) {\r\n      try {\r\n        const response = await this.$axios.get(link);\r\n\r\n        this.data = response.data.data;\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n      }\r\n    },\r\n\r\n    cancelMeeting(id) {\r\n      this.meeting_id = id;\r\n      this.modal = \"cancel\";\r\n    },\r\n\r\n    async handleCancel() {\r\n      this.modal = \"\";\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.put(\r\n          \"student/cancel/activities/\" + this.meeting_id,\r\n          {\r\n            person: \"student\",\r\n          }\r\n        );\r\n\r\n        this.getData();\r\n\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        // console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again.\");\r\n      }\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.getData();\r\n  },\r\n\r\n  updated() {\r\n    if (this.new_meeting) {\r\n      this.getData();\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":""}]}