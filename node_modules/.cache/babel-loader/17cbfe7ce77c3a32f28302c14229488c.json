C,GAAR,CAAY,KAAZ;AACD,OAVH;AAWD,KAdM;AAgBP,IAAA,OAhBO,mBAgBC,IAhBD,EAgBO;AAAA;;AACZ,WAAK,MAAL,CACG,GADH,CACO,IADP,EAEG,IAFH,CAEQ,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,MAAL,GAAc,QAAQ,CAAC,IAAT,CAAc,IAA5B,CADkB,CAElB;AACD,OALH,EAMG,KANH,CAMS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OARH;AASD,KA1BM;AA4BP,IAAA,UA5BO,wBA4BM;AAAA;;AACX,WAAK,MAAL,CAAY,OAAZ;AACA,WAAK,MAAL,CACG,GADH,CACO,KAAK,IAAL,GAAY,+BAAZ,GAA8C,KAAK,MAAL,CAAY,IADjE,EAEG,IAFH,CAEQ,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,QAAA,MAAI,CAAC,MAAL,GAAc,QAAQ,CAAC,IAAT,CAAc,IAA5B;AACA,QAAA,MAAI,CAAC,MAAL,CAAY,GAAZ,GAAkB,IAAlB,CAHkB,CAIlB;AACD,OAPH,EAQG,KARH,CAQS,UAAC,KAAD,EAAW;AAChB,QAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAXH;AAYD,KA1CM;AA4CP,IAAA,WA5CO,yBA4CO;AACZ,WAAK,MAAL,CAAY,GAAZ,GAAkB,KAAlB;AACA,WAAK,MAAL,CAAY,IAAZ,GAAmB,EAAnB;AACA,WAAK,OAAL;AACD;AAhDM,GAjBI;AAmEb,EAAA,OAnEa,qBAmEH;AACR,SAAK,OAAL;AACD;AArEY,CAAf","sourcesContent":["<template>\r\n  <div id=\"webinar\">\r\n    <transition name=\"fade\">\r\n      <div class=\"\" v-if=\"menus.submenu == ''\">\r\n        <div class=\"row my-4\">\r\n          <div class=\"col-md-6 text-start\">\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-mentoring\"\r\n              v-model=\"search.name\"\r\n              @change=\"searchData\"\r\n              placeholder=\"Search\"\r\n            />\r\n            <br />\r\n            <span\r\n              class=\"badge bg-primary px-3 d-inline-block\"\r\n              v-if=\"search.bar\"\r\n            >\r\n              {{ search.name }}\r\n              <i\r\n                class=\"fa-solid fa-close ms-3 pointer\"\r\n                @click=\"closeSearch\"\r\n              ></i>\r\n            </span>\r\n          </div>\r\n          <div class=\"col-md-6 text-md-end text-center\">\r\n            <button\r\n              class=\"btn-mentoring btn-type-2\"\r\n              @click=\"this.$router.push({ path: '/admin/events/add' })\"\r\n            >\r\n              Add a New Event\r\n            </button>\r\n          </div>\r\n        </div>\r\n        <!-- {{ events }} -->\r\n        <div class=\"card-white\">\r\n          <div class=\"table-responsive\">\r\n            <table class=\"table align-middle table-hover\">\r\n              <thead>\r\n                <tr class=\"text-center\">\r\n                  <th width=\"2%\">No</th>\r\n                  <th>Event Name</th>\r\n                  <th>Participants</th>\r\n                  <th>Status</th>\r\n                  <th>Date & Time</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr\r\n                  class=\"text-center pointer\"\r\n                  v-for=\"(i, index) in events.data\"\r\n                  :key=\"index\"\r\n                  @click=\"\r\n                    this.$router.push({ path: '/admin/events/detail/' + i.id })\r\n                  \"\r\n                >\r\n                  <td>{{ events.from + index }}</td>\r\n                  <td>{{ i.dtl_name }}</td>\r\n                  <td>{{ i.student_activities_count }}</td>\r\n                  <td>\r\n                    <i\r\n                      class=\"fa-regular fa-circle-check text-success\"\r\n                      v-if=\"i.status == 'active'\"\r\n                    ></i>\r\n                    <i\r\n                      class=\"fa-regular fa-circle-xmark text-danger\"\r\n                      v-if=\"i.status != 'active'\"\r\n                    ></i>\r\n                  </td>\r\n                  <td>{{ $customDate.date(i.created_at) }}</td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n          <div class=\"text-center\" v-if=\"events.from == null\">\r\n            <hr />\r\n            <h6>Sorry, data is not found</h6>\r\n          </div>\r\n          <nav class=\"mt-2\" v-if=\"events.from != null\">\r\n            <ul class=\"pagination justify-content-center\">\r\n              <li class=\"page-item\" v-if=\"events.current_page != 1\">\r\n                <a class=\"page-link\" @click=\"getPage(events.links[0].url)\">\r\n                  <i class=\"fa-solid fa-chevron-left\"></i>\r\n                </a>\r\n              </li>\r\n              <div v-for=\"i in events.last_page\" :key=\"i\">\r\n                <li\r\n                  class=\"page-item\"\r\n                  v-if=\"\r\n                    events.current_page - 2 < i && events.current_page + 2 > i\r\n                  \"\r\n                >\r\n                  <a\r\n                    class=\"page-link\"\r\n                    :class=\"\r\n                      events.current_page == i ? 'bg-primary text-white' : ''\r\n                    \"\r\n                    href=\"#\"\r\n                    @click=\"getPage(events.path + '?page=' + i)\"\r\n                    >{{ i }}</a\r\n                  >\r\n                </li>\r\n              </div>\r\n              <li\r\n                class=\"page-item\"\r\n                v-if=\"events.current_page != events.last_page\"\r\n              >\r\n                <a class=\"page-link\" @click=\"getPage(events.next_page_url)\">\r\n                  <i class=\"fa-solid fa-chevron-right\"></i>\r\n                </a>\r\n              </li>\r\n            </ul>\r\n          </nav>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n\r\n    <transition name=\"fade\">\r\n      <v-event-detail\r\n        :menus=\"menus\"\r\n        v-if=\"menus.menu == 'events' && menus.submenu != ''\"\r\n      ></v-event-detail>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport eventDetail from \"@/components/admin/event/detail\";\r\n\r\nexport default {\r\n  name: \"event\",\r\n  props: {\r\n    menus: Object,\r\n  },\r\n  components: {\r\n    \"v-event-detail\": eventDetail,\r\n  },\r\n  data() {\r\n    return {\r\n      events: [],\r\n      search: {\r\n        bar: false,\r\n        name: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    getData() {\r\n      this.$alert.loading();\r\n      this.$axios\r\n        .get(this.$url + \"list/programme/event\")\r\n        .then((response) => {\r\n          this.$alert.close();\r\n          this.events = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$alert.close();\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    getPage(link) {\r\n      this.$axios\r\n        .get(link)\r\n        .then((response) => {\r\n          this.events = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    searchData() {\r\n      this.$alert.loading();\r\n      this.$axios\r\n        .get(this.$url + \"list/programme/event?keyword=\" + this.search.name)\r\n        .then((response) => {\r\n          this.$alert.close();\r\n          this.events = response.data.data;\r\n          this.search.bar = true;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$alert.close();\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    closeSearch() {\r\n      this.search.bar = false;\r\n      this.search.name = \"\";\r\n      this.getData();\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":""}]}