OAlBO,mBAkBC,IAlBD,EAkBO;AAAA;;AACZ,WAAK,MAAL,CACG,GADH,CACO,IADP,EAEG,IAFH,CAEQ,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,KAAL,GAAa,QAAQ,CAAC,IAAT,CAAc,IAA3B,CADkB,CAElB;AACD,OALH,EAMG,KANH,CAMS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OARH;AASD,KA5BM;AA8BP,IAAA,UA9BO,wBA8BM;AAAA;;AACX,WAAK,MAAL,CAAY,OAAZ;AACA,WAAK,MAAL,CACG,GADH,CACO,KAAK,IAAL,GAAY,gCAAZ,GAA+C,KAAK,MAAL,CAAY,IADlE,EAEG,IAFH,CAEQ,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,QAAA,MAAI,CAAC,KAAL,GAAa,QAAQ,CAAC,IAAT,CAAc,IAA3B;AACA,QAAA,MAAI,CAAC,MAAL,CAAY,GAAZ,GAAkB,IAAlB;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,QAAZ;AACD,OAPH,EAQG,KARH,CAQS,UAAC,KAAD,EAAW;AAChB,QAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAXH;AAYD,KA5CM;AA8CP,IAAA,WA9CO,yBA8CO;AACZ,WAAK,MAAL,CAAY,GAAZ,GAAkB,KAAlB;AACA,WAAK,MAAL,CAAY,IAAZ,GAAmB,EAAnB;AACA,WAAK,OAAL;AACD;AAlDM,GAbI;AAiEb,EAAA,OAjEa,qBAiEH;AACR,SAAK,OAAL;AACD;AAnEY,CAAf","sourcesContent":["<template>\r\n  <div id=\"successMail\">\r\n    <div class=\"row my-4\">\r\n      <div class=\"col-md-6 text-start\">\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-mentoring\"\r\n          v-model=\"search.name\"\r\n          @change=\"searchData\"\r\n          placeholder=\"Search\"\r\n        />\r\n        <br />\r\n        <span class=\"badge bg-primary px-3 d-inline-block\" v-if=\"search.bar\">\r\n          {{ search.name }}\r\n          <i class=\"fa-solid fa-close ms-3 pointer\" @click=\"closeSearch\"></i>\r\n        </span>\r\n      </div>\r\n      <!-- <div class=\"col-md-6 text-md-end text-center\">\r\n        <button class=\"btn-mentoring btn-type-1 me-2\">\r\n          Sync Students Data\r\n        </button>\r\n        <button class=\"btn-mentoring btn-type-2\">Add a New Student</button>\r\n      </div> -->\r\n    </div>\r\n    <div class=\"card-white\">\r\n      <!-- {{ mails }} -->\r\n      <div class=\"table-responsive\">\r\n        <table class=\"table align-middle table-hover\">\r\n          <thead>\r\n            <tr class=\"text-center\">\r\n              <th>No</th>\r\n              <th>Receiver</th>\r\n              <th>Sender</th>\r\n              <th>Subject</th>\r\n              <th>Date</th>\r\n              <th>Status</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr\r\n              class=\"text-center\"\r\n              v-for=\"(i, index) in mails.data\"\r\n              :key=\"index\"\r\n            >\r\n              <td>{{ mails.from + index }}</td>\r\n              <td>\r\n                <i class=\"fa-regular fa-user fa-fw\"></i> {{ i.recipient }}\r\n              </td>\r\n              <td><i class=\"fa-regular fa-user fa-fw\"></i> {{ i.sender }}</td>\r\n              <td>{{ i.subject }}</td>\r\n              <td>\r\n                <i class=\"fa-regular fa-calendar fa-fw\"></i>\r\n                {{ $customDate.date(i.date_sent) }}\r\n              </td>\r\n              <td class=\"pointer\">\r\n                <div\r\n                  class=\"text-primary\"\r\n                  v-if=\"i.error_status\"\r\n                  @click=\"checkDetail(i)\"\r\n                >\r\n                  <i class=\"fa-solid fa-triangle-exclamation fa-fw\"></i>\r\n                  Solved\r\n                </div>\r\n                <div class=\"text-success\" v-if=\"!i.error_status\">\r\n                  <i class=\"fa-regular fa-circle-check fa-fw\"></i>\r\n                  Success\r\n                </div>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <div class=\"text-center\" v-if=\"mails.from == null\">\r\n        <hr />\r\n        <h6>Sorry, data is not found</h6>\r\n      </div>\r\n      <nav class=\"mt-2\" v-if=\"mails.from != null\">\r\n        <ul class=\"pagination justify-content-center\">\r\n          <li class=\"page-item\" v-if=\"mails.current_page != 1\">\r\n            <a class=\"page-link\" @click=\"getPage(mails.links[0].url)\">\r\n              <i class=\"fa-solid fa-chevron-left\"></i>\r\n            </a>\r\n          </li>\r\n          <div v-for=\"(i, index) in mails.last_page\" :key=\"index\">\r\n            <li\r\n              class=\"page-item\"\r\n              v-if=\"mails.current_page - 2 < i && mails.current_page + 2 > i\"\r\n            >\r\n              <a\r\n                class=\"page-link\"\r\n                :class=\"mails.current_page == i ? 'bg-primary text-white' : ''\"\r\n                href=\"#\"\r\n                @click=\"getPage(mails.path + '?page=' + i)\"\r\n                >{{ i }}</a\r\n              >\r\n            </li>\r\n          </div>\r\n          <li class=\"page-item\" v-if=\"mails.current_page != mails.last_page\">\r\n            <a class=\"page-link\" @click=\"getPage(mails.next_page_url)\">\r\n              <i class=\"fa-solid fa-chevron-right\"></i>\r\n            </a>\r\n          </li>\r\n        </ul>\r\n      </nav>\r\n    </div>\r\n\r\n    <transition name=\"fade\">\r\n      <div class=\"\" v-if=\"detail\">\r\n        <div class=\"vue-modal vue-modal-md\">\r\n          <h6>Error Description</h6>\r\n          <hr class=\"my-0 mb-2\" />\r\n          <p>\r\n            {{ detailData.error_message }}\r\n          </p>\r\n        </div>\r\n        <div class=\"vue-modal-overlay\" @click=\"detail = false\"></div>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"successMail\",\r\n  data() {\r\n    return {\r\n      mails: [],\r\n      search: {\r\n        bar: false,\r\n        name: \"\",\r\n      },\r\n      detail: false,\r\n      detalData: [],\r\n    };\r\n  },\r\n  methods: {\r\n    checkDetail(i) {\r\n      this.detailData = i;\r\n      this.detail = true;\r\n    },\r\n\r\n    getData() {\r\n      this.$axios\r\n        .get(this.$url + \"list/mail/log/success\")\r\n        .then((response) => {\r\n          this.mails = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    getPage(link) {\r\n      this.$axios\r\n        .get(link)\r\n        .then((response) => {\r\n          this.mails = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    searchData() {\r\n      this.$alert.loading();\r\n      this.$axios\r\n        .get(this.$url + \"list/mail/log/success?keyword=\" + this.search.name)\r\n        .then((response) => {\r\n          this.$alert.close();\r\n          this.mails = response.data.data;\r\n          this.search.bar = true;\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$alert.close();\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    closeSearch() {\r\n      this.search.bar = false;\r\n      this.search.name = \"\";\r\n      this.getData();\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":""}]}