;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OARH;AASD,KAXM;AAaP,IAAA,OAbO,mBAaC,IAbD,EAaO;AAAA;;AACZ,WAAK,MAAL,CACG,GADH,CACO,IADP,EAEG,IAFH,CAEQ,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,KAAL,GAAa,QAAQ,CAAC,IAAT,CAAc,IAA3B,CADkB,CAElB;AACD,OALH,EAMG,KANH,CAMS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OARH;AASD,KAvBM;AAyBP,IAAA,UAzBO,wBAyBM;AAAA;;AACX,WAAK,MAAL,CAAY,OAAZ;AACA,WAAK,MAAL,CACG,GADH,CACO,KAAK,IAAL,GAAY,gCAAZ,GAA+C,KAAK,MAAL,CAAY,IADlE,EAEG,IAFH,CAEQ,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,QAAA,MAAI,CAAC,KAAL,GAAa,QAAQ,CAAC,IAAT,CAAc,IAA3B;AACA,QAAA,MAAI,CAAC,MAAL,CAAY,GAAZ,GAAkB,IAAlB,CAHkB,CAIlB;AACD,OAPH,EAQG,KARH,CAQS,UAAC,KAAD,EAAW;AAChB,QAAA,MAAI,CAAC,MAAL,CAAY,KAAZ;;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAXH;AAYD,KAvCM;AAyCP,IAAA,WAzCO,yBAyCO;AACZ,WAAK,MAAL,CAAY,GAAZ,GAAkB,KAAlB;AACA,WAAK,MAAL,CAAY,IAAZ,GAAmB,EAAnB;AACA,WAAK,OAAL;AACD;AA7CM,GAjBI;AAgEb,EAAA,OAhEa,qBAgEH;AACR,SAAK,OAAL;AACD;AAlEY,CAAf","sourcesContent":["<template>\r\n  <div id=\"paid\">\r\n    <div class=\"row mb-4\">\r\n      <div class=\"col-md-6 text-start\">\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-mentoring\"\r\n          v-model=\"search.name\"\r\n          @change=\"searchData\"\r\n          placeholder=\"Search\"\r\n        />\r\n        <br />\r\n        <span class=\"badge bg-primary px-3 d-inline-block\" v-if=\"search.bar\">\r\n          {{ search.name }}\r\n          <i class=\"fa-solid fa-close ms-3 pointer\" @click=\"closeSearch\"></i>\r\n        </span>\r\n      </div>\r\n    </div>\r\n    <div class=\"row\">\r\n      <!-- {{ paids }} -->\r\n      <div class=\"col\">\r\n        <table class=\"table table-hover pointer\">\r\n          <thead>\r\n            <tr>\r\n              <th width=\"1%\">No</th>\r\n              <th>Full Name</th>\r\n              <th>Service</th>\r\n              <th>Date</th>\r\n              <th>Proof of Payment</th>\r\n              <th>Status</th>\r\n              <th>Report</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"(i, index) in paids.data\" :key=\"index\">\r\n              <td>{{ paids.from + index }}</td>\r\n              <td>\r\n                {{\r\n                  i.student_activities.students.first_name +\r\n                  \" \" +\r\n                  i.student_activities.students.last_name\r\n                }}\r\n              </td>\r\n              <td>\r\n                <div v-if=\"!i.student_activities.programme_details\">\r\n                  {{ i.student_activities.prog_id == 1 ? \"1on1 Call\" : \"\" }}\r\n                </div>\r\n                <div v-if=\"i.student_activities.programme_details\">\r\n                  {{ i.student_activities.programme_details.dtl_name }}\r\n                </div>\r\n              </td>\r\n              <td>\r\n                <i class=\"fa-regular fa-calendar fa-fw\"></i>\r\n                {{ $customDate.date(i.created_at) }}\r\n              </td>\r\n              <td @click=\"check = true\">\r\n                <vue-feather\r\n                  type=\"search\"\r\n                  size=\"18\"\r\n                  class=\"float-start mt-1 me-3\"\r\n                ></vue-feather>\r\n                Check\r\n              </td>\r\n              <td style=\"text-transform: capitalize\">{{ i.status }}</td>\r\n              <td>\r\n                <!-- <a\r\n                  :href=\"$base_url + 'transaction/' + i.trx_id + '/invoice'\"\r\n                  target=\"_blank\"\r\n                  class=\"btn btn-sm btn-outline-info me-2\"\r\n                >\r\n                  <i class=\"fa-regular fa-file fa-fw\"></i>\r\n                  Invoice\r\n                </a> -->\r\n                <a\r\n                  :href=\"$base_url + 'transaction/' + i.trx_id + '/receipt'\"\r\n                  target=\"_blank\"\r\n                  class=\"btn btn-sm btn-outline-success\"\r\n                >\r\n                  <i class=\"fa-regular fa-file fa-fw\"></i>\r\n                  Receipt\r\n                </a>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n        <div class=\"text-center\" v-if=\"paids.from == null\">\r\n          <hr />\r\n          <h6>Sorry, data is not found</h6>\r\n        </div>\r\n        <v-pagination :datas=\"paids.from\" @result=\"getPage\" />\r\n      </div>\r\n    </div>\r\n\r\n    <transition name=\"fade\">\r\n      <div class=\"p-3\" v-if=\"check\">\r\n        <div class=\"vue-modal vue-modal-md\">\r\n          <h6>Proof of Payment</h6>\r\n          <hr />\r\n          <img src=\"https://picsum.photos/500\" class=\"w-100\" alt=\"\" />\r\n        </div>\r\n        <div class=\"vue-modal-overlay\" @click=\"check = false\"></div>\r\n      </div>\r\n    </transition>\r\n\r\n    <transition name=\"fade\">\r\n      <div class=\"\" v-if=\"report != ''\">\r\n        <div class=\"vue-modal vue-modal-lg\">\r\n          <v-invoice v-if=\"report == 'invoice'\" />\r\n          <v-receipt v-if=\"report == 'receipt'\" />\r\n        </div>\r\n        <div class=\"vue-modal-overlay\" @click=\"report = ''\"></div>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Invoice from \"@/components/admin/transaction/report/invoice\";\r\nimport Receipt from \"@/components/admin/transaction/report/receipt\";\r\n\r\nexport default {\r\n  name: \"paid\",\r\n  components: {\r\n    \"v-invoice\": Invoice,\r\n    \"v-receipt\": Receipt,\r\n  },\r\n  data() {\r\n    return {\r\n      search: {\r\n        bar: false,\r\n        name: \"\",\r\n      },\r\n      paids: [],\r\n      check: false,\r\n      report: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    getData() {\r\n      this.$axios\r\n        .get(this.$url + \"list/transaction/paid\")\r\n        .then((response) => {\r\n          this.paids = response.data.data;\r\n          console.log(response.data.data);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    getPage(link) {\r\n      this.$axios\r\n        .get(link)\r\n        .then((response) => {\r\n          this.paids = response.data.data;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    searchData() {\r\n      this.$alert.loading();\r\n      this.$axios\r\n        .get(this.$url + \"list/transaction/paid?keyword=\" + this.search.name)\r\n        .then((response) => {\r\n          this.$alert.close();\r\n          this.paids = response.data.data;\r\n          this.search.bar = true;\r\n          // console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.$alert.close();\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    closeSearch() {\r\n      this.search.bar = false;\r\n      this.search.name = \"\";\r\n      this.getData();\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.form-control,\r\n.btn {\r\n  box-shadow: none;\r\n}\r\n</style>"],"sourceRoot":""}]}