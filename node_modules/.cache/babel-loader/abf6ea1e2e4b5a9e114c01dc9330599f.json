CAAkB,OAAlB,EAA2B,mBAA3B;AACF;;AAvCiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAyCpB,KA1CM;AA4CP,IAAA,SA5CO,qBA4CG,CA5CH,EA4CM;AACX,WAAK,KAAL,GAAa,MAAb;AACA,WAAK,QAAL,GAAgB,CAAC,CAAC,EAAlB;AACA,WAAK,IAAL,CAAU,IAAV,GAAiB,CAAC,CAAC,SAAnB;;AACA,UAAI,CAAC,CAAC,eAAF,IAAqB,CAAC,CAAC,eAAF,CAAkB,MAAlB,GAA2B,CAApD,EAAuD;AACrD,aAAK,IAAL,CAAU,MAAV,GAAmB,CAAC,CAAC,eAAF,CAAkB,CAAlB,CAAnB;AACF,OAFA,MAEO;AACL,aAAK,IAAL,CAAU,MAAV,GAAmB;AACjB,UAAA,EAAE,EAAE,SADa;AAEjB,UAAA,QAAQ,EAAE,SAFO;AAGjB,UAAA,SAAS,EAAE;AAHM,SAAnB;AAKF;AACD,KAzDM;AA2DD,IAAA,YA3DC,0BA2Dc;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACf,gBAAA,SADe,GACH,EADG;AAEnB,gBAAA,SAAQ,GAAI;AACV,kBAAA,QAAQ,EAAE,MAAI,CAAC,QADL;AAEV,kBAAA,IAAI,EAAE,MAAI,CAAC,IAAL,CAAU;AAFN,iBAAZ;AAKA,gBAAA,MAAI,CAAC,KAAL,GAAa,EAAb;;AAEA,gBAAA,MAAI,CAAC,MAAL,CAAY,OAAZ;;AATmB;AAAA;AAAA,uBAWM,MAAI,CAAC,MAAL,CAAY,IAAZ,CACrB,sBADqB,EAErB,SAFqB,CAXN;;AAAA;AAWX,gBAAA,QAXW;AAgBjB,gBAAA,OAAO,CAAC,GAAR,CAAY,QAAQ,CAAC,IAArB;;AACA,gBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,EAAoB,MAApB;;AACA,oBAAI,QAAQ,CAAC,IAAT,CAAc,OAAlB,EAA2B;AACzB,kBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C;AACF,iBAFA,MAEO;AACL,kBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,KAA3C;AACF;;AAtBiB;AAAA;;AAAA;AAAA;AAAA;AAwBjB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AACA,gBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,EAAoB,MAApB;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,kBAA3B;;AA1BiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA4BpB,KAvFM;AAyFP,IAAA,WAzFO,uBAyFK,CAzFL,EAyFQ;AACb,WAAK,KAAL,GAAa,QAAb;AACA,WAAK,QAAL,GAAgB,CAAhB;AACD,KA5FM;AA8FD,IAAA,YA9FC,0BA8Fc;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACnB,gBAAA,MAAI,CAAC,KAAL,GAAa,EAAb;;AAEA,gBAAA,MAAI,CAAC,MAAL,CAAY,OAAZ;;AAHmB;AAAA;AAAA,uBAKM,MAAI,CAAC,MAAL,CAAY,MAAZ,CACrB,0BAA0B,MAAI,CAAC,QADV,CALN;;AAAA;AAKX,gBAAA,QALW;;AASjB;AACA,gBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,EAAoB,MAApB;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,QAAQ,CAAC,IAAT,CAAc,OAA3C;;AAXiB;AAAA;;AAAA;AAAA;AAAA;AAajB,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,QAAd;;AACA,gBAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,OAAlB,EAA2B,kBAA3B;;AAdiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBpB;AA9GM;AAhDI,CAAf","sourcesContent":["<template>\r\n  <div id=\"transcript\">\r\n    <div class=\"req-header p-2 py-1\">\r\n      <i\r\n        class=\"fa-solid fa-circle-xmark me-1 text-danger\"\r\n        v-if=\"data?.length == 0\"\r\n      ></i>\r\n      <i\r\n        class=\"fa-solid fa-circle-check me-1 text-success\"\r\n        v-if=\"data?.length != 0\"\r\n      ></i>\r\n      Transcript\r\n      <div class=\"float-end\">\r\n        <i class=\"fa-solid fa-add pointer\" @click=\"modal = 'files'\"></i>\r\n      </div>\r\n    </div>\r\n    <div class=\"req-body p-2 py-4\">\r\n      <!-- IF EMPTY  -->\r\n      <div class=\"row\" v-if=\"data?.length == 0\">\r\n        <div class=\"col text-center text-muted pb-4\" @click=\"modal = 'add'\">\r\n          Please upload your transcript here.\r\n        </div>\r\n      </div>\r\n      <div class=\"row row-cols-md-5 row-cols-1\">\r\n        <div class=\"col\" v-for=\"i in data\" :key=\"i\">\r\n          <div class=\"card shadow-sm docs h-100\">\r\n            <div class=\"card-body text-center d-flex align-items-center\">\r\n              <div class=\"w-100 justify-content-center docs-detail\">\r\n                <div class=\"d-block mb-2\">\r\n                  <i\r\n                    class=\"fa-solid fa-file-pdf fa-2x text-danger\"\r\n                    v-if=\"i.med_file_format == 'pdf'\"\r\n                  ></i>\r\n                  <i\r\n                    class=\"fa-solid fa-file-word fa-2x text-primary\"\r\n                    v-if=\"\r\n                      i.med_file_format == 'docx' || i.med_file_format == 'doc'\r\n                    \"\r\n                  ></i>\r\n                  <i\r\n                    class=\"fa-solid fa-file-excel fa-2x text-success\"\r\n                    v-if=\"i.med_file_format == 'xlsx'\"\r\n                  ></i>\r\n                  <i\r\n                    class=\"fa-solid fa-file-image fa-2x text-secondary\"\r\n                    v-if=\"\r\n                      i.med_file_format.toLowerCase() == 'png' ||\r\n                      i.med_file_format.toLowerCase() == 'jpeg' ||\r\n                      i.med_file_format.toLowerCase() == 'jpg'\r\n                    \"\r\n                  ></i>\r\n                </div>\r\n                {{ i.med_title }}\r\n              </div>\r\n              <div class=\"docs-content\">\r\n                <div class=\"\">\r\n                  <a\r\n                    :href=\"$base_url + i.med_file_path\"\r\n                    target=\"_blank\"\r\n                    class=\"btn btn-mentoring bg-white p-1 px-3 mx-1\"\r\n                    download\r\n                  >\r\n                    <i class=\"fa-solid fa-download text-primary\"></i>\r\n                  </a>\r\n                  <button\r\n                    class=\"btn btn-mentoring bg-white p-1 px-3 mx-1\"\r\n                    @click=\"editMedia(i)\"\r\n                  >\r\n                    <i class=\"fa-solid fa-edit text-info\"></i>\r\n                  </button>\r\n                  <button\r\n                    class=\"btn btn-mentoring bg-white p-1 px-3 mx-1\"\r\n                    @click=\"deleteMedia(i.id)\"\r\n                  >\r\n                    <i class=\"fa-solid fa-trash text-danger\"></i>\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"vue-modal-overlay\" v-if=\"modal != ''\" @click=\"modal = ''\"></div>\r\n  <!-- Modal Document  -->\r\n\r\n  <!-- Upload  -->\r\n  <transition name=\"pop\">\r\n    <div class=\"vue-modal vue-modal-md\" v-if=\"modal == 'files'\">\r\n      <h6>Upload New Transcript</h6>\r\n      <form @submit.prevent=\"handleUpload\" method=\"post\">\r\n        <div class=\"border p-2\" v-if=\"tabModal == 'new'\">\r\n          <div class=\"my-3\">\r\n            <input-group>\r\n              <input\r\n                type=\"text\"\r\n                class=\"form-mentoring form-control w-100\"\r\n                v-model=\"file.name\"\r\n                required\r\n                id=\"fileName\"\r\n                placeholder=\"fill in here ...\"\r\n              />\r\n              <label for=\"fileName\">File Name</label>\r\n            </input-group>\r\n          </div>\r\n          <div class=\"border mb-2\">\r\n            <div\r\n              @dragenter.prevent=\"toggleActive\"\r\n              @dragleave.prevent=\"toggleActive\"\r\n              @dragover.prevent\r\n              @drop.prevent=\"drop\"\r\n              :class=\"{ 'active-dropzone': active }\"\r\n              class=\"dropzone\"\r\n            >\r\n              <span>Drag or Drop File</span>\r\n              <span>OR</span>\r\n              <label for=\"dropzoneFile\">Select File</label>\r\n              <input\r\n                type=\"file\"\r\n                id=\"dropzoneFile\"\r\n                class=\"dropzoneFile\"\r\n                @change=\"selectedFile\"\r\n              />\r\n            </div>\r\n            <div class=\"my-2\">\r\n              <span class=\"file-info\">\r\n                File: {{ dropzoneFile.name }}\r\n                <i\r\n                  class=\"fa-solid fa-times text-danger ms-2 pointer\"\r\n                  v-if=\"dropzoneFile\"\r\n                  @click=\"deleteFile\"\r\n                ></i>\r\n              </span>\r\n            </div>\r\n          </div>\r\n          <div class=\"text-end\">\r\n            <button type=\"submit\" class=\"btn-mentoring btn-sm py-1 bg-primary\">\r\n              <i class=\"fa-solid fa-save me-2\"></i>\r\n              Submit\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </transition>\r\n\r\n  <!-- Edit  -->\r\n  <transition name=\"pop\">\r\n    <div class=\"vue-modal vue-modal-md\" v-if=\"modal == 'edit'\">\r\n      <h6>Update Transcript</h6>\r\n      <form @submit.prevent=\"handleUpdate\" method=\"post\">\r\n        <hr class=\"mt-1 mb-3\" />\r\n        <div class=\"mb-2\">\r\n          <input v-model=\"media_id\" hidden />\r\n          <input-group>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-mentoring form-control w-100\"\r\n              v-model=\"file.name\"\r\n              required\r\n              id=\"fileName\"\r\n              placeholder=\"fill in here ...\"\r\n            />\r\n            <label for=\"fileName\">File Name</label>\r\n          </input-group>\r\n        </div>\r\n\r\n        <div class=\"float-start\">\r\n          <button\r\n            type=\"button\"\r\n            class=\"btn-mentoring btn-sm py-1 bg-secondary mt-3\"\r\n            @click=\"modal = ''\"\r\n          >\r\n            Cancel\r\n          </button>\r\n        </div>\r\n        <div class=\"float-end mt-3\">\r\n          <button type=\"submit\" class=\"btn-mentoring btn-sm py-1 bg-primary\">\r\n            Submit\r\n          </button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </transition>\r\n\r\n  <!-- Delete  -->\r\n  <transition name=\"pop\">\r\n    <div class=\"vue-modal vue-modal-sm bg-primary\" v-if=\"modal == 'delete'\">\r\n      <div class=\"text-center\">\r\n        <i class=\"fa-solid fa-circle-exclamation fa-2xl\"></i>\r\n        <h5 class=\"mt-2\">Are you sure to delete?</h5>\r\n        <div class=\"mt-3\">\r\n          <button\r\n            class=\"btn-mentoring btn-warning btn-sm py-1 mx-1\"\r\n            @click=\"modal = ''\"\r\n          >\r\n            Cancel</button\r\n          ><button\r\n            class=\"btn-mentoring btn-outline-success btn-sm py-1 mx-1\"\r\n            @click=\"handleDelete\"\r\n          >\r\n            Yes\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </transition>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from \"vue\";\r\n\r\nexport default {\r\n  name: \"transcript\",\r\n  props: {\r\n    data: Object,\r\n  },\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n      tabModal: \"new\",\r\n      media_id: \"\",\r\n      file: {\r\n        uni_id: \"\",\r\n        name: \"\",\r\n        file_category: \"transcript\",\r\n        subject: \"\",\r\n        uploaded_file: [],\r\n      },\r\n    };\r\n  },\r\n\r\n  setup() {\r\n    const active = ref(false);\r\n    const toggleActive = () => {\r\n      active.value = !active.value;\r\n    };\r\n\r\n    let dropzoneFile = ref(\"\");\r\n    const drop = (e) => {\r\n      dropzoneFile.value = e.dataTransfer.files[0];\r\n    };\r\n\r\n    const selectedFile = () => {\r\n      dropzoneFile.value = document.querySelector(\".dropzoneFile\").files[0];\r\n    };\r\n\r\n    const deleteFile = () => {\r\n      dropzoneFile.value = \"\";\r\n    };\r\n\r\n    return {\r\n      active,\r\n      toggleActive,\r\n      dropzoneFile,\r\n      drop,\r\n      selectedFile,\r\n      deleteFile,\r\n    };\r\n  },\r\n  methods: {\r\n    async handleUpload() {\r\n      this.modal = \"\";\r\n\r\n      if (this.dropzoneFile) {\r\n        this.file.uploaded_file = this.dropzoneFile;\r\n      }\r\n\r\n      let form = new FormData();\r\n      form.append(\"uni_id\", this.file.uni_id);\r\n      form.append(\"name\", this.file.name);\r\n      form.append(\"uploaded_file\", this.file.uploaded_file);\r\n      form.append(\"file_category\", this.file.file_category);\r\n\r\n      // for (var pair of form.entries()) {\r\n      //   console.log(pair[0] + \", \" + pair[1]);\r\n      // }\r\n\r\n      this.$alert.loading();\r\n\r\n      try {\r\n        const response = await this.$axios.post(\r\n          \"student/document/requirement\",\r\n          form\r\n        );\r\n\r\n        this.file.name = \"\";\r\n        this.file.uploaded_file = [];\r\n\r\n        this.$emit(\"check\", \"file\");\r\n        this.$alert.toast(\"success\", response.data.message);\r\n        console.log(response.data);\r\n      } catch (e) {\r\n        console.log(e.response.data);\r\n        if (e.response.data.error.uploaded_file) {\r\n          this.$alert.toast(\"error\", e.response.data.error.uploaded_file[0]);\r\n        } else if (e.response.data.error.uni_id) {\r\n          this.$alert.toast(\"error\", e.response.data.error.uni_id[0]);\r\n        } else {\r\n          this.$alert.toast(\"error\", \"Please try again.\");\r\n        }\r\n      }\r\n    },\r\n\r\n    editMedia(i) {\r\n      this.modal = \"edit\";\r\n      this.media_id = i.id;\r\n      this.file.name = i.med_title;\r\n      if (i.uni_shortlisted && i.uni_shortlisted.length > 0) {\r\n        this.file.uni_id = i.uni_shortlisted[0];\r\n      } else {\r\n        this.file.uni_id = {\r\n          id: \"general\",\r\n          uni_name: \"General\",\r\n          uni_major: \"\",\r\n        };\r\n      }\r\n    },\r\n\r\n    async handleUpdate() {\r\n      let form_data = [];\r\n      form_data = {\r\n        media_id: this.media_id,\r\n        name: this.file.name,\r\n      };\r\n\r\n      this.modal = \"\";\r\n\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.post(\r\n          \"student/media/update\",\r\n          form_data\r\n        );\r\n\r\n        console.log(response.data);\r\n        this.$emit(\"check\", \"file\");\r\n        if (response.data.success) {\r\n          this.$alert.toast(\"success\", response.data.message);\r\n        } else {\r\n          this.$alert.toast(\"success\", response.data.error);\r\n        }\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$emit(\"check\", \"file\");\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n\r\n    deleteMedia(i) {\r\n      this.modal = \"delete\";\r\n      this.media_id = i;\r\n    },\r\n\r\n    async handleDelete() {\r\n      this.modal = \"\";\r\n\r\n      this.$alert.loading();\r\n      try {\r\n        const response = await this.$axios.delete(\r\n          \"student/media/delete/\" + this.media_id\r\n        );\r\n\r\n        // console.log(response.data);\r\n        this.$emit(\"check\", \"file\");\r\n        this.$alert.toast(\"success\", response.data.message);\r\n      } catch (e) {\r\n        console.log(e.response);\r\n        this.$alert.toast(\"error\", \"Please try again\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":""}]}