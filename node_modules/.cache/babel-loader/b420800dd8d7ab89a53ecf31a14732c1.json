ate>\r\n  <div id=\"progress\">\r\n    <div class=\"container p-1\">\r\n      <div class=\"row p-4\" v-if=\"group?.data?.length == 0\">\r\n        <div class=\"col text-center\">\r\n          <p>No project group yet.</p>\r\n        </div>\r\n      </div>\r\n      <div\r\n        class=\"row row-cols-md-2 row-cols-1 g-3 pb-4 align-items-lg-stretch\"\r\n        v-if=\"group?.data?.length != 0\"\r\n      >\r\n        <div class=\"col\" v-for=\"i in group.data\" :key=\"i\">\r\n          <div\r\n            class=\"card card-group border shadow-sm h-100\"\r\n            @click=\"detail(i.id)\"\r\n          >\r\n            <!-- <div class=\"card-status bg-success\">\r\n              <i class=\"fa-solid fa-thumbs-up mx-1\"></i>\r\n            </div> -->\r\n            <div class=\"card-body\">\r\n              <h5 class=\"mb-1\">{{ i.project_name }}</h5>\r\n              <span class=\"badge bg-secondary rounded-pill text-dark\">\r\n                {{ i.project_type }}\r\n              </span>\r\n              <div class=\"project-desc mt-2\">\r\n                {{ $customText.text(i.project_desc, 20) }}\r\n              </div>\r\n              <hr class=\"my-2\" />\r\n              <div class=\"row align-items-center\">\r\n                <div class=\"col-6\">\r\n                  <small>\r\n                    <i class=\"fa-solid fa-users me-2\"></i\r\n                    >{{ i.group_participant_count }} Members\r\n                  </small>\r\n                </div>\r\n                <div class=\"col-6 text-end\">\r\n                  <div\r\n                    class=\"badge\"\r\n                    :class=\"\r\n                      (i,\r\n                      progress_status == 'ahead' ? 'bg-success' : 'bg-danger')\r\n                    \"\r\n                  >\r\n                    {{ i.progress_status }}\r\n                  </div>\r\n                  <i class=\"fa-solid fa-arrow-right\"></i>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"progress\",\r\n  props: {\r\n    group: Object,\r\n  },\r\n  data() {\r\n    return {};\r\n  },\r\n  methods: {\r\n    detail(i) {\r\n      this.$router.push({ path: \"/mentor/activity/group/in-progress/\" + i });\r\n    },\r\n  },\r\n  created() {},\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.card-group {\r\n  position: relative;\r\n  overflow: hidden;\r\n  cursor: pointer;\r\n  border-radius: 15px;\r\n}\r\n\r\n.card-group:hover {\r\n  background: #223872;\r\n  color: #fff;\r\n}\r\n\r\n.card-status {\r\n  position: absolute;\r\n  right: -10px;\r\n  top: -10px;\r\n  padding: 10px 10px 4px 4px;\r\n  border-radius: 50%;\r\n  color: #fff;\r\n}\r\n</style>"],"sourceRoot":""}]}