n        <div class=\"col-12\">\r\n          <fieldset class=\"border p-2\" :class=\"add ? 'd-block' : 'd-none'\">\r\n            <legend class=\"float-none w-auto\" style=\"font-size: 0.8em\">\r\n              New Interest\r\n            </legend>\r\n            <div class=\"field\">\r\n              <div class=\"data\">\r\n                <v-select :options=\"['Canada', 'United States']\"></v-select>\r\n                <select\r\n                  id=\"single\"\r\n                  @change=\"interestCheck()\"\r\n                  v-model=\"interest.name\"\r\n                >\r\n                  <option value=\"value 1\">Value 1</option>\r\n                  <option value=\"value 2\">Value 2</option>\r\n                  <option value=\"value 3\">Value 3</option>\r\n                  <option value=\"new\">Others</option>\r\n                </select>\r\n\r\n                <!-- new -->\r\n                <input\r\n                  type=\"text\"\r\n                  name=\"\"\r\n                  class=\"form-mentoring w-100 form-control-sm mt-2\"\r\n                  placeholder=\"New Interest\"\r\n                  v-if=\"newField\"\r\n                />\r\n\r\n                <div class=\"text-end mt-3\">\r\n                  <button class=\"btn-mentoring btn-sm btn-outline-success py-1\">\r\n                    Save Changes\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </fieldset>\r\n          <table class=\"table table-bordered table-hover mt-2\">\r\n            <tbody>\r\n              <tr v-for=\"i in 3\" :key=\"i\">\r\n                <td width=\"5%\" align=\"center\">1</td>\r\n                <td>\r\n                  <div class=\"d-flex\">\r\n                    <div class=\"text\" style=\"width: 95%\">\r\n                      Lorem ipsum dolor sit amet consectetur adipisicing elit.\r\n                    </div>\r\n                    <div class=\"icon text-end\" style=\"width: 5%\">\r\n                      <i class=\"fa-solid fa-trash text-danger\"></i>\r\n                    </div>\r\n                  </div>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SlimSelect from \"slim-select\";\r\nexport default {\r\n  name: \"interest\",\r\n  data() {\r\n    return {\r\n      add: false,\r\n      newField: false,\r\n      interest: {\r\n        name: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    addCheck() {\r\n      this.add = !this.add;\r\n      if (this.add) {\r\n        new SlimSelect({\r\n          select: \"#single\",\r\n        });\r\n      }\r\n    },\r\n\r\n    interestCheck() {\r\n      if (this.interest.name == \"new\") {\r\n        this.newField = true;\r\n      }\r\n    },\r\n  },\r\n  updated() {\r\n    new SlimSelect({\r\n      select: \"#single\",\r\n    });\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.placeholder {\r\n  background-color: white !important;\r\n  color: #000;\r\n}\r\n</style>"],"sourceRoot":""}]}