                 <div class=\"row align-items-center g-1\">\r\n                        <div class=\"col-4 text-center p-3\">\r\n                          <img\r\n                            v-lazy=\"'https://picsum.photos/500/500'\"\r\n                            alt=\"\"\r\n                            class=\"u-speaker-img\"\r\n                          />\r\n                        </div>\r\n                        <div class=\"col-8 p-3\">\r\n                          <div class=\"u-speaker-name\">Full Name</div>\r\n                          <div class=\"u-speaker-label\">\r\n                            Software Engineer at PT ABCD\r\n                          </div>\r\n                          <div class=\"u-speaker-desc\">\r\n                            Lorem ipsum dolor sit amet consectetur, adipisicing\r\n                            elit. Saepe aut quo doloribus molestias fugit,\r\n                            pariatur odio accusantium amet adipisci, quidem\r\n                            architecto odit.\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"col-md-4 mt-md-0 mt-3\">\r\n            <div class=\"sticky-top\" style=\"top: 20px\">\r\n              <table class=\"table table-hover align-middle\">\r\n                <thead>\r\n                  <tr>\r\n                    <th colspan=\"2\">\r\n                      <vue-feather\r\n                        type=\"calendar\"\r\n                        class=\"float-start mt-0 me-2\"\r\n                      ></vue-feather>\r\n                      Date & Time\r\n                    </th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr>\r\n                    <td>24 Feb 2022</td>\r\n                    <td class=\"text-end\">\r\n                      09.00-12.00 <br />\r\n                      14.00-16.00\r\n                    </td>\r\n                  </tr>\r\n                  <tr>\r\n                    <td>25 Feb 2022</td>\r\n                    <td class=\"text-end\">\r\n                      09.00-12.00 <br />\r\n                      14.00-16.00\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n              <div class=\"card border-0 shadow-sm mb-3\">\r\n                <div class=\"u-price-title\">\r\n                  <vue-feather\r\n                    type=\"tag\"\r\n                    class=\"float-start mt-1 me-2 ms-1\"\r\n                  ></vue-feather>\r\n                  Price\r\n                </div>\r\n                <div class=\"u-price\">Rp. 4.500.000</div>\r\n                <div class=\"u-discount\">Rp. 5.000.000</div>\r\n              </div>\r\n              <button class=\"btn btn-join shadow-sm mt-3\">Join Now</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n<script>\r\nimport Header from \"@/components/layout/student/header\";\r\n\r\nexport default {\r\n  name: \"upcoming\",\r\n  props: {\r\n    tab: String,\r\n    eventData: String,\r\n  },\r\n  components: {\r\n    \"v-header\": Header,\r\n  },\r\n  data() {\r\n    return {\r\n      loading: true,\r\n      event: [],\r\n    };\r\n  },\r\n  methods: {\r\n    back() {\r\n      this.$router.push({\r\n        name: \"userDashboard\",\r\n        params: { newTab: this.tab },\r\n      });\r\n    },\r\n  },\r\n  created() {\r\n    document.title = \"Upcoming Event\";\r\n    setTimeout(() => {\r\n      this.event = JSON.parse(this.$route.params.eventData);\r\n      this.loading = false;\r\n    }, 500);\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.card-img {\r\n  height: 350px;\r\n  width: 100%;\r\n  overflow: hidden;\r\n}\r\n\r\n.card-img img {\r\n  width: 